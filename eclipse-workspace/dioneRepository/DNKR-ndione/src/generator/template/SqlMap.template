<mapper namespace="com.globaldenso.dnkr.integration.dao.$name$Dao">

  
  <sql id="select_01">

    SELECT
$column$
    FROM $table$

  </sql>

  
  <!-- ?? SQL?????? -->
  <select id="searchByCondition" resultMap="com.globaldenso.dnkr.domain.$name$Domain" parameterType="com.globaldenso.dnkr.domain.$name$Domain">

    SELECT *
    FROM (
    <include refid="select_01"/>
    ) AA
