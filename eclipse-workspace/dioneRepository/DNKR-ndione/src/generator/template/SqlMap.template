      $keyValue$  
  </select>
  

  <insert id="create" parameterType="com.globaldenso.dnkr.domain.$name$Domain">

    INSERT INTO $table$ (
$column$
    ) VALUES (
$values$
    )

  </insert>

  <update id="update" parameterType="com.globaldenso.dnkr.domain.$name$Domain">

    UPDATE $table$ SET
$updateValue$
    WHERE 1=1 
$keyValue$

  </update>

