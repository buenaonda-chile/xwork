function getFMSZ110(){


	if(!validationChk()){
		return;
	}

	var sKind		= "getFMSZ110";						
	var sMethodName = "service::psfaclib/selectFMSZ110.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_FMSZ110=ds_FMSZ110";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
		sArgument += "@LIB1=" + quote(Lib1);
		sArgument += "@LIB2=" + quote(Lib2);
		sArgument += "@COMPS=" + quote(ComboBoxPlant.Value); //사업부
		sArgument += "@TRNDT=" + quote(cmb_TRNDT.Value); //실적일자
		sArgument += "@PROCS=" + quote(ed_PROCS.Text); //작업공정
	var str = dialog("Common::PGMLOAD_FAC.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");
	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
		setMessage("1004");
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));
		setMessage("1003");	
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}
}

/********************************************************************** 
 * 내용설명 : insert, update, delete
 **********************************************************************/  
function setFMSZ110(){

	if(!validationChk()){
		return;
	}
		
	if(ds_FMSZ110.GetRowType(ds_FMSZ110.currow)=="insert" || ds_FMSZ110.GetRowType(ds_FMSZ110.currow)=="update" ){
		if(!ds_FMSZ110_CanRowPosChange()){
			return;
		}
	} 
	var sKind		= "setFMSZ110";						
	var sMethodName = "service::psfaclib/saveFMSZ110.do";    // 서버에서 호출 될 Method 명
	var sInDataSet  = "ds_FMSZ110=ds_FMSZ110:U";               //자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_FMSZ110_CHECK=ds_FMSZ110_CHECK";  	   // 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
		sArgument += " LIB2=" + quote(Lib2);
	http.Sync=true;
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");
	http.Sync=false;

}
/********************************************************************** 
 * 내용설명 : Transaction 리턴후 실행 함수
 * 주요변수 : 
 * 			- sMethodName	Transaction 고유 ID
 * 			- ErrorCode 	에러코드
 * 			- ErrorMSG 		에러 메세지
 **********************************************************************/ 
function callback(svcId, rtnCd, rtnMsg){
	if (ds_FMSZ110_CHECK.GetRowCount() > 0){
		for(var i=0;i<ds_FMSZ110_CHECK.GetRowCount();i++){
			alert("품번 : " +ds_FMSZ110_CHECK.GetColumn(i,"PSPNO") + " 적일자 : " +ds_FMSZ110_CHECK.GetColumn(i,"TRNDT") + " = " + ds_FMSZ110_CHECK.GetColumn(i,"RTMSG"));
		}
		getFMSZ110();  //error항목이 존재시에는 재조회
		return;
	}// tran 실패
	if(rtnCd<0){
		alert(setPopMessage("1002"));
		setMessage("1002");
		
	}else{
		alert(setPopMessage("1001"));
		setMessage("1001");	
	}
}

/********************************************************************** 
 * 내용설명 : validation 체크
 **********************************************************************/ 
 
function validationChk(){
	var result = true;
		ed_PROCS.BKColor="white";
		cmb_TRNDT.BKColor="white";
		
	if (length(trim(ed_PROCS.Text))==0){
		ed_PROCS.BKColor="paleturquoise";
		//alert("[알림]작업공정을  입력하세요.");
		alert(setPopMessage("6016"));//[알림]작업공정을  입력하세요.
		setMessage("6016");
		result = false;
	}else if(cmb_TRNDT.Value==""){
		cmb_TRNDT.BKColor="paleturquoise";
		//alert("[알림]실적일자를 입력하세요.");
		alert(setPopMessage("6062"));//[알림]실적일자를 입력하세요.
		setMessage("6062");
		result = false;
	}else if(cmb_TRNDT.Value!=""){
		http.Sync 	= true;
		var sKind		= "getMST090PF";		
		var sMethodName = "service::pscomlib/selectMST090PF.do"; 
		var sInDataSet  = "";
		var sOutDataSet = "ds_MST090PF=ds_MST090PF";
		var sArgument   = "";
			sArgument += " LIB2=" + quote(Lib2);
			sArgument += " COMPS=" + quote(ComboBoxPlant.Value); //사업부
			sArgument += " PRCDT=" + quote(cmb_TRNDT.Value); //작업일자
		http.Sync 	= true;
		Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
		http.Sync 	= false;
		if (ds_MST090PF.GetRowCount() < 1){
			alert(setPopMessage("4061"));
			setMessage("4061");	
			cmb_TRNDT.SetFocus();
			result = false;
		}
	}
	return result;
}

