function ed_PROCS_OnKillFocus(obj)
{
	if(length(trim(obj.text))!=0){
		http.usewaitcursor = false;
		var sKind		= "getBM001PR";						
		var sMethodName = "service::pscomlib/selectBM001PR.do";    // 서버에서 호출 될 Method 명
		var sInDataSet  = "";               //자료 저장 시 보낼 DataSet 명칭
		var sOutDataSet = "ds_BM001PR=ds_BM001PR";  	   // 조회 후 돌려 받을 DataSet 명칭
		var sArgument   = "";
			sArgument += " LIB1=" + quote(Lib1);
			sArgument += " PROCS=" + quote(ed_PROCS.Text); //공정코드
		http.Sync=true;
		Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
		http.Sync=false;
		http.usewaitcursor = true;
		if(ds_BM001PR.rowcount<1){
			//alert("[알림] PROCESS MA에 미등록된 공정코드입니다.");
			alert(setPopMessage("4746"));//[알림] PROCESS MA에 미등록된 공정코드입니다.
			setMessage("4746");
			ed_PROCS.SetFocus();
			return;		
		}else{
			http.usewaitcursor = false;
			var sKind		= "getFHD000PF";		
			var sMethodName = "service::psfaclib/selectFHD000.do"; 
			var sInDataSet  = "";
			var sOutDataSet = "ds_FHD000PF=ds_FHD000PF";
			var sArgument   = "";
				sArgument  += " LIB2=" + quote(Lib2);
				sArgument  += " COMPS=" + quote(ComboBoxPlant.Value);
				sArgument  += " PROCS=" + quote(ed_PROCS.Text);
			http.Sync=true;
			Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
			http.Sync=false;
			http.usewaitcursor = true;
			if (ds_FHD000PF.GetRowCount() < 1){
				alert(setPopMessage("1002"));
				setMessage("1002");	
				ed_PROCS.SetFocus();
				return;
			}
		}
	}

}

function getITNBR(tval){

	var sKind		= "getITNBR";		
	var sMethodName = "service::pscomlib/selectBM008PR.do"; 
	var sInDataSet  = "";
	var sOutDataSet = "ds_BM008PR=ds_BM008PR";
	var sArgument   = "";
		sArgument  += " LIB1=" + quote(Lib1);
		sArgument  += " ITNBR=" + quote(tval);
	http.Sync=true;
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	http.Sync=false;
	if(ds_BM008PR.rowcount < 1){
		return 0;	
	}
	else{
		return 1;
	}
}
function ds_FMSZ110_CanColumnChange(obj,nRow,strColumnID,varOldValue,varNewValue,nPivotIndex)
{
	if(strColumnID == "PSPNO"){
		if(varNewValue != ""){
		//if(varNewValue != "" && varOldValue != varNewValue){
			var tval=getITNBR(varNewValue);
			if(tval==0){
				//alert("[경고] Item Master에 미등록된 품번입니다.");  //[경고] Item Master에 미등록된 품번입니다.
				alert(setPopMessage("1509"));
				setMessage("1509");	
				return false;
			}else{
				if(ds_FMSZ110.FindRow("PSPNO",varNewValue)!="-1"){
					//alert("[경고]동일한 품번이 등록되어 있습니다.");
					alert(setPopMessage("4681"));//[경고]동일한 품번이 등록되어 있습니다.
					setMessage("4681");
					if(ds_FMSZ110.GetRowType(nRow)=="insert"){
						ds_FMSZ110.DeleteRow(nRow);
						ds_FMSZ110.SelectRow(ds_FMSZ110.FindRow("PSPNO",varNewValue));				
					}
					return false;
				}
			}
		}
	}
}

function ds_FMSZ110_CanRowPosChange(obj,nOldRow,nRow)
{
	if(ds_FMSZ110.GetRowType(ds_FMSZ110.currow)=="update" ||ds_FMSZ110.GetRowType(ds_FMSZ110.currow)=="insert" ){
		var pspno=ds_FMSZ110.GetColumn(ds_FMSZ110.currow,"PSPNO");
