function fn_Input()
{
	if(!validationChk()){
		return;
	}
	
	if(checkInsertCount("ds_FMSZ110")== false) {
		return;
	}
	if(ds_FMSZ110.rowcount < 1) {
		fn_Search();
	}
	ds_FMSZ110.AddRow();
	ds_FMSZ110.SetColumn(ds_FMSZ110.currow,"COMPS",ComboBoxPlant.Value);
	ds_FMSZ110.SetColumn(ds_FMSZ110.currow,"PROCS",ed_PROCS.Text);
	ds_FMSZ110.SetColumn(ds_FMSZ110.currow,"TRNDT",cmb_TRNDT.Value);
	ds_FMSZ110.SetColumn(ds_FMSZ110.currow,"DHCOD",trim(ds_FHD000PF.GetColumn(0,"DHCODE")));
	ds_FMSZ110.SetColumn(ds_FMSZ110.currow,"PRDCD","");
	ds_FMSZ110.SetColumn(ds_FMSZ110.currow,"SUBQY",0);
}

function fn_Delete()
{

	if(ds_FMSZ110.rowcount < 1) return;			//삭제가 없으면 건너뛰기		
	ds_FMSZ110.DeleteRow(ds_FMSZ110.currow);	//삭제		
	alert(setPopMessage("1011"));				//[알림] 화면에서만 제거되었습니다. 저장하시면 삭제됩니다.		
	setMessage("1011");
}

