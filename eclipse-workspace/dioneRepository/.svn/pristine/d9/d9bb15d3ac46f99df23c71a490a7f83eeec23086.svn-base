function ds_HMDZ023B_CanColumnChange(obj,nRow,strColumnID,varOldValue,varNewValue,nPivotIndex)
{
	http.usewaitcursor = false;

	if (strColumnID == "SABUN"){//작업자 확인
		if(varNewValue!=""){
			//인사마스터
			http.Sync = true;
			var sKind		= "getHMD020PF";		
			var sMethodName = "service::pscomlib/selectINSAMasterPopup.do"; 
			var sInDataSet  = "";
			var sOutDataSet = "ds_HMD020PF=ds_Select";
			var sArgument   = "";
				sArgument  += " LIB2=" + quote(Lib2);
				sArgument  += " LIB3=" + quote(Lib3);
				sArgument  += " COMPS=" + quote(ComboBoxPlant.Value); //사업부
				sArgument  += " SABUN=" + quote(varNewValue);
			Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
			http.Sync = false;
			
			if (ds_HMD020PF.rowcount < 1){
				alert(setPopMessage("4062")); //[확인] 사용자가 존재하지 않습니다.
				setMessage("4062");	
				return false;
			}
			var duChk ="";
			var duVal =0;
				duChk=parseInt(varNewValue);
			for(i=0; i <ds_HMDZ023B.rowcount; i++){
				if(duChk == parseInt(ds_HMDZ023B.GetColumn(i,"SABUN"))){
					duVal++;
				}
				if(duVal > 0){
					alert("[경고] 이미 등록된 자료입니다.");	//[경고] 이미 등록된 자료입니다.
					alert(setPopMessage("1520"));	//[경고] 이미 등록된 자료입니다.
					ds_HMDZ023B.SelectRow(ds_HMDZ023B.FindRow("SABUN",varNewValue));
					ds_HMDZ023B.DeleteRow(nRow);
					return false;
				}	
			}

			p_KORNM=ds_HMD020PF.GetColumn(0,"A2");
			p_DPCONM =ds_HMD020PF.GetColumn(0,"A3");
			p_GNDAT= ds_HMD020PF.GetColumn(0,"GNDAT");
		}
	}
	if(strColumnID=="GJANB2"){ //중복체크
		if(varNewValue!=""){
			if(ds_HMDZ023B.FindRow("GJANB2",varNewValue)!="-1"){
				alert(setPopMessage("2533"));
				//ds_HMDZ023B.SelectRow(ds_HMDZ023B.FindRow("GJANB2",varNewValue));
				//ds_HMDZ023B.DeleteRow(nRow);
				return false;
			}
		}
	}
	if (strColumnID == "BNKCD2"){//은행코드 확인
		if(varNewValue!=""){
			//거래처마스타
			var p_VNDOR="";
				p_VNDOR=lpad(varNewValue,"0",6);
			http.Sync = true;
			var sKind		= "getMST060";		
			var sMethodName = "service::psmstlib/selectMST600.do"; 
			var sInDataSet  = "";
			var sOutDataSet = "ds_MST600PF=ds_MST600";
			var sArgument   = "";
				sArgument  += " LIB2=" + quote(Lib2);
				sArgument  += " COMPS=" + quote(ComboBoxPlant.Value); //사업부
				sArgument  += " VNDOR=" + quote(p_VNDOR);
			Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
			http.Sync = false;
			
			if (ds_MST600PF.rowcount < 1){
				alert(setPopMessage("4064")); //[확인] 지급은행을 확인바랍니다.
				setMessage("4064");	
				return false;
			}
			p_CAPNM=ds_MST600PF.GetColumn(0,"CAPNM");
		}
	}
	http.usewaitcursor = true;
}

