/********************************************************************** 
 * 내용설명 : 화면 조회 
 **********************************************************************/ 
function getHMDZ023B(){

	var p_SABUN1,p_SABUN2;
	p_SABUN1 = iif (length(trim(ed_SABUNFR.Text)) == 0, '0', trim(ed_SABUNFR.Text));
	p_SABUN2 = iif (length(trim(ed_SABUNTO.Text)) == 0 , '9999999', trim(ed_SABUNTO.Text));
	
	var sKind		= "getHMDZ023B";						
	var sMethodName = "service::pshumlib/selectHMDZ023B.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                            				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_HMDZ023B=ds_HMDZ023B";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
		sArgument += "@LIB1=" + quote(Lib1);
		sArgument += "@LIB2=" + quote(Lib2);
		sArgument += "@LIB3=" + quote(Lib3);
		sArgument += "@COMPS=" + quote(ComboBoxPlant.Value);
		sArgument += "@SABUNFR=" + quote(p_SABUN1);	//사번FR
		sArgument += "@SABUNTO=" + quote(p_SABUN2);	//사번TO
		sArgument += "@CNDCD=" + quote(chK_CNDCD.Value);	//퇴사자포함여부
		sArgument += "@GNDATFR=" + quote(cmb_GNDATFR.Value);	//입사일자FR
		sArgument += "@GNDATTO=" + quote(cmb_GNDATTO.Value);	//입사일자TO
	var str = dialog("Common::PGMLOAD_HUM.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");	
	http.Sync = false;
	
	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
		setMessage("1004");	
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));
		setMessage("1003");	
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}
}

/********************************************************************** 
 * 내용설명 : insert, update, delete
 **********************************************************************/  
function setHMDZ023B(){

	var sKind		= "setHMDZ023B";						
	var sMethodName = "service::pshumlib/saveHMDZ023B.do";    // 서버에서 호출 될 Method 명
	var sInDataSet  = "ds_HMDZ023B=ds_HMDZ023B:U";               //자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_HMDZ023B_CHECK=ds_HMDZ023B_CHECK";  	   // 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
		sArgument += " LIB2=" + quote(Lib2);
		sArgument += " LIB3=" + quote(Lib3);
		sArgument += " COMPS=" + quote(ComboBoxPlant.Value);
	http.Sync=true;
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");
	http.Sync=false;
}
/********************************************************************** 
 * 내용설명 : Transaction 리턴후 실행 함수
 * 주요변수 : 
 * 			- sMethodName	Transaction 고유 ID
 * 			- ErrorCode 	에러코드
 * 			- ErrorMSG 		에러 메세지
 **********************************************************************/ 
function callback(svcId, rtnCd, rtnMsg){
	if (ds_HMDZ023B_CHECK.GetRowCount() > 0){
		for(var i=0;i<ds_HMDZ023B_CHECK.GetRowCount();i++){
			alert("사번 : " +ds_HMDZ023B_CHECK.GetColumn(i,"SABUN") + " = " + ds_HMDZ023B_CHECK.GetColumn(i,"RTMSG"));
		}
		getFGPZ050();  //error항목이 존재시에는 재조회
		return;
	}// tran 실패
	if(rtnCd<0){
		alert(setPopMessage("1002"));
		setMessage("1002");
	}else{
		alert(setPopMessage("1001"));
		setMessage("1001");	
	}
}
