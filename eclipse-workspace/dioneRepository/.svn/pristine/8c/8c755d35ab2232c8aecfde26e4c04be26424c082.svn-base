/********************************************************************** 
 * 내용설명 : 화면 조회 
 **********************************************************************/ 
function getDWH(){
		
	if(!validationChk()){
		return;
	}
	
	var sKind		= "getDWH";						
	var sMethodName = "service::dwhlib/selectManagement.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_DWH=ds_DWH";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
	sArgument += "@LIB1=" + quote(Lib1);
	sArgument += "@LIB2=" + quote(Lib2);
	sArgument += "@LIB3=" + quote(Lib3);
	sArgument += "@COMPS=" + quote(ComboBoxPlant.Value);	//사업부
	sArgument += "@KNYYM=" + quote(substr(cmb_KNYYM.Value,0,6));	//기준년월
	sArgument += "@PAYCD=" + quote(cmb_GUBCD.Value);	//급여구분
	sArgument += "@SELCD=" + quote(rdoggub.Value);	//조회구분
	sArgument += "@SQLID=" + "selectDWHZ046Q";	
	var str = dialog("Common::PGMLOAD.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");	
	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
		setMessage("1004");	
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));
		setMessage("1003");	
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}
}

/********************************************************************** 
 * 내용설명 : Transaction 리턴후 실행 함수
 * 주요변수 : 
 * 			- sMethodName	Transaction 고유 ID
 * 			- ErrorCode 	에러코드
 * 			- ErrorMSG 		에러 메세지
 **********************************************************************/ 
function callback(svcId, rtnCd, rtnMsg){
	if(svcId=="getDWH"){
		if(rtnCd<0){
			alert(setPopMessage("1002"));
			setMessage("1002");
		}else{
			if(ds_DWH.rowcount<1){
				alert(setPopMessage("1003"));
				setMessage("1003");	
			}else{
				alert(setPopMessage("1004"));
				setMessage("1004");	
			}
		}
	}
}
/********************************************************************** 
 * 내용설명 : validation 체크
 **********************************************************************/ 
function validationChk(){

	var result = true;
	cmb_KNYYM.BKColor="white";
		
	if (cmb_KNYYM.Value == ""){
		cmb_KNYYM.BKColor="paleturquoise";
		alert(setPopMessage("4053"));//[확인] 기준년월을 확인바랍니다.
		setMessage("4053");
		result = false;
	}
	return result;
}

