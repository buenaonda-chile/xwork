function validationChk(){
	for(var i=0; i<ds_ogm010.rowcount(); i++){
		if(length(trim(ds_ogm010.GetColumn(i, "PRTNO")))==0){
			alert(setPopMessage("2501"));
			setMessage("2501");
			ds_ogm010.row = i;
			return false;
		}
		if(length(trim(ds_ogm010.GetColumn(i, "PRTNM")))==0){
			alert(setPopMessage("2502"));
			setMessage("2502");
			ds_ogm010.row = i;
			return false;
		}
		if(length(trim(ds_ogm010.GetColumn(i, "MODEL")))==0){
			alert(setPopMessage("2503"));
			setMessage("2503");
			ds_ogm010.row = i;
			return false;
		}
		if(length(trim(ds_ogm010.GetColumn(i, "PGUCD")))==0){
			alert(setPopMessage("2504"));
			setMessage("2504");
			ds_ogm010.row = i;
			return false;
		}
		if(length(trim(ds_ogm010.GetColumn(i, "DPTCD")))==0){
			alert(setPopMessage("2505"));
			setMessage("2505");
			ds_ogm010.row = i;
			return false;
		}
		if(length(trim(ds_ogm010.GetColumn(i, "POINT")))==0){
			alert(setPopMessage("2506"));
			setMessage("2506");
			ds_ogm010.row = i;
			return false;
		}
	}
	return true;
}

