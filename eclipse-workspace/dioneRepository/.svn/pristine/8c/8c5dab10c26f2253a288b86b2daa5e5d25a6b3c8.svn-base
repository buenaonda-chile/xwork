function duplicateChk(){
	http.Sync 	= true;
	var sKind		= "getogm010_dchk";						
	//var sMethodName = "service::psogmlib/selectOGM010_dchk.do";   REV OM003 추가시 입출고내역이 있을 경우 추가 不
	var sMethodName = "service::psogmlib/selectOGM010_dchkall.do";   //REV OM003 추가시 입출고내역이 있을 경우 추가 不
	var sInDataSet  = "";
	var sOutDataSet = "ds_OGM010_dchk=ds_OGM010_dchk";
	var sArgument   = "";
	sArgument += " LIB2=" + quote(Lib2);
	sArgument += " COMPS=" + quote(ComboBoxPlant.Value); //사업부
	sArgument += " GROUP=" + quote(ComboBoxClass.Value); //그룹
	sArgument += " PRTNO=" + quote(trim(Edit0.Value)); //신규품번		
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	if(ds_OGM010_dchk.rowcount() > 0){
		if(ds_OGM010_dchk.GetColumn(0,"PRTNO") == "ogm110pf" || ds_OGM010_dchk.GetColumn(0,"PRTNO") == "ogm120pf" || ds_OGM010_dchk.GetColumn(0,"PRTNO") == "ogm140pf"){
			alert("[알림] 삭제된 자료가 존재합니다.");
		}else{
			alert(setPopMessage("2039"));	// 2039 == 중복된data가 존재합니다..
			setMessage("2039");
		}
		return false;
	}
	return true;	
}

/********************************************************************** 
 * 내용설명 : validation 체크 (notnull,중복,코드명조회)
 **********************************************************************/ 
function ds_ogm010_CanColumnChange(obj,nRow,strColumnID,varOldValue,varNewValue,nPivotIndex)
{
	
	if( strColumnID == "PRTNO" ){
		if( length(varNewValue) == 0 ){
			alert(setPopMessage("2501"));
			setMessage("2501");
			return false;
		}
		else{
			if(!duplicateChk()){
				Edit0.SetFocus();
				return false;  //중복check
				}
			return true;
			}
	}
	if( strColumnID == "PRTNM" ){
		if( length(varNewValue) == 0 ){
			alert(setPopMessage("2502"));
			setMessage("2502");
			return false;
		}
		else{
			return true;
			}
	}
	if( strColumnID == "MODEL" ){
		if( length(varNewValue) == 0 ){
			alert(setPopMessage("2503"));
			setMessage("2503");
			return false;
		}
		else{
			return true;
			}
	}
	if( strColumnID == "PGUCD" ){
		if( length(varNewValue) == 0 ){
			alert(setPopMessage("2504"));
			setMessage("2504");
			return false;
		}
		else{
			ed_PGUCD_Name(varNewValue);
			if(ed_pgucdnm.value == ""){ 
				alert("확인바랍니다. (부품구분)");
				ed_PGUCD.SetFocus();
				return false;
			}
			return true;
		}
	}
	if( strColumnID == "DPTCD" ){
		if( length(varNewValue) == 0 ){
			alert(setPopMessage("2505"));
			setMessage("2505");
			return false;
		}
		else{
			ed_DPTCD_Name(varNewValue);
			if(Edit21.value == ""){ 
				alert("확인바랍니다. (관리부서)");
				ed_DPTCD.SetFocus();
				return false;
			}
			return true;
		}
	}
	if( strColumnID == "VNDOR" ){
		if( length(varNewValue) != 0 ){
			ed_VNDOR_Name(varNewValue);
			return true;
		}
	}
	if( strColumnID == "MAKER" ){
		if( length(varNewValue) != 0 ){
			ed_MAKER_Name(varNewValue);
			return true;
		}
	}
	if( strColumnID == "mshno" ){
		if( length(varNewValue) != 0 ){
			ed_MSHNO_Name(varNewValue);
			return true;
		}
	}
}

