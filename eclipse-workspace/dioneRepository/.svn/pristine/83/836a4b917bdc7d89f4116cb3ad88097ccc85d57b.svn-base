/********************************************************************** 
 * 개 발 자 : 황규철 
 * 개발일자 : 2010-11-26
 * 내용설명 : CSV파일 업로드
 * 주요변수 : 
 **********************************************************************/ 
function uploadCsv()
{
	//ds_SVHZ553U.DeleteAll();
	ds_SVHZ553U.ClearData();
	ds_SVHZ553U_CHK.ClearData();
	FileDialog0.Filter = "ALL File(csv.*)|*.csv";	
	FileDialog0.Type = "OPEN";
	if (!FileDialog0.Open()) {
		alert(setPopMessage("1009"));	// 20110103l-koowgnojeel, 1009 == 파일 선택 창을 여는데 실패하였습니다.
		return;
	}
	var file_name = FileDialog0.FileName;
//alert(file_name);
	if( file_name!="사본 - CKDD9.csv"){
		alert("파일명을 확인해주시기바랍니다. 선택된 파일명 : "+file_name);
	return;
	}	
	var file_path = FileDialog0.FilePath;
	var path = file_path + "\\" + file_name;
	setMessageDirect(path);
	//Ext_Csv2Ds(path,"ds_SVHZ554U",false,"DUM_T,VVNDNO,VFACGU,VPATNO,VINPSR,VINPCD,VINPQY,VINPAM,VBOXAM,VGLOVI");
	Ext_Csv2Ds(path,"ds_SVHZ553U",false,"DUM_T,VFACGU,VVNDNO,VJOBYM,VJAJEG,VPATNO,VDANWI,VINPCD,VINPQY,VINPAM,VBOXAM");

	for(i=0; i< ds_SVHZ553U.rowcount; i++){
		ds_SVHZ553U.SetColumn(i,"VDUMCD",substr(ds_SVHZ553U.GetColumn(i,"DUM_T"),0,2));//PGM(SVHZ553U)
		ds_SVHZ553U.SetColumn(i,"VDUMYM",substr(ds_SVHZ553U.GetColumn(i,"DUM_T"),2,8));//PGM(SVHZ553U)
		ds_SVHZ553U.SetColumn(i,"VDUMFC",substr(ds_SVHZ553U.GetColumn(i,"DUM_T"),10,4));//PGM(SVHZ553U)
		ds_SVHZ553U.SetColumn(i,"SJYYM",substr(addMonth(ds_SVHZ553U.GetColumn(i,"VDUMYM"),-1),0,6));//PGM(SVHZ550U1)
		var pcodeVal="";
		var vfacgu=ds_SVHZ553U.GetColumn(i,"VFACGU");
		//PGM(SVHZ550U1)
		if(vfacgu="HV11" || vfacgu="HV12" || vfacgu="HV13" || vfacgu="HV14" || vfacgu="HV15" || vfacgu="HE11"){
			pcodeVal="0H";
		}
		if(vfacgu='HV31' || vfacgu='HE31'){
			pcodeVal="0C";
		}	
		if(vfacgu='HK31'){
			pcodeVal="0C";
		}
		if(vfacgu='HK11'){
			pcodeVal="1H";
		}
		if(vfacgu='HK21'){
			pcodeVal="1M";
		}	
		if(vfacgu='HE21' || vfacgu='HV21'){
			pcodeVal="0M";
		}										
		ds_SVHZ553U.SetColumn(i,"PCODE",pcodeVal);
	}
	MainGrid.CreateFormat();
}

function onCellCilck(obj,nRow,nCell,nX,nY,nPivotIndex)
{
	var tableNm  = ds_list.GetColumn(nRow,"tableNm");
	var yyyymm   = ds_list.GetColumn(nRow,"date");
	//var colCnt   = ds_list.GetColumn(nRow,"columnCnt"); //필요없음 
	var pgmName  = ds_list.GetColumn(nRow,"id");	
	var fileType  = ds_list.GetColumn(nRow,"fileType");		

	if(nCell == 3)
	{
		if(fileType == "1"){
			uploadExcel();
			//saveDataSet(tableNm,pgmName);		
		} else if (fileType == "2") {
			uploadText();
			//saveDataSet(tableNm,pgmName);		
		} else {
			//grd_board.Color
			uploadCsv();
