function btn_DUCHK_OnClick(obj)
{
	if(ds_SVHZ553U.rowcount<1){
		return;
	}
	ds_SVHZ553U_CHK.ClearData(); //dataset 초기화
	
	var tmp_facChk=""; //이전 전체
	var facChk=""; //전체
	var tmp_facgu="";	//이전 사업부
	var tmp_vdumym="";	//이전 날짜
	var facgu="";	//사업부
	var vdumym="";	//날짜

	for(i=0; i< ds_SVHZ553U.rowcount; i++){
		facgu = ds_SVHZ553U.GetColumn(i,"VFACGU");
		vdumym = ds_SVHZ553U.GetColumn(i,"VDUMYM");
		facChk = ds_SVHZ553U.GetColumn(i,"VFACGU")+"||"+ds_SVHZ553U.GetColumn(i,"VDUMYM");

		if(i=0){
			ds_SVHZ553U_CHK.AddRow();
			ds_SVHZ553U_CHK.SetColumn(0,"FACGU",ds_SVHZ553U.GetColumn(i,"VFACGU"));
			ds_SVHZ553U_CHK.SetColumn(0,"DUMYM",ds_SVHZ553U.GetColumn(i,"VDUMYM"));
		}
		else{
			if(tmp_facChk!=facChk){
				var rcnt=ds_SVHZ553U_CHK.rowcount;
				ds_SVHZ553U_CHK.AddRow();
				ds_SVHZ553U_CHK.SetColumn(rcnt,"FACGU",ds_SVHZ553U.GetColumn(i,"VFACGU"));
				ds_SVHZ553U_CHK.SetColumn(rcnt,"DUMYM",ds_SVHZ553U.GetColumn(i,"VDUMYM"));
			}
		}
		tmp_facChk =facChk;
		tmp_facgu = facgu;
		tmp_vdumym = vdumym;
	}
	trace(ds_SVHZ553U_CHK.SaveXML());
}



