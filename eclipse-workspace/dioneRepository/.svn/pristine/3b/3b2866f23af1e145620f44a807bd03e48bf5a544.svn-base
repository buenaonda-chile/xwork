function ds_CZZZ100_OnColumnChanged(obj,nRow,strColumnID,varOldValue,varNewValue,nPivotIndex)
{
	if (strColumnID == "MANHR"){
		if(length(varNewValue)!=0){
			var arr = Split(ds_CZZZ100.GetColumn(nRow,"MANHR"),".");
			if(length(ds_CZZZ100.GetColumn(nRow,"MANHR"))!=7){
				if(arr[1]==""){
					arr[1]="00";
				}
				var setInput = lpad(arr[0],0,5)+rpad(arr[1],0,2);
				ds_CZZZ100.SetColumn(nRow, "MANHR", setInput); // man			
			}
		}
	}
	if (strColumnID == "MACHR"){
		if(length(varNewValue)!=0){
			var arr = Split(ds_CZZZ100.GetColumn(nRow,"MACHR"),".");
			if(length(ds_CZZZ100.GetColumn(nRow,"MACHR"))!=7){
				if(arr[1]==""){
					arr[1]="00";
				}
				var setInput = lpad(arr[0],0,5)+rpad(arr[1],0,2);
				ds_CZZZ100.SetColumn(nRow, "MACHR", setInput); // man			
			}
		}
	}	
	if (strColumnID == "DICST"){
		if(length(varNewValue)!=0){
			var arr = Split(ds_CZZZ100.GetColumn(nRow,"DICST"),".");
			if(length(ds_CZZZ100.GetColumn(nRow,"DICST"))!=7){
				if(arr[1]==""){
					arr[1]="000";
				}
				var setInput= lpad(arr[0],0,4)+rpad(arr[1],0,3);
				ds_CZZZ100.SetColumn(nRow, "DICST", setInput); // man			
			}
		}
	}
}
