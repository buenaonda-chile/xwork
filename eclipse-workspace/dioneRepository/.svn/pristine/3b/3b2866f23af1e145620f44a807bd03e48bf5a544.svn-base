}

function getCZZZ100(){

	if(!validationChk()){
		return;
	}
	var sKind		= "getCZZZ100";						
	var sMethodName = "service::pscstlib/selectCZZZ100.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_CZZZ100=ds_CZZZ100";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
		sArgument += "@LIB1=" + quote(Lib1);
		//sArgument += "@LIB2=" + quote(Lib2);
		sArgument += "@COMPS=" + quote(ComboBoxPlant.Value); //사업부
		sArgument += "@SEL=" + quote(cmb_SEL.Value); //조회구분
		sArgument += "@INPUT=" + quote(ed_INPUT.Value); //INPUT(PROCESS, ITEM NO)
	var str = dialog("Common::PGMLOAD_CST.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");
	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
		setMessage("1004");
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));
		setMessage("1003");	
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}
}

/********************************************************************** 
 * 내용설명 : insert, update, delete
 **********************************************************************/  
function setCZZZ100(){

	var sKind		= "setCZZZ100";						
	var sMethodName = "service::pscstlib/saveCZZZ100.do";    // 서버에서 호출 될 Method 명
	var sInDataSet  = "ds_CZZZ100=ds_CZZZ100:U";               //자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_CZZZ100_CHECK=ds_CZZZ100_CHECK";  	   // 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
		sArgument += " LIB1=" + quote(Lib1);
		sArgument += " LIB2=" + quote(Lib2);
	http.Sync=true;
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");
	http.Sync=false;

}
/********************************************************************** 
 * 내용설명 : Transaction 리턴후 실행 함수
 * 주요변수 : 
 * 			- sMethodName	Transaction 고유 ID
 * 			- ErrorCode 	에러코드
 * 			- ErrorMSG 		에러 메세지
 **********************************************************************/ 
function callback(svcId, rtnCd, rtnMsg){
	if (ds_CZZZ100_CHECK.GetRowCount() > 0){
		for(var i=0;i<ds_CZZZ100_CHECK.GetRowCount();i++){
			alert("Item No : " +ds_CZZZ100_CHECK.GetColumn(i,"PRTNO") + " = " + ds_CZZZ100_CHECK.GetColumn(i,"RTMSG"));
		}
		getCZZZ100();  //error항목이 존재시에는 재조회
		return;
	}// tran 실패
	if(rtnCd<0){
		alert(setPopMessage("1002"));
		setMessage("1002");
		
	}else{
		alert(setPopMessage("1001"));
		setMessage("1001");	
	}
}

/********************************************************************** 
 * 내용설명 : validation 체크
 **********************************************************************/ 
 
function validationChk(){
	var result = true;
	var input = ed_INPUT.Text;
		ed_INPUT.BKColor="white";
		
	if (input == "")
	{
		ed_INPUT.BKColor="paleturquoise";
		alert(setPopMessage("6034"));//[알림]SELECT 조건에 맞는 INPUT 값을 입력하세요.
		setMessage("6034");
		result = false;
	}
	return result;
}

