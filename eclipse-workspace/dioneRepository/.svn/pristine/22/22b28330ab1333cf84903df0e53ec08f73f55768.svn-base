	if (cmb_KNYYM.Value == ""){
		cmb_KNYYM.BKColor="paleturquoise";
		alert(setPopMessage("4053"));//[확인] 기준년월을 확인바랍니다.
		setMessage("4053");
		result = false;
	}else if(cmb_KYMD.Value == ""){
		cmb_KYMD.BKColor="paleturquoise";
		alert(setPopMessage("4053"));//[확인] 기준년월을 확인바랍니다.
		setMessage("4053");
		result = false;
	}
	return result;
}
/********************************************************************** 
 * 내용설명 : Transaction 리턴후 실행 함수
 * 주요변수 : 
 * 			- sMethodName	Transaction 고유 ID
 * 			- ErrorCode 	에러코드
 * 			- ErrorMSG 		에러 메세지
 **********************************************************************/ 
function callback(svcId, rtnCd, rtnMsg){
	if(svcId=="getOBNZ100Q"){
		if(rtnCd<0){// tran 실패
			alert(setPopMessage("1002"));
			setMessage("1002");
		// 조회건수가 없을경우
		} else if(ds_OBNZ100Q_A.rowcount<1 && ds_OBNZ100Q_B.rowcount<1){
			//alert("조회DATA없음!");
			alert(setPopMessage("1003"));
			setMessage("1003");	
		// 정상 조회
		} else {
			//alert("정상조회.!");
			alert(setPopMessage("1004"));
			setMessage("1004");				
		}
	}

}

