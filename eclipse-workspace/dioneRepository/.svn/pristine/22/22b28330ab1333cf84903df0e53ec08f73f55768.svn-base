		setMessage("1002");	
	}
*/	
	var p_PYEAR=0;
	if(toNumber(substr(cmb_KNYYM.Value,4,2)) < 4){
		p_PYEAR = toNumber(substr(cmb_KNYYM.Value,0,4)) - 1;
	}else{
		p_PYEAR = toNumber(substr(cmb_KNYYM.Value,0,4));
	}
	var sKind		= "getOBNZ100Q";						
	var sMethodName = "service::psogmlib/selectOBNZ100Q.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                            				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_OBNZ100Q_A=ds_OBNZ100Q_A ds_OBNZ100Q_B=ds_OBNZ100Q_B ds_OBNZ100Q_C=ds_OBNZ100Q_C";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
		sArgument += " LIB1=" + quote(Lib1);
		sArgument += " LIB2=" + quote(Lib2);
		sArgument += " COMPS=" + quote(ComboBoxPlant.Value);
		//sArgument += " PYEAR=" + quote(substr(cmb_KNYYM.Value,0,4));	//년도		
		sArgument += " PYEAR=" + quote(substr(p_PYEAR));	//년도		
		sArgument += " MOND=" + quote(P_MOND);	//년월	
		//sArgument += " NOWDAY=" + quote(cmb_KNYYM.Value);	//당일
		sArgument += " NOWDAY=" + quote(cmb_KYMD.Value);	//당일(임시)
		sArgument += " PYYMM=" + quote(substr(cmb_KNYYM.Value,0,6));	//년월		
		sArgument += " M_MOND=" + quote(M_MOND);	//계획점검(지연건수 SQL)	
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");
	http.Sync = false;
}

