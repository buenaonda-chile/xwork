	}else if(substr(cmb_KNYYM.Value,4,2)="08"){
		P_MOND="TRIM(A.MOND5)";
	}else if(substr(cmb_KNYYM.Value,4,2)="09"){
		P_MOND="TRIM(A.MOND6)";
	}else if(substr(cmb_KNYYM.Value,4,2)="10"){
		P_MOND="TRIM(A.MOND7)";
	}else if(substr(cmb_KNYYM.Value,4,2)="11"){
		P_MOND="TRIM(A.MOND8)";
	}else if(substr(cmb_KNYYM.Value,4,2)="12"){
		P_MOND="TRIM(A.MOND9)";
	}else if(substr(cmb_KNYYM.Value,4,2)="01"){
		P_MOND="TRIM(A.MOND10)";
	}else if(substr(cmb_KNYYM.Value,4,2)="02"){
		P_MOND="TRIM(A.MOND11)";
	}else if(substr(cmb_KNYYM.Value,4,2)="03"){
		P_MOND="TRIM(A.MOND12)";
	}
	//계획점검 SQL사용 변수
	//일정초과  단순히 해당날짜까지의 실적합~계획합의 차이를 구함. 
	var M_MOND="";	//전송param
	var M_PLN="";	//계획
	var M_SIL="";	//실적
	//var G_DAY=substr(cmb_KYMD.Value,6,2);
	var G_DAY=parseInt(substr(cmb_KYMD.Value,6,2));
	//alert(G_DAY);
	for(i=1; i<=G_DAY; i++){
		if(length(M_PLN)!=0){
			M_PLN+=" + (CASE WHEN +A.PLN"+lpad(i,"0",2)+"='T' THEN 1 ELSE 0 END)";	
			M_SIL+=" + (CASE WHEN +A.SIL"+lpad(i,"0",2)+"='T' THEN 1 ELSE 0 END)";	
		}else{
			M_PLN+="(CASE WHEN +A.PLN"+lpad(i,"0",2)+"='T' THEN 1 ELSE 0 END)";
			M_SIL+="(CASE WHEN +A.SIL"+lpad(i,"0",2)+"='T' THEN 1 ELSE 0 END)";
		}
	}
	M_MOND="SUM("+M_PLN+") - SUM("+M_SIL+") AS DELAY";
	//alert(M_MOND);
	
