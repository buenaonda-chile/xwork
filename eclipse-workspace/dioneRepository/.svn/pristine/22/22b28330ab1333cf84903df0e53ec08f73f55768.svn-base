		sArgument += "@PYEAR=" + quote(substr(cmb_KNYYM.Value,0,4));	//년도		
		sArgument += "@MOND=" + quote(P_MOND);	//년월	
		//sArgument += "@NOWDAY=" + quote(cmb_KNYYM.Value);	//당일
		sArgument += "@NOWDAY=" + quote(cmb_KYMD.Value);	//당일(임시)
