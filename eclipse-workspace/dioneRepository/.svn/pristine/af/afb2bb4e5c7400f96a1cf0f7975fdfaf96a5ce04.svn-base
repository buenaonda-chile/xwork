function incSEQ(rowNm){

	var sSeq=1;
	var cSeq=0;
	if( rowNm==0){
		return sSeq;
	}	
	if(ds_SCDZ106R.GetColumn(rowNm,"TRKNO") != ds_SCDZ106R.GetColumn(rowNm-1,"TRKNO")){
		ds_SCDZ106R.SetColumn(rowNm,"ROWNUM",1);
		sSeq=1;
	}
	else{
		cSeq=parseInt(ds_SCDZ106R.GetColumn(rowNm-1,"ROWNUM"));
		ds_SCDZ106R.SetColumn(rowNm,"ROWNUM",cSeq+1);
		sSeq=cSeq+1;
	}
	return sSeq;	
