function getSCDZ106R(){
	var sJPNO1="";
	var sJPNO2="";

	if(length(ed_JPNO1.Text)==0){
		sJPNO1="0000000";
	}
	else{
		sJPNO1=ed_JPNO1.Text;
	}	
	if(length(ed_JPNO2.Text)==0){
		sJPNO2="9999999";
	}
	else{
		sJPNO2=ed_JPNO2.Text;
	}	
	getSYSDATE(); //SYSTEM 날짜
	var sysDt =ds_SYSDATE.GetColumn(0,"DT");
	var systm =ds_SYSDATE.GetColumn(0,"TM");
			
	var sKind		= "getSCDZ106R";						
	var sMethodName = "service::pssallib/selectSCDZ106R.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_SCDZ106R=ds_SCDZ106R";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
	sArgument += "@LIB1=" + quote(Lib1);
	sArgument += "@LIB2=" + quote(Lib2);
	sArgument += "@COMPS=" + quote(ComboBoxPlant.Value); 	//사업부
	sArgument += "@JPNO1=" + quote(sJPNO1); //업체
	sArgument += "@JPNO2=" + quote(sJPNO2); //업체
	var str = dialog("Common::PGMLOAD_SAL.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");
	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
		setMessage("1004");
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));
		setMessage("1003");	
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}

	//Ubi0.setJrfFileDir("http://10.74.139.50/ui/PSSALLIB/REPORT/");
	//Ubi0.setJrfFileDir("http://10.72.209.23/ui/PSSALLIB/REPORT/");
	Ubi0.setJrfFileDir(G_baseURL+"ui/PSSALLIB/REPORT/");
	Ubi0.setJrfFileName("SCDZ106R.jrf");
	Ubi0.SetDataset("ds_SCDZ106R", "ds_SCDZ106R");
	Ubi0.SetArgument("SYSDATE",sysDt);
	Ubi0.SetArgument("SYSTIME",systm);	
	Ubi0.Retrieve();
}

