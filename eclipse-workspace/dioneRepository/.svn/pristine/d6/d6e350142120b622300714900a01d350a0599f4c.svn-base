function setGBKcolor2(drtdt, basno){
	var bkcolor="";
	var chkDT = today();
	
	if(basno !=3){
		if(drtdt >= chkDT){
			bkColor="lime";
		}
		else{
			bkColor="red";
		}	
	}
	return bkColor;
}
