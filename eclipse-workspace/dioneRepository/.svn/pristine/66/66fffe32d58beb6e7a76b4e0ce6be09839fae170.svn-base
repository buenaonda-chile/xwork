	}
	else{
		return 1;
	}
}

//1 :일시불일 때 개발투자비 0:상각일 때 상각수량, 개발투지비
function ds_SVZE500_OnColumnChanged(obj,nRow,strColumnID,varOldValue,varNewValue,nPivotIndex)
{

	if( strColumnID == "SGGUB" ){
		if( varNewValue == 1){
				ds_SVZE500.SetColumn(ds_SVZE500.currow,"SGQTY",0);
		}

	}
	if( strColumnID == "CARCD" ){
		if( varNewValue !=""){
				ds_SVZE500.SetColumn(ds_SVZE500.currow,"CARNM",ds_MST050.GetColumn(0,"MSTSM"));
		}
		else{
				ds_SVZE500.SetColumn(ds_SVZE500.currow,"CARNM","");
		}

	}
	if( strColumnID == "BDYCD" ){
		if( varNewValue !=""){
				ds_SVZE500.SetColumn(ds_SVZE500.currow,"BDYNM",ds_MST050.GetColumn(0,"MSTSM"));
		}
		else{
				ds_SVZE500.SetColumn(ds_SVZE500.currow,"BDYNM","");
		}

	}
}
