function getSvze500(){
	//rowFlag="T";
	//ed_PYVND.TabStop=true;
	//ed_CAR.TabStop=true;
	
	if(!validationChk()){
		return;
	}	

	var sKind		= "getSvze500";						
	var sMethodName = "service::pssallib/selectSVZE500.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_SVZE500=ds_SVZE500";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
	sArgument += "@LIB1=" + quote(Lib1);
	sArgument += "@LIB2=" + quote(Lib2);
	sArgument += "@COMPS=" + quote(ComboBoxPlant.Value); //사업부
	sArgument += "@PYV=" + quote(ed_PYVND.Text); //업체
	sArgument += "@CAR=" + quote(ed_CAR.Text); //품번
	var str = dialog("Common::PGMLOAD_SAL.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");

	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
		setMessage("1004");
		//ed_PYVND.TabStop=false;
		//ed_CAR.TabStop=false;		
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));
		setMessage("1003");	
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}
}

/********************************************************************** 
 * 내용설명 : insert, update, delete
 **********************************************************************/ 
 
 //등록시 업체코드 존재유무 체크함. 없으면 동록불가.
function setSvze500(){
	if(length(trim(st_PYVND.Text)) < 1){
		alert(setPopMessage("1502"));  //[확인] 거래처코드 확인바랍니다.
		setMessage("1502");	
		ed_PYVND.SetFocus();
		return;
	}	

	if(ds_SVZE500.GetRowType(ds_SVZE500.currow)=="update" ||ds_SVZE500.GetRowType(ds_SVZE500.currow)=="insert" ){
		if(!ds_SVZE500_CanRowPosChange()){
			return;
		}
	} 		
	var sKind		= "setSvze500";						
	var sMethodName = "service::pssallib/insertSVZE500.do";     // 서버에서 호출 될 Method 명
	var sInDataSet  = "ds_SVZE500=ds_SVZE500:U";               //자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_SVZE500_CHECK=ds_SVZE500_CHECK";  							     // 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
		sArgument += " LIB1=" + quote(Lib1);
		sArgument += " LIB2=" + quote(Lib2);
		sArgument += " SCOMPS=" + quote(ComboBoxPlant.Value); //사업부
		sArgument += " SPYVND=" + quote(ed_PYVND.Text); //업체
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");
}
/********************************************************************** 
 * 내용설명 : Transaction 리턴후 실행 함수
 * 주요변수 : 
 * 			- sMethodName	Transaction 고유 ID
 * 			- ErrorCode 	에러코드
 * 			- ErrorMSG 		에러 메세지
 **********************************************************************/ 
function callback(svcId, rtnCd, rtnMsg){
	if (ds_SVZE500_CHECK.GetRowCount() > 0){
		for(var i=0;i<ds_SVZE500_CHECK.GetRowCount();i++){
			alert("차종 : " +ds_SVZE500_CHECK.GetColumn(i,"CARCD") + ", 규격 : " +ds_SVZE500_CHECK.GetColumn(i,"BDYCD") +  ", 구분 : " +ds_SVZE500_CHECK.GetColumn(i,"SGGUB") + 
					", MP : " +ds_SVZE500_CHECK.GetColumn(i,"MPDAT") + " = " + ds_SVZE500_CHECK.GetColumn(i,"RTMSG"));
		}
		getSvze500();  //error항목이 존재시에는 재조회
		return;
	}
	// tran 실패
	if(rtnCd<0){
		alert(setPopMessage("1002"));
		setMessage("1002");
		
	}else 
	{
		alert(setPopMessage("1001"));
		setMessage("1001");	
	}
}

