			MainGrid.SetCellProp("body",7,"Edit","none");
		}

	}
}

/********************************************************************** 
 * 내용설명 : 구분 값이 0,1이 아닐경우 이동불가
 **********************************************************************/ 
 
function ds_SVZE500_CanRowPosChange(obj,nOldRow,nRow)
{

	if(ds_SVZE500.GetRowType(ds_SVZE500.currow)=="update" ||ds_SVZE500.GetRowType(ds_SVZE500.currow)=="insert" ){

		var rowFlag =""; 
		var carcd=ds_SVZE500.GetColumn(ds_SVZE500.currow,"CARCD");
		var bdycd=ds_SVZE500.GetColumn(ds_SVZE500.currow,"BDYCD");
		var sggub=ds_SVZE500.GetColumn(ds_SVZE500.currow,"SGGUB");
		var gbamt=ds_SVZE500.GetColumn(ds_SVZE500.currow,"GBAMT"); //개발투자비
		var sgqty=ds_SVZE500.GetColumn(ds_SVZE500.currow,"SGQTY"); //상각수량
		var s9qty=ds_SVZE500.GetColumn(ds_SVZE500.currow,"S9QTY"); //상각수량
		var mpdat=ds_SVZE500.GetColumn(ds_SVZE500.currow,"MPDAT"); //MPDAT
		
		if(length(carcd)!=0){
			if(length(sggub) !=0){
				if( sggub==1){	//일시불
					if(gbamt==0 || length(gbamt)==0){
						//ds_SVZE500.SetColumn(ds_SVZE500.currow,"SGQTY",0);
						alert(setPopMessage("4668"));//[알림]일시불일경우 개발투자비를 입력하세요.
						//MainGrid.SetFocus();
						//MainGrid.AutoEnter=true;	
						return false;
					}
				}
				if(sggub==0){	//상각
					if(gbamt==0 || length(gbamt)==0 || sgqty==0 || length(sgqty)==0){
						alert(setPopMessage("4669"));//[알림]상각일 경우 개발투자비와 상각수량을 입력하세요.
						return false;
					}
				}				
			}
			else{
				alert(setPopMessage("4667"));//[확인]구분코드을 입력하세요(상각은 0, 일시불은 1)
				return false;
			}		
			rowFlag = getMST100CHK(carcd, bdycd);
			if(rowFlag==0){
				alert(setPopMessage("2508"));//[확인] 품목이 존재하지 않습니다. 확인바랍니다.
				return false;
			}
			if(length(mpdat)==0){
				alert(setPopMessage("4670"));//[알림]MP일자를 입력하세요.
				return false;
			}
			if(length(s9qty)==0){
				ds_SVZE500.SetColumn(ds_SVZE500.currow,"S9QTY",0);
			}		
		}
		return true;	
	}
}

