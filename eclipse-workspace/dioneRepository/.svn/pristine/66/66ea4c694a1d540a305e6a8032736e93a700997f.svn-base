/********************************************************************** 
 * 내용설명 : 사업부 선택
 * 주요변수 : 
 **********************************************************************/ 
function getComCode(){
	var sKind		= "getComCode";						
	var sMethodName = "service::login/selectComCode.do"; 
	var sInDataSet  = "";
	var sOutDataSet = "ds_ComCode_PLNT=ds_ComCode_PLNT";
	var sArgument   = "";
		sArgument   += " CLASS='PLNT'";
		sArgument   += " CODE1=" + quote(company);		
	http.Sync=true;
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	http.Sync=false;
	ComboBoxPlant.Value=comps;
	ComboBoxPlant.Enable = false; 
}

/********************************************************************** 
 * 내용설명 : 화면조회
 * 주요변수 : 
 **********************************************************************/ 
function getCJJE200Q(){

	if(!validationChk()){
		return;
	}
	
	var sKind		= "getCJJE200Q";						
	var sMethodName = "service::pscstlib/selectCJJE200Q.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_CJJE200Q=ds_CJJE200Q";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
	sArgument += "@LIB1=" + quote(Lib1);
	sArgument += "@LIB2=" + quote(Lib2);
	sArgument += "@COMPS=" + quote(ComboBoxPlant.Value); //사업부
	//sArgument += "@ITNBR=" + quote(ed_ITNBR.Text);	//품번
	var str = dialog("Common::PGMLOAD_CST.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");	
	//Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");
	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
		setMessage("1004");	
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));
		setMessage("1003");	
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}
}



/********************************************************************** 
 * 내용설명 : validation 체크
 **********************************************************************/  
function validationChk(){

	var result = true;
	ComboBoxPlant.BKColor="white";
	ed_ITNBR.BKColor="white";
	
	if(length(trim(ComboBoxPlant.Value)) < 1){
		alert("[알림] 지원하지 않는 시스템 입니다. 정보관리팀으로 문의하십시요.");
		//alert(setPopMessage("1006"));  //[알림] 지원하지 않는 시스템 입니다. 정보관리팀으로 문의하십시요.
		//setMessage("1006");	
		result = false;
	}
	else if((ComboBoxPlant.Value != "E1")){
		alert("[알림]마산공장만 사용가능한 프로그램입니다.");
		result = false;
	}
	
	else if(length(trim(ed_ITNBR.Text)) = 0){
		ed_ITNBR.BKColor="paleturquoise";
		alert("[확인] 품번을 입력하세요.");
		//alert(setPopMessage("4020"));//[확인] 마감년월을 확인바랍니다.
		//setMessage("4020");
		ed_ITNBR.SetFocus();
		result = false;
	}
		
	return result;
}


