	searchBar.Width = G_ds_design.GetColumn(0,"Width");
	searchBar.Top = G_ds_design.GetColumn(0,"SBTop");
}

function getCGMPARMHC(){
	var sKind		= "getCGMPARMHC";						
	var sMethodName = "service::pscomlib/selectCGMPARMHC.do"; 
	var sInDataSet  = "";
	var sOutDataSet = "ds_CGMPARMHC=ds_CGMPARMHC";
	var sArgument   = "";
		sArgument += " LIB1=" + quote(Lib1);
	http.Sync=true;
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	http.Sync=false;
}

function getBC010PR(flag){
	var sKind		= "getBC010PR";						
	var sMethodName = "service::pscomlib/selectBC010PR.do"; 
	var sInDataSet  = "";
	var sOutDataSet = "ds_BC010PR=ds_BC010PR";
	var sArgument   = "";
		sArgument += " LIB1=" + quote(Lib1);
		sArgument += " WCDATE=" + quote(substr(cmb_MAKDT.Value,0,6));
		sArgument += " WCDATE_P1=" + quote(substr(cmb_MAKDT.Value,0,6)+"01");
		sArgument += " WCDATE_P2=" + quote(cmb_MAKDT.Value);
		sArgument += " FLAG=" + quote(flag);
	http.Sync=true;
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	if(flag=="MRUND"){
		st_MRUND.Text=ds_BC010PR.SUM("RUND");
		//st_MRUND.Text=21.5;
	}
	if(flag=="NRUND"){
		st_NRUND.Text=ds_BC010PR.SUM("RUND");
	}
	http.Sync=false;
}

function getFPMC107Q(){

	if(!validationChk()){
		return;
	}

	var flag1 = "MRUND";
	var flag2 = "NRUND";
	getBC010PR(flag1);
	getBC010PR(flag2);
	
	var mrund=parseFloat(st_MRUND.Value);
	var nrund=parseFloat(st_NRUND.Value);
	var chkVal=0;
	if(mrund>0){
		var chkVal=(nrund/mrund)*100;
		if(chkVal >999.9){
			chkVal=999.9;
		}
		st_MRATD.Value=truncate(chkVal,1);
	}

	var p_pgub;
