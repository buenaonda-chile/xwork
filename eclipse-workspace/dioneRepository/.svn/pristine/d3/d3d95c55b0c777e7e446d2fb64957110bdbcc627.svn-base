function validationChk(){
	var result = true;
	cmb_MAKDT.BKColor="white";
	var cigmaPreYM = substr(addMonth(ds_CGMPARMHC.GetColumn(0,"ADSDT"),-1),0,6);

	//trace(cigmaYM+"=="+cigmaNextYM);
	if (cmb_MAKDT.Value == ""){
		cmb_MAKDT.BKColor="paleturquoise";
		//alert("[알림]생산일자를 입력하세요.");
		alert(setPopMessage("6066"));//[알림]생산일자를 입력하세요.
		setMessage("6066");
		result = false;
	}else if(cmb_MAKDT.Value > ds_CGMPARMHC.GetColumn(0,"ADSDT")){
		cmb_MAKDT.BKColor="paleturquoise";
		//alert("[알림]생산일자를 확인하세요.(CIGMA일자 이후 입력불가)");
		alert(setPopMessage("6067"));//[알림]생산일자를 확인하세요.(CIGMA일자 이후 입력불가)
		setMessage("6067");
		result = false;	
	}else if(substr(cmb_MAKDT.Value,0,6) < cigmaPreYM){
		cmb_MAKDT.BKColor="paleturquoise";
		//alert("[알림]처리년월은 CIGMA년월-1까지 입력가능합니다.");
		alert(setPopMessage("6068"));//[알림]처리년월은 CIGMA년월-1까지 입력가능합니다.
		setMessage("6068");
		result = false;	
	}
	return result;
}

