{
	if(nCell != 0) lib_grid_SetGridSort(obj,nCell);
}

function ds_ogm205_obj220pf_OnRowPosChanged(obj,nOldRow,nRow)
{
		//로우타입 insert만 ED박스 enable
	if(ds_ogm205_obj220pf.GetRowType(ds_ogm205_obj220pf.currow)=="insert"){
		txtPrtNm.Enable=true;
		txtPrtNo.Enable=true;
		txtModel.Enable=true;
		txtMaker.Enable=true;
		txtPrice.Enable=true;
		selPrtNo.Visible=true;
	}else{
		txtPrtNm.Enable=false;
		txtPrtNo.Enable=false;
		txtModel.Enable=false;
		txtMaker.Enable=false;
		txtPrice.Enable=false;	
		selPrtNo.Visible=false;
	}
}

