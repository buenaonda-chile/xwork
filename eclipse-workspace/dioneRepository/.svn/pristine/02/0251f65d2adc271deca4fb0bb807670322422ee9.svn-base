function setogm205_delete(comps, seqno){
	
	var sKind		= "setogm205_delete";						
	var sMethodName = "service::psogmlib/deleteOGM205.do"; 
	var sInDataSet  = "";
	var sOutDataSet = "";
	var sArgument   = "";
		sArgument += " LIB2=" + quote(Lib2);
		sArgument += " COMPS=" + quote(comps); //사업부
		sArgument += " SEQNO=" + quote(seqno); //코드
	http.Sync = true;
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");
	http.Sync = false;
	
}


