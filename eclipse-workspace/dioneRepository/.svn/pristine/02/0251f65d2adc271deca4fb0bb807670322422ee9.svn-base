/********************************************************************** 
 * 내용설명 : 예비품 조회
 **********************************************************************/ 
function getogm010pf(PrtNo){
	
	var sKind		= "getogm010pf";						
	var sMethodName = "service::psogmlib/selectOgm010pf.do"; 
	var sInDataSet  = "";
	var sOutDataSet = "ds_ogm010pf=ds_ogm010pf";
	var sArgument   = "";
		sArgument += " LIB2=" + quote(Lib2);
		sArgument += " COMPS=" + quote(ComboBoxPlant.Value); //사업부
		sArgument += " PRTNO=" + quote(PrtNo); //예비품No
	http.Sync = true;
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	http.Sync = false;
}

/********************************************************************** 
 * 내용설명 : 교환부품등록
 **********************************************************************/ 
function setogm205(){
	
	var sKind		= "setogm205";						
	var sMethodName = "service::psogmlib/saveOGM205.do"; 
	var sInDataSet  = "";
	var sOutDataSet = "";
	var sArgument   = "";
		sArgument += " LIB2=" + quote(Lib2);
		sArgument += " COMPS=" + quote(ComboBoxPlant.Value); //사업부
		//sArgument += " SEQNO=" + quote(PrtNo); //코드
		sArgument += " JISNO=" + quote(trim(txtjisno2.Text)); //지시No
		sArgument += " PRTNO=" + quote(trim(txtPrtNo.Text)); //예비품No
		sArgument += " PRTNM=" + quote(trim(txtPrtNm.Text)); //부품명
		sArgument += " PTYPE=" + quote(trim(txtModel.Text)); //규격
		sArgument += " MAKER=" + quote(trim(txtMaker.Text)); //Maker
		sArgument += " QUATY=" + quote(trim(txtQuaty.Text)); //수량
		sArgument += " PRICE=" + quote(trim(txtPrice.Text)); //단가
		sArgument += " AMONT=" + quote(txtAmont.Text); //금액
	http.Sync = true;
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");
	http.Sync = false;
	getogm205pf();
}
