function txtPrtNo_OnKeyDown(obj,nChar)
{

	if(nChar != 13)
		return;
	if(length(trim(txtPrtNo.Text)) =0 ){
		alert(setPopMessage("3035"));
		return;
	}		
/*
	if(length(subPrtNo) !=0){
		txtPrtNo.Text=subPrtNo;
	}
	else {
		if(length(trim(txtPrtNo.Text)) =0 ){
			alert(setPopMessage("3035"));
			return;
		}
	}
*/	

		
	var sPrtno=txtPrtNo.Text;
	for( i=0; i<ds_ogm205_obj220pf.GetRowCount(); i++){
		if(sPrtno==ds_ogm205_obj220pf.GetColumn(i,"PRTNO")){
			alert("예비품No가 등록되어있습니다. 해당내역을 수정하세요");
			ds_ogm205_obj220pf.DeleteRow(ds_ogm205_obj220pf.currow);
			btnadd2_OnClick();
			subPrtNo="";
			return false;
		}
	}
	getogm010pf(trim(txtPrtNo.Text));
	if(ds_ogm010pf.GetColCount()==0){
		alert(setPopMessage("3036"));
		txtPrtNo.Text="";
		txtPrtNm.Text="";
		txtPrtNm.Enable=true;
		txtModel.Text="";
		txtModel.Enable=true;
		txtMaker.Text="";
		txtMaker.Enable=true;
		txtPrice.Text="";
		txtPrice.Enable=true;
	}else{
		txtPrtNm.Text = trim(ds_ogm010pf.GetColumn(0,"prtnm"));
		txtPrtNm.Enable=false;
		//txtPrtNm.BKColor="paleturquoise";
		txtModel.Text = trim(ds_ogm010pf.GetColumn(0,"model"));
		txtModel.Enable=false;
		//txtModel.BKColor="paleturquoise";
		txtMaker.Text = trim(ds_ogm010pf.GetColumn(0,"maknm"));
		txtMaker.Enable=false;
		//txtMaker.BKColor="paleturquoise";
		txtPrice.Text = trim(ds_ogm010pf.GetColumn(0,"pcost"));
		txtPrice.Enable=false;
		//txtPrice.BKColor="paleturquoise";
	}
}

function txtQuaty_OnKillFocus(obj)
{
	var tot = parseFloat(txtQuaty.Text)*parseFloat(txtPrice.Text);
	txtAmont.Text=tot;
}

function txtPrice_OnKillFocus(obj)
{
	var tot = parseFloat(txtQuaty.Text)*parseFloat(txtPrice.Text);
	txtAmont.Text=tot;
}



function btnOk_OnClick(obj)
{
	if(length(trim(txtjisno2.Text)) = 0)
	{
		alert(setPopMessage("3034"));
		return;
	}
	
	if(length(trim(txtPrtNm.Text)) = 0 || length(trim(txtQuaty.Text)) = 0 || length(trim(txtPrice.Text)) = 0)
	{
		alert(setPopMessage("3037"));
		return;
	}
		
	var sKind		= "setogm205";						
	var sMethodName = "service::psogmlib/saveOGM205.do"; 
	var sInDataSet  = "ds_ogm205_obj220pf=ds_ogm205_obj220pf:U";               //자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "";
	var sArgument   = "";
		sArgument += " LIB2=" + quote(Lib2);
		sArgument += " COMPS=" + quote(ComboBoxPlant.Value); //사업부
		//sArgument += " SEQNO=" + quote(PrtNo); //코드
		sArgument += " JISNO=" + quote(trim(txtjisno2.Text)); //지시No
	http.Sync = true;
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");
	http.Sync = false;
	

	getogm205pf();

	/*
	setogm205();
	getogm205pf();
	*/
	
}

function ds_obj220pf_OnColumnChanged(obj,nRow,strColumnID,varOldValue,varNewValue,nPivotIndex)
