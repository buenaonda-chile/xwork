function btnadd2_OnClick(obj)
{
	//검색시 그 결과값이 없을 때 추가ㅣ버튼을 누르면 로우가 추가되지 않기에 강제추가
	if(ds_ogm205_obj220pf.GetRowCount() < 1){
	
		ds_ogm205_obj220pf.AddColumn("PRTNO");
		ds_ogm205_obj220pf.AddColumn("PRTNM");
		ds_ogm205_obj220pf.AddColumn("PTYPE");
		ds_ogm205_obj220pf.AddColumn("MAKER");
		ds_ogm205_obj220pf.AddColumn("QUATY");
		ds_ogm205_obj220pf.AddColumn("PRICE");
		ds_ogm205_obj220pf.AddColumn("AMONT");
		ds_ogm205_obj220pf.AddColumn("DELCHK");
		ds_ogm205_obj220pf.AddColumn("SEQNO");
	}
	//로우추가 - 데이터가 있으면 그냥 로우 추가만
	ds_ogm205_obj220pf.AddRow();
	txtPrtNo.Enable=true;
	txtPrtNm.Enable=true;
	txtModel.Enable=true;
	txtMaker.Enable=true;
	txtPrtNo.Enable=true;
	txtPrice.Enable=true;
	
	selPrtNo.Visible=true;
}

