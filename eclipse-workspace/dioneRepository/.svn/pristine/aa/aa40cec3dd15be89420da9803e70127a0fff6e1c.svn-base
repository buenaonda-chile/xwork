var strSeparator = "	";
var Lib1 = G_ds_user.GetColumn(0,"LIB1"); // cigma library(pereslib,psreslib)
var Lib2 = G_ds_user.GetColumn(0,"LIB2"); // 회사 library(psedblib,pscdblib)
var Lib3 = G_ds_user.GetColumn(0,"LIB3"); // 사업부 library(psedblib,pscdblib,pshdblib,pssdblib)
var company = G_ds_user.GetColumn(0,"COMPANY");
var comps = G_ds_user.GetColumn(0,"PLANT_CD");
var nowDate = today();

// 년월취득
var nowDateYYYY = substr(today(),0,4);   //현재 년
var nowDateMM = substr(today(),4,2);     //현재 월
var nowDateYYYYPlusMM = substr(addMonth(today(), 1),0,4);   //다음달 년(2011년12월 경우 다음달은 2012년01월이 된다)
var nowDateMMPlusMM = substr(addMonth(today(), 1),4,2);     // 다음달 월

function on_LoadCompleted(obj){
	getTitle();	
	getDesign();
	getComCode(); //공장구분 선택
	gFcOnTopButtonColor("0"); //상단 버튼 활성화 여부 (기본:0,조회:1,변경:2)	
	//날짜를 포함한 문구 표시(대상수량은 제외)
    SearchResultStatic0.Text = nowDateYYYY + " 년 " + nowDateMM + " 월" + "        대의 교정대상 장비가 있습니다.";
    SearchResultStatic1.Text = nowDateYYYYPlusMM + " 년 " + nowDateMMPlusMM + " 월" + "        대의 교정예정대상 장비가 있습니다.";
    SearchResultStatic2.Text = nowDateYYYY + " 년 " + nowDateMM + " 월" + " 현재        대의 교정 미실시 장비가 있습니다.";
	//대상수량 취득
	getogm400();
}

function getComCode(){
	http.Sync 	= true;
	var sKind		= "getComCode";						
	var sMethodName = "service::login/selectComCode.do"; 
	var sInDataSet  = "";
	var sOutDataSet = "ds_ComCode_PLNT=ds_ComCode_PLNT";
	var sArgument   = "";
		sArgument   += " CLASS='PLNT'";
		sArgument   += " CODE1=" + quote(company);		
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	
	ComboBoxPlant.Value=comps;
	ComboBoxPlant.Enable = false; 
}

function getDesign(){
	searchBar.Width = G_ds_design.GetColumn(0,"Width");
	searchBar.Top = G_ds_design.GetColumn(0,"SBTop");
	Width = G_ds_design.GetColumn(0,"Width");
}


/********************************************************************** 
 * 내용설명 : 화면 조회 
 **********************************************************************/ 
function getogm400(){

	var sKind		= "getOgm400";						
	var sMethodName = "service::psogmlib/selectOGM400.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_OGM400=ds_OGM400";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
	
	
	//trace("Lib2:" + Lib2);
	//trace("Lib3:" + Lib3);
	//trace("ComboBoxPlant.Value:" + ComboBoxPlant.Value);
	//trace("nowDateYYYY + nowDateMM:" + nowDateYYYY + nowDateMM);
	//trace("nowDateYYYYPlusMM + nowDateMMPlusMM:" + nowDateYYYYPlusMM + nowDateMMPlusMM);	
	
	sArgument += " LIB2=" + quote(Lib2);
	sArgument += " LIB3=" + quote(Lib3);
	sArgument += " COMPS=" + quote(ComboBoxPlant.Value); //사업부
	sArgument += " YEDAT0=" + quote(nowDate); //현재 년월일
	sArgument += " YEDAT1=" + quote(nowDateYYYY + nowDateMM); //현재 년월
	sArgument += " YEDAT2=" + quote(nowDateYYYYPlusMM + nowDateMMPlusMM); //현재 년월 + 1개월
	sArgument += " YEDAT3=" + quote(nowDate); //현재 년월일	
    
    //trace("sArgument:" + sArgument);
    Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");

}

/********************************************************************** 
 * 내용설명 : Transaction 리턴후 실행 함수
 **********************************************************************/ 
function callback(svcId, rtnCd, rtnMsg){
	// tran 실패
	if(rtnCd<0){
		alert(setPopMessage("1002"));
		setMessage("1002");
	// tran 성공	
	}else if(svcId=="insertOGM400"){
		getOGM400();  //처리후 재조회
		alert(setPopMessage("1001"));
		setMessage("1001");
	}	
}


