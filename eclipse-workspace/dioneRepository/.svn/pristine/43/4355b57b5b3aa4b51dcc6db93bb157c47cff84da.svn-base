function loadingChart_Def(){

	var chartCnt = ds_OBNZ340Q_A_PIVOT.rowcount;
	//Div_contents.xChart_MTBF.OpenData("COD_Values", 0, chartCnt); //뭔지모름
	Div_contents.xChart_MTBF.SerLegBox = true;
	Div_contents.xChart_MTBF.SerLeg(0) = "계획";
	Div_contents.xChart_MTBF.SerLeg(1) = "실적";
	//Div_contents.xChart_MTBF.AxisY.LabelsFormat.Decimals = 1;
	//범례사이즈
	Div_contents.xChart_MTBF.SerLegBoxObj.width = 65;
	
	Div_contents.xChart_MTTR.SerLegBox = true;
	Div_contents.xChart_MTTR.SerLeg(0) = "계획";
	Div_contents.xChart_MTTR.SerLeg(1) = "실적";
	//Div_contents.xChart_MTTR.AxisY.LabelsFormat.Decimals = 1;
	//범례사이즈
	Div_contents.xChart_MTTR.SerLegBoxObj.width = 65;
	//Y축 값설정
	Div_contents.xChart_MTBF.AxisY.Min = 0;
	Div_contents.xChart_MTBF.AxisY.Max = 40;
	Div_contents.xChart_MTTR.AxisY.Min = 0;
	Div_contents.xChart_MTTR.AxisY.Max = 40;
	
	for(var i=0;i<chartCnt;i++){
		//MTBF
		Div_contents.xChart_MTBF.Value(0, i) = ds_OBNZ340Q_A_PIVOT.GetColumn(i,"MTBF_PLN");	//계획
		Div_contents.xChart_MTBF.Value(1, i) = ds_OBNZ340Q_A_PIVOT.GetColumn(i,"MTBF_RLT");	//실적
		//MTTR
		Div_contents.xChart_MTTR.Value(0, i) = ds_OBNZ340Q_A_PIVOT.GetColumn(i,"MTTR_PLN");	//계획
		Div_contents.xChart_MTTR.Value(1, i) = ds_OBNZ340Q_A_PIVOT.GetColumn(i,"MTTR_RLT");	//실적
	}	
	//X축 값설정
	for(var i=0;i<chartCnt;i++){
		Div_contents.xChart_MTBF.Legend(i) = ds_OBNZ340Q_A_PIVOT.GetColumn(i,"MONTH");// MTBF년월		
		Div_contents.xChart_MTTR.Legend(i) = ds_OBNZ340Q_A_PIVOT.GetColumn(i,"MONTH");// MTTR년월		
	}
   
}

function loadingChart(){
	//MTBF 그래프 Y축 Range설정
	var getMaxPlt = ds_OBNZ340Q_A_PIVOT.Max("MTBF_PLN");
	var getMaxRlt = ds_OBNZ340Q_A_PIVOT.Max("MTBF_RLT");
	if(parseInt(getMaxPlt) > parseInt(getMaxRlt)){
		Div_contents.xChart_MTBF.AxisY.Max = parseInt(getMaxPlt)+5;
	}else{
		Div_contents.xChart_MTBF.AxisY.Max = parseInt(getMaxRlt)+5;
	}
	//MTTR 그래프 Y축 Range설정
	getMaxPlt = ds_OBNZ340Q_A_PIVOT.Max("MTTR_PLN");
	getMaxRlt = ds_OBNZ340Q_A_PIVOT.Max("MTTR_RLT");
	if(parseInt(getMaxPlt) > parseInt(getMaxRlt)){
		Div_contents.xChart_MTTR.AxisY.Max = parseInt(getMaxPlt)+5;
	}else{
		Div_contents.xChart_MTTR.AxisY.Max = parseInt(getMaxRlt)+5;
	}

	var chartCnt = ds_OBNZ340Q_A_PIVOT.rowcount;
	//Y축 값설정
	for(var i=0;i<chartCnt;i++){
		//MTBF
		Div_contents.xChart_MTBF.Value(0, i) = ds_OBNZ340Q_A_PIVOT.GetColumn(i,"MTBF_PLN");	//계획
		Div_contents.xChart_MTBF.Value(1, i) = ds_OBNZ340Q_A_PIVOT.GetColumn(i,"MTBF_RLT");	//실적
		//MTTR
		Div_contents.xChart_MTTR.Value(0, i) = ds_OBNZ340Q_A_PIVOT.GetColumn(i,"MTTR_PLN");	//계획
		Div_contents.xChart_MTTR.Value(1, i) = ds_OBNZ340Q_A_PIVOT.GetColumn(i,"MTTR_RLT");	//실적
	}	
	//X축 값설정
	for(var i=0;i<chartCnt;i++){
		Div_contents.xChart_MTBF.Legend(i) = ds_OBNZ340Q_A_PIVOT.GetColumn(i,"MONTH");// MTBF년월		
		Div_contents.xChart_MTTR.Legend(i) = ds_OBNZ340Q_A_PIVOT.GetColumn(i,"MONTH");// MTTR년월		
	}
}

