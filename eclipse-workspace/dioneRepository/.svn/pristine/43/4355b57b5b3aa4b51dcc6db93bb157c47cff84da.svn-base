/********************************************************************** 
 * 내용설명 : Transaction 리턴후 실행 함수
 * 주요변수 : 
 * 			- sMethodName	Transaction 고유 ID
 * 			- ErrorCode 	에러코드
 * 			- ErrorMSG 		에러 메세지
 **********************************************************************/ 
function callback(svcId, rtnCd, rtnMsg){
	if(svcId=="getOBNZ340Q"){
		if(rtnCd<0){
			alert(setPopMessage("1002"));
			setMessage("1002");
		}else{
			if(ds_OBNZ340Q_A.rowcount<1){
				alert(setPopMessage("1003"));
				setMessage("1003");	
			}else{
				//trace(ds_OBNZ340Q_A.SaveXML());
				var getMtbfPlt = ds_OBNZ340Q_B.GetColumn(0,"MTBF_MON_PLT");
				var getMtbfRlt = ds_OBNZ340Q_B.GetColumn(0,"MTBF_MON_RLT");
				var getMttrPlt = ds_OBNZ340Q_B.GetColumn(0,"MTTR_MON_PLT");
				var getMttrRlt = ds_OBNZ340Q_B.GetColumn(0,"MTTR_MON_RLT");
				Div_bottom.ed_MTBF_MON_Rate.Value = parseFloat(getMtbfPlt)/parseFloat(getMtbfRlt);
				Div_bottom.ed_MTTR_MON_Rate.Value = parseFloat(getMttrPlt)/parseFloat(getMttrRlt);
								
				ds_OBNZ340Q_A_PIVOT.ClearData();
				for(i=1; i<13; i++){
					ds_OBNZ340Q_A_PIVOT.AddRow();
					ds_OBNZ340Q_A_PIVOT.SetColumn(ds_OBNZ340Q_A_PIVOT.currow, "COMPS", ds_OBNZ340Q_A.GetColumn(0,"COMPS"));
					if(i <10){
						//ds_OBNZ340Q_A_PIVOT.SetColumn(ds_OBNZ340Q_A_PIVOT.currow, "MONTH", substr(cmb_PYEAR.Value,0,4)+lpad(i+3,"0",2));
						ds_OBNZ340Q_A_PIVOT.SetColumn(ds_OBNZ340Q_A_PIVOT.currow, "MONTH", lpad(i+3,"0",2));
					}else{
						//ds_OBNZ340Q_A_PIVOT.SetColumn(ds_OBNZ340Q_A_PIVOT.currow, "MONTH", (parseInt(substr(cmb_PYEAR.Value,0,4))+1)+lpad(i-9,"0",2));
						ds_OBNZ340Q_A_PIVOT.SetColumn(ds_OBNZ340Q_A_PIVOT.currow, "MONTH", lpad(i-9,"0",2));
					}
					ds_OBNZ340Q_A_PIVOT.SetColumn(ds_OBNZ340Q_A_PIVOT.currow, "MTBF_PLN", ds_OBNZ340Q_A.GetColumn(0,"MTBF_PLN"+lpad(i,"0",2)));
					ds_OBNZ340Q_A_PIVOT.SetColumn(ds_OBNZ340Q_A_PIVOT.currow, "MTBF_RLT", ds_OBNZ340Q_A.GetColumn(0,"MTBF_RLT"+lpad(i,"0",2)));
					ds_OBNZ340Q_A_PIVOT.SetColumn(ds_OBNZ340Q_A_PIVOT.currow, "MTTR_PLN", ds_OBNZ340Q_A.GetColumn(0,"MTTR_PLN"+lpad(i,"0",2)));
					ds_OBNZ340Q_A_PIVOT.SetColumn(ds_OBNZ340Q_A_PIVOT.currow, "MTTR_RLT", ds_OBNZ340Q_A.GetColumn(0,"MTTR_RLT"+lpad(i,"0",2)));
				}
				//trace(ds_OBNZ340Q_A_PIVOT.SaveXML());
				loadingChart();
				//칸트차트사용으로 미사용
				alert(setPopMessage("1004"));
				setMessage("1004");	
			}
		}
	}
}
