	Object('med_A'+temp_mm).Value = toNumber(Object('med_Q'+temp_mm).Value) * toNumber(Object('med_P'+temp_mm).Value);
	Object('med_Q'+temp_99+'_99').Value = 
	toNumber(Object('med_Q'+temp_99+'_04').Value)+toNumber(Object('med_Q'+temp_99+'_05').Value)+toNumber(Object('med_Q'+temp_99+'_06').Value)+
	toNumber(Object('med_Q'+temp_99+'_07').Value)+toNumber(Object('med_Q'+temp_99+'_08').Value)+toNumber(Object('med_Q'+temp_99+'_09').Value)+
	toNumber(Object('med_Q'+temp_99+'_10').Value)+toNumber(Object('med_Q'+temp_99+'_11').Value)+toNumber(Object('med_Q'+temp_99+'_12').Value)+
	toNumber(Object('med_Q'+temp_99+'_01').Value)+toNumber(Object('med_Q'+temp_99+'_02').Value)+toNumber(Object('med_Q'+temp_99+'_03').Value);
	/*
	Object('med_P'+temp_99+'_99').Value = 
	toNumber(Object('med_P'+temp_99+'_04').Value)+toNumber(Object('med_P'+temp_99+'_05').Value)+toNumber(Object('med_P'+temp_99+'_06').Value)+
	toNumber(Object('med_P'+temp_99+'_07').Value)+toNumber(Object('med_P'+temp_99+'_08').Value)+toNumber(Object('med_P'+temp_99+'_09').Value)+
	toNumber(Object('med_P'+temp_99+'_10').Value)+toNumber(Object('med_P'+temp_99+'_11').Value)+toNumber(Object('med_P'+temp_99+'_12').Value)+
	toNumber(Object('med_P'+temp_99+'_01').Value)+toNumber(Object('med_P'+temp_99+'_02').Value)+toNumber(Object('med_P'+temp_99+'_03').Value);
	*/
	Object('med_A'+temp_99+'_99').Value = 
	toNumber(Object('med_A'+temp_99+'_04').Value)+toNumber(Object('med_A'+temp_99+'_05').Value)+toNumber(Object('med_A'+temp_99+'_06').Value)+
	toNumber(Object('med_A'+temp_99+'_07').Value)+toNumber(Object('med_A'+temp_99+'_08').Value)+toNumber(Object('med_A'+temp_99+'_09').Value)+
	toNumber(Object('med_A'+temp_99+'_10').Value)+toNumber(Object('med_A'+temp_99+'_11').Value)+toNumber(Object('med_A'+temp_99+'_12').Value)+
	toNumber(Object('med_A'+temp_99+'_01').Value)+toNumber(Object('med_A'+temp_99+'_02').Value)+toNumber(Object('med_A'+temp_99+'_03').Value);
	Object('med_P'+temp_99+'_99').Value = round(toNumber(Object('med_A'+temp_99+'_99').Value) / toNumber(Object('med_Q'+temp_99+'_99').Value));
	
	//Object('med_A'+temp_mm).Text = (Object('med_Q'+temp_mm).Value)*(Object('med_P'+temp_mm).Value);
}

function clearEditObject()
{
	for (var z=1;z<6;z++){
		for( var x=0;x<arrMM.length();x++){
			Object('med_Q'+toString(z)+'_'+arrMM[x]) = 0;
			Object('med_P'+toString(z)+'_'+arrMM[x]) = 0;
			Object('med_A'+toString(z)+'_'+arrMM[x]) = 0;
		}
		Object('med_Q'+toString(z)+'_'+'99') = 0;
		Object('med_P'+toString(z)+'_'+'99') = 0;
		Object('med_A'+toString(z)+'_'+'99') = 0;
	}
	med_PRICE.Text = '';
	ed_PLTCD.Text = '';
	ed_PRDCD.Text = '';
	ed_CARCD.Text = '';
	ed_BDYCD.Text = '';
	ed_EXDSP.Text = '';
}

function enabledEditObject()
{
	for (var z=1;z<6;z++){
		for( var x=0;x<arrMM.length();x++){
			Object('med_Q'+toString(z)+'_'+arrMM[x]).Enable = 1;
			Object('med_P'+toString(z)+'_'+arrMM[x]).Enable = 1;
		}
	}
}

function disabledEditObject()
{
	for (var z=1;z<6;z++){
		for( var x=0;x<arrMM.length();x++){
			Object('med_Q'+toString(z)+'_'+arrMM[x]).Enable = 0;
			Object('med_P'+toString(z)+'_'+arrMM[x]).Enable = 0;
		}
	}
}

