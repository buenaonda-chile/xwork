 function setSPYZ201(){
	if(med_Q1_04.Enable==0){
		alert(setPopMessage("1012"));  //[경고] 조회후 실행바랍니다.
		setMessage("1012");	
		return;
	}
	if(length(trim(st_pyvnd.Text)) < 1){
		alert(setPopMessage("1502"));  //[확인] 거래처코드 확인바랍니다.
		setMessage("1502");	
		ed_pyvnd.SetFocus();
		return;
	}	
	if(length(trim(st_pspno.Text)) < 1){
		alert(setPopMessage("1501"));  //[확인] 품번 확인바랍니다.
		setMessage("1501");	
		ed_pspno.SetFocus();
		return;
	}
 	var yyyy1 = cmb_yyyy.Value;
	var yyyy2 = toNumber(cmb_yyyy.Value) + 1;
	var yyyy3 = toNumber(cmb_yyyy.Value) + 2;
	var yyyy4 = toNumber(cmb_yyyy.Value) + 3;
	var yyyy5 = toNumber(cmb_yyyy.Value) + 4;
	var yyyy6 = toNumber(cmb_yyyy.Value) + 5;
	
	//EDIT값들을 배열에 저장하여 dataset으로 저장
	var arrValueM = array();   //년월저장
	var arrValueQ = array();   //월수량저장
	var arrValueP = array();   //월단가저장
	var arrValueT = array();   //년Total수량
	var arrValueR = array();   //월배율
	var yyyyMMdd = yyyy1+arrMM[0]+'01';
	for (var m=0;m<60;m++){
		arrValueM[m] = substr(AddMonth(yyyyMMdd,m),0,6);
	}
	var y=0;
	var y1Q = 0;  //yyyy년 Total수량
	var y2Q = 0;  //yyyy년+1 Total수량
	var y3Q = 0;  //yyyy년+2 Total수량
	var y4Q = 0;  //yyyy년+3 Total수량
	var y5Q = 0;  //yyyy년+4 Total수량
	for (var z=1;z<6;z++){   //월수량및단가 배열에 넣기 , 년total수량 구하기
		for( var x=0;x<arrMM.length();x++){
			arrValueQ[y]=Object('med_Q'+toString(z)+'_'+arrMM[x]).Value;
			arrValueP[y]=Object('med_P'+toString(z)+'_'+arrMM[x]).Value;
			if(z==1) y1Q=y1Q+toNumber(arrValueQ[y]);
			if(z==2) y2Q=y2Q+toNumber(arrValueQ[y]);
			if(z==3) y3Q=y3Q+toNumber(arrValueQ[y]);
			if(z==4) y4Q=y4Q+toNumber(arrValueQ[y]);
			if(z==5) y5Q=y5Q+toNumber(arrValueQ[y]);
			y=y+1;
		}
	}
	for (var a=0;a<60;a++){  //년total수량 배열에 넣기  ,월배율 배열에 넣기
		if (a<12) {
			arrValueT[a] = y1Q; arrValueR[a] = iif(y1Q != 0, round(toNumber(arrValueQ[a])/y1Q*100,2),0); 
		}else if(a<24){
			arrValueT[a] = y2Q; arrValueR[a] = iif(y2Q != 0, round(toNumber(arrValueQ[a])/y2Q*100,2),0); 
		}else if(a<36){
			arrValueT[a] = y3Q; arrValueR[a] = iif(y3Q != 0, round(toNumber(arrValueQ[a])/y3Q*100,2),0); 
		}else if(a<48){
			arrValueT[a] = y4Q; arrValueR[a] = iif(y4Q != 0, round(toNumber(arrValueQ[a])/y4Q*100,2),0); 
		}else if(a<60){
			arrValueT[a] = y5Q; arrValueR[a] = iif(y5Q != 0, round(toNumber(arrValueQ[a])/y5Q*100,2),0); 
		}
	}
	ds_SPYZ201insert.ClearData();
	var i=0;
	for (var j=0;j<60;j++){
		if(toNumber(arrValueQ[j])*toNumber(arrValueP[j]) != 0){
			ds_SPYZ201insert.InsertRow(i);
			ds_SPYZ201insert.SetColumn(i,"COMPS",cb_searchCodeName1.Value);
			ds_SPYZ201insert.SetColumn(i,"PLNYM",arrValueM[j]);
			ds_SPYZ201insert.SetColumn(i,"PLNGB",cb_plngb.Value);
			ds_SPYZ201insert.SetColumn(i,"PYVND",ed_pyvnd.Text);
			ds_SPYZ201insert.SetColumn(i,"SHPTO","0");
			ds_SPYZ201insert.SetColumn(i,"PSPNO",ed_pspno.Text);
			ds_SPYZ201insert.SetColumn(i,"PLTCD",ed_PLTCD.Text);
			ds_SPYZ201insert.SetColumn(i,"PRDCD",ed_PRDCD.Text);
			ds_SPYZ201insert.SetColumn(i,"SPTEX","" );
			ds_SPYZ201insert.SetColumn(i,"EXDSP",ed_EXDSP.Text);
			ds_SPYZ201insert.SetColumn(i,"CARCD",ed_CARCD.Text);
			ds_SPYZ201insert.SetColumn(i,"BDYCD",ed_BDYCD.Text);
			ds_SPYZ201insert.SetColumn(i,"YERQY",arrValueT[j]);
			ds_SPYZ201insert.SetColumn(i,"YESCS",arrValueP[j]);
			ds_SPYZ201insert.SetColumn(i,"MRATE",arrValueR[j]);
			ds_SPYZ201insert.SetColumn(i,"MONQY",arrValueQ[j]);
			i=i+1;
		}
	}
	
 	http.Sync = true;
	var sKind		= "SAVE_SPYZ201";						
	var sMethodName = "service::pssallib/insertSPYZ201.do";
	var sInDataSet  = "ds_SPYZ201insert=ds_SPYZ201insert:U";
	var sOutDataSet = "";
	var sArgument   = "";
		sArgument += " LIB2=" + quote(Lib2);
		sArgument += " COMPS=" + quote(cb_searchCodeName1.Value);
		sArgument += " PLNGB=" + quote(cb_plngb.Value);
		sArgument += " PYVND=" + quote(ed_pyvnd.Text);
		sArgument += " PSPNO=" + quote(ed_pspno.Text);
		sArgument += " PLM104=" + quote(yyyy1+'04');
		sArgument += " PLM503=" + quote(yyyy6+'03');
		
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");
	
	disabledEditObject();
	ed_pspno.SetFocus();
 }

