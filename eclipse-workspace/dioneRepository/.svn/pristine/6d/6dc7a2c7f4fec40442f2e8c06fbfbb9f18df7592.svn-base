				case j+'10' : 
					Object(medQtemp+'_10').Text = ds_SPYZ201.GetColumn(i,"MONQY");
					Object(medPtemp+'_10').Text = ds_SPYZ201.GetColumn(i,"YESCS");
					med_OnCharChanged(Object(medQtemp+'_10'));
