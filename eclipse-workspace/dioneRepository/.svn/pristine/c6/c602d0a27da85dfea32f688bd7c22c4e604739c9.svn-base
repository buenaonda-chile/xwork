 **********************************************************************/ 
function ds_ogm115_OnColumnChanged(obj,nRow,strColumnID,varOldValue,varNewValue,nPivotIndex)
{
	if( strColumnID == "INQTY" ){
		if (varNewValue != "0") {
			ds_ogm115.SetColumn(nRow,"SEL","1");
		}else{
			ds_ogm115.SetColumn(nRow,"SEL","0");
		}
		var newAmt = ToNumber(varNewValue) * ToNumber(ds_ogm115.GetColumn(nRow,"ICOST"));
		ds_ogm115.SetColumn(nRow,"IAMT",newAmt);  //quote(newAmt)
	}
	if( strColumnID == "ICOST" ){
		var newAmt = ToNumber(varNewValue) * ToNumber(ds_ogm115.GetColumn(nRow,"INQTY"));
		ds_ogm115.SetColumn(nRow,"IAMT",newAmt);  //quote(newAmt)
	}
}


function ds_OGM115_CanColumnChange(obj,nRow,strColumnID,varOldValue,varNewValue,nPivotIndex)
{
	if(strColumnID == "INQTY"){
		if (ToNumber(varNewValue) > ToNumber(ds_ogm115.GetColumn(nRow,"JANQY"))) {
			alert(setPopMessage("2512"));
			setMessage("2512");	
			return false;
		}
		if (ToNumber(varNewValue) < 0) {
			alert(setPopMessage("2517"));
			setMessage("2517");	
			return false;
		}
	}
}


