/********************************************************************** 
 * 내용설명 : 화면 조회 
 **********************************************************************/ 
function getogm115(){
	var group = ComboBoxClass.Value;
	var sorderno = trim(ed_ordno.Text); //발주번호
	var sPorno = trim(edPorno.Text);    //구매발주번호
	var sPrtno = trim(txtprtno.Text);    //품번
	var sVndor = trim(txtvndor.Text);    //구입선
	var sOrdatfrom = "";    //
	var sOrdatto = "";    //
	var sDptcd = trim(ed_dptcd.Text);    //부서
	
	if ((txtvndor.Text != "" && sVndor != "") && (txtvname.Text == "")){
		var sKind		= "getOGM040PF_dchk";						
		var sMethodName = "service::psogmlib/selectOGM040PF_dchk.do"; 
		var sInDataSet  = "";
		var sOutDataSet = "ds_OGM040PF_dchk=ds_OGM040PF_dchk";
		var sArgument   = "";
			sArgument  += " LIB2=" + quote(Lib2);
			sArgument  += " VNDNRS=" + quote(sVndor);		
		Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
		if (ds_OGM040PF_dchk.GetRowCount() != 0){
			txtvname.Text = ds_OGM040PF_dchk.GetColumn(0,"VNAME");
		}
	}
	if ((txtprtno.Text != "" && sPrtno != "") && (txtprtnm.Text == "")){
		var sKind		= "getogm010_dchk";						
		var sMethodName = "service::psogmlib/selectOGM010_dchk.do"; 
		var sInDataSet  = "";
		var sOutDataSet = "ds_OGM010_dchk=ds_OGM010_dchk";
		var sArgument   = "";
			sArgument  += " LIB2=" + quote(Lib2);
			sArgument  += " COMPS=" + quote(ComboBoxPlant.Value); //사업부
			sArgument  += " GROUP=" + quote(ComboBoxClass.Value); //그룹
			sArgument  += " PRTNO=" + quote(trim(txtprtno.Text)); //신규품번		
		Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
		if (ds_OGM010_dchk.GetRowCount() != 0){
			txtprtnm.Text = ds_OGM010_dchk.GetColumn(0,"PRTNM");
		}
	}
	
	var sKind		= "getOgm115";						
	var sMethodName = "service::psogmlib/selectOGM115.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_OGM115=ds_OGM115";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
	
	sArgument += "@LIB2=" + quote(Lib2);
	sArgument += "@COMPS=" + quote(ComboBoxPlant.Value); //사업부
	sArgument += "@GROUP=" + quote(group); //그룹
	sArgument += "@ORDNO=" + quote(sorderno); //발주번호
	sArgument += "@SPAREORDATFROM=" + quote(sOrdatfrom); //
	sArgument += "@SPAREORDATTO=" + quote(sOrdatto); //
	sArgument += "@PORNO=" + quote(sPorno); //
	sArgument += "@PRTNO=" + quote(sPrtno); //
	sArgument += "@VNDOR=" + quote(sVndor); //
	sArgument += "@DPTCD=" + quote(sDptcd);      //관리부서코드
	var str = dialog("Common::PGMLOAD.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");

	if (saveQuery == "F"){
		if (str == 'T'){  //정상조회
			alert(setPopMessage("1004"));
			setMessage("1004");	
		}else if (str == 'B'){   //data값이 없을때
			alert(setPopMessage("1003"));
			setMessage("1003");	
		}else if (str == 'E'){   //error발생시
			alert(setPopMessage("1002"));
			setMessage("1002");	
		}
	}
}

/********************************************************************** 
 * 내용설명 : 저장 update
 **********************************************************************/ 
function setogm115(){
	if (!closeYN(Lib2,ComboBoxPlant.Value,ComboBoxClass.Value,substr(Calendar0.Value,0,6))){
		alert(setPopMessage("2500"));
		setMessage("2500");	
		return;
	}
	
	if(ds_ogm115.rowcount() < 1) {
		alert(setPopMessage("2509"));
		setMessage("2509");	
		return;
	}
	var iCount=0;
	for(var i=0; i<ds_ogm115.rowcount(); i++){
		if (ds_ogm115.GetColumn(i, "SEL") == "1"){
			iCount = iCount+1;
		}
	}
	if (iCount == 0){ 
		alert(setPopMessage("2510"));
		setMessage("2510");	
		return;
	}
	
	if (st_indidName.Text == "" || trim(ed_indid.Text) == ""){
		alert(setPopMessage("2511"));
		setMessage("2511");	
		return;
	}
	
	if(confirm("선택하신 품목을 입고처리 하시겠습니까? ") == false) return;
	
	var iNo = 0;
	for(var i=0; i<ds_ogm115.rowcount(); i++){
		if (ds_ogm115.GetColumn(i, "SEL") == "1" ){
			if(ToNumber(ds_ogm115.GetColumn(i, "ICOST")) > 0 && ToNumber(ds_ogm115.GetColumn(i, "INQTY")) > 0){
			    //------------------UPDATE
				var janqty = ToNumber(ds_ogm115.GetColumn(i, "JANQY"));
				var inqty = ToNumber(ds_ogm115.GetColumn(i, "INQTY"));
				var icost = ToNumber(ds_ogm115.GetColumn(i, "ICOST"));
				var insts = "40";
				var newJanqy =janqty - inqty;
				if (janqty > inqty) insts = "30";
				http.Sync 	= true;
				var sKind		= "updateOGM115";						
				var sMethodName = "service::psogmlib/updateOGM115.do";  // 서버에서 호출 될 Method 명
				var sInDataSet  = "";                                    //자료 저장 시 보낼 DataSet 명칭
				var sOutDataSet = "";    							     // 조회 후 돌려 받을 DataSet 명칭
				var sArgument   = "";
				sArgument += " LIB2=" + quote(Lib2);
				//target
				sArgument += " COMPS=" + quote(ComboBoxPlant.Value);
				sArgument += " GROUP=" + quote(ComboBoxClass.Value);  
				sArgument += " ORDNO=" + quote(ds_ogm115.GetColumn(i, "ORDNO")); 
				sArgument += " PRTNO=" + quote(ds_OGM115.GetColumn(i,"PRTNO"));//prtno
				//target value
				sArgument += " ORQTY=" + quote("ORQTY");
				sArgument += " JANQY=" + quote(newJanqy);
				sArgument += " ORSTS=" + quote(insts);   
				Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");
				
				//------------------INSERT
				var idnum = substr(G_ds_user.GetColumn(0,"USER_ID"),1,7);
				if (iNo == 0){
					maxNumberSelect();//발주번호구하기
					var max_no = ds_OGM115_maxNumber.GetColumn(0,"MAXINPNO");
					iNo = ToNumber(max_no);
				}else{
					iNo = iNo+1;
				}
				
				var inpnumber;
				if (iNo > 99999999) {
					inpnumber = ""+iNo;
				}else if (iNo > 9999999) {
					inpnumber = "0"+iNo;
				}else if (iNo > 999999) {
					inpnumber = "00"+iNo;
				}else if (iNo > 99999) {
					inpnumber = "000"+iNo;
				}else if (iNo > 9999) {
					inpnumber = "0000"+iNo;
				}else if (iNo > 999) {
					inpnumber = "00000"+iNo;
				}else if (iNo > 99) {
					inpnumber = "000000"+iNo;
				}else if (iNo > 9) {
					inpnumber = "0000000"+iNo;
				}else if (iNo > 0) {
					inpnumber = "00000000"+iNo;
				}
				http.Sync 	= true;
				var sKind		= "insertOGM115";						
				var sMethodName = "service::psogmlib/insertOGM115.do";     // 서버에서 호출 될 Method 명
				var sInDataSet  = "";                                     //자료 저장 시 보낼 DataSet 명칭
				var sOutDataSet = "";    							     // 조회 후 돌려 받을 DataSet 명칭
				var sArgument   = "";
				sArgument += " LIB2=" + quote(Lib2);
				sArgument += " COMPS=" + quote(ComboBoxPlant.Value);
				sArgument += " GROUP=" + quote(ComboBoxClass.Value);  
				sArgument += " INPNO=" + quote(inpnumber); 
				sArgument += " VNDOR=" + quote(ds_OGM115.GetColumn(i,"VNDOR"));//vndor
				sArgument += " PRTNO=" + quote(ds_OGM115.GetColumn(i,"PRTNO"));//prtno
				sArgument += " PUNIT=" + quote(ds_OGM115.GetColumn(i,"PUNIT"));//punit
				sArgument += " INDAT=" + quote(Calendar0.Value);
				sArgument += " INPYM=" + quote(substr(Calendar0.Value,0,6));
				sArgument += " INDID=" + quote(ed_indid.Value);
				sArgument += " INGUB=" + quote("10");  //10:정상, 20:강제, 90:취소
				sArgument += " INQTY=" + quote(inqty);
				sArgument += " ICOST=" + quote(ds_ogm115.GetColumn(i, "ICOST"));
				sArgument += " INUSE=" + quote(ds_ogm115.GetColumn(i, "ORUSE"));
				sArgument += " ORDNO=" + quote(ds_ogm115.GetColumn(i, "ORDNO"));
				sArgument += " ICNNO=" + quote("0");
				//addat,adtim
				sArgument += " ADUSR=" + quote(idnum);  //등록자
				//chdat,chtim
				Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");	
				
				//------------------UPDATE
				var ijg = ToNumber(ds_ogm115.GetColumn(i, "JG"));
				var ipcost = ToNumber(ds_ogm115.GetColumn(i, "PCOST"));
				var iamt = ijg * ipcost;
				var iamt2 = inqty * icost;
				ipcost = ( iamt + iamt2 ) / ( ijg + inqty );
				
				http.Sync 	= true;
				var sKind		= "updateOGM115_1";						
				var sMethodName = "service::psogmlib/updateOGM115_1.do";  // 서버에서 호출 될 Method 명
				var sInDataSet  = "";                                    //자료 저장 시 보낼 DataSet 명칭
				var sOutDataSet = "";    							     // 조회 후 돌려 받을 DataSet 명칭
				var sArgument   = "";
				sArgument += " LIB2=" + quote(Lib2);
				//target
				sArgument += " COMPS=" + quote(ComboBoxPlant.Value);
				sArgument += " GROUP=" + quote(ComboBoxClass.Value);  
				sArgument += " PRTNO=" + quote(ds_OGM115.GetColumn(i,"PRTNO"));//prtno
				//target value
				sArgument += " PCOST=" + quote(ipcost);
				sArgument += " NCOST=" + quote(icost);
				Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");
			}
		}
	}
	alert(setPopMessage("1001"));
	setMessage("1001");
	saveQuery="T";
	getOgm115();  //처리후 재조회

}

