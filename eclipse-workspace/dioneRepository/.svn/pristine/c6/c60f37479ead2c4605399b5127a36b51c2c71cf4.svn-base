	cmb_mngno.Index = 0;
	//wb_preview.PageUrl = "C:/Desert.jpg";//str_param_server;
	//wb_preview.Run();	
}

function getComCode(){
	http.Sync 	= true;
	var sKind		= "getComCode";						
	var sMethodName = "service::login/selectComCode.do"; 
	var sInDataSet  = "";
	var sOutDataSet = "ds_ComCode_PLNT=ds_ComCode_PLNT";
	var sArgument   = "";
		sArgument   += " CLASS='PLNT'";
		sArgument   += " CODE1=" + quote(company);		
	http.Sync=true;
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");	
	http.Sync=false;
	ComboBoxPlant.Value=comps;
	ComboBoxPlant.Enable = false; 
}

function getDesign(){
	searchBar.Width = G_ds_design.GetColumn(0,"Width");
	searchBar.Top = G_ds_design.GetColumn(0,"SBTop");	
}

function getJbcode(){
	http.Sync 	= true;
	var sKind		= "getJbcode";						
	var sMethodName = "service::psogmlib/selectORPE001Rjbcode.do"; 
	var sInDataSet  = "";
	var sOutDataSet = "ds_orpe001r_jbcode=ds_orpe001r_jbcode";
	var sArgument   = "";	
	http.Sync=true;
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");	
	http.Sync=false;
	
	ds_orpe001r_jbcode.InsertRow(0);
	ds_orpe001r_jbcode.SetColumn(0, "NAME", "전체선택");
	ds_orpe001r_jbcode.SetColumn(0, "MNGNO", "%");
}

function fn_Excel() {
	if (ds_orpe005_select.rowcount > 0) {	
		var fName   = "사외성적서";
		var arrSheets  =  Array();  
	
		arrSheets[0]  = "MainGrid|사외성적서";
		createExcelSheetS2(FileDialog0, fName ,arrSheets);
	} else {
		alert("다운받을 자료가 없습니다.");
	}	
}

function fn_Input() {	
	ds_orpe002_main.DeleteAll();		
	ds_orpe002_main.AddRow();
	ed_repno.Text = "";
	//cal_kjdt.DropDown();	
}

function fn_Search() {
	
	var sKind		= "selectORPE005";						
	var sMethodName = "service::psogmlib/selectORPE005.do"; 
	var sInDataSet  = "";
	var sOutDataSet = "ds_orpe005_select=ds_orpe005_select";
	var sArgument   = "";	
	
	if(length (trim(ed_repno.Text)) > 0) {		
		sArgument += "@REPNO=" + quote(trim(ed_repno.Text));	
	} else	{		
		sArgument += "@REPNO='%'";			
	}	
	
	sArgument += "@GUBUN=" + quote("O");		
	
	if(length(trim(cal_validdt_from.Value)) > 0) {		
		sArgument += "@VALIDDT_FROM=" + quote(trim(cal_validdt_from.Value));	
	} else	{		
		sArgument += "@VALIDDT_FROM='%'";			
	}	
	if(length(trim(cal_validdt_to.Value)) > 0) {		
		sArgument += "@VALIDDT_TO=" + quote(trim(cal_validdt_to.Value));	
	} else	{			
		sArgument += "@VALIDDT_TO='%'";	
	}		
	if(length(trim(cal_repdt_from.Value)) > 0) {		
		sArgument += "@REPDT_FROM=" + quote(trim(cal_repdt_from.Value));	
	} else	{		
		sArgument += "@REPDT_FROM='%'";		
	}	
	if(length(trim(cal_repdt_to.Value)) > 0) {		
		sArgument += "@REPDT_TO=" + quote(trim(cal_repdt_to.Value));	
	} else	{		
		sArgument += "@REPDT_TO='%'";		
	}		
	if(length(trim(cal_nextrepdt_from.Value)) > 0) {		
		sArgument += "@NEXT_REPDT_FROM=" + quote(trim(cal_nextrepdt_from.Value));	
	} else	{		
		sArgument += "@NEXT_REPDT_FROM='%'";		
	}			
	if(length(trim(cal_nextrepdt_to.Value)) > 0) {		
		sArgument += "@NEXT_REPDT_TO=" + quote(trim(cal_nextrepdt_to.Value));	
	} else	{		
		sArgument += "@NEXT_REPDT_TO='%'";			
	}					
	if(length(trim(ds_orpe001r_jbcode.GetColumn(ds_orpe001r_jbcode.row,"MNGNO"))) > 0) {		
		sArgument += "@MNGNO=" + quote(cmb_mngno.Value);	
	} else	{		
		sArgument += "@MNGNO='%'";			
	}			
	if(length(trim(ed_file.Text)) > 0) {		
		sArgument += "@FILENAME=" + quote(trim(ed_file.Text));	
	} else	{			
		sArgument += "@FILENAME='%'";			
	}				
	if(length(trim(ed_org.Text)) > 0) {		
		sArgument += "@GIKWAN=" + quote(trim(ed_org.Text));	
	} else	{			
		sArgument += "@GIKWAN='%'";			
	}		  

	//http.Sync=true;
	//Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");	
	//http.Sync=false;		
	var str = dialog("Common::PGMLOAD.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument, 272, 64, "TitleBar=false");	
}

function fn_Delete() {
	del_filename = ed_file.Text;
	ds_orpe002_main.DeleteRow(ds_orpe002_main.Row); 
	repno = trim(ed_repno.Text);
	ed_repno.Text = "";
}

