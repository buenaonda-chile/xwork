 **********************************************************************/ 
function callback(svcId, rtnCd, rtnMsg){
	if(svcId=="getHMDZ008Q"){
		if(rtnCd<0){
			alert(setPopMessage("1002"));
			setMessage("1002");
			
		}else{
			if(ds_HMDZ008Q.rowcount<1){
				alert(setPopMessage("1003"));
				setMessage("1003");	
			}else{
				alert(setPopMessage("1004"));
				setMessage("1004");	
			}
		}
	}
}
/********************************************************************** 
 * 내용설명 : validation 체크
 **********************************************************************/ 
function validationChk(){

	var result = true;
	cmb_KNYYMF.BKColor="white";
	cmb_KNYYMT.BKColor="white";
		
	if (cmb_KNYYMF.Value == ""){
		cmb_KNYYMF.BKColor="paleturquoise";
		alert(setPopMessage("4053"));//[확인] 기준년월을 확인바랍니다.
		setMessage("4053");
		result = false;
	}else if(cmb_KNYYMT.Value == ""){
		cmb_KNYYMT.BKColor="paleturquoise";
		alert(setPopMessage("4053"));//[확인] 기준년월을 확인바랍니다.
		setMessage("4053");
		result = false;
	}else if(cmb_KNYYMF.Value > cmb_KNYYMT.Value){
		cmb_KNYYMF.BKColor="paleturquoise";
		alert(setPopMessage("4053"));//[확인] 기준년월을 확인바랍니다.
		setMessage("4053");
		result = false;
	}
	else if((substr(cmb_KNYYMT.Value,0,4) - substr(cmb_KNYYMF.Value,0,4)) > 2) {
		cmb_KNYYMF.BKColor="paleturquoise";
		alert("[알림]조회기간은 최대 2년입니다.");//[확인] 기준년월을 확인바랍니다.
		//alert(setPopMessage("4053"));//[확인] 기준년월을 확인바랍니다.
		setMessage("4053");
		result = false;
	} 
	return result;
}

