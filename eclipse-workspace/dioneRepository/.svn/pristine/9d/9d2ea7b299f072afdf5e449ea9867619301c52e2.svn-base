	http.TimeOut = 300;	
	*/
	//수정 2016.03.25
	
	var i=0;
	var cntMon=0;	//월계산
	var p_KNYYMF="";
	for(i=0; i<=12; i++){
		 if(substr(addMonth(cmb_KNYYMF.Value,i),0,6) == substr(cmb_KNYYMT.Value,0,6)){
			i++;
			break;
		 }
	}	
	//alert(i);
	cntMon=i;

	for(j=0; j<cntMon; j++){
