/********************************************************************** 
 * 내용설명 : 화면 데이터 APPEND
 **********************************************************************/ 
function subMSTC410Q_12(dsObj)
{
	var intStep2 = 0;

	strStep = strStep + (intStep+1);

	intStep++;

	ds_tmp2.Copy(object(dsObj));
	ds_tmp.ClearData();

	for(i=0;i<ds_tmp2.rowcount;i++)
	{
		var sArgument   = "";
		
		intStep2++;
		var qty_sub_12;
		if(trim(ds_tmp2.GetColumn(i,"ITNBR")) == "73610-1012"){
			qty_sub_12 = 0;
		}
		qty_sub_12 = iif(ds_tmp2.GetColumn(i,"QTYPR") == null,"0",ds_tmp2.GetColumn(i,"QTYPR"));
		http.Sync = true;
		var sKind		= "subMSTC410Q_12";						
		var sMethodName = "service::psmstlib/selectMSTC410QSub_12.do";     		// 서버에서 호출 될 Method 명
		var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
		var sOutDataSet = "ds_LEVEL1From=ds_LEVEL1From"; 
		var sArgument   = "";
		sArgument += " LIB2=" + quote(Lib2);	
		sArgument += " PINBR=" + quote(ds_tmp2.GetColumn(i,"ITNBR"));
		sArgument += " STEP=" + quote(ds_tmp2.GetColumn(i,"ROWNUMBER"));
		sArgument += " STEP2=" + quote(intStep2);
		sArgument += " STEP_TXT=" + quote(makePoint(intStep) + intStep);
		sArgument += " QTY=" + quote(qty_sub_12);
		Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");

		ds_MSTC410Q.AppendDataset("ds_LEVEL1From");
		ds_tmp.AppendDataset("ds_LEVEL1From");
	}

	if(object(dsObj).rowcount != 0)
	{
		subMSTC410Q_12("ds_tmp");
	} else {
		if(cb_grpcd.Value = "2"){
			ds_MSTC410Q.Sort("ROWNUMBER");
		}
		alert(setPopMessage("1004"));
		setMessage("1004");			
	}
}
