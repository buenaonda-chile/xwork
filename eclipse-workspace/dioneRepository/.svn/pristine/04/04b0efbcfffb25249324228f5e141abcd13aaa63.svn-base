function getMSTC410Q_12(){
	if(length(trim(ed_pspno.Value)) == 0){
		alert(setPopMessage("2017"));	// 20110103l-koowgnojeel, 2017 == 품번을 입력하세요.
		ed_pspno.SetFocus();
		return;
	}
	if(length(trim(ed_qty.Value)) == 0){
		ed_qty.Value = 0;
	}
	http.Sync = true;
	var sKind		= "getMSTC410Q_12";
	var sMethodName = "service::psmstlib/selectMSTC410Q_12.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_LEVEL1=ds_LEVEL1";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
	sArgument += " LIB2=" + quote(Lib2);
	sArgument += " PINBR=" + quote(ed_pspno.Value);
	sArgument += " STEP='1'";
	sArgument += " STEP_TXT='1'";
	sArgument += " QTY=" + quote(ed_qty.Value); 

	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");
}

