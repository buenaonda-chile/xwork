	}else if(svcId=="getMSTC410Q_12"){
		// 조회건수가 없을경우
		if(ds_LEVEL1.rowcount <= 0){
			alert(setPopMessage("1003"));
			setMessage("1003");	
		// 정상 조회
		} else {
			if(cb_grpcd.Value = "1"){         //일단계부품표
				ds_MSTC410Q.Copy("ds_LEVEL1");
				alert(setPopMessage("1004"));
				setMessage("1004");	
			}else if(cb_grpcd.Value = "2" || cb_grpcd.Value = "3"){     //전단계부품표,요약부품표
				ds_MSTC410Q.Copy("ds_LEVEL1");
				subMSTC410Q_12("ds_LEVEL1");
			}
		}
	}else if(svcId=="getMSTC410Q_34"){
		// 조회건수가 없을경우
		if(ds_LEVEL1.rowcount <= 0){
			alert(setPopMessage("1003"));
			setMessage("1003");	
		// 정상 조회
		} else {
			if(cb_grpcd.Value = "4"){         //일단계사용처
				ds_MSTC410Q.Copy("ds_LEVEL1");
				alert(setPopMessage("1004"));
				setMessage("1004");	
			}else if(cb_grpcd.Value = "5"){   //완제품사용처
				//ds_MSTC410Q.Copy("ds_LEVEL1");
				subMSTC410Q_34("ds_LEVEL1");
			}
		}
	}
}

