function ed_pspno_OnChanged(obj,strText)
{
	ds_MSTC410Q.ClearData();
	http.usewaitcursor = false;
	http.Sync = true;
	var sKind		= "MST300PF_ITNBR";						
	var sMethodName = "service::psmstlib/selectMST300PF_ITNBR.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_main=ds_main"; 
	var sArgument   = "";
	sArgument += " LIB2=" + quote(Lib2);	
	sArgument += " ITNBR=" + quote(ed_pspno.Value);
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	if(ds_main.rowcount == 0){
		st_pspno.Text = "";
		st_ittyp.Text = "";
		alert(setPopMessage("2017"));	// 20110103l-koowgnojeel, 2017 == 품번을 입력하세요.
	}else{
		st_pspno.Text = ds_main.GetColumn(0,"ITDSC");
		st_ittyp.Text = ds_main.GetColumn(0,"ITTYP");
	}
	http.usewaitcursor = true;
}
