function getComCode(){
	http.Sync 	= true;

	var sKind		= "getComCode";						
	var sMethodName = "service::login/selectComCode.do"; 
	var sInDataSet  = "";
	var sOutDataSet = "ds_ComCode_PLNT=ds_ComCode_PLNT";
	var sArgument   = "";
		sArgument   += " CLASS='PLNT'";
		sArgument   += " CODE1=" + quote(company);		
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	//ds_VComCode_PLNT.Copy("ds_ComCode_PLNT");
	//cb_searchCodeName1.Value = comps;
	ds_ComCode_PLNT.InsertRow(0);
	ds_ComCode_PLNT.SetColumn(0,"CODE2","");
	ds_ComCode_PLNT.SetColumn(0,"CODE_NAME","[전체]");
	cb_searchCodeName1.Index = 0;
}
function getDWH000(){
	http.Sync 	= true;
	var sKind		= "getDWHZ000Q";						
	var sMethodName = "service::dwhlib/selectDWHZ000Q.do"; 
	var sInDataSet  = "";
	var sOutDataSet = "ds_DWHZ000Q=ds_DWHZ000Q";
	var sArgument   = "";
		sArgument  += " LIB2=" + quote(Lib2);
		sArgument  += " JOBFL=" + quote(jobpfnm);
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	http.Sync 	= false;
	ca_RVSDT.Value = ds_DWHZ000Q.GetColumn(0,"JYYMM")+"01";
}
/********************************************************************** 
 * 내용설명 : 화면조회
 * 주요변수 : 
 **********************************************************************/ 
function getDWHC107Q()
{
	if(length(trim(ca_RVSDT.Value)) < 8){
		alert(setPopMessage("2016"));  //[알림] 날짜를 입력하세요.
		setMessage("2016");	
		ca_RVSDT.SetFocus();
		return;
	}	

	var sKind		= "getDWHXXXXQ";					
	var sMethodName = "service::dwhlib/selectDWHXXXXQ.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_DWHXXXXQ=ds_DWHXXXXQ";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
	
	sArgument += "@LIB2=" + quote(Lib2);
	//sArgument += "@COMPS=" + quote(cb_searchCodeName1.Value);
	//sArgument += "@RVSDT=" + quote(substr(ca_RVSDT.Value,0,6)); //수금년월
	sArgument += "@JOBFL=" + quote(jobpfnm);
	var str = dialog("Common::PGMLOAD.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");

	if (str == 'T'){  //정상조회
		ds_DWHXXXXQ.Sort("ITCLS,PSPNO");
		alert(setPopMessage("1004"));
		setMessage("1004");	
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));  //[알림] 조회 된 데이터가 없습니다.
		setMessage("1003");	
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}
}

