function on_LoadCompleted(obj){
	getTitle();	
	getDesign();
	gFcOnTopButtonColor("1"); //조회
	getComCode(); //공장구분 선택
}

function getComCode(){
	var sKind		= "getComCode";						
	var sMethodName = "service::login/selectComCode.do"; 
	var sInDataSet  = "";
	var sOutDataSet = "ds_ComCode_PLNT=ds_ComCode_PLNT";
	var sArgument   = "";
		sArgument   += " CLASS='PLNT'";
		//sArgument   += " CODE1=" + quote(company);		
		sArgument   += " CODE1=" + quote("PS");		
	http.Sync=true;
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	http.Sync=false;
	ComboBoxPlant.Value=comps;
	//ComboBoxPlant.Enable = false; 
	ds_ComCode_PLNT.InsertRow(0);
	ds_ComCode_PLNT.SetColumn(0,"CLASS","PLNT");
	ds_ComCode_PLNT.SetColumn(0,"CODE1","PS");
	ds_ComCode_PLNT.SetColumn(0,"CODE2","PS");
	ds_ComCode_PLNT.SetColumn(0,"CODE_NAME","DNKA 전체");
	
	ds_ComCode_PLNT.InsertRow(0);
	ds_ComCode_PLNT.SetColumn(0,"CLASS","PLNT");
	ds_ComCode_PLNT.SetColumn(0,"CODE1","PE");
	ds_ComCode_PLNT.SetColumn(0,"CODE2","E1");
	ds_ComCode_PLNT.SetColumn(0,"CODE_NAME","DNKE 마산");
	if(company="PE"){
		ComboBoxPlant.index = 0;
	}else{
		ComboBoxPlant.index = 1;
	}
	//trace(ds_ComCode_PLNT.SaveXML());
}

function getDesign(){
	MainGrid.BkColor = G_ds_design.GetColumn(0,"BkColor");
