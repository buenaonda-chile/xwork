		}else {
			p_TABLE="MST650PFKA";
		}
	}else{	//실환경
		p_TABLE="MST650PF";
		if(ComboBoxPlant.Value =="E1"){
			Lib2="PSEDBLIB";
		}else {
			Lib2="PSCDBLIB";
		}
	}
	// END
	var sKind		= "getMSTZ650Q";						
	var sMethodName = "service::psmstlib/selectMSTZ650Q.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_MSTZ650Q=ds_MSTZ650Q";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
		//sArgument += "@LIB1=" + quote(Lib1);
		sArgument += "@LIB2=" + quote(Lib2);
		sArgument += "@LIB3=" + quote(Lib3);
		sArgument += "@COMPS=" + quote(ComboBoxPlant.Value); //사업부
		sArgument += "@DPEDSW=" + quote(cmb_GUBCD.Value); //사용여부
		sArgument += "@TABLE=" + quote(p_TABLE); //TEST환경 TABLE명
	var str = dialog("Common::PGMLOAD_MST.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");

	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
		setMessage("1004");
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));
		setMessage("1003");	
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}	
}

