					sArgument += "@MAKDT5=" + quote(makdt[4]); //작업공정FR
					sArgument += "@MAKDT6=" + quote(makdt[5]); //작업공정FR
					sArgument += "@MAKDT7=" + quote(makdt[6]); //작업공정FR
					sArgument += "@MAKDT8=" + quote(makdt[7]); //작업공정FR
					sArgument += "@MAKDT9=" + quote(makdt[8]); //작업공정FR
					sArgument += "@MAKDT10=" + quote(makdt[9]); //작업공정FR
					sArgument += "@MAKDT11=" + quote(makdt[10]); //작업공정FR
					sArgument += "@MAKDT12=" + quote(makdt[11]); //작업공정FR
					sArgument += "@MAKDT13=" + quote(makdt[12]); //작업공정FR
					sArgument += "@MAKDT14=" + quote(makdt[13]); //작업공정FR
					sArgument += "@PROCSF=" + quote(p_procs1); //작업공정FR
					sArgument += "@PROCST=" + quote(p_procs2); //작업공정TO		
				var str = dialog("Common::PGMLOAD_FAC.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");
				if (str == 'T'){  //정상조회
					alert(setPopMessage("1004"));
					setMessage("1004");	
				}else if (str == 'B'){   //data값이 없을때
					alert(setPopMessage("1003"));
					setMessage("1003");	
				}else if (str == 'E'){   //error발생시
					alert(setPopMessage("1002"));
					setMessage("1002");	
				}
				SetWaitCursor(false);
				for(i=0; i<14; i++){
					MainGrid.SetCellProp("Head", 5+i, "text", substr(addDate(cmb_MAKDT.Value,i),4,2)+"/"+substr(addDate(cmb_MAKDT.Value,i),6,2));
				}
			}else if (substr(WTA_RemoteCall0.RC_GetReturnParm(1),50,3) == "888") {
				WTA_RemoteCall0.RC_Disconnect();	//연결종료
				var ruser = trim(substr(WTA_RemoteCall0.RC_GetReturnParm(1),50,10));
				var rcount = toNumber(substr(WTA_RemoteCall0.RC_GetReturnParm(1),60,2))-1;
				alert("사용자("+ruser+")외 "+rcount+"명이 사용중입니다. 작업이 불가능 합니다.");
				return false;
			}else if (substr(WTA_RemoteCall0.RC_GetReturnParm(1),50,3) == "777") {
				WTA_RemoteCall0.RC_Disconnect();	//연결종료
				alert(setPopMessage("1002")); //[경고] 처리 중 오류가 발생하였습니다.
				setMessage("1002");	
				return false;
			}else {
				WTA_RemoteCall0.RC_Disconnect();	//연결종료
				alert(setPopMessage("1015")); //[경고] AS400 CL 처리 중 오류가 발생하였습니다.
				setMessage("1015");	
				return false;
			}
		}else{
			var bstrErrorMessage = WTA_RemoteCall0.bstrErrorMessage;
			WTA_RemoteCall0.RC_Disconnect();	//연결종료
			SetWaitCursor(false);
			//오류 코드 경고창
			alert("에러 메세지=======>" + bstrErrorMessage);
		}
	//접속실패시
	}else{
		var ulErrorCode = WTA_RemoteCall0.ulErrorCode;
		WTA_RemoteCall0.RC_Disconnect();	//연결종료
		SetWaitCursor(false);
		//오류 코드 경고창
		alert("접속 실패 오류=======>" + ulErrorCode);
	}
}

/********************************************************************** 
 * 내용설명 : validation 체크
 **********************************************************************/ 
 
function validationChk(){
	var result = true;
	cmb_MAKDT.BKColor="white";
	var cigmaYM = substr(ds_CGMPARMHC.GetColumn(0,"ADSDT"),0,6);
	var cigmaNextYM = substr(addMonth(ds_CGMPARMHC.GetColumn(0,"ADSDT"),1),0,6);

	//trace(cigmaYM+"=="+cigmaNextYM);
	if (cmb_MAKDT.Value == ""){
		cmb_MAKDT.BKColor="paleturquoise";
		//alert("[알림]기준일자를 입력하세요.");
		alert(setPopMessage("4715"));//[알림]기준일자를 입력하세요.
		setMessage("4715");
		result = false;
	}else if(substr(cmb_MAKDT.Value,0,6) != cigmaYM && substr(cmb_MAKDT.Value,0,6) != cigmaNextYM){
		cmb_MAKDT.BKColor="paleturquoise";
		//alert("[알림]기준일자는 CIGMA년월+1까지 입력가능합니다.");
		alert(setPopMessage("6061"));//[알림]기준일자는 CIGMA년월+1까지 입력가능합니다.
		setMessage("6061");
		result = false;	
	}
	return result;
}

