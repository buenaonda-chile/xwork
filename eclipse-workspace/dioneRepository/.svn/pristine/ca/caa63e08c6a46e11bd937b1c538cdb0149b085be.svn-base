function ds_BJOZ110_CanColumnChange(obj,nRow,strColumnID,varOldValue,varNewValue,nPivotIndex)
{
	http.usewaitcursor = false;
	
	if(strColumnID=="ISIPN"){ //중복체크
		var newcl = ds_BJOZ110.GetColumn(nRow, "ISICL");
		var newpn = quote(varNewValue);
	
		if(varNewValue!=""){
			if(ds_BJOZ110.FindRow("ISIPN",varNewValue)!="-1"){
				alert("이미 등록된 품번 입니다.");//[경고]이미등록된부서코드입니다.				
				if(ds_BJOZ110.GetRowType(nRow)=="insert"){
					ds_BJOZ110.DeleteRow(nRow);
					ds_BJOZ110.SelectRow(ds_BJOZ110.FindRow("ISIPN",varNewValue));				
				}else{
					ds_BJOZ110.SelectRow(nRow, false);
					ds_BJOZ110.SelectRow(ds_BJOZ110.FindRow("ISIPN",varNewValue));				
				}
				return false;
			}else{
						
			if(!validationChk()) return;
	
				http.Sync = true;
				var sKind		= "getBJOZ110Cnt";		
				var sMethodName = "service::psimmlib/selectBJOZ110.do"; 
				var sInDataSet  = "";
				var sOutDataSet = "ds_BJOZ110_CHECK=ds_BJOZ110_CHECK";
				var sArgument   = "";
					sArgument  += " LIB2=" + quote(Lib2);
					sArgument  += " LIB3=" + quote(Lib2);
					sArgument  += " SQLID=" + "selectBJM100";					
					sArgument  += " COMPS=" + quote(ComboBoxPlant.Value); //사업부
					sArgument  += " ISICL=" + newcl;
					sArgument  += " ISIPN=" + newpn;
				Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
				http.Sync = false;
				if (ds_BJOZ110_CHECK.GetRowCount() < 1){
					//alert("[알림]부서코드확인바랍니다."); //[알림]부서코드확인바랍니다.
					alert("등록되지 않은 품번입니다. 재 입력하십시요."); //[알림]부서코드확인바랍니다.					
					return false;
				}
			}
		}
	}
