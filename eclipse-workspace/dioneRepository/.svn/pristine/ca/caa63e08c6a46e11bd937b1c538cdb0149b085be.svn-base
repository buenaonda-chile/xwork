/********************************************************************** 
 * 내용설명 : 화면 조회 
 **********************************************************************/ 
function getBJOZ110(){
		
	if(!validationChk()) return;
	
	var sKind		= "getBJOZ110";						
	var sMethodName = "service::psimmlib/selectBJOZ110.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             					// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_BJOZ110=ds_BJOZ110";    						// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
	
	sArgument += "@LIB2=" + quote(Lib2);
	sArgument += "@COMPS=" + quote(ComboBoxPlant.Value);  				// 사업부
	sArgument += "@SQLID=" + "selectBJOZ110";  			  				// SQL ID
	sArgument += "@ISIJP=" + "IS-"+ed_rpijp.Value;		  		  		// 전표 번호
	sArgument += "@ISDPT=" + ed_SGBVN.Value;  				  			// 업체코드
	
	var str = dialog("Common::PGMLOAD_IMM.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");
	if(ds_BJOZ110.count ==0){
		for(var i=0; i<=10; i++) {
			fn_Input();
		}
	} 
	
	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
		setMessage("1004");	
	}else if (str == 'B'){   //data값이 없을때
		
		alert(setPopMessage("1003"));
		setMessage("1003");	
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}
}

/********************************************************************** 
 * 내용설명 : insert, update, delete
 **********************************************************************/ 
function setBJOZ110(){
	
	if(!validationChk()) return;
		
	// var idnum = substr(G_ds_user.GetColumn(0,"USER_ID"),1,7); 사용자 아이디 취득
	
	var sKind		= "setBJOZ110";						
	var sMethodName = "service::psimmlib/saveBJOZ110.do";     // 서버에서 호출 될 Method 명
	var sInDataSet  = "ds_BJOZ110=ds_BJOZ110:U";               //자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_BJOZ110_CHECK=ds_BJOZ110_CHECK";     // 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
	sArgument += " LIB2=" + quote(Lib2);	
	sArgument += " COMPS=" + quote(ComboBoxPlant.Value);  					// 사업분류	
	sArgument += " RPIJPBEFORE=" + quote(ToInteger(ed_rpijp.Value)-1);       				// 전표번호	
	sArgument += " MAJCD=" + "BJOZ110";  								// BJOZ110 전표 번호 수집 KEY
	
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");
}

/********************************************************************** 
 * 내용설명 : Transaction 리턴후 실행 함수
 **********************************************************************/ 
function callback(svcId, rtnCd, rtnMsg){
	if (svcId = "setBJOZ110"){
		if (ds_BJOZ110_CHECK.GetRowCount() > 0){			
			for(var i=0;i<ds_BJOZ110_CHECK.GetRowCount();i++){		
						
				var errMessage = "'" + ds_BJOZ110_CHECK.GetColumn(i,"RPIVN") + "'";
				errMessage +=" 는 품번에 대해서 다시 확인이 필요합니다.";
				alert(errMessage);	
				/*
				for(var i=0; i<ds_BJOZ110.rowcount(); i++){
					if(trim(ds_BJOZ110.GetColumn(i,"RPIJP")) == rpijp){						
						ds_BJOZ110.SetColumn(i,"");
						continue;
					}
				}
				*/
			}	
			return;	
		}
	
		getMaxBJOZ110(); // 초기 전표조회	
	}
	
	// tran 실패
	if(rtnCd<0){
		alert(setPopMessage("1002"));
		setMessage("1002");
	// tran 성공	
	}else if(svcId=="setBJOZ110"){
		alert(setPopMessage("1001"));
		setMessage("1001");
	}	
}

/********************************************************************** 
 * 내용설명 : validation 체크 1
 **********************************************************************/ 
function validationChk(){

	if(length(trim(ed_rpijp.Text))==0){
		alert("전표 번호를 입력해 주세요.");
		ed_rpijp.SetFocus();
		return false;
	}
	
	if(length(trim(ed_SGBVN.Text))==0){
		alert("업체 코드를 입력해 주세요.");
		ed_SGBVN.SetFocus();
		return false;
	}

	return true;
}

/********************************************************************** 
 * 내용설명 : 업체 검색
 **********************************************************************/ 
function MainDiv_ed_SGBVN_OnChanged(obj,strText)
{
	http.Sync 	= true;
	
	if(rdoggub.Value == "N0"){
		var sKind		= "getVENNAM";		
		var sMethodName = "service::pscomlib/selectVENNAM.do"; 
		var sInDataSet  = "";
		var sOutDataSet = "ds_VENNAM=ds_VENNAM";
		var sArgument   = "";
			sArgument  += " LIB1=" + quote(Lib1);
			sArgument  += " VNDNR=" + quote(strText);
		Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
		if (ds_VENNAM.GetRowCount() != 1){
			alert(setPopMessage("4674"));
			setMessage("4674");	
			st_VNAME.Text = "";
			ed_SGBVN.SetFocus();
			return;
		}
		st_VNAME.Text = ds_VENNAM.GetColumn(0,"VNAME");	
	} else if(rdoggub.Value == "M6") {
		var sKind		= "getVENNAM";		
		var sMethodName = "service::pscomlib/selectMST650.do"; 
		var sInDataSet  = "";
		var sOutDataSet = "ds_MST650PF=ds_MST650PF";
		var sArgument   = "";		
			sArgument  += " COMPS=" + quote(ComboBoxPlant.Value);
			sArgument  += " LIB2=" + quote(Lib2);
			sArgument  += " DPCODE=" + quote(strText);
		Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
		if (ds_MST650PF.GetRowCount() != 1){
			alert(setPopMessage("4674"));
			setMessage("4674");	
			st_VNAME.Text = "";
			ed_SGBVN.SetFocus();
			return;
		}
		st_VNAME.Text = ds_MST650PF.GetColumn(0,"DPCONM");
	}
	
	getBJOZ110();
}

