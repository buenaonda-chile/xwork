function getSMDZ123P(){

	if(!validationChk()){
		return;
	}
	var sJPNNO1="";
	var sJPNNO2="";
	var sPYVND1="";
	var sPYVND2="";
	var sJPSDT1="";
	var sJPSDT2="";
	var sJPTDT1="";
	var sJPTDT2="";	
	//전표
	if(length(ed_JPNNO1.Text)==0){
		sJPNNO1="0000000000";
	}
	else{
		sJPNNO1=ed_JPNNO1.Text;
	}	
	if(length(ed_JPNNO2.Text)==0){
		sJPNNO2="9999999999";
	}
	else{
		sJPNNO2=ed_JPNNO2.Text;
	}
	//업체
	if(length(ed_PYVND1.Text)==0){
		sPYVND1="00000000";
	}
	else{
		sPYVND1=ed_PYVND1.Text;
	}	
	if(length(ed_PYVND2.Text)==0){
		sPYVND2="99999999";
	}
	else{
		sPYVND2=ed_PYVND2.Text;
	}	
	//전표
	if(cmb_JPSDT1.Value == ""){
		sJPSDT1="00000000";
	}
	else if(cmb_JPSDT1.Value != ""){
		sJPSDT1=cmb_JPSDT1.Value;
	}
	if(cmb_JPSDT2.Value == ""){
		//sJPSDT2="99999999";
		alert(setPopMessage("4027"));
		setMessage("4027");	
		return;
	}
	else if(cmb_JPSDT2.Value != ""){
		sJPSDT2=cmb_JPSDT2.Value;
	}
	//마감
	if(cmb_JPTDT1.Value == ""){
		sJPTDT1="00000000";
	}
	else if(cmb_JPTDT1.Value != ""){
		sJPTDT1=cmb_JPSDT1.Value;
	}
	if(cmb_JPTDT2.Value == ""){
		sJPTDT2="99999999";
	}
	else if(cmb_JPTDT2.Value != ""){
		sJPTDT2=cmb_JPTDT2.Value;
	}
	getSYSDATE(); //SYSTEM 날짜
	var sysDt =ds_SYSDATE.GetColumn(0,"DT");
	var systm =ds_SYSDATE.GetColumn(0,"TM");
					
	var sKind		= "getSMDZ123P";						
	var sMethodName = "service::pssallib/selectSMDZ123P.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_SMDZ123P=ds_SMDZ123P";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
	sArgument += "@LIB1=" + quote(Lib1);
	sArgument += "@LIB2=" + quote(Lib2);
	sArgument += "@COMPS=" + quote(ComboBoxPlant.Value); //사업부
	sArgument += "@JPNNO1=" + quote(sJPNNO1);
	sArgument += "@JPNNO2=" + quote(sJPNNO2);
	sArgument += "@PYVND1=" + quote(sPYVND1);
	sArgument += "@PYVND2=" + quote(sPYVND2);
	sArgument += "@JPSDT1=" + quote(sJPSDT1);
	sArgument += "@JPSDT2=" + quote(sJPSDT2);
	sArgument += "@JPTDT1=" + quote(sJPTDT1); 
	sArgument += "@JPTDT2=" + quote(sJPTDT2);
	sArgument += "@DYES=" + quote(cmb_DYES.Value);
	var str = dialog("Common::PGMLOAD_SAL.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");

	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
		setMessage("1004");	
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));  //[알림] 신규입력입니다.
		setMessage("1003");	
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}	
	
	//Ubi0.setJrfFileDir("http://10.74.139.50/ui/PSSALLIB/REPORT/");
//	Ubi0.setJrfFileDir("http://10.72.209.23/ui/PSSALLIB/REPORT/");
	Ubi0.setJrfFileDir(G_baseURL+"ui/PSSALLIB/REPORT/");
	Ubi0.setJrfFileName("SMDZ123P.jrf");
	Ubi0.SetDataset("ds_SMDZ123P", "ds_SMDZ123P");
	Ubi0.SetArgument("SYSDATE",sysDt);
	Ubi0.SetArgument("SYSTIME",systm);
	Ubi0.Retrieve();
	
}

