function validationChk(){

	var result = true;
	var sdVal = substr(cmb_JPSDT1.Value,0,6);
	var edVal = substr(cmb_JPSDT2.Value,0,6);
	cmb_JPSDT1.BKColor="white";
	cmb_JPSDT2.BKColor="white";	

	if(sdVal > edVal)
	{
		cmb_JPSDT1.BKColor="paleturquoise";
		cmb_JPSDT2.BKColor="paleturquoise";
		alert(setPopMessage("4027"));
		setMessage("4027");	
		result =false;
	}
	return result;
}

