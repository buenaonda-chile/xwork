function getSMDZ123P_CHK()
{
	var sJPSDT1 = addDate(today(),-4);
	var sJPSDT2 = addDate(today(),-1);
	var sKind		= "getSMDZ123P_CHK";						
	var sMethodName = "service::pssallib/selectSMDZ123P_CHK.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";
	var sOutDataSet = "ds_SMDZ123P_CHK=ds_SMDZ123P_CHK";
	var sArgument   = "";
		sArgument += " LIB1=" + quote(Lib1);
		sArgument += " LIB2=" + quote(Lib2);	
		sArgument += " COMPS=" + quote(ComboBoxPlant.Value); //사업부
