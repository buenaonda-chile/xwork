function getComCode(){
	var sKind		= "getComCode";						
	var sMethodName = "service::login/selectComCode.do"; 
	var sInDataSet  = "";
	var sOutDataSet = "ds_ComCode_PLNT=ds_ComCode_PLNT";
	var sArgument   = "";
		sArgument   += " CLASS='PLNT'";
		sArgument   += " CODE1=" + quote(company);		
	http.Sync=true;
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	http.Sync=false;
	Div0.ComboBoxPlant.Value=comps;
	//ComboBoxPlant.Enable = false; 
}


/********************************************************************** 
 * 내용설명 : 마감
 * 주요변수 : 
 **********************************************************************/ 
function setFPWS895C(){
	if(length(trim(Div0.ComboBoxPlant.Value)) < 1){
		alert(setPopMessage("1006"));  //[알림] 지원하지 않는 시스템 입니다. 정보관리팀으로 문의하십시요.
		setMessage("1006");	
		return;
	}
	if(length(trim(Div0.cmb_PNYMD.Value)) < 8){
		//alert("[알림]지시일자를 입력하세요.");
		alert(setPopMessage("6076"));  //[알림]지시일자를 입력하세요.
		setMessage("6076");	
		Div0.cmb_PNYMD.SetFocus();
		return;
	}

	if(confirm(Div0.cmb_PNYMD.Value+"일자의 제품BarCode발행분 이동 작업입니다. 작업하시겠습니까? ") == false) return;

	//if(Div0.cmb_TYS.Value=="N"){
	if(Div0.cmb_TYS.Value=="N" && Div0.cmb_PYS.Value=="N"){//PART라벨 추가2015.05.27

		//alert("[알림]TAG 이동 작업이 'N'으로 사용불가능합니다.");
		alert(setPopMessage("6077"));  //[알림]TAG 이동 작업이 'N'으로 사용불가능합니다.
		setMessage("6077");	
		Div0.cmb_TYS.SetFocus();
		return;
	}
	st_Msg.Visible = true;
	Div0.Enable = false;
	SetWaitCursor(true);
	SetCapture();

	var sKind		= "setFPWS895C";
	var sMethodName = "service::psfaclib/saveFPWS895C.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "";    										// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
		sArgument  += " LIB1=" + quote(Lib1);
		sArgument  += " LIB2=" + quote(Lib2);
		//sArgument  += " COMPS=" + quote(Div0.ComboBoxPlant.Value);
		sArgument  += " PNYMD=" + quote(Div0.cmb_PNYMD.Value);
		sArgument  += " PROCS=" + quote(Div0.ed_PROCS.Text);
		sArgument  += " PRTNO=" + quote(Div0.ed_PRTNO.Text);
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");

}

/********************************************************************** 
 * 내용설명 : Transaction 리턴후 실행 함수
 * 주요변수 : 
 * 			- svcId	Transaction 고유 ID
 * 			- rtnCd 	에러코드
 * 			- rtnMsg 		에러 메세지
 **********************************************************************/ 
function callback(svcId, rtnCd, rtnMsg){
	// tran 실패
	if(rtnCd<0){
		alert(setPopMessage("1002")); //[경고] 처리 중 오류가 발생하였습니다.
		setMessage("1002");
	}else{
		ReleaseCapture();
		SetWaitCursor(false);
		st_Msg.Visible = false;
		alert(setPopMessage("1001")); //[알림] 정상적으로 처리가 완료되었습니다.
		setMessage("1001");
	}
}


