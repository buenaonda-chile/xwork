 * 주요변수 : 
 * 			- sMethodName	Transaction 고유 ID
 * 			- ErrorCode 	에러코드
 * 			- ErrorMSG 		에러 메세지
 **********************************************************************/ 
var SQLState;
function callback(svcId, rtnCd, rtnMsg){
	if (ds_SPMZ100_CHECK.GetRowCount() > 0){
		for(var i=0;i<ds_SPMZ100_CHECK.GetRowCount();i++){
			alert(ds_SPMZ100_CHECK.GetColumn(i,"PSPNO") + "=" + ds_SPMZ100_CHECK.GetColumn(i,"RTMSG"));
		}
		getSPMZ100();  //error항목이 존재시에는 재조회
		return;
	}
	// tran 실패
	if(rtnCd<0){
		alert(setPopMessage("1002"));
		setMessage("1002");
		
	}else {
		if(SQLState == "57033"){
			SQLState="";
			alert(setPopMessage("1008"));  //[경고] 현재 시스템에 락이 걸렸습니다. 전산관리자에게 문의하세요
			setMessage("1008");	
		}else{
			alert(setPopMessage("1001"));
			setMessage("1001");	
		}
	}
}

