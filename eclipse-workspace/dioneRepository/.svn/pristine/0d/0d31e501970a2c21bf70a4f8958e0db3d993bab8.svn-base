function fn_Input()
{
	if(length(trim(st_pyvnd.Text)) < 1){
		alert(setPopMessage("1502"));  //[확인] 거래처코드 확인바랍니다.
		setMessage("1502");	
		ed_pyvnd.SetFocus();
		return;
	}
	if(checkInsertCount("ds_SPMZ100")== false) return;
	if(ds_SPMZ100.rowcount < 1) getSPMZ100();
	ed_Action.Text = "A";
	ds_SPMZ100.AddRow();
	if (ds_SPMZ100.rowcount == 1){
		ds_SPMZ100.SetColumn(ds_SPMZ100.Row, "PYSEQ", ds_SPMZ100.rowcount); // default값 설정
	}else{
		ds_SPMZ100.SetColumn(ds_SPMZ100.Row, "PYSEQ", toNumber(ds_SPMZ100.GetColumn(ds_SPMZ100.rowcount-2,"PYSEQ"))+1); // default값 설정
	}
	ds_SPMZ100.SetColumn(ds_SPMZ100.Row, "COMPS", cb_searchCodeName1.Value); // default값 설정
	ds_SPMZ100.SetColumn(ds_SPMZ100.Row, "PLNYM", substr(cal_COMYM.Value,0,6)); // default값 설정
	ds_SPMZ100.SetColumn(ds_SPMZ100.Row, "PYVND", ed_pyvnd.Text); // default값 설정
	displayDiv.ed_pspno.SetFocus();
}

