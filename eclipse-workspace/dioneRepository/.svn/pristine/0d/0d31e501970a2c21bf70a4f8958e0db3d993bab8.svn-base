/********************************************************************** 
 * 내용설명 : validation Check
 **********************************************************************/ 
function ds_SPMZ100_CanColumnChange(obj,nRow,strColumnID,varOldValue,varNewValue,nPivotIndex)
{
	if( strColumnID == "PSPNO" ){
		if(length(trim(varNewValue)) < 1) return false;
		//중복Check
		if(ds_SPMZ100.FindRow("PSPNO", trim(varNewValue)) >= 0){
			alert(setPopMessage("1520"));  //[경고] 이미 등록된 자료입니다.
			setMessage("1520");	
			displayDiv.ed_pspno.SetFocus();
			return false;
		}
		
		//품번Check,조회
		http.Sync = true;
		var sKind		= "getBM008PR";		
		var sMethodName = "service::pscomlib/selectBM008PR.do"; 
		var sInDataSet  = "";
		var sOutDataSet = "ds_BM008PR=ds_BM008PR";
		var sArgument   = "";
			sArgument  += " LIB1=" + quote(Lib1);
			sArgument  += " ITNBR=" + quote(varNewValue);
		Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
		if (ds_BM008PR.GetRowCount() < 1){
			alert(setPopMessage("1501"));  //[확인] 품번 확인바랍니다.
			setMessage("1501");	
			displayDiv.ed_pspno.SetFocus();
			displayDivClear();
			return false;
		}
		displayDiv.st_pspno.Text=ds_BM008PR.GetColumn(0,"ITDSC");
		displayDiv.st_PACKC.Text=ds_BM008PR.GetColumn(0,"PACKC");
		
		//oem품번유무Check
		http.Sync = true;
		var sKind		= "getEM000PR";		
		var sMethodName = "service::pscomlib/selectEM000PR.do"; 
		var sInDataSet  = "";
		var sOutDataSet = "ds_EM000PR=ds_EM000PR";
		var sArgument   = "";
			sArgument  += " LIB1=" + quote(Lib1);
			sArgument  += " CUSNO=" + quote(ed_pyvnd.Text);
			sArgument  += " PRTNO=" + quote(varNewValue);
		Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
		if (ds_EM000PR.GetRowCount() < 1){
			alert(setPopMessage("4001"));  //Part Number Cross Ref MA에 미등록된 득의선,품번입니다.
			setMessage("4001");	
			displayDiv.ed_pspno.SetFocus();
			displayDivClear();
			return false;
		}
		
		//단가Check, 조회
		http.Sync = true;
		var sKind		= "getSalePrice";		
		var sMethodName = "service::pscomlib/selectEM001PR.do"; 
		var sInDataSet  = "";
		var sOutDataSet = "ds_EM001PR=ds_EM001PR";
		var sArgument   = "";
			sArgument  += " LIB1=" + quote(Lib1);
			sArgument  += " CUSNO=" + quote(ed_pyvnd.Text);
			sArgument  += " PRTNO=" + quote(varNewValue);
		Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
		if (ds_EM001PR.GetRowCount() < 1){
			alert(setPopMessage("4002"));  //[확인] Sale Price MA에 미 등록된 자료입니다.
			setMessage("4002");	
			displayDiv.ed_pspno.SetFocus();
			displayDivClear();
			return false;
		}
		displayDiv.st_REASN.Text = ds_EM001PR.GetColumn(0,"REASN");
		displayDiv.st_PRICE.Text = ds_EM001PR.GetColumn(0,"PRICE");
		
		//차종명 조회
		http.Sync = true;
		var sKind		= "getCar";		
		var sMethodName = "service::pscomlib/selectMST050100PF.do"; 
		var sInDataSet  = "";
		var sOutDataSet = "ds_MST050100PF=ds_MST050100PF";
		var sArgument   = "";
			sArgument  += " LIB2=" + quote(Lib2);
			sArgument  += " COMPS=" + quote(cb_searchCodeName1.Value);
			sArgument  += " PRTNO=" + quote(varNewValue);
			sArgument  += " GRPCD=" + quote("CAR");
		Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
		displayDiv.st_MSTRM.Text = ds_MST050100PF.GetColumn(0,"MSTRM");
		
		//제품코드 조회
		http.Sync = true;
		var sKind		= "getmst100pf";		
		var sMethodName = "service::pscomlib/selectMST100PF.do"; 
		var sInDataSet  = "";
		var sOutDataSet = "ds_MST100PF=ds_MST100PF";
		var sArgument   = "";
			sArgument  += " LIB2=" + quote(Lib2);
			sArgument  += " COMPS=" + quote(cb_searchCodeName1.Value);
			sArgument  += " PRTNO=" + quote(varNewValue);
		Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
		displayDiv.st_PRDCD.Text = ds_MST100PF.GetColumn(0,"PRDCD");
		
		//특소세여부 조회
		http.Sync = true;
		var sKind		= "getSPMZ100_SPTEX";		
		var sMethodName = "service::pssallib/selectSPMZ100_SPTEX.do"; 
		var sInDataSet  = "";
		var sOutDataSet = "ds_SPMZ100_SPTEX=ds_SPMZ100_SPTEX";
		var sArgument   = "";
			sArgument  += " LIB2=" + quote(Lib2);
			sArgument  += " COMPS=" + quote(cb_searchCodeName1.Value);
			sArgument  += " PSPNO=" + quote(varNewValue);
			sArgument  += " PLNYM=" + quote(substr(cal_COMYM.Value,0,6));
			sArgument  += " PYVND=" + quote(ed_pyvnd.Text);
		Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
		if (ds_SPMZ100_SPTEX.GetRowCount() > 0){
			displayDiv.st_SPTEX.Text = "Y";
		}else{
			displayDiv.st_SPTEX.Text = " ";
		}
		
		//전월계획 조회
		http.Sync = true;
		var beforym = substr(AddMonth(cal_COMYM.Value,-1),0,6);
		var sKind		= "getSPMZ100_BEFOR";		
		var sMethodName = "service::pssallib/selectSPMZ100_BEFOR.do"; 
		var sInDataSet  = "";
		var sOutDataSet = "ds_SPMZ100_BEFOR=ds_SPMZ100_BEFOR";
		var sArgument   = "";
			sArgument  += " LIB2=" + quote(Lib2);
			sArgument  += " COMPS=" + quote(cb_searchCodeName1.Value);
			sArgument  += " PSPNO=" + quote(varNewValue);
			sArgument  += " PLNYM=" + quote(beforym);
			sArgument  += " PYVND=" + quote(ed_pyvnd.Text);
		Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
		displayDiv.st_SM0QY1.Text = ds_SPMZ100_BEFOR.GetColumn(0,"SM0QY");
		displayDiv.st_SM1QY1.Text = ds_SPMZ100_BEFOR.GetColumn(0,"SM1QY");
		displayDiv.st_SM2QY1.Text = ds_SPMZ100_BEFOR.GetColumn(0,"SM2QY");
		displayDiv.st_SM3QY1.Text = ds_SPMZ100_BEFOR.GetColumn(0,"SM3QY");
		displayDiv.st_SM4QY1.Text = ds_SPMZ100_BEFOR.GetColumn(0,"SM4QY");
		displayDiv.st_SM5QY1.Text = ds_SPMZ100_BEFOR.GetColumn(0,"SM5QY");
		displayDiv.st_SM6QY1.Text = ds_SPMZ100_BEFOR.GetColumn(0,"SM6QY");
	}
	if( strColumnID == "SM0QY" || strColumnID == "SM1QY" || strColumnID == "SM2QY" || strColumnID == "SM3QY" || strColumnID == "SM4QY" || strColumnID == "SM5QY" || strColumnID == "SM6QY" ){
		if (!numberChk(varNewValue)){
			alert("수량은 마이너스가 될수 없습니다."); 
			return false;
		}
	}
}

