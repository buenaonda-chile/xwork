	// tran 실패
	if(rtnCd<0){
		alert(setPopMessage("1002"));
		setMessage("1002");
	}else{
		alert(rtnCode);
		if (rtnCode == 0){
			alert(setPopMessage("1001"));
			setMessage("1001");
		}else if (rtnCode == 1){
			alert("타사용자가 사용중입니다. 확인바랍니다.");
			//setMessage("1002");
		}else{
			alert(setPopMessage("1002"));
			setMessage("1002");
		}
	}
}

