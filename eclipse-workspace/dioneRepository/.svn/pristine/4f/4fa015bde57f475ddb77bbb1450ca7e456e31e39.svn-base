function fn_printzzzzz()
{
	var ESC9 = Chr(27); //&H1B; //制御文字ESC
	var LF9 = Chr(10); //&HA; //制御文字LF
	var NUL9 = Chr(0); //&H0;
	
	//var ESC = &H1B   //制御文字ESC
	//var LF = &HA    //'制御文字LF
	//var NUL = &H0   //'制御文字NUL
	//var SOH = &H1   //'制御文字SOH
	//var STX = &H2   //'制御文字STX
	fncAxCommOpen();
	//스테이터스 요구 커멘드를 송신해 현재의 프린터 상태를 확인
    //cmmB570.Output = ESC9 + "WR" + LF9 + NUL9;
	//alert(cmmB570.CommEvent);
	    //프린터 스테이터스 수신
	    //sRtnSts = fncPrnStsRcv
	//라벨 포맷 송신
	//SetTimer(1,1000);
	
	trace(cmmB570.Input);
	trace(cmmB570.CTSHolding);
	trace(cmmB570.DSRHolding);
	
	var sLabelSize = ESC9 + ("D") + ("0150,0500,0130") + LF9 + NUL9;
	var sLabelAX = ESC9 + ("AX;") + ("-004,-004,+00") + LF9 + NUL9;  //LABEL OR TAG 사이즈 결정
	var sLabelAY = ESC9 + ("AY") + (";+00,0") + LF9 + NUL9;
	var sLabelFeed = ESC9 + ("T") + ("21C21") + LF9 + NUL9;           //LABEL PRINTER의 특징설정
	var sSizeBuf   = (sLabelSize) + (sLabelAX)+(sLabelAY)+(sLabelFeed);
	cmmB570.Output = sSizeBuf;
	//alert(cmmB570.CommEvent);
	
	
	//BUFFER CLEAR
	var sLabelClr = ESC9 + ("C") + LF9 + NUL9;
	
	var sBarFmt = ESC9 + ("XB") + ("00;0030,0010,Q,20,04,01,0") + LF9 + NUL9  ;
	
	var sQRFmt = ESC9 + ("XB") + ("01;0390,0010,T,L,04,A,0=") + "ASDfgghjhjkj" + LF9 + NUL9  ;
	
	var sCharFmt0 = ESC9 + ("PC") + ("000;0130,0055,1,15,E,00,B") + LF9 + NUL9 ;
	var sCharFmt11 = ESC9 + ("PC") + ("011;0255,0050,1,1,H,00,B") + LF9 + NUL9 ;
	var sCharFmt3 = ESC9 + ("PC") + ("003;0420,0117,05,1,A,00,B") + LF9 + NUL9 ;
	var sCharFmt4 = ESC9 + ("PC") + ("004;0130,0085,05,05,K,00,B") + LF9 + NUL9 ;
	var sCharFmt6 = ESC9 + ("PC") + ("006;0130,0110,05,05,K,00,B") + LF9 + NUL9 ;
	var sCharFmt = "";
	sCharFmt = sCharFmt + (sCharFmt0);
	sCharFmt = sCharFmt + (sCharFmt11);
	sCharFmt = sCharFmt + (sCharFmt3);
	sCharFmt = sCharFmt + (sCharFmt4);
	sCharFmt = sCharFmt + (sCharFmt6);
	
	var sBarData = ESC9 + ("RB") + ("00;") + ("P068") + LF9 + NUL9 ;; 
	
	var sChrData0 = ESC9 + ("RC") + ("000;") + ("ALC") + LF9 + NUL9 ;
	var sChrData11 = ESC9 + ("RC") + ("011;") + ("201501") + LF9 + NUL9 ;
	var sChrData3 = ESC9 + ("RC") + ("003;") + ("001") + LF9 + NUL9 ;
	var sChrData4 = ESC9 + ("RC") + ("004;") + ("OEMPRTNO") + LF9 + NUL9 ;
	var sChrData6 = ESC9 + ("RC") + ("006;") + ("DENSOPSPNO") + LF9 + NUL9 ;
	var sChrData = "";
    sChrData = sChrData + (sChrData0);
    //sChrData = sChrData + (sChrData1);
    sChrData = sChrData + (sChrData11);
    sChrData = sChrData + (sChrData3);
    sChrData = sChrData + (sChrData4);
    sChrData = sChrData + (sChrData6);

	var sLabelQty = ESC9 + ("XS") + (";I,0001") + (",0002C7100") + LF9 + NUL9 ;
	
	//print
	var sLabelBuf = "";
	sLabelBuf = sLabelBuf + (sLabelClr);
	sLabelBuf = sLabelBuf + (sBarFmt);      //XB,39 BARCODE FORMAT
	sLabelBuf = sLabelBuf + (sQRFmt);       //QR CODE FORMAT
	sLabelBuf = sLabelBuf + (sCharFmt);    //PC,문자 위치
	sLabelBuf = sLabelBuf + (sBarData);    //RB,39 BARCODE 값을 줌
	sLabelBuf = sLabelBuf + (sChrData);    //RC,문자값
	sLabelBuf = sLabelBuf + (sLabelQty);
  cmmB570.InBufferCount = 0;
  cmmB570.OutBufferCount =0;
  cmmB570.Output = sLabelBuf;
	
	if(cmmB570.PortOpen == -1){
		alert("1번의 Port 열다.");
		cmmB570.PortOpen= false;
		alert(cmmB570.PortOpen);
		return;
	}
	
	if(cmmB570.PortOpen != true){
		alert(cmmB570.PortOpen);
		alert("1번의 Port가 열리지 않습니다.");
		cmmB570.PortOpen= false;
		alert(cmmB570.PortOpen);
		return;
	}
	fncOutPut("S3");       // 라벨 프린트 속도 (1 ~ 6)
	fncOutPut("D10");      // 헤드 온도 (1 ~ 15)
	fncOutPut("ZB");       // 프린트 디렉션 (반대 = ZT)
	fncOutPut("JF");       // media back feed (반대 = JB)
	fncOutPut("j120");     // 메디아 피드값 조정
	fncOutPut("R0,0");     // X,Y 포지션 (이값을 변경하면 이미지 전체를 변경함
	fncOutPut("N");
	fncOutPut("Q160,24");  // 라벨 길이 및 갭 설정 (1mm * 8 dot) 1200 / 8 = 150 mm, 24 / 8 = 3 mm)
	fncOutPut("q736");     // 라벨 넓이 설정 640 /8 = 80mm
	
	// A ::::::: 텍스트 format
	//    x축
	//   ,y축
	//   ,회전(0,1,2,3)
	//   ,폰트종류(1,2,3,4,5,8(한글))
	//'   ,x크기
	//   ,y크기
	//   ,N=Normal image(R = Reverse image),데이타
	
	var t_Label = "12345";
	var t_BarCode = "asdfghjk";
	var t_ed_PrnQty  = "33";
	
	fncOutPut("A40,55,0,4,1,1,N,"+chr(34)+t_Label+chr(34));	
	fncOutPut("B45,80,0,1,2,0,50,B,"+chr(34)+t_BarCode+chr(34));	
	fncOutPut("A400,55,0,4,1,1,N,"+chr(34)+t_Label+chr(34));	
	fncOutPut("B405,80,0,1,2,0,50,B,"+chr(34)+t_BarCode+chr(34));	
	fncOutPut("P"+t_ed_PrnQty);
		
	fncAxCommClose();	

	if(confirm("선택된 제품작업지시TAG 인쇄합니까? ") == false) return;
	
	if(ds_FPQE140Q_4.rowcount <= 0 || ds_FPQE140Q_4.GetRowType(ds_FPQE140Q_4.row) != 'normal') return ;
	
	ds_FPQE140Q_5.ClearData();
	for (var row = 0 ; row < ds_FPQE140Q_4.RowCount ; row++ ){   //선택된 것 만 ds_FPQE140Q_5에 ADD
		if(ds_FPQE140Q_4.GetSelect(row) == true){
			ds_FPQE140Q_5.addRow();
			ds_FPQE140Q_5.copyrow(ds_FPQE140Q_5.row,"ds_FPQE140Q_4",row);
		}
	}
	
	if(ds_FPQE140Q_5.rowcount <= 0) return ;

	var str = dialog("BARCODE::FPQE140QR.xml",
					" TABLE=" + quote(tableSelect) +
					" COMPS=" + quote(ComboBoxPlant.Value) +
					" PNYMD=" + quote(cal_YMD.Value) +
					" ds_SEL="+ quote(ds_FPQE140Q_5.SaveXML()),1100,600);		
}

