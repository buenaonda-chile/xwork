}
function siuFPW737PF(i,j)
{
	
	var sKind		= "getFPQE140Q_737PF";						
	//var sMethodName = "service::psfaclib/selectFPQE140Q_737PF.do";     		// 서버에서 호출 될 Method 명
	var sMethodName = "service::barcode/selectFPQE140Q/737PF";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                            				// 자료 저장 시 보낼 DataSet 명칭
	var	sOutDataSet = "ds_FPQE140Q_737PF=ds_FPQE140Q_737PF barcodeStatus=barcodeStatus";    				// 조회 후 돌려 받을 DataSet 명칭

	var sArgument   = "";
		sArgument += " LIB2=" + quote(Lib2);
		sArgument += " COMPS=" + quote("E1"); //사업부
		
		sArgument += " PRTNO=" + quote(ds_FPQE140Q_5.GetColumn(i,"PRTNO")); //품번
		sArgument += " PNYMD=" + quote(ds_FPQE140Q_5.GetColumn(i,"PNYMD")); //년월일
		
		sArgument += " CUSNO=" + quote(ds_FPQE140Q_5.GetColumn(i,"CUSNO")); //업체
		sArgument += " PROCS=" + quote(ds_FPQE140Q_5.GetColumn(i,"PROCS")); //공정		
		sArgument += " ORCQY=" + quote(ds_FPQE140Q_5.GetColumn(i,"ORCQY")); //생산지수
		sArgument += " CPRTN=" + quote(ds_FPQE140Q_5.GetColumn(i,"CPRTN")); //업체품번
		sArgument += " MKALC_CARCD=" + quote(ds_FPQE140Q_5.GetColumn(i,"MKALC")+" "+ds_FPQE140Q_5.GetColumn(i,"CARCD")); //A
		
		sArgument += " BCSID=" + quote(bcsid); // 세션 번호
		sArgument += " ISSUENUMBER=" + quote(j); // 인쇄 번호
		
		if (tableSelect=="FPW730PF"){
			sArgument += " TABLE=" + quote("FPW737PF");  // DNKE사내용 part label Seq No   
		}else{
			sArgument += " TABLE=" + quote("FPW837PF");  // 협력업체용(A Part) part label Seq No  
		}
		
	http.Sync = true;
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");
	http.Sync = false;			
}
function BarSizePrint()
{
	Var sLabelSize, sLabelFeed, sSizeBuf, sLabelWR, sLabelAX, sLabelAY ;

	cmmB570.Output = ESC1 + "WR" + LF;
	cmmB570.Output = ext_VB_CHR(0);
	waitTime(5000);  // 5초 슬립
	switch (rdoggub.Value)
	{
		case "0" :
		cmmB570.Output = ESC1 + ("D") + ("0150,0500,0130") + LF;
		cmmB570.Output = ext_VB_CHR(0);
		break;
		case "1" :
		cmmB570.Output = ESC1 + ("D") + ("0170,0450,0150") + LF;
		cmmB570.Output = ext_VB_CHR(0);
		break;
		case "2" :
		cmmB570.Output = ESC1 + ("D") + ("0350,0700,0330") + LF;
		cmmB570.Output = ext_VB_CHR(0);
		break;
	}
	
	cmmB570.Output = ESC1 + ("AX;") + ("-004,-004,+00") + LF;  //LABEL OR TAG 사이즈 결정
	cmmB570.Output = ext_VB_CHR(0);
	cmmB570.Output = ESC1 + ("AY") + (";+00,0") + LF;
	cmmB570.Output = ext_VB_CHR(0);
	cmmB570.Output = ESC1 + ("T") + ("21C21") + LF;           //LABEL PRINTER의 특징설정
	cmmB570.Output = ext_VB_CHR(0);
}

