// 2015-07-02 프린터 상태 체크 Function 추가
function printStatusChk(){	
	cmmB570.Output = ESC1 + ("C") + LF;   //BUFFER CLEAR
	cmmB570.Output = ext_VB_CHR(0);
	cmmB570.Output = ESC1 + "WS" + LF;
	cmmB570.Output = ext_VB_CHR(0);
	var sRcv = "";
	cmmB570.InputLen = 1;
						
	waitTime(100);  //0.1초 Sleep	
	//sRcv = cmmB570.Input;
	for (var x=0; x < 100; x++){
		sRcv = cmmB570.Input;
		if (sRcv == chr(1)) break;
	}
	//sRcv = cmmB570.Input;
	for (var x=0; x < 100; x++){
		sRcv = cmmB570.Input;
		if (sRcv == chr(2)) break;
	}
	
	cmmB570.InputLen = 11;  //status 7byte+ ETX + EOT + CR + LF
	for (var x=0; x < 100; x++){
		sRcv = cmmB570.Input;
		if (sRcv != "") break;
	}
	
	var sRtnSts = left(sRcv, 7);
	trace("상태값 : " + sRtnSts);			
	cmmB570.InputLen = 0;
	
	return sRtnSts;
}

