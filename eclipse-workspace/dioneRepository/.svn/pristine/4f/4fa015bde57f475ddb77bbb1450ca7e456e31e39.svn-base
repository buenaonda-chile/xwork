			cmmB570.Output = ESC1 + "PC000;0140,0050,1,1,H,00,B" + LF;  cmmB570.Output = ext_VB_CHR(0); // PRCCODE
			cmmB570.Output = ESC1 + "PC011;0310,0076,1,1,G,00,B" + LF;  cmmB570.Output = ext_VB_CHR(0); // 계획일자
			cmmB570.Output = ESC1 + "PC003;0420,0123,05,1,A,00,B" + LF;  cmmB570.Output = ext_VB_CHR(0); // SEQ
			cmmB570.Output = ESC1 + "PC004;0140,0095,1,1,G,00,B" + LF; cmmB570.Output = ext_VB_CHR(0); // 업체품번
			cmmB570.Output = ESC1 + "PC006;0140,0120,1,1,G,00,B" + LF; cmmB570.Output = ext_VB_CHR(0); // DNKE품번
			
			var SPT_RS = ">^"; 	// >^= RS
			var SPT_GS = ">]";	// >] = GS
			var SPT_EOT = ">D";	// EOT = EOT

			var barD =   "[)>0"+SPT_RS
						+ "06"+SPT_GS
						+ "V" + "AZIS"+ SPT_GS  
						+ "P" +trim(replace(a010,"-","")) + SPT_GS

						+ "S" + trim(barBuf)+ SPT_GS			            

						+ "T" +substr(trim(a013)+space(6),2,6)     // 계획일자
							 +"ED1" + prsnm_f // 고정값 : ED1 + 제품구분
			//				 +"A"+"0"+substr(trim(qrData2)+space(3),0,3) + trim(a014) + SPT_GS // (A + 부품 개별시리얼번호)						    
			// 2017년 수정
							 +"A"+"0"+substr(trim(qrData2)+space(3),0,3) + prsnm_s + SPT_GS // (A + 부품 개별시리얼번호)						    
						+ SPT_RS + SPT_EOT ; //일련번호			
			
			cmmB570.Output = ESC1 + "RB00;"+ barD + LF;  cmmB570.Output = ext_VB_CHR(0); //39 BARCODE값  
			
			//문자값MOVE
			cmmB570.Output = ESC1 + "RC000;"+ barBuf + LF;  cmmB570.Output = ext_VB_CHR(0); // ALC코드

			if(trim(a006) == ""){
				cmmB570.Output = ESC1 + "RC011;"+ substr(a013,2,6) + LF;  cmmB570.Output = ext_VB_CHR(0); // 계획일자
			}else{
				cmmB570.Output = ESC1 + "RC011;"+ a005 + LF;  cmmB570.Output = ext_VB_CHR(0); // KE일자코드
			}			
			
			cmmB570.Output = ESC1 + "RC003;"+ qrData2 + LF;  cmmB570.Output = ext_VB_CHR(0); // SEQ
			cmmB570.Output = ESC1 + "RC004;"+ a010 + LF;  cmmB570.Output = ext_VB_CHR(0); // 업체품번
			cmmB570.Output = ESC1 + "RC006;"+ a002 + LF;  cmmB570.Output = ext_VB_CHR(0); // DNKE품번
			//라벨 프린터 발행명령
			cmmB570.Output = ESC1 + ("XS;I,0001,0002C7100") + LF; cmmB570.Output = ext_VB_CHR(0);
		break;
		

		default :  //그외는 case 4 포멧으로 적용
			barBuf=a008+a005;
			cmmB570.Output = ESC1 + ("C") + LF; cmmB570.Output = ext_VB_CHR(0); //BUFFER CLEAR
			//X좌표 왼쪽 여백 1.5MM부분이 X좌표원점임
			cmmB570.Output = ESC1 + ("XB00;0050,0015,B,1,03,03,06,06,03,0,0050,+0000000000,0,00") + LF; cmmB570.Output = ext_VB_CHR(0);//39 BARCODE FORMAT,X:5.00MM,Y=1.0MM
			cmmB570.Output = ESC1 + ("XB01;0390,0015,T,L,04,A,0=") + qrData + LF;   cmmB570.Output = ext_VB_CHR(0);   //QR BARCODE FORMAT,X:0400,Y:=4.00MM,Y:=1.0MM
		
			//하기 출력할 문자값,좌표및 특성결정
			cmmB570.Output = ESC1 + "PC000;0050,0095,1,1,C,00,B" + LF;   cmmB570.Output = ext_VB_CHR(0);//업체품번 
			cmmB570.Output = ESC1 + "PC001;0050,0130,1,1,D,00,B" + LF; cmmB570.Output = ext_VB_CHR(0);//계획일자
			cmmB570.Output = ESC1 + "PC002;0240,0125,05,05,D,00,B" + LF; cmmB570.Output = ext_VB_CHR(0); //풍성품번 
			cmmB570.Output = ESC1 + "PC003;0420,0122,05,1,A,00,B" + LF;  cmmB570.Output = ext_VB_CHR(0); // QR일련번호
			cmmB570.Output = ESC1 + "RB00;"+ barBuf + LF; cmmB570.Output = ext_VB_CHR(0); //39 BARCODE값  
			//문자값MOVE			
			cmmB570.Output = ESC1 + "RC000;"+ a010 + LF;  cmmB570.Output = ext_VB_CHR(0);//업체품번
			cmmB570.Output = ESC1 + "RC001;"+ a013 + LF;  cmmB570.Output = ext_VB_CHR(0);//계획일자
			cmmB570.Output = ESC1 + "RC002;"+ a002 + LF;  cmmB570.Output = ext_VB_CHR(0);//풍성품번 
			cmmB570.Output = ESC1 + "RC003;"+ qrData2 + LF; cmmB570.Output = ext_VB_CHR(0); // QR일련번호
			//라벨 프린터 발행명령
			cmmB570.Output = ESC1 + ("XS;I,0001,0002C7100") + LF;  cmmB570.Output = ext_VB_CHR(0);
			break;
	}
}

function BarCutting()
{
	waitTime(500);  //0.5초 Sleep	
	cmmB570.InBufferCount = 0;
	cmmB570.OutBufferCount =0;
	cmmB570.Output = ESC1 + "C" + LF ;
	cmmB570.Output = ext_VB_CHR(0);
	cmmB570.Output = ESC1 + "XS;I,0001,0252C6100" + LF;
	cmmB570.Output = ext_VB_CHR(0);
}

