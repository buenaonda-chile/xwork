	var cnt = 0;
	var st_PROCS="(";
	var in_PROCS="";
	var ed_PROCS=")";
	var arrValuePROCS = array(); 

	for(var i=0; i<ds_FPQC100Q_1.RowCount(); i++){
		if(ds_FPQC100Q_1.GetSelect(i) == true){
			arrValuePROCS[cnt] = trim(ds_FPQC100Q_1.GetColumn(i,"PROCS"));
			cnt++;	
		}
	}
	
	if(cnt == 0 ) return ;
	for(var j=0; j<cnt; j++){
		if(j==0){
			in_PROCS="'"+arrValuePROCS[j]+"'";
		}else{
			in_PROCS=in_PROCS+",'"+arrValuePROCS[j]+"'";
		}
	}
	in_PROCS=st_PROCS+in_PROCS+ed_PROCS;
	var sKind		= "getFPQE140Q_234";						
	//var sMethodName = "service::psfaclib/selectFPQE140Q_4.do";     		// 서버에서 호출 될 Method 명
	var sMethodName = "service::barcode/selectFPQE140Q/list4";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                            				// 자료 저장 시 보낼 DataSet 명칭
	var	sOutDataSet = "ds_FPQE140Q_4=ds_FPQE140Q_4";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
