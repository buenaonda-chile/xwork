		sArgument += " LIB2=" + quote(Lib2);
		sArgument += " COMPS=" + quote(ComboBoxPlant.Value); //사업부
		sArgument += " PNYMD=" + quote(cal_YMD.Value); //년월일
		sArgument += " PROCS=" + quote(in_PROCS); //공정코드 multi select
		sArgument += " TABLE=" + quote(tableSelect);
