function ds_HUMAN_CanColumnChange(obj,nRow,strColumnID,varOldValue,varNewValue,nPivotIndex)
{
	if(strColumnID=="SABUN"){ //중복체크
		if(varNewValue!=""){
			if(ds_HUMAN.FindRow("SABUN",varNewValue)!="-1"){
				alert(setPopMessage("2533"));
				ds_HUMAN.SelectRow(ds_HUMAN.FindRow("SABUN",varNewValue));
				ds_HUMAN.DeleteRow(nRow);
				return false;
			}
		}
	}
}

function ds_HUMAN_CanRowPosChange(obj,nOldRow,nRow)
{
	http.usewaitcursor = false;
	var p_SABUN=ds_HUMAN.GetColumn(ds_HUMAN.currow,"SABUN"); //교육시작일

	if(length(p_SABUN)==0){
		ds_HUMAN.DeleteRow(ds_HUMAN.currow);	//삭제	
	}
	return true;
	http.usewaitcursor = true;
}

function btn_FINDFILE_OnClick(obj)
{
	var file_name;

	FileDialog0.Filter = "ALL File(.xls)|*.xls";	
	FileDialog0.Type = "OPEN";
	if (!FileDialog0.Open()) {
		alert(setPopMessage("1009"));	//  파일 선택 창을 여는데 실패하였습니다.
		return;
	} 
	
	ds_HUMAN.ClearData();
	
	file_name = FileDialog0.FileName;
	
	var file_path = FileDialog0.FilePath;
	var path = file_path + "\\" + file_name;
	
	setMessageDirect(path);	
	
	ext_ExcelImportByIndex(path,0,"ds_HUMAN",1,0,0,2);//upload1  		
	
	if (ds_HUMAN.rowcount < 1){
		alert("[경고] 생성할 data가 없습니다. 확인바랍니다."); 
		return;	
	}
