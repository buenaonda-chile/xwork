/********************************************************************** 
 * 내용설명 : 화면 조회 
 **********************************************************************/ 
function getHMDZ004Q(){

	if(!validationChk()){
		return;
	}
	
	if(ds_HUMAN.GetRowType(ds_HUMAN.currow)=="update" ||ds_HUMAN.GetRowType(ds_HUMAN.currow)=="insert" ){
		if(!ds_HUMAN_CanRowPosChange()){
			return;
		}
	} 
	
	if(ds_HUMAN.rowcount<1){
		alert("[알림]조회대상자를 입력하시기바랍니다.");
		return;
	}
	
	var arrSABUN="('";
	for(i=0; i<ds_HUMAN.rowcount; i++){
		if(i == ds_HUMAN.rowcount-1){
			arrSABUN = arrSABUN+ds_HUMAN.GetColumn(i,"SABUN") + "')";
		}else{
			arrSABUN = arrSABUN+ds_HUMAN.GetColumn(i,"SABUN") + "','";
		}
	}

	var p_YYEAR="";
	if(ComboBoxPlant.Value!="E1"){
		p_YYEAR=parseInt(substr(cmb_YYEAR.Value,0,4))-1;
	}else{
		p_YYEAR=substr(cmb_YYEAR.Value,0,4);
	}
	
	if(ComboBoxPlant.Value =="C1"){
		Lib3="PSCDBLIB";
	}else if(ComboBoxPlant.Value =="H1"){
		Lib3="PSHDBLIB";
	}else if(ComboBoxPlant.Value =="S1"){
		Lib3="PSSDBLIB";
	}
	
	/*
	if(chK_CNDCD.Value==true){
		if(cmb_CNDAT.Value=""){
			alert("[알림]퇴사자포함일자를 입력해주세요");
			return;
		}
	}	
	*/
	var sKind		= "getHMDZ004Q";						
	var sMethodName = "service::pshumlib/selectHMDZ004Q.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                            				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_HMDZ004Q=ds_HMDZ004Q";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
		sArgument += "@LIB1=" + quote(Lib1);
		sArgument += "@LIB2=" + quote(Lib2);
		sArgument += "@LIB3=" + quote(Lib3);
		sArgument += "@COMPS=" + quote(ComboBoxPlant.Value);
		sArgument += "@KNYYMF=" + quote(substr(cmb_KNYYMF.Value,0,6));	//기준년월(FR)
		sArgument += "@KNYYMT=" + quote(substr(cmb_KNYYMT.Value,0,6));	//기준년월(TO)
		sArgument += "@YYEAR=" + quote(p_YYEAR);	//년차지급년도
		sArgument += "@YYEAR_ORG=" + quote(substr(cmb_YYEAR.Value,0,4));	//년차지급년도
		sArgument += "@arrSABUN=" + quote(arrSABUN);	//대상자LIST
				
		sArgument += "@CNDCD=" + quote(chK_CNDCD.Value);	//퇴사자포함여부
		sArgument += "@CNDAT=" + quote(cmb_CNDAT.Value);	//퇴사일자
	var str = dialog("Common::PGMLOAD_HUM.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");	
	http.Sync = false;
	
	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
		setMessage("1004");	
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));
		setMessage("1003");	
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}
}
/********************************************************************** 
 * 내용설명 : Transaction 리턴후 실행 함수
 * 주요변수 : 
 * 			- sMethodName	Transaction 고유 ID
 * 			- ErrorCode 	에러코드
 * 			- ErrorMSG 		에러 메세지
 **********************************************************************/ 
function callback(svcId, rtnCd, rtnMsg){
	if(svcId=="getHMDZ004Q"){
		if(rtnCd<0){
			alert(setPopMessage("1002"));
			setMessage("1002");
			
		}else{
			if(ds_HMDZ004Q.rowcount<1){
				alert(setPopMessage("1003"));
				setMessage("1003");	
			}else{
				alert(setPopMessage("1004"));
				setMessage("1004");	
			}
		}
	}
}
/********************************************************************** 
 * 내용설명 : validation 체크
 **********************************************************************/ 
function validationChk(){

	var result = true;
	cmb_KNYYMF.BKColor="white";
	cmb_KNYYMT.BKColor="white";
	cmb_YYEAR.BKColor="white";
		
	if (cmb_KNYYMF.Value == ""){
		cmb_KNYYMF.BKColor="paleturquoise";
		alert(setPopMessage("4053"));//[확인] 기준년월을 확인바랍니다.
		setMessage("4053");
		result = false;
	}else if(cmb_KNYYMT.Value == ""){
		cmb_KNYYMT.BKColor="paleturquoise";
		alert(setPopMessage("4053"));//[확인] 기준년월을 확인바랍니다.
		setMessage("4053");
		result = false;
	}else if(cmb_KNYYMF.Value > cmb_KNYYMT.Value){
		cmb_KNYYMF.BKColor="paleturquoise";
		cmb_KNYYMT.BKColor="paleturquoise";
		alert(setPopMessage("4053"));//[확인] 기준년월을 확인바랍니다.
		setMessage("4053");
		result = false;
	}else if(cmb_YYEAR.Value == ""){
		cmb_YYEAR.BKColor="paleturquoise";
		alert(setPopMessage("년차지급년월을 입력하세요"));//[확인] 기준년월을 확인바랍니다.
		//setMessage("4053");
		result = false;
	}
	return result;
}

