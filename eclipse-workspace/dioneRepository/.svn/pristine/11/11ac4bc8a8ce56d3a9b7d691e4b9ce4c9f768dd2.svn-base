var strSeparator = "	";
var Lib1 = G_ds_user.GetColumn(0,"LIB1"); // cigma library(pereslib,psreslib)
var Lib2 = G_ds_user.GetColumn(0,"LIB2"); // 회사 library(psedblib,pscdblib)
//var Lib3 = G_ds_user.GetColumn(0,"LIB3"); // 사업부 library(psedblib,pscdblib,pshdblib,pssdblib)
var company = G_ds_user.GetColumn(0,"COMPANY");
var comps = G_ds_user.GetColumn(0,"PLANT_CD");

function on_LoadCompleted(obj){
	getTitle();	
	getDesign();
	gFcOnTopButtonColor("1.4"); //상단 버튼 활성화 여부 (기본:0,조회:1,변경:2)	1.3
	getComCode(); //공장구분 선택
	Ubi0.utfData= MainGrid.Height;
}

function getComCode(){
	var sKind		= "getComCode";						
	var sMethodName = "service::login/selectComCode.do"; 
	var sInDataSet  = "";
	var sOutDataSet = "ds_ComCode_PLNT=ds_ComCode_PLNT";
	var sArgument   = "";
		sArgument   += " CLASS='PLNT'";
		sArgument   += " CODE1=" + quote(company);		
	http.Sync=true;
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	http.Sync=false;
	ComboBoxPlant.Value=comps;
	//ComboBoxPlant.Enable = false; 
}

function getDesign(){
	Ubi0.HeadHeight = G_ds_design.GetColumn(0,"HeadHeight");
	searchBar.Width = G_ds_design.GetColumn(0,"Width");
	searchBar.Top = G_ds_design.GetColumn(0,"SBTop");		
}

function getSZZE201R(){
	
	if(!validationChk()){
		return;
	}
	var slpno2 = "";
	slpno2 = ed_SLPNOC2.Text;
	if(slpno2==""){
		slpno2="9999999999";
	}
	var sKind		= "getSZZE201R";						
	var sMethodName = "service::pssallib/selectSZZE201R.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_SZZE201R=ds_SZZE201R";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
	sArgument += "@LIB1=" + quote(Lib1);
	sArgument += "@LIB2=" + quote(Lib2);
	sArgument += "@COMPS=" + quote(ComboBoxPlant.Value); //사업부
	sArgument += "@PRSDPC=" + quote(cmb_PRSDPC.Value); //처리부서
	sArgument += "@SLPNO1=" + quote(ed_SLPNOC1.Text); //SLIP NO FROM
	sArgument += "@SLPNO2=" + quote(slpno2); //SLIP NO TO
	sArgument += "@TRNDTS=" + quote(cmb_TRNDTC.Value); //TRANSFER DATE
	var str = dialog("Common::PGMLOAD_SAL.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");
	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
		setMessage("1004");
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));
		setMessage("1003");	
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}

	//Ubi0.setJrfFileDir("http://10.74.139.50/ui/PSSALLIB/REPORT/");
	//Ubi0.setJrfFileDir("http://10.72.209.23/ui/PSSALLIB/REPORT/");
	//Ubi0.setJrfFileDir("http://dione.dnpe.co.kr/ui/PSSALLIB/REPORT/");
//	Ubi0.setJrfFileDir("http://dione.dnke.kr/ui/PSSALLIB/REPORT/");
	Ubi0.setJrfFileDir(G_baseURL+"ui/PSSALLIB/REPORT/");
	Ubi0.setJrfFileName("SZZE201R.jrf");
	Ubi0.SetDataset("ds_SZZE201R", "ds_SZZE201R");	
	Ubi0.Retrieve();

}
/********************************************************************** 
 * 내용설명 : validation 체크
 **********************************************************************/ 
 
function validationChk(){

	var result = true;
	
	if(cmb_TRNDTC.Value =="" || cmb_TRNDTC.Value < 20030101 || cmb_TRNDTC.Value > 99991231){
		alert(setPopMessage("4021"));
		setMessage("4021");
		cmb_TRNDTC.Value="";
		result = false;
	}
	return result;
}

