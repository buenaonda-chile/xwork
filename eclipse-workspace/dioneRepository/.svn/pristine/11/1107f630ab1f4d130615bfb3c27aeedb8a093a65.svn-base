}


function getSJTZ295Q(){

	var sMGMDT1="";
	var sMGMDT2="";
	var sMGMTM1="";
	var sMGMTM2="";
	
	//업체
	if(cmb_MGMDT1.Value==""){
		sMGMDT1="00000000";
	}
	else{
		sMGMDT1=cmb_MGMDT1.Value;
	}	
	if(cmb_MGMDT2.Value==""){
		sMGMDT2="99999999";
	}
	else{
		sMGMDT2=cmb_MGMDT2.Value;
	}
	//년월
	if(length(ed_MGMTM1.Text)==0){
		sMGMTM1="000000";
	}
	else{
		sMGMTM1=ed_MGMTM1.Text;
	}	
	if(length(ed_MGMTM2.Text)==0){
		sMGMTM2="999999";
	}
	else{
		sMGMTM2=ed_MGMTM2.Text;
	}

	var sKind		= "getSJTZ295Q";						
	var sMethodName = "service::pssallib/selectSJTZ295Q.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_SJTZ295Q=ds_SJTZ295Q";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
	sArgument += "@LIB1=" + quote(Lib1);
	sArgument += "@LIB2=" + quote(Lib2);
	sArgument += "@COMPS=" + quote(ComboBoxPlant.Value); //사업부
	sArgument += "@MGMDT1=" + quote(sMGMDT1); //마감일자TO
	sArgument += "@MGMDT2=" + quote(sMGMDT2); //마감일자FROM
	sArgument += "@MGMTM1=" + quote(sMGMTM1); //마감시간TO
	sArgument += "@MGMTM2=" + quote(sMGMTM2); //마감시간FROM

	var str = dialog("Common::PGMLOAD_SAL.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");
	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
		setMessage("1004");	
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));  //[알림] 신규입력입니다.
		setMessage("1003");	
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}
}

