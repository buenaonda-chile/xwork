 **********************************************************************/ 
function callback(svcId, rtnCd, rtnMsg){
	// tran 실패
	if(rtnCd<0){
		alert(setPopMessage("1002"));
		setMessage("1002");
	// tran 성공	
	}else if(svcId=="getSPDE141Q"){
		if(ds_SPDE141Q_142PF.rowcount <= 0 && ds_SPDE141Q_148PF.rowcount <= 0){   //data값이 없을때
			alert(setPopMessage("1003"));
			setMessage("1003");
		}else{
			alert(setPopMessage("1004"));
			setMessage("1004");
			rdoggub.Enable=true;
		}	
	}
}
