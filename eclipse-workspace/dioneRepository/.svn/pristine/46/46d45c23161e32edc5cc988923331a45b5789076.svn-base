function getSPDE141Q(){

	if(!validationChk()){
		return;
	}

	var sPYVND1="";
	var sPYVND2="";

	//업체
	if(length(ed_PYVND1.Text)==0){
		sPYVND1="00000000";
	}
	else{
		sPYVND1=ed_PYVND1.Text;
	}	
	if(length(ed_PYVND2.Text)==0){
		sPYVND2="99999999";
	}
	else{
		sPYVND2=ed_PYVND2.Text;
	}
	
	var orgcomps="";
	if(ComboBoxPlant.Value=="E1"){
		orgcomps ="PE";
	}
	else{
		orgcomps ="PS";
	}
	var sKind		= "getHOUSE";		
	var sMethodName = "service::pscomlib/selectMST040PF_HOUSE.do"; 
	var sInDataSet  = "";
	var sOutDataSet = "ds_MST040PF_HOUSE=ds_MST040PF_HOUSE";
	var sArgument   = "";
		sArgument += " LIB2=" + quote(Lib2);
		sArgument += " GCOMPS=" + quote(orgcomps); //회사(PE/PS)
		sArgument += " COMPS=" + quote(ComboBoxPlant.Value); //사업부
	http.Sync=true;
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	http.Sync=false;
	if (ds_MST040PF_HOUSE.rowcount < 1){
		alert(setPopMessage("4033"));
		setMessage("4033");
		return;
	}
	var arrHouse = "('";
	for(i=0; i< ds_MST040PF_HOUSE.rowcount; i++){
		if(i == ds_MST040PF_HOUSE.rowcount-1){
			arrHouse = arrHouse+ds_MST040PF_HOUSE.GetColumn(i,"MAJCD_H") + "')";
		}else{
			arrHouse = arrHouse+ds_MST040PF_HOUSE.GetColumn(i,"MAJCD_H") + "','";
		}
	}
	
	http.Sync=true;

	var sKind		= "getSPDE141Q";						
	var sMethodName = "service::pssallib/selectSPDE141Q.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_SPDE141Q_142PF=ds_SPDE141Q_142PF ds_SPDE141Q_148PF=ds_SPDE141Q_148PF";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
	sArgument += " LIB1=" + quote(Lib1);
	sArgument += " LIB2=" + quote(Lib2);
	sArgument += " COMPS=" + quote(ComboBoxPlant.Value); //사업부
	sArgument += " SHPNO=" + quote(0); //shpno
	sArgument += " CHLDT=" + quote(cmb_CHLDT.Value); //기준일자
	sArgument += " PYVND1=" + quote(sPYVND1); //업체FR
	sArgument += " PYVND2=" + quote(sPYVND2); //업체TO
	sArgument += " HOUSE=" + quote(arrHouse); //업체TO
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");

	var pyvnd="";
	var makdt142="";
	var makdt148="";
	http.Sync=false;
	if(ds_SPDE141Q_142PF.rowcount>1){
		pyvnd = ds_SPDE141Q_142PF.GetColumn(0,"PYVND");
		makdt142 = ds_SPDE141Q_142PF.GetColumn(0,"CHLDT");
		var sKind		= "getSAL100";		
		var sMethodName = "service::pscomlib/selectSAL100_SE.do"; 
		var sInDataSet  = "";
		var sOutDataSet = "ds_SAL100=ds_SAL100";
		var sArgument   = "";
			sArgument += " LIB2=" + quote(Lib2);
			sArgument += " COMPS=" + quote(ComboBoxPlant.Value); //
			sArgument += " PYVND=" + quote(pyvnd); //업체
			sArgument += " MAKDT=" + quote(makdt142); //기준일자
			sArgument += " CHKCNT=" + quote(14); //리턴수
		http.Sync=true;
		Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
		http.Sync=false;
			
		MainGrid.SetCellProp("Head", 20, "text", ds_SAL100.GetColumn(0,"MAKMK")+" "+ds_SAL100.GetColumn(0,"CLHCD"));
		MainGrid.SetCellProp("Head", 21, "text", ds_SAL100.GetColumn(1,"MAKMK")+" "+ds_SAL100.GetColumn(1,"CLHCD"));
		MainGrid.SetCellProp("Head", 22, "text", ds_SAL100.GetColumn(2,"MAKMK")+" "+ds_SAL100.GetColumn(2,"CLHCD"));
