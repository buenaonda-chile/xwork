function getFHDC111R(){
	
	if(!validationChk()){
		return;
	}

	getSYSDATE(); //SYSTEM 날짜
	var sysDt =ds_SYSDATE.GetColumn(0,"DT");
	var systm =ds_SYSDATE.GetColumn(0,"TM");
	var chkdpnm =st_CODE.Text;
	var chkdpcd =ed_CODE.Text;
	var chkwrkdt =cmb_WRKDT.Value;
	
	var selTab1="";
	var selTab2="";
	
	if(rdoggub.Value==1){//마감전
		selTab1="FHD110PF";
		selTab2="FHD610PF";
	}
	else{//마감후
		selTab1="FHD100PF";
		selTab2="FHD600PF";	
	}
	var sKind		= "getFHDC111R";						
	var sMethodName = "service::psfaclib/selectFHDC111R.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_FHDC111R=ds_FHDC111R";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
		sArgument += "@LIB1=" + quote(Lib1);
		sArgument += "@LIB2=" + quote(Lib2);
		sArgument += "@LIB3=" + quote(Lib3);
		sArgument += "@COMPS=" + quote(ComboBoxPlant.Value); //사업부
		sArgument += "@DPCODE=" + quote(ed_CODE.Text); //부서
		sArgument += "@WRKDT=" + quote(cmb_WRKDT.Value); //근태일자
		sArgument += "@WRKTABLE1=" + quote(selTab1); //기능직 근태
		sArgument += "@WRKTABLE2=" + quote(selTab2); //직반장 근태
	var str = dialog("Common::PGMLOAD_FAC.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");
	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
		setMessage("1004");
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));
		setMessage("1003");
		return;
	}else if (str == 'E'){   //erro r발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
		return;
	}
	Ubi0.setJrfFileDir(G_baseURL+"ui/PSFACLIB/REPORT/");
	//Ubi0.setJrfFileDir("http://dione.dnke.kr/ui/PSFACLIB/REPORT/");
	//Ubi0.setJrfFileDir("http://10.72.209.23/ui/PSFACLIB/REPORT/");
	Ubi0.setJrfFileName("FHDC111R.jrf");
	Ubi0.SetDataset("ds_FHDC111R", "ds_FHDC111R");
	Ubi0.SetArgument("SYSDATE",sysDt);
	Ubi0.SetArgument("SYSTIME",systm);
	Ubi0.SetArgument("CHKWRKDT",chkwrkdt);
	Ubi0.SetArgument("CHKDPNM",chkdpnm);
	Ubi0.SetArgument("CHKDPCD",chkdpcd);
	Ubi0.Retrieve();

}

/********************************************************************** 
 * 내용설명 : validation 체크
 **********************************************************************/ 
 
function validationChk(){
	var result = true;
	ed_CODE.BKColor="white";	
	cmb_WRKDT.BKColor="white";
	if(cmb_WRKDT.Value=="")	{
		cmb_WRKDT.BKColor="paleturquoise";
		alert(setPopMessage("4061"));
		//alert("[확인] 작업일자를 확인바랍니다.");
		cmb_WRKDT.SetFocus();
		result =false;
	}
	else if(ComboBoxPlant.Value !="E1"){
		if(length(trim(ed_CODE.Text))!=0){
			http.usewaitcursor = false;
			http.Sync 	= true;
			var sKind		= "getMST650PF";		
			var sMethodName = "service::pscomlib/selectMST650.do";  					
			var sInDataSet  = "";
			var sOutDataSet = "ds_MST650PF=ds_MST650PF";
			var sArgument   = "";
			sArgument += " LIB2=" + quote(Lib2);
			sArgument += " COMPS=" + quote(COMPS); //사업부
			sArgument += " DPCODE=" + quote(ed_CODE.Text); //부서코드
			Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
			http.Sync 	= false;
			if (ds_MST650PF.GetRowCount() < 1){
				ed_CODE.BKColor="paleturquoise";
				//alert("[알림]부서코드확인바랍니다.");
				alert(setPopMessage("4708"));
				setMessage("4708");	
				ed_CODE.SetFocus();
				result =false;
			}
			st_CODE.Text=trim(ds_MST650PF.GetColumn(0,"DPCONM"));
			http.usewaitcursor = true;			
		}
		else if(length(trim(st_CODE.Text)) < 1)	{
			ed_CODE.BKColor="paleturquoise";
			alert(setPopMessage("2544"));
			//alert("[경고] 필수입력항목입니다. (부서코드)");
			ed_CODE.SetFocus();
			result =false;
		}
	}
	return result;
}

