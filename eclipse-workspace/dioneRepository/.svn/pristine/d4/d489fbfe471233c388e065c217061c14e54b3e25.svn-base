function getSYSDATE(){
	var sKind		= "getSYSDATE";						
	var sMethodName = "service::pscomlib/selectSYSDATE.do"; 
	var sInDataSet  = "";
	var sOutDataSet = "ds_SYSDATE=ds_SYSDATE";
	var sArgument   = "";
	http.Sync=true;
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	http.Sync=false;	
}

function ed_CODE_OnKeyDown(obj,nChar,bShift,bCtrl, bAlt,LLParam,HLParam)
{
	if(nChar == 13){
		ds_FHDC111R.ClearData();
		Ubi0.Retrieve();
		fn_Search();
	}	
}
