	ds_HUM090PF.SetColumn(ds_HUM090PF.currow,"USER_YN","1");//사용유무 :1 사용 2 미사용
	ds_HUM090PF.SetColumn(ds_HUM090PF.currow,"INPDT",today());//등록일자
	ds_HUM090PF.SetColumn(ds_HUM090PF.currow,"INPTM",substr(getDate(),8));//등록일자
