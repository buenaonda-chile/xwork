/********************************************************************** 
 * 내용설명 : 화면 조회 
 **********************************************************************/ 
function getHASS04(){
		
	var sKind		= "getHASS04";						
	var sMethodName = "service::pshumlib/selectHASS04.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                            				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_HUM090PF=ds_HUM090PF";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
		sArgument += "@LIB2=" + quote(Lib2);
		sArgument += "@LIB3=" + quote(Lib3);
		sArgument += "@COMPS=" + quote(ComboBoxPlant.Value);//사업부
		sArgument += "@USER_YN=" + quote("1");//사용이 1인것만조회
	var str = dialog("Common::PGMLOAD_HUM.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");	
	http.Sync = false;
}
/********************************************************************** 
 * 내용설명 : insert, update, delete
 **********************************************************************/  
function setHASS04(){

	var sKind		= "setHASS04";						
	var sMethodName = "service::pshumlib/saveHASS04.do";    // 서버에서 호출 될 Method 명
	var sInDataSet  = "ds_HUM090PF=ds_HUM090PF:U";               //자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_HUM090PF_CHECK=ds_HUM090PF_CHECK";  	   // 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
		sArgument += " LIB2=" + quote(Lib2);
		sArgument += " INPMN=" + quote(usrID);//등록ID
		sArgument += " CHGMN=" + quote(usrID);//수정ID
	http.Sync=true;
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");
	http.Sync=false;

}
/********************************************************************** 
 * 내용설명 : Transaction 리턴후 실행 함수
 * 주요변수 : 
 * 			- sMethodName	Transaction 고유 ID
 * 			- ErrorCode 	에러코드
 * 			- ErrorMSG 		에러 메세지
 **********************************************************************/ 
function callback(svcId, rtnCd, rtnMsg){
	if (ds_HUM090PF_CHECK.GetRowCount() > 0){			
		for(var i=0;i<ds_HUM090PF_CHECK.GetRowCount();i++){		
			alert("DIONE ID : " +ds_HUM090PF_CHECK.GetColumn(i,"DIOID") + ", 사번 : " +ds_HUM090PF_CHECK.GetColumn(i,"SABUN") 
			+ ", 이름 : " +ds_HUM090PF_CHECK.GetColumn(i,"KORNM") + " = " + ds_HUM090PF_CHECK.GetColumn(i,"RTMSG"));
		}
		getHASS04();        //error항목이 존재시에는 재조회		
		return;		
	}// tran 실패
	if(rtnCd<0){
		alert(setPopMessage("1002"));
		setMessage("1002");
	}else{
		alert(setPopMessage("1001"));
		setMessage("1001");	
	}
}

