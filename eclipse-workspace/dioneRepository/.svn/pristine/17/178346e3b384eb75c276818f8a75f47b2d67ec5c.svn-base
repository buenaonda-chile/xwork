function ds_SVZE050_DETAIL_OnColumnChanged(obj,nRow,strColumnID,varOldValue,varNewValue,nPivotIndex)
{
	if( strColumnID == "CINBR"){
	http.usewaitcursor = false;
		if(varNewValue != "" ){
			if(length(varOldValue)!=0 || varOldValue != varNewValue){
				var tval=getMKPNO(toUpper(varNewValue));
				if(tval==0){
					alert(setPopMessage("4001"));  //Part Number Cross Ref MA에 미등록된 득의선,품번입니다.
					setMessage("4001");	
					//alert("Part Number Cross Ref MA에 미등록된 득의선,품번입니다.");  //Part Number Cross Ref MA에 미등록된 득의선,품번입니다.
					ds_SVZE050_DETAIL.SetColumn(ds_SVZE050_DETAIL.currow,"CINBR", "");
					return false;
				}else{
					ds_SVZE050_DETAIL.SetColumn(ds_SVZE050_DETAIL.currow,"CINBR", trim(ds_EM000PR.GetColumn(0,"PRTNO")));
				}
			}
		}
		if(varNewValue == "" ){
			ds_SVZE050_DETAIL.SetColumn(ds_SVZE050_DETAIL.currow,"CINBR", "");
		}			
	http.usewaitcursor = true;
	}	
}
function getMKPNO(tval){

	http.usewaitcursor = false;
	var sKind		= "getMKPNO";		
	var sMethodName = "service::pscomlib/selectEM000PR.do"; 
	var sInDataSet  = "";
	var sOutDataSet = "ds_EM000PR=ds_EM000PR";
	var sArgument   = "";
		sArgument  += " LIB1=" + quote(Lib1);
		sArgument  += " CUSNO=" + quote(ds_SVZE050_DETAIL.GetColumn(ds_SVZE050_DETAIL.currow,"PYVND"));
		sArgument  += " PRTNO=" + quote(tval);
	http.Sync=true;
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	http.Sync=false;
	if(ds_EM000PR.rowcount < 1){
		return 0;	
	}
	else{
		return 1;
	}
	http.usewaitcursor = true;
}]]></Script>
