function fn_Input()
{
	if(!validationChk()){
		return;
	}
	if(rdoggub.Value=1){
		alert("작업구분이 추가인 경우에만 추가가능합니다.");
		return;
	}
		
	if(checkInsertCount("ds_SVZE050")== false) return;
		if(ds_SVZE050.rowcount < 1) {
		http.Sync 	= true;
		var sKind		= "getMKPNO";		
		var sMethodName = "service::pscomlib/selectEM000PR.do"; 
		var sInDataSet  = "";
		var sOutDataSet = "ds_EM000PR=ds_EM000PR";
		var sArgument   = "";
			sArgument  += " LIB1=" + quote(Lib1);
			sArgument  += " CUSNO=" + quote(ed_PYVND.Text); //업체
			sArgument  += " PRTNO=" + quote(ed_PSPNO.Text); //품번
		Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
		if (ds_EM000PR.GetRowCount() < 1){
			//alert("Part Number Cross Ref MA에 미등록된 득의선,품번입니다.");
			alert(setPopMessage("4001"));
			setMessage("4001");
			return false;
		}	
		fn_Search();  
	}
	ds_SVZE050_DETAIL.AddRow();
	ds_SVZE050_DETAIL.SetColumn(ds_SVZE050_DETAIL.currow, "COMPS", ComboBoxPlant.Value);
	ds_SVZE050_DETAIL.SetColumn(ds_SVZE050_DETAIL.currow, "PYVND", ed_PYVND.Text);	
	ds_SVZE050_DETAIL.SetColumn(ds_SVZE050_DETAIL.currow, "PSPNO", ed_PSPNO.Text);	
}

function fn_Delete()
{
	if(ds_SVZE050_DETAIL.rowcount < 1) return;			//삭제가 없으면 건너뛰기		
	ds_SVZE050_DETAIL.DeleteRow(ds_SVZE050_DETAIL.currow);	//삭제		
	alert(setPopMessage("1011"));				//[알림] 화면에서만 제거되었습니다. 저장하시면 삭제됩니다.		
}

