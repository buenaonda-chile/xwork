function fn_Input()
{
	if(!validationChk()){
		return;
	}
	//1.등록시 PART NO CROSS REFERANCE MASTER CHECK
	var sKind		= "getEM000PR";						
	var sMethodName = "service::pscomlib/selectEM000PR.do";    // 서버에서 호출 될 Method 명
	var sInDataSet  = "";               //자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_EM000PR=ds_EM000PR";  	   // 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
		sArgument += " LIB1=" + quote(Lib1);
		sArgument += " CUSNO=" + quote(ed_PYVND.Text); //업체
		sArgument += " PRTNO=" + quote(ed_PSPNO.Text); //품번
	http.Sync=true;
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	http.Sync=false;
	if(ds_EM000PR.rowcount<1){
		//alert("[확인] Part Number Cross Ref MA에 미 등록된 득의선,품번입니다.");
		alert(setPopMessage("4001"));//[확인] Part Number Cross Ref MA에 미 등록된 득의선,품번입니다.
		setMessage("4001");			
		return;
	}else{
		if(ds_FZME109.rowcount < 1) fn_Search();
		if(ds_FZME109.rowcount < 1){
			//alert("[알림] 신규입력입니다.");
			//alert(setPopMessage("1013"));//[알림] 신규입력입니다.
			//setPopMessage("1013");
			
			var sKind		= "getOGMPrtno";		
			var sMethodName = "service::pscomlib/selectMST050100PF.do"; 
			var sInDataSet  = "";
			var sOutDataSet = "ds_MST100PF=ds_MST050100PF";
			var sArgument   = "";
				sArgument  += " LIB2=" + quote(Lib2);
				sArgument  += " COMPS=" + quote(ComboBoxPlant.Value);//사업부
				sArgument  += " GRPCD=" + quote("CAR");
				sArgument  += " PRTNO=" + quote(ed_PSPNO.Text);
			http.Sync 	= true;
			Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
			http.Sync 	= false;

			ds_FZME109.AddRow();
			ds_FZME109.SetColumn(0,"PYVND",ed_PYVND.Text);
			ds_FZME109.SetColumn(0,"PSPNO",ed_PSPNO.Text);
			ds_FZME109.SetColumn(0,"CUSNM",st_PYVND.Text);
			ds_FZME109.SetColumn(0,"ITDSC",st_PSPNO.Text);
			ds_FZME109.SetColumn(0,"CARCD",trim(ds_MST100PF.GetColumn(0,"MSTCD")));
			ds_FZME109.SetColumn(0,"CARNM",trim(ds_MST100PF.GetColumn(0,"MSTSM")));
			ds_FZME109.SetColumn(0,"HNDTM",0);//공수(입력내용없음)
			ds_FZME109.SetColumn(0,"INPQY",0);//in수량
			ds_FZME109.SetColumn(0,"OUTQY",0);//out수량
			ds_FZME109.SetColumn(0,"OUTAM",0);//out비용
			ds_FZME109.SetColumn(0,"PA1QY",0);//포장1수량
			ds_FZME109.SetColumn(0,"PA2QY",0);//포장2수량
			ds_FZME109.SetColumn(0,"PAAMT",0);//비닐비용
			ds_FZME109.SetColumn(0,"SOMOY",0);//소모품비율(입력내용없음)
			ds_FZME109.SetColumn(0,"JEROY",0);//재료관리비율(입력내용없음)
			ds_FZME109.SetColumn(0,"JERCL","");//재료계정(입력내용없음)
			ds_FZME109.SetColumn(0,"JERPN","");//재료품번(입력내용없음)

		}
		/*
		else{
			alert("[알림] 기존에 등록된 ");
		}
		*/
	}
}

