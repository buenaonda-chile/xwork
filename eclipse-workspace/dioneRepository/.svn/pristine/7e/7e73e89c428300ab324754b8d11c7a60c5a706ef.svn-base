function getFPMC101Q(){
	getCGMPARMHC();
//	trace(ds_CGMPARMHC.SaveXML());
	var bpnslt="";
	var chgAdsdt="";
	if(ds_CGMPARMHC.rowcount<1){
	 return;
	}
	if(ds_CGMPARMHC.GetColumn(0,"SEQMON")==1){//당월이 M 인경우
		bpnslt=1;
		chgAdsdt = addMonth(ds_CGMPARMHC.GetColumn(0,"ADSDT"),-1);
		//ds_CGMPARMHC.SetColumn(0,"ADSDT",chgAdsdt);
	}else{//당월이 M+1인경우
		bpnslt=2;
		chgAdsdt = addMonth(ds_CGMPARMHC.GetColumn(0,"ADSDT"),-2);
		//ds_CGMPARMHC.SetColumn(0,"ADSDT",chgAdsdt);
	}
//	trace(ds_CGMPARMHC.SaveXML());
//	trace(ds_SYSDATE.SaveXML());

//alert(substr(cmb_PLNYM.Value,0,6) + "===="+ substr(chgAdsdt,0,6));
	var cigmaChk="";
	//if(parseInt(ds_SYSDATE.GetColumn(0,"DT3")) >= parseInt(substr(ds_CGMPARMHC.GetColumn(0,"ADSDT"),0,6))){
	//if(substr(cmb_PLNYM.Value,0,6) <= substr(ds_CGMPARMHC.GetColumn(0,"ADSDT"),0,6)){
	if(substr(cmb_PLNYM.Value,0,6) <= substr(chgAdsdt,0,6)){//원본!
	//if(substr(chgAdsdt,0,6)<=substr(ds_CGMPARMHC.GetColumn(0,"ADSDT"),0,6)){ //수정본!
		cigmaChk='1';
	}else{
		cigmaChk='2';
	}
//	alert(cigmaChk);
	var bastm = substr(addMonth(cmb_PLNYM.Value,-1),0,6);
	var bfsym = substr(addMonth(cmb_PLNYM.Value,-1),0,4)+"01";
//	alert(bastm);
//	alert(bfsym);

	var sKind		= "getFPMC101Q";						
	var sMethodName = "service::psfaclib/selectFPMC101Q.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                            				// 자료 저장 시 보낼 DataSet 명칭
	var	sOutDataSet = "ds_FPMC101Q=ds_FPMC101Q";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
		sArgument += "@LIB1=" + quote(Lib1);
		sArgument += "@LIB2=" + quote(Lib2);
		sArgument += "@COMPS=" + quote(ComboBoxPlant.Value); //사업부
		sArgument += "@PYEAR=" + quote(substr(cmb_PLNYM.Value,0,6)); //계획년월
		sArgument += "@SELET=" + quote(cigmaChk); //계획이 cigma에 없고 interface에 있으며 1, cigma에 있으면 2
		sArgument += "@BFYMM=" + quote(bfsym); //
		sArgument += "@BLYMM=" + quote(bastm); //
		sArgument += "@SLT=" + quote(cmb_SLT.Value); //선택구분
		sArgument += "@BSLT=" + quote(bpnslt); //전월계획여부
	var str = dialog("Common::PGMLOAD_FAC.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");
	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
		setMessage("1004");
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));
		setMessage("1003");	
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}

	if(ds_FPMC101Q.rowcount>0){
		getSC003PR(bastm);
		if(cmb_SLT.Value==1){
			for(i=0; i<7; i++){
				MainGrid.SetCellProp("Head", 4+i, "text", substr(addMonth(cmb_PLNYM.Value,+i),4,2)+"월 계획");
			}	
			MainGrid.SetCellProp("Head", 11, "text", substr(bastm,4,2)+"월 실적");
		}else if(cmb_SLT.Value=2){
			for(i=0; i<7; i++){
				MainGrid2.SetCellProp("Head", 5+i, "text", substr(addMonth(cmb_PLNYM.Value,+i),4,2)+"월 계획");
			}	
			MainGrid2.SetCellProp("Head", 12, "text", substr(bastm,4,2)+"월 실적");
		}else if(cmb_SLT.Value==3){
			for(i=0; i<4; i++){
				if(i==0){
					MainGrid3.SetCellProp("Head", 3+i, "text", substr(addMonth(bastm+"01",+i),4,2)+"월 생산실적" + " ("+ds_SC003PR.GetColumn(i,"WKDAY")+")");
				}else{
					MainGrid3.SetCellProp("Head", 3+i, "text", substr(addMonth(bastm+"01",+i),4,2)+"월 생산계획" + " ("+ds_SC003PR.GetColumn(i,"WKDAY")+")");	
				}
			}	
		}
	}
}


/********************************************************************** 
 * 내용설명 : 상위메뉴 변경시 초기화
 **********************************************************************/ 
function common_OnChanged(obj,strCode,strText,nOldIndex,nNewIndex)
{
	ds_FPMC101Q.ClearData();
	if(cmb_SLT.Value==1){
		MainGrid.Visible=true;
		MainGrid2.Visible=false;
		MainGrid3.Visible=false;
		MainGrid.Enable=true;
		MainGrid2.Enable=false;
		MainGrid3.Enable=false;
		ds_FPMC101Q.GroupKey="COMPS";
	}else if(cmb_SLT.Value==2){
		MainGrid.Visible=false;
		MainGrid2.Visible=true;
		MainGrid3.Visible=false;
		MainGrid.Enable=false;
		MainGrid2.Enable=true;
		MainGrid3.Enable=false;
		ds_FPMC101Q.GroupKey="COMPS, COMPS:PRDCD";
	}else if(cmb_SLT.Value==3){
		MainGrid.Visible=false;
		MainGrid2.Visible=false;
		MainGrid3.Visible=true;
		MainGrid.Enable=false;
		MainGrid2.Enable=false;
		MainGrid3.Enable=true;
		ds_FPMC101Q.GroupKey="COMPS";
	}
}

