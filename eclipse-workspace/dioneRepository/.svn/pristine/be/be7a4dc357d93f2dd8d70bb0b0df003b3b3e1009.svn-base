/********************************************************************** 
 * 내용설명 : 화면 조회 
 **********************************************************************/ 
function getHMDZ100Q(){

	if(!validationChk()){
		return;
	}
	
   	var p_PAYCDF,p_PAYCDT,p_JOGCDF,p_JOGCDT,p_DPTCDF,p_DPTCDT,p_GRADEF,p_GRADET;
	p_PAYCDF = iif (length(trim(ed_PAYCDF.Text)) == 0, '1', trim(ed_PAYCDF.Text));
	p_PAYCDT = iif (length(trim(ed_PAYCDT.Text)) == 0 , '9', trim(ed_PAYCDT.Text));
	p_JOGCDF = iif (length(trim(ed_JOGCDF.Text)) == 0 , '00', trim(ed_JOGCDF.Text));
	p_JOGCDT = iif (length(trim(ed_JOGCDT.Text)) == 0 , '99', trim(ed_JOGCDT.Text));
	p_DPTCDF = iif (length(trim(ed_DPTCDF.Text)) == 0 , '0000', trim(ed_DPTCDF.Text));
	p_DPTCDT = iif (length(trim(ed_DPTCDT.Text)) == 0 , '9999', trim(ed_DPTCDT.Text));
	p_GRADEF = iif (length(trim(ed_GRADEF.Text)) == 0 , '00', trim(ed_GRADEF.Text));
	p_GRADET = iif (length(trim(ed_GRADET.Text)) == 0 , '99', trim(ed_GRADET.Text));
	
	/*
	if(ComboBoxPlant.Value =="C1"){
		Lib3="PSCDBLIB";
	}else if(ComboBoxPlant.Value =="H1"){
		Lib3="PSHDBLIB";
	}else if(ComboBoxPlant.Value =="S1"){
		Lib3="PSSDBLIB";
	}
	*/
	var sKind		= "getHMDZ100Q";						
	var sMethodName = "service::pshumlib/selectHMDZ100Q.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                            				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_HMDZ100Q=ds_HMDZ100Q";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
		sArgument += "@LIB1=" + quote(Lib1);
		sArgument += "@LIB2=" + quote(Lib2);
		sArgument += "@LIB3=" + quote(Lib3);
		sArgument += "@COMPS=" + quote(ComboBoxPlant.Value);
		sArgument += "@CNDCD=" + quote(chK_CNDCD.Value);	//퇴사자포함여부
		sArgument += "@GUBCD=" + quote(chK_GUBCD.Value);	//통상급적용여부
		sArgument += "@PAYCD=" + quote(chK_PAYCD.Value);	//기능직일급여부
		sArgument += "@PAYCDF=" + quote(p_PAYCDF);	//급여구분(FR)
		sArgument += "@PAYCDT=" + quote(p_PAYCDT);	//급여구분(TO)
		sArgument += "@JOGCDF=" + quote(p_JOGCDF);	//조직코드(FR)
		sArgument += "@JOGCDT=" + quote(p_JOGCDT);	//조직코드(TO)
		sArgument += "@DPTCDF=" + quote(p_DPTCDF);	//부서코드(FR)
		sArgument += "@DPTCDT=" + quote(p_DPTCDT);	//부서코드(TO)
		sArgument += "@GRADEF=" + quote(p_GRADEF);	//직급코드(FR)
		sArgument += "@GRADET=" + quote(p_GRADET);	//직급코드(TO)
		sArgument += "@GNDATF=" + quote(cmb_GNDATF.Value);	//입사일자(FO)
		sArgument += "@GNDATT=" + quote(cmb_GNDATT.Value);	//입사일자(TO)
	var str = dialog("Common::PGMLOAD_HUM.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");	
	http.Sync = false;
	
	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
		setMessage("1004");	
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));
		setMessage("1003");	
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}
}
/********************************************************************** 
 * 내용설명 : Transaction 리턴후 실행 함수
 * 주요변수 : 
 * 			- sMethodName	Transaction 고유 ID
 * 			- ErrorCode 	에러코드
 * 			- ErrorMSG 		에러 메세지
 **********************************************************************/ 
function callback(svcId, rtnCd, rtnMsg){
	if(svcId=="getHMDZ100Q"){
		if(rtnCd<0){
			alert(setPopMessage("1002"));
			setMessage("1002");
			
		}else{
			if(ds_HMDZ100Q.rowcount<1){
				alert(setPopMessage("1003"));
				setMessage("1003");	
			}else{
				alert(setPopMessage("1004"));
				setMessage("1004");	
			}
		}
	}
}
/********************************************************************** 
 * 내용설명 : validation 체크
 **********************************************************************/ 
function validationChk(){

	var result = true;
	cmb_GNDATF.BKColor="white";
	cmb_GNDATT.BKColor="white";
		
	if (cmb_GNDATF.Value == ""){
		cmb_GNDATF.BKColor="paleturquoise";
		alert(setPopMessage("4053"));//[확인] 기준년월을 확인바랍니다.
		setMessage("4053");
		result = false;
	}else if (cmb_GNDATT.Value == ""){
		cmb_GNDATT.BKColor="paleturquoise";
		alert(setPopMessage("4053"));//[확인] 기준년월을 확인바랍니다.
		setMessage("4053");
		result = false;
	}else if (cmb_GNDATF.Value > cmb_GNDATT.Value){
		cmb_GNDATF.BKColor="paleturquoise";
		alert(setPopMessage("4053"));//[확인] 기준년월을 확인바랍니다.
		setMessage("4053");
		result = false;
	}
	return result;
}
