

/* grid 관련*/
.wj-flexgrid {
	max-height: 620px;
	height: auto;
	width: 100%;
}

.wj-header {
 	background: white;
    
}


/* tab 관련*/
.wj-tabpane {
    padding: 12px;
}


.wj-tabpanel.custom-headers .wj-tabheaders .wj-tabheader {
  	
  	border: 1px solid #dddddd;
}

.wj-tabheader{
	margin: 1px .2em 0 0 !important;
	
}

/*차트 관련*/

/* custom size/weight for header */
.wj-flexchart .wj-header .wj-title {
    font-size: 120%;
    font-weight: bold;
}

.wj-flexchart .wj-footer .wj-title {
    font-size: 90%;
    font-weight: bold;
}

