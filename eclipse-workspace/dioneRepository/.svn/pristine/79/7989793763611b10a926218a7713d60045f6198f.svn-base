 }

/*
1.FRME103S 이용하여 Default 날짜 가져옴.
2.
*/
function getComCode(){
	var sKind		= "getComCode";						
	var sMethodName = "service::login/selectComCode.do"; 
	var sInDataSet  = "";
	var sOutDataSet = "ds_ComCode_PLNT=ds_ComCode_PLNT";
	var sArgument   = "";
		sArgument   += " CLASS='PLNT'";
		sArgument   += " CODE1=" + quote(company);		
	http.Sync=true;
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	http.Sync=false;
	ComboBoxPlant.Value=comps;
	//ComboBoxPlant.Enable = false; 
}
function getDesign(){
	//MainGrid.ToolTipText = setPopMessage("2053");
	MainGrid.BkColor = G_ds_design.GetColumn(0,"BkColor");
	MainGrid.BkColor2 = G_ds_design.GetColumn(0,"BkColor2");
	MainGrid.BkSelColor = G_ds_design.GetColumn(0,"BkSelColor");
	MainGrid.Face3dColor = G_ds_design.GetColumn(0,"Face3dColor");
	MainGrid.BoldHead = G_ds_design.GetColumn(0,"BoldHead");
	MainGrid.HeadHeight = G_ds_design.GetColumn(0,"HeadHeight");
	MainGrid.Width = G_ds_design.GetColumn(0,"Width");
	MainGrid.UserData = MainGrid.Height;
	
	searchBar.Width = G_ds_design.GetColumn(0,"Width");
	searchBar.Top = G_ds_design.GetColumn(0,"SBTop");	

	//Static0.BkColor = G_ds_design.GetColumn(0,"BTBkColor2");
	//Static3.BkColor = G_ds_design.GetColumn(0,"BTBkColor2");
}
function setHeadTitle(){

	var sKind		= "setHeadTitle";						
	var sMethodName = "service::psfaclib/selectFRME103Q_PLNYM.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";
	var sOutDataSet = "ds_PLNYM=ds_PLNYM";
	var sArgument   = "";
		sArgument += " LIB2=" + quote(Lib2);
	http.Sync=true;
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	http.Sync=false;
	cmb_PLNYM.Value= ds_PLNYM.GetColumn(0,"PLNYM")+"01";
	MainGrid.SetCellProp("Head", 9, "text", "M("+substr(cmb_PLNYM.Value,0,4)+"/"+substr(cmb_PLNYM.Value,4,2)+")");
	MainGrid.SetCellProp("Head", 10, "text", "M("+substr(addMonth(cmb_PLNYM.Value,1),0,4)+"/"+substr(addMonth(cmb_PLNYM.Value,1),4,2)+")");
	MainGrid.SetCellProp("Head", 11, "text", "M("+substr(addMonth(cmb_PLNYM.Value,2),0,4)+"/"+substr(addMonth(cmb_PLNYM.Value,2),4,2)+")");
	MainGrid.SetCellProp("Head", 12, "text", "M("+substr(addMonth(cmb_PLNYM.Value,3),0,4)+"/"+substr(addMonth(cmb_PLNYM.Value,3),4,2)+")");
	MainGrid.SetCellProp("Head", 13, "text", "M("+substr(addMonth(cmb_PLNYM.Value,4),0,4)+"/"+substr(addMonth(cmb_PLNYM.Value,4),4,2)+")");

}
function getFRME103Q(){

	var procs2 = "";
	var proln2 = "";
	progr2 = ed_PROGR2.Text;
	if(progr2==""){
		progr2="999";
	}	
	proln2 = ed_PROLN2.Text;
	if(proln2==""){
		proln2="99";
	}	
	var sKind		= "getFRME103Q";						
	var sMethodName = "service::psfaclib/selectFRME103Q.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                            				// 자료 저장 시 보낼 DataSet 명칭
	var	sOutDataSet = "ds_FRME103Q=ds_FRME103Q";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
		sArgument += "@LIB1=" + quote(Lib1);
		sArgument += "@LIB2=" + quote(Lib2);
		sArgument += "@COMPS=" + quote(ComboBoxPlant.Value); //사업부
		sArgument += "@PLNYM=" + quote(substr(cmb_PLNYM.Value,0,4)); //계획년월
		sArgument += "@PROGR1=" + quote(ed_PROGR1.Text); //작업그룹FR
		sArgument += "@PROGR2=" + quote(progr2); //작업그룹TO
		sArgument += "@PROLN1=" + quote(ed_PROLN1.Text); //작업라인FR
		sArgument += "@PROLN2=" + quote(proln2); //작업라인TO
		sArgument += "@SLT=" + quote(cmb_SLT.Value); //선택
	var str = dialog("Common::PGMLOAD_FAC.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");
	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
		setMessage("1004");
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));
		setMessage("1003");	
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}
}

