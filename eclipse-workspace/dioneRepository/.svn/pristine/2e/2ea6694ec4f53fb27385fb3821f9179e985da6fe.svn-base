//글자길이계산 메소드
function lengCal(input)
{
	var availMsg = 0;//이전DATA TYPE
	var chkbyte = 0; //byte(길이계산
	var calj = 0; //실제length 계산이 한글과기타문자사이에는 길이 1+함,
	var msgVal = input;
	var rval = array(2);
	for(var i = 0 ; i < msgVal.length; i++){
		var ch =Asc(msgVal.CharAt(i));
		if(i==msgVal.length-1){//ex)'가'입력시 4byte
			if(i==0){//한글자입력시
				if( length(urlEncode(ch)) > 4){
					chkbyte +=4;//한글4
					calj+=2;
				}else{
					chkbyte++;//기타1
				}	
			}else{//이전에 값이 있는경우
				if(length(availMsg)>4){//이전데이터가 한글
					if( length(urlEncode(ch)) > 4){//한글2
						chkbyte +=3;
						calj++;
					}else{//기타1+1
						chkbyte+=2;
					}				
				}else{
					if( length(urlEncode(ch)) > 4){//한글2
						chkbyte +=3;
						calj+=2;
					}else{//기타1
						chkbyte+=1;
					}					
				}
			}					
		}else{
			if(i==0){//처음에 값이 없는 경우
				if( length(urlEncode(ch)) > 4){
					chkbyte +=3;
					calj++;
				}else{
					chkbyte++;
				}	
			}else{//값이 있는경우
				if(length(availMsg)>4){//이전데이터가 한글
					if( length(urlEncode(ch)) > 4){//한글2
						chkbyte+=2;				
					}else{//기타1
						chkbyte+=2;				
						calj++;
					}	
				}else{
					if( length(urlEncode(ch)) > 4){//한글2
						chkbyte +=3;
						calj++;
					}else{//기타1
						chkbyte+=1;
					}					
				}
			}			
		}
		availMsg=urlEncode(ch);

		if(chkbyte>=18){
			rval[0]=i;
			//rval[1]=i+1+calj;
			rval[1]=chkbyte;
			return rval;
		}
	}
	return rval;	
}
