function ds_FHDZ040_CanColumnChange(obj,nRow,strColumnID,varOldValue,varNewValue,nPivotIndex)
{
	if (strColumnID == "DHCODE"){//부서코드확인
		if(varNewValue!=""){
			DPCONM="";
			http.Sync = true;
			var sKind		= "getDPARTMST";		
			var sMethodName = "service::pscomlib/selectDPARTMasterPopup.do"; 
			var sInDataSet  = "";
			var sOutDataSet = "ds_DPART=ds_Select";
			var sArgument   = "";
				sArgument  += " LIB2=" + quote(Lib2);
				sArgument  += " COMPS=" + quote(ComboBoxPlant.Value);
				sArgument  += " DPCODE=" + quote(varNewValue);
			Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
			http.Sync = false;
			if (ds_DPART.rowcount < 1){
				//alert("[알림]부서코드확인바랍니다.");
				alert(setPopMessage("4708")); //[알림]부서코드확인바랍니다.
				setMessage("4708");	
				return false;
			}else{
				http.Sync = true;
				var sKind		= "getFHDZ040_CHK";		
				var sMethodName = "service::psfaclib/selectFHDZ040.do";     		// 서버에서 호출 될 Method 명
				var sInDataSet  = "";
				var sOutDataSet = "ds_FHDZ040_CHECK=ds_FHDZ040";
				var sArgument   = "";
					sArgument  += " LIB2=" + quote(Lib2);
					sArgument  += " COMPS=" + quote(ComboBoxPlant.Value);
					sArgument  += " DHCODE=" + quote(varNewValue);
				Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
				http.Sync = false;
				if (ds_FHDZ040_CHECK.rowcount > 0){
					//alert("[경고] 이미 등록된 자료입니다. 조회 후 수정하시기 바랍니다.");
					alert(setPopMessage("4022")); //[경고] 이미 등록된 자료입니다. 조회 후 수정하시기 바랍니다.
					setMessage("4022");	
					return false;
				}
			}
			DPCONM=ds_DPART.GetColumn(0,"A2");
		}
	}
	if (strColumnID == "PASWD"){//암호확인
		if(varNewValue!=""){
			if(varNewValue=="DNPS"){ //DNPS로 입력불가
				//alert("[알림]입력한 암호는 사용불가능합니다."); //[알림]입력한 암호는 사용불가능합니다.
				alert(setPopMessage("6106")); //[알림]입력한 암호는 사용불가능합니다.
				setMessage("6106");	
				return false;
			}
		}
	}
	if (strColumnID == "PASMK"){//참조사항 글자수계산
		if(varNewValue!=""){//FHD000PF AND MST650PF 한번에 처리함.(확인됨)
			var chkLeng = array(2);
				chkLeng=lengCal(varNewValue);
			if(chkLeng[1] == 18){
				//alert("[알림]참조사항 입력길이를 초과하였습니다."); //[알림]참조사항 입력길이를 초과하였습니다.
				alert(setPopMessage("6097")); //[알림]참조사항 입력길이를 초과하였습니다.
				setMessage("6097");	
				return false;
			}
		}
	}	
}

