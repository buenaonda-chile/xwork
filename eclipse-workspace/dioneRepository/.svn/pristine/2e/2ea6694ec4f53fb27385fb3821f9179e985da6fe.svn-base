	MainGrid.HeadHeight = G_ds_design.GetColumn(0,"HeadHeight");
	//MainGrid.Width = G_ds_design.GetColumn(0,"Width");
	MainGrid.UserData = MainGrid.Height;
	
	searchBar.Width = G_ds_design.GetColumn(0,"Width");
	searchBar.Top = G_ds_design.GetColumn(0,"SBTop");	

}

function getFHDZ040(){

	if(!validationChk()){
		return;
	}

	var sKind		= "getFHDZ040";						
	var sMethodName = "service::psfaclib/selectFHDZ040.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_FHDZ040=ds_FHDZ040";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
		sArgument += "@LIB1=" + quote(Lib1);
		sArgument += "@LIB2=" + quote(Lib2);
		sArgument += "@COMPS=" + quote(ComboBoxPlant.Value); //사업부
		sArgument += "@DHCODE=" + quote(ed_CODE.Text); //부서코드
	var str = dialog("Common::PGMLOAD_FAC.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");
	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
		setMessage("1004");
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));
		setMessage("1003");	
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}
}

/********************************************************************** 
 * 내용설명 : insert, update, delete
 **********************************************************************/  
function setFHDZ040(){
/*
	if(ds_FHDZ040.GetRowType(ds_FHDZ040.currow)=="update" ||ds_FHDZ040.GetRowType(ds_FHDZ040.currow)=="insert" ){
		if(!ds_FHDZ040_CanRowPosChange()){
			return;
		}
	}
*/
	var sKind		= "setFHDZ040";						
	var sMethodName = "service::psfaclib/saveFHDZ040.do";    // 서버에서 호출 될 Method 명
	var sInDataSet  = "ds_FHDZ040=ds_FHDZ040:U";               //자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_FHDZ040_CHECK=ds_FHDZ040_CHECK";  	   // 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
		sArgument += " LIB1=" + quote(Lib1);
		sArgument += " LIB2=" + quote(Lib2);
	http.Sync=true;
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");
	http.Sync=false;

}
/********************************************************************** 
 * 내용설명 : Transaction 리턴후 실행 함수
 * 주요변수 : 
 * 			- sMethodName	Transaction 고유 ID
 * 			- ErrorCode 	에러코드
 * 			- ErrorMSG 		에러 메세지
 **********************************************************************/ 
function callback(svcId, rtnCd, rtnMsg){
	if (ds_FHDZ040_CHECK.GetRowCount() > 0){
		for(var i=0;i<ds_FHDZ040_CHECK.GetRowCount();i++){
			alert("부서코드 : " +ds_FHDZ040_CHECK.GetColumn(i,"DHCODE") + " = " + ds_FHDZ040_CHECK.GetColumn(i,"RTMSG"));
		}
		getFHDZ040();  //error항목이 존재시에는 재조회
		return;
	}// tran 실패
	if(rtnCd<0){
		alert(setPopMessage("1002"));
		setMessage("1002");
	}else{
		alert(setPopMessage("1001"));
		setMessage("1001");	
	}
}

/********************************************************************** 
 * 내용설명 : validation 체크
 **********************************************************************/ 
 
function validationChk(){
	var result = true;
		ed_CODE.BKColor="white";
		
	if (length(trim(ed_CODE.Text))!=0){
		http.Sync = true;
		var sKind		= "getDPARTMST";		
		var sMethodName = "service::pscomlib/selectDPARTMasterPopup.do"; 
		var sInDataSet  = "";
		var sOutDataSet = "ds_DPART=ds_Select";
		var sArgument   = "";
			sArgument  += " LIB2=" + quote(Lib2);
			sArgument  += " COMPS=" + quote(ComboBoxPlant.Value);
			sArgument  += " DPCODE=" + quote(ed_CODE.Text);
		Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
		http.Sync = false;
		if (ds_DPART.rowcount < 1){
			//alert("[알림]부서코드확인바랍니다.");
			alert(setPopMessage("4708")); //[알림]부서코드확인바랍니다.
			setMessage("4708");
			result = false;
		}
		st_CODE.Text=ds_DPART.GetColumn(0,"A2");
	}
	return result;
}

