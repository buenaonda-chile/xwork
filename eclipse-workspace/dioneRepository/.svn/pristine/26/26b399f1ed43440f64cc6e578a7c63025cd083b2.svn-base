function fn_Input()
{
	if(!validationChk()){
		return;
	}
	if(checkInsertCount("ds_HMDZ150")== false) {
		return;
	}
	if(ds_HMDZ150.rowcount < 1) {
		fn_Search();
	}
	ds_HMDZ150.AddRow();
	ds_HMDZ150.SetColumn(ds_HMDZ150.currow,"SABUN",ed_SABUN.Text);
	ds_HMDZ150.SetColumn(ds_HMDZ150.currow,"KORNM",st_KORNM.Text);
	ds_HMDZ150.SetColumn(ds_HMDZ150.currow,"SCHCD",00);	//학력코드
	ds_HMDZ150.SetColumn(ds_HMDZ150.currow,"ADRCD",00);	//소재지코드
	ds_HMDZ150.SetColumn(ds_HMDZ150.currow,"MAJCD",00);	//전공코드
	ds_HMDZ150.SetColumn(ds_HMDZ150.currow,"INDAT",0);	//입학일자(관리안함)
	ds_HMDZ150.SetColumn(ds_HMDZ150.currow,"OUDAT",0);	//졸업일자	
	ds_HMDZ150.SetColumn(ds_HMDZ150.currow,"GRDCD",00);	//졸업코드
	
	MainGrid.SetCellPos(1);
	MainGrid.SetFocus();
}

function fn_Delete()
{
	if(ds_HMDZ150.rowcount < 1) return;			//삭제가 없으면 건너뛰기		
	ds_HMDZ150.DeleteRow(ds_HMDZ150.currow);	//삭제		
	alert(setPopMessage("1011"));	
	setPopMessage("1011");
}

