function imgCODE_OnClick(obj,nX,nY)
{
	var sTemp="zzzzzzzzzz";
	var selectnumber = "H6";
	var PAYCD = 3; //1이면 기능직 2이면 직반장 3이면 전체
	var rtn = dialog("Common::commonPopup.xml","SEL=" + selectnumber + " " + "CODE=" +  sTemp + " " + "PAYCD=" + PAYCD,584,528);
	if (length(rtn) != 0){
		ed_SABUN.Text=rtn[0];
		st_KORNM.Text=rtn[1];
	}	
}

function ed_SABUN_OnKillFocus(obj)
{
	if(length(trim(obj.Text)) < 1) return;
	http.usewaitcursor = false;
	http.Sync 	= true;
	var sKind		= "getHMD020PF";		
	var sMethodName = "service::pscomlib/selectINSAMasterPopup.do"; 
	var sInDataSet  = "";
	var sOutDataSet = "ds_HMD020PF=ds_Select";
	var sArgument   = "";
		sArgument  += " LIB2=" + quote(Lib2);
		sArgument  += " LIB3=" + quote(Lib3);
		sArgument  += " COMPS=" + quote(ComboBoxPlant.Value); //사업부
		sArgument  += " SABUN=" + quote(ed_SABUN.Text);
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	http.Sync = false;
	if (ds_HMD020PF.GetRowCount() < 1){
		alert(setPopMessage("4062")); //[확인] 사용자가 존재하지 않습니다.
		setMessage("4062");
		st_KORNM.Text = "";
		ed_SABUN.SetFocus();
		return;
	}
	st_KORNM.Text = ds_HMD020PF.GetColumn(0,"A2");	//이름
	http.usewaitcursor = true;	
}

function ed_SABUN_OnCharChanged(obj,strPreText,strPostText)
{
	st_KORNM.Text ="";
}
function ComboBoxPlant_OnChanged(obj,strCode,strText,nOldIndex,nNewIndex)
{
	ds_HMDZ150.ClearData();

	if(ComboBoxPlant.Value =="C1"){
		Lib3="PSCDBLIB";
	}else if(ComboBoxPlant.Value =="H1"){
		Lib3="PSHDBLIB";
	}else if(ComboBoxPlant.Value =="S1"){
		Lib3="PSSDBLIB";
	}
	getSSCD();	//학력코드
	getBPCD();	//현적지코드
	getMJCD();	//전공코드
	getGDCD();	//졸업코드
}

function ds_HMDZ150_OnColumnChanged(obj,nRow,strColumnID,varOldValue,varNewValue,nPivotIndex)
{
	if (strColumnID == "OUDAT"){//졸업일자
		if(length(trim(varNewValue))==0){
			ds_HMDZ150.SetColumn(nRow,"OUDAT",0);
		}
	}
}

function ds_HMDZ150_CanRowPosChange(obj,nOldRow,nRow)
{
	http.usewaitcursor = false;

	var p_SCHCD=ds_HMDZ150.GetColumn(ds_HMDZ150.currow,"SCHCD"); //학력코드
	if(ds_HMDZ150.GetRowType(ds_HMDZ150.currow)=="update" ||ds_HMDZ150.GetRowType(ds_HMDZ150.currow)=="insert" ){
		if(trim(p_SCHCD)=="0"){	
			alert("[알림] 학력코드를 입력하세요.");
			//alert(setPopMessage("6122")); //[경고] 교육완료일을 입력하세요.
			//setMessage("6122");	
			return false;
		}
	}
	return true;
	http.usewaitcursor = true;
}
