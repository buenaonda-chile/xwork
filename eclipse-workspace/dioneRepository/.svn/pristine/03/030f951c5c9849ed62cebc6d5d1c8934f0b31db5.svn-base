/********************************************************************** 
 * 내용설명 : 화면 조회 
 **********************************************************************/ 
function getMSTC302Q(){
	if(length(trim(ed_FPSPNO.Text)) <1){
		alert(setPopMessage("1501"));  //[확인] 품번 확인바랍니다.
		setMessage("1501");	
		ed_FPSPNO.SetFocus();
		return;
	}
	var FPSPNO = iif(length(trim(ed_FPSPNO.Text)) <1," ",trim(ed_FPSPNO.Text));
	var TPSPNO = iif(length(trim(ed_TPSPNO.Text)) <1,"999999999999999",trim(ed_TPSPNO.Text));
	var FALTPC = iif(length(trim(ed_FALTPC.Text)) <1," ",trim(ed_FALTPC.Text));
	var TALTPC = iif(length(trim(ed_TALTPC.Text)) <1,"99999",trim(ed_TALTPC.Text));
	var FITTYP = iif(length(trim(ed_FITTYP.Text)) <1," ",trim(ed_FITTYP.Text));
	var TITTYP = iif(length(trim(ed_TITTYP.Text)) <1,"9",trim(ed_TITTYP.Text));
	var FITCLS = iif(length(trim(ed_FITCLS.Text)) <1," ",trim(ed_FITCLS.Text));
	var TITCLS = iif(length(trim(ed_TITCLS.Text)) <1,"9999",trim(ed_TITCLS.Text));
	
	http.Sync = true;
	var sKind		= "getMSTC302Q";
	var sMethodName = "service::psmstlib/selectMSTC302Q.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_MSTC302Q=ds_MSTC302Q";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
	sArgument += "@LIB2=" + quote(Lib2);
	sArgument += "@GUB=" + quote(cb_grpcd.Value);
	sArgument += "@FPSPNO=" + quote(FPSPNO);
	sArgument += "@TPSPNO=" + quote(TPSPNO);
	sArgument += "@FALTPC=" + quote(FALTPC);
	sArgument += "@TALTPC=" + quote(TALTPC);
	sArgument += "@FITTYP=" + quote(FITTYP);
	sArgument += "@TITTYP=" + quote(TITTYP);
	sArgument += "@FITCLS=" + quote(FITCLS);
	sArgument += "@TITCLS=" + quote(TITCLS);
	var str = dialog("Common::PGMLOAD_MST.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");	
	//Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");
	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
		setMessage("1004");	
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));
		setMessage("1003");	
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}
}

