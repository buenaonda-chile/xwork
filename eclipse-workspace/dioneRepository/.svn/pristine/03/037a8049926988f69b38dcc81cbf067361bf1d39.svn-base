			var sArgument   = "";
			sArgument += " LIB1=" + quote(Lib1);
			sArgument += " LIB2=" + quote(Lib2);
			sArgument += " JOBFL=" + quote(arrjobpfnm[totcnt]);
			sArgument += " STEP=" + quote(step);
			sArgument += " F_ROWNUM=" + quote(start_rownumber);
			sArgument += " T_ROWNUM=" + quote(end_rownumber);	
			http.Sync = true;
			Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");
			http.Sync = false;
			if(object("ds_DWHXXXYQ_"+step).rowcount != 10000){
				step=21;
			}
		}
		//Dataset Append (ds_DWHXXXYQ)
		for(step=1 ;step<21;step++){
			if (object("ds_DWHXXXYQ_"+step).rowcount > 0) object("ds_DWHXXXYQ_tot"+totcnt).AppendDataset("ds_DWHXXXYQ_"+step);
			object("ds_DWHXXXYQ_"+step).ClearData();
		}
	}






























