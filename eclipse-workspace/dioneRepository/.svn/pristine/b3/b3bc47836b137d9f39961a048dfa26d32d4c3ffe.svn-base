/********************************************************************** 
 * 내용설명 : 화면조회
 * 주요변수 : 
 **********************************************************************/ 
function getSPME111Q(){
	//조회구분 선택시 grid제목변경 및 특정컬럼숨김
	// 1: 규격발상세 2:규격별집계  3: 차종별상세 4:차종별집계 5:업체별상세 6:업체별집계
	switch (rdoggub.Value){
		case '1' : 
			MainGrid.SetCellProp("Head", 2, "text", "규격");
			MainGrid.SetCellProp("Head", 3, "text", "규격명");
			break;
		case '2' :
			MainGrid.SetCellProp("Head", 2, "text", "규격");
			MainGrid.SetCellProp("Head", 3, "text", "규격명");
			break;
		case '3' :
			MainGrid.SetCellProp("Head", 2, "text", "차종");
			MainGrid.SetCellProp("Head", 3, "text", "차종명");
			break;
		case '4' :
			MainGrid.SetCellProp("Head", 2, "text", "차종");
			MainGrid.SetCellProp("Head", 3, "text", "차종명");
			break;
	}
	var sum_qty_gb;
	if (rdoggub.Value = '2' || rdoggub.Value == '4' || rdoggub.Value == '6'){
		MainGrid.SetColProp(4,"Width",62);  //구분표시
		MainGrid.SetColProp(5,"Width",0);  //품번숨김
		MainGrid.SetColProp(6,"Width",0);  //품명숨김
		MainGrid.SetColProp(7,"Width",0);  //제품코드 숨김
		sum_qty_gb = 'S';  //집계
	}else{
		MainGrid.SetColProp(4,"Width",0);  //구분숨김
		MainGrid.SetColProp(5,"Width",115);
		MainGrid.SetColProp(6,"Width",220);
		MainGrid.SetColProp(7,"Width",50);  //제품코드 숨김

		sum_qty_gb = 'D';  //상세
	}
	if (rdoggub.Value = '5' || rdoggub.Value == '6'){
		MainGrid.SetColProp(2,"Width",0);  //코드숨김
		MainGrid.SetColProp(3,"Width",0);  //코드명숨김
	}else{
		MainGrid.SetColProp(2,"Width",60);
		MainGrid.SetColProp(3,"Width",120);
	}

	var p_pyvnd1,p_pyvnd2,p_plngb;
	p_pyvnd1 = iif (length(trim(ed_pyvnd_from.Text)) == 0, '0000', trim(ed_pyvnd_from.Text));
	p_pyvnd2 = iif (length(trim(ed_pyvnd_to.Text)) == 0 , '9999', trim(ed_pyvnd_to.Text));
	switch (cb_plngb.Value){
		case 'A' : p_plngb=""; break;
		case 'B' : p_plngb="('2','4')"; break;
		case 'C' : p_plngb="('6','8')"; break;
		case 'D' : p_plngb="(' ')"; break;
	}
	var str;
	if (rdoggub.Value = '1' || rdoggub.Value == '3' || rdoggub.Value == '5'){  //상세
		var sKind		= "getSPME111Q_1";						
		var sMethodName = "service::pssallib/selectSPME111Q_1.do";     		// 서버에서 호출 될 Method 명
		var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
		var sOutDataSet = "ds_SPME111Q=ds_SPME111Q";    				// 조회 후 돌려 받을 DataSet 명칭
		var sArgument   = "";
		sArgument += "@LIB1=" + quote(Lib1);
		sArgument += "@LIB2=" + quote(Lib2);
		sArgument += "@COMPS=" + quote(cb_searchCodeName1.Value);
		sArgument += "@PLNGB=" + quote(p_plngb);
		sArgument += "@PLNYM=" + quote(substr(cal_PLNYM.Value,0,6));
		sArgument += "@PYVND1=" + quote(p_pyvnd1);
		sArgument += "@PYVND2=" + quote(p_pyvnd2);	
		sArgument += "@G_GUB="  + quote(rdoggub.Value);	// 규격별/차종별/거래처별 구분
		str = dialog("Common::PGMLOAD_SAL.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");	
	}else{   //집계
		var sKind		= "getSPME111Q_2";						
		var sMethodName = "service::pssallib/selectSPME111Q_2.do";     		// 서버에서 호출 될 Method 명
		var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
		var sOutDataSet = "ds_SPME111Q=ds_SPME111Q";    				// 조회 후 돌려 받을 DataSet 명칭
		var sArgument   = "";
		sArgument += "@LIB1=" + quote(Lib1);
		sArgument += "@LIB2=" + quote(Lib2);
		sArgument += "@COMPS=" + quote(cb_searchCodeName1.Value);
		sArgument += "@PLNGB=" + quote(p_plngb);
		sArgument += "@PLNYM=" + quote(substr(cal_PLNYM.Value,0,6));
		sArgument += "@PYVND1=" + quote(p_pyvnd1);
		sArgument += "@PYVND2=" + quote(p_pyvnd2);	
		sArgument += "@G_GUB="  + quote(rdoggub.Value);	// 규격별/차종별/거래처별 구분
		//sArgument += "@Q_GUB="  + quote(sum_qty_gb);    // 상세/집계 구분
		str = dialog("Common::PGMLOAD_SAL.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");	
	}
	//Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");
	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
		setMessage("1004");	
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));
		setMessage("1003");	
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}
	ed_pyvnd_from.SetFocus();
}


