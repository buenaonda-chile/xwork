function MST050CHK_OnKillFocus(obj)
{

//제품코드, 차종코드, 규격, 차륜, 수량계산, 관리코드, 지역코드  사용
//색상제외
	if(length(trim(obj.Text)) < 1) return;
	//품번Check
	var grpVal = "";
	var grpChk = substr(obj.ID,3,5);
	if( grpChk == "PRDCD"){
		grpVal="PRD";
	}
	else if( grpChk == "CARCD"){
		grpVal="CAR";
	}
	else if( grpChk == "BDYCD"){
		grpVal="BDY";
	}
	else if( grpChk == "ROLCD"){
		grpVal="ROL";
	}
	else if( grpChk == "MGMCD"){
		grpVal="MGM";
	}
	else if( grpChk == "COLOR"){
		grpVal="COL";
	}
	else if( grpChk == "REGCD"){
		grpVal="REG";
	}				
	http.usewaitcursor = false;
	http.Sync 	= true;
	var sKind		= "getMST050";		
	var sMethodName = "service::pscomlib/selectMST050PF.do"; 
	var sInDataSet  = "";
	var sOutDataSet = "ds_MST050PF=ds_MST050PF";
