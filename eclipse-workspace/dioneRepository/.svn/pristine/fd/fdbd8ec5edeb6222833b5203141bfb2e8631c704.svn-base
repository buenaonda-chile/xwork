function getMSTZ100(){
	
	if(!validationChk()){
		return;
	}		
	
	var sKind		= "getMSTZ100";						
	var sMethodName = "service::psmstlib/selectMSTZ100.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_MSTZ100=ds_MSTZ100";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
	sArgument += "@LIB1=" + quote(Lib1);
	sArgument += "@LIB2=" + quote(Lib2);
	sArgument += "@COMPS=" + quote(ComboBoxPlant.Value); //사업부
	sArgument += "@PSPNO=" + quote(ed_PSPNO.Text); //업체
	var str = dialog("Common::PGMLOAD_MST.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");
	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
		setMessage("1004");	
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));  //[알림] 신규입력입니다.
		setMessage("1003");	
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}
}

function setMSTZ100(){
	
	var sKind		= "setMSTZ100";						
	var sMethodName = "service::psmstlib/saveMSTZ100.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "ds_MSTZ100=ds_MSTZ100:U";               //자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_MSTZ100_CHECK=ds_MSTZ100_CHECK";  	   // 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
		sArgument += " LIB2=" + quote(Lib2);
	http.Sync=true;
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");
	http.Sync=false;

}

/********************************************************************** 
 * 내용설명 : Transaction 리턴후 실행 함수
 * 주요변수 : 
 * 			- sMethodName	Transaction 고유 ID
 * 			- ErrorCode 	에러코드
 * 			- ErrorMSG 		에러 메세지
 **********************************************************************/ 
function callback(svcId, rtnCd, rtnMsg){
	if (ds_MSTZ100_CHECK.GetRowCount() > 0){
		for(var i=0;i<ds_MSTZ100_CHECK.GetRowCount();i++){
			alert("품번 : " +ds_MSTZ100_CHECK.GetColumn(i,"PRTNO") + " = " + ds_MSTZ100_CHECK.GetColumn(i,"RTMSG"));
		}
		getMSTZ100();  //error항목이 존재시에는 재조회
		return;
	}// tran 실패
	if(rtnCd<0){
		alert(setPopMessage("1002"));
		setMessage("1002");
		
	}else{
		alert(setPopMessage("1001"));
		setMessage("1001");	
	}
}
