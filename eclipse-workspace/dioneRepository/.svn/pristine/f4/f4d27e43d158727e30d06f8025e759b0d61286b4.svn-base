function getINOUT_TIME(){
	http.Sync = true;
	var sKind		= "getInOutTime";						
	var sMethodName = "service::pshumlib/selectInOutTime.do";     	// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "DS_MAIN=DS_MAIN";    						// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
	
	
	if(length(trim(Calendar0.Value)) == 0){
		alert("조회월을 선택하세요!");	
		Calendar0.SetFocus();
		return;
	}
		
	sArgument += " MONTH=" + quote(LEFT(Calendar0.Value,6) + '%');
		
	dialog("Common::PGMLOAD.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument, 272, 64, "TitleBar=false");				
}


