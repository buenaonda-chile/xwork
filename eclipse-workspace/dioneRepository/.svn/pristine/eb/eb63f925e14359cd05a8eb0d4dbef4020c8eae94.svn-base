function getOgm319_onload(wndats, wndate, prtcd, lincd, mshno, jobcd, sdpcd, wrkm1, selcd, CheckedCount, rvalue){
//alert(selvalue1+"^");
	var sKind		= "getOgm319_onload";						
	var sMethodName = "service::psogmlib/selectOgm319_onload.do"; 
	var sInDataSet  = "";
	var sOutDataSet = "ds_obj200pf_1=ds_obj200pf_1";
	var sArgument   = "";
		sArgument += "@LIB2=" + quote(Lib2);
		sArgument += "@COMPS=" + quote(comps1);
		sArgument += "@WNDATS=" + quote(wndats.Value);
		sArgument += "@WNDATE=" + quote(wndate.Value);
		sArgument += "@PRTCD=" + quote(prtcd);
		sArgument += "@LINCD=" + quote(lincd);
		sArgument += "@MSHNO=" + quote(mshno);
		sArgument += "@JOBCD=" + quote(jobcd);
		sArgument += "@SDPCD=" + quote(sdpcd);
		sArgument += "@WRKM1=" + quote(wrkm1);
		sArgument += "@SELCD=" + quote(selcd);
		sArgument += "@SELVALUE=" + quote(trim(lblselvalue_319.Text));
		sArgument += "@SELCD2=" + quote(trim(lblselcd2_319.Text));
		sArgument += "@SELVALUE2=" + quote(trim(lblselcd2_319.Text));
		sArgument += "@KOGUBCHK=" + quote(parseInt(CheckedCount));
		sArgument += "@KOGUB=" + quote(rvalue);
/*
	http.Sync = true;
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	http.Sync = false;

*/
	http.Sync = true;
	var str = dialog("Common::PGMLOAD.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");
	http.Sync = false;
	if (str == 'T'){  //정상조회
		//alert(setPopMessage("1004"));
		setMessage("1004");	
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));
		setMessage("1003");	
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}		
}

/********************************************************************** 
 * 내용설명 : Transaction 리턴후 실행 함수
