function getWRKDTMM(){
	var st_TEXT1="";
	st_TEXT1 = "F          " + substr(cmb_WRKDT.Value,4,2) + " 월도 목표지수 (%)";
	return st_TEXT1;
}
function getMst040()
{
	var mm = parseInt(substr(cmb_WRKDT.Value,4,2));
	var st_TEXT1="";
	var st_TEXT2="";
	var jobyl=0;
	var gisuyl=0;
		
	var sKind		= "getMST040PF";						
	var sMethodName = "service::pscomlib/selectMST040.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_MST040PF=ds_MST040PF";   					// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
	sArgument += " LIB2=" + quote(Lib2);
	sArgument += " COMPS=" + quote(ComboBoxPlant.Value);
	sArgument += " MAJCD=" + quote("JOBYL");
	http.Sync 		= true;
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	http.Sync 		= false;
	if(ds_MST040PF.rowcount<1){
		st_TEXT1 = "F          " + substr(cmb_WRKDT.Value,4,2) + " 월도 목표지수 (%)";
		ds_FHDZ202Q.SetColumn(0,"st_TEXT1",st_TEXT1);
		gisuyl = 100 - ((0/12) * mm);
		st_TEXT2 = "F => 100 - " + " / 12 * M = "+gisuyl +" %";
		ds_FHDZ202Q.SetColumn(0,"st_TEXT2",st_TEXT2);
	}
	else{
		st_TEXT1 = "F          " + substr(cmb_WRKDT.Value,4,2) + " 월도 목표지수 (%)";
		ds_FHDZ202Q.SetColumn(0,"st_TEXT1",st_TEXT1);
		jobyl=parseInt(ds_MST040PF.GetColumn(0,"MJINT"));
		gisuyl = 100 -((jobyl/12)*mm);
		st_TEXT2 = "F => 100 - " + jobyl + " / 12 * M = "+gisuyl +" %";
		ds_FHDZ202Q.SetColumn(0,"st_TEXT2",st_TEXT2);
	}
	var downtp = "";
	var dalsuf = "";
		downtp =truncate(ds_FHDZ202Q.GetColumn(0,"DOWNTP"),1);
	if(downtp !=0){
		if((gisuyl/downtp)*100 < 999.9 && (gisuyl/downtp)*100 > 0){
			dalsuf=(gisuyl/downtp)*100;
		}else{
			dalsuf="999.9";
		}
		ds_FHDZ202Q.SetColumn(0,"DALSUF",dalsuf);
	}
}

