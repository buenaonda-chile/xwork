	var arrProcs ="";
	if(ds_FHD000PF.rowcount>0){
		arrProcs = "('";
		for (i=0; i <ds_FHD000PF.rowcount; i++){
			if (i == ds_FHD000PF.rowcount-1){
				arrProcs = arrProcs+trim(ds_FHD000PF.GetColumn(i,"PROCS")) + "')";
			}else{
				arrProcs = arrProcs+trim(ds_FHD000PF.GetColumn(i,"PROCS")) + "','";
			}
		}
	}
	
	var sKind		= "getFHDZ202Q";						
	var sMethodName = "service::psfaclib/selectFHDZ202Q.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                            				// 자료 저장 시 보낼 DataSet 명칭
	var	sOutDataSet = "ds_FHDZ202Q=ds_FHDZ202Q";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
		sArgument += "@LIB1=" + quote(Lib1);
		sArgument += "@LIB2=" + quote(Lib2);
		sArgument += "@COMPS=" + quote(ComboBoxPlant.Value); //사업부
		sArgument += "@DHCODE=" + quote(ed_CODE.Text); //부서코드
		sArgument += "@WRKDT=" + quote(cmb_WRKDT.Value); //작업일자
		sArgument += "@PROCS=" + quote(arrProcs); //작업일자
	var str = dialog("Common::PGMLOAD_FAC.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");
	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
		setMessage("1004");
		getMst040();		
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));
		setMessage("1003");	
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}
}

