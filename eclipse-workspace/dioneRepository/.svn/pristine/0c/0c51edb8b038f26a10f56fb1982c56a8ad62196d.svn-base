	<Form Height="88" Id="PGMPOPUP" Left="8" OnLoadCompleted="form_OnLoadCompleted" PidAttrib="7" Title="PGMPOPUP" ToolTipFont="Default,0" Top="8" Ver="1.0" Width="440" WorkArea="true">
		<Image Height="48" Id="Image3" ImageID="popup_title" Left="19" TabOrder="3" Top="14" Width="398"></Image>
		<Static Font="굴림,12,Bold" Height="24" Id="Static3" Left="102" TabOrder="1" Text="프로그램이&#32;실행중&#32;입니다." Top="29" Width="216"></Static>
		<Image Height="16" Id="Image0" ImageID="title_ico" Left="84" TabOrder="2" Top="28" Width="17"></Image>
