@CHARSET "UTF-8";
.doc-wrap {
	width:100%; 
	min-width:950px;
	padding: 20px;
	box-sizing: border-box;
}

.board-bList-wrap {
    margin: auto;
}

.input_tr input, .update_tr input, .form-group input {
	height: 17px;
}

.list-top {
	overflow: visible;
}

[data-ax5autocomplete] {
	display: inline-block;
	width: 250px;
	vertical-align: bottom;
}

.form-control {
	height: 30px;
	padding: 3px 12px;
}

.ax5autocomplete-display .ax5autocomplete-display-table 
[data-ax5autocomplete-display="label"]{
	padding: 0;
}

.AXButton.Black {
	background: #000;
    border: 1px solid #000;
    color: #fff;
    text-shadow: none;
}

.authMenuCfgTbl th:nth-child(1)
,.authMenuCfgTbl th:nth-child(6)
,.authMenuCfgTbl th:nth-child(7)
,.authMenuCfgTbl th:nth-child(8)
,.authMenuCfgTbl th:nth-child(9) 
,.authMenuCfgTbl th:nth-child(10){
	width:60px;
	max-width : 60px;
}

.authMenuCfgTbl th:nth-child(2) {
	width:130px;
	max-width : 130px;
}

.authMenuCfgTbl th:nth-child(3) {
	width:220px;
	max-width : 220px;
}

.authMenuCfgTbl th:nth-child(4) {
	max-width : 210px;
}

[data-ax5autocomplete] > span{
	position: absolute;
    z-index: 1000;
    padding: 5px 10px;
    color: gray;
    width: 100%;
    text-overflow: ellipsis;
    white-space: nowrap;
    word-wrap: normal;
    overflow: hidden;
    
}

select {
	max-width: 150px;
}

/* 종합관리부분 */
.research-conB-bnt button, button {
 padding: 0;
}

.AXButton {
    height: 33px;
    box-sizing: border-box;
    vertical-align: baseline;
}

.AXButton.Black {
    background: #1e1e1e;
    border: 1px solid #000;
    color: #cecece;
    text-shadow: none;
}

.AXButton.Black:focus, .AXButton.Black:hover {
    background: #000;
    text-decoration: none;
    color: #fff;
}
/* 종합관리부분 */