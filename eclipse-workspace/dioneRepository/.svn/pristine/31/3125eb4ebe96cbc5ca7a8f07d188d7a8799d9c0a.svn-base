/********************************************************************** 
 * 내용설명 : 화면조회
 * 주요변수 : 
 **********************************************************************/ 
function getBZOE210(){
	
	if(!validationChk1()){
		return;
	}
	
	var sKind		= "getBZOE210";				
	var sMethodName = "service::psimmlib/selectBZOE210.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_BZOE210=ds_BZOE210";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";	
	sArgument += " LIB1=" + quote(Lib1);
	sArgument += " LIB2=" + quote(Lib2);
	sArgument += " COMPS=" + quote(ComboBoxPlant.Value);
	sArgument += " PLANN=" + quote(trim(ed_PLANN.Text));
	sArgument += " CHLDT=" + quote(ca_CHLDT.Value);
	sArgument += " DPTCD=" + quote(trim(ed_DPTCD.Text));
	sArgument += " CGMOK_P=" + quote(trim(ed_CGMOK.Text));
	http.Sync = true;
	//var str = dialog("Common::PGMLOAD_IMM.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");
	http.Sync = false;
	if(ds_BZOE210.rowcount() > 0){
		var rtn = ds_BZOE210.FindRow("CGMOK"," ");
		if(rtn != -1){
			ds_BZOE210.row = rtn;
			ds_BZOE210.ClearSelect();
			//ds_BZOE210.SelectRow(rtn);
			MainGrid.SetFocus();
			MainGrid.SetCellPos(9);
		}else{
			ds_BZOE210.row = 0;
			ds_BZOE210.ClearSelect();
			ds_BZOE210.SelectRow(0);
			MainGrid.SetFocus();
			MainGrid.SetCellPos(9);
		}
	}
}


/********************************************************************** 
 * 내용설명 : 저장
 * 주요변수 : 
 * 			
 **********************************************************************/ 
 
 function setBZOE210(){
 	if(!validationChk1()) return;
 	if(!validationChk2()) return;
	var sKind		= "setBZOE210";						
	var sMethodName = "service::psimmlib/saveBZOE210.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "ds_BZOE210=ds_BZOE210:U";                      // 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_BZOE210_Check=ds_BZOE210_Check";							// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
	sArgument += " LIB2=" + quote(Lib2);
	sArgument += " COMPS=" + quote(ComboBoxPlant.Value);
	sArgument += " PLANN=" + quote(trim(ed_PLANN.Text));
	sArgument += " CHLDT=" + quote(ca_CHLDT.Value);
	sArgument += " DPTCD=" + quote(trim(ed_DPTCD.Text));
	sArgument += " USRNM=" + quote(G_ds_user.GetColumn(0,"USER_ID"));
