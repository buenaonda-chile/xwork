function callback(svcId, rtnCd, rtnMsg){
	
	if (svcId == "setBZOE210"){
		if (ds_BZOE210_Check.GetRowCount() > 0){			
			for(var i=0;i<ds_BZOE210_Check.GetRowCount();i++){		
				alert(ds_BZOE210_Check.GetColumn(i,"PSPNO")+" : "+ds_BZOE210_Check.GetColumn(i,"RTMSG"));	
			}
		}
	
		// tran 실패
		if(rtnCd<0){
			alert(setPopMessage("1002"));
			setMessage("1002");
		}else {
			alert(setPopMessage("1001"));
			setMessage("1001");	
		}
	}
}
/********************************************************************** 
 * 내용설명 : 컴포넌트 validation 체크
 **********************************************************************/ 
function validationChk1(){
	if(length(trim(ComboBoxPlant.text)) < 1){
		alert(setPopMessage("1510")); // [경고] 사업부 구분을 확인바랍니다.
		setMessage("1510");
		return false;
	}
	if(!(ed_CGMOK.text == "" || ed_CGMOK.text == "Y")){
		alert("미출고 확인바랍니다."); 
		return false;
	}
	if(length(trim(ed_PLANN.text)) < 1){
		alert(setPopMessage("2521")); //[[확인] 품번 확인바랍니다.
		setMessage("2521");
		return false;
	}
	if(!isDate(ca_CHLDT.Value)){
		alert(setPopMessage("6064")); // [확인] 출고일자를 입력하세요.
		setMessage("6064");
		return false;
	}
	if(length(trim(ed_DPTCD.text)) < 1){
		alert(setPopMessage("6080")); // [알림]작업반을 입력하세요.
		setMessage("6080");
		return false;
	}
	return true;
}

function validationChk2(){
	for (var i=0;i<ds_BZOE210.rowcount();i++){
		if(ds_BZOE210.GetRowType(i)=="insert"){
			var vPSPNO = trim(ds_BZOE210.GetColumn(i,"PSPNO"));
			var vPROCS = trim(ds_BZOE210.GetColumn(i,"PROCS"));
			var vREQGU = trim(ds_BZOE210.GetColumn(i,"REQGU"));
			if (vPSPNO == null || vPSPNO == ""){
				alert(setPopMessage("1501")); //[[확인] 품번 확인바랍니다.
				setMessage("1501");
				return false;
			}
			if (vPROCS == null || vPROCS == ""){
				alert(setPopMessage("6016")); // [알림]작업공정을 입력하세요.
				setMessage("6016");
				return false;
			}
			if (vREQGU == null || vREQGU == ""){
				alert(setPopMessage("2202"));  // [알림] SEQNO를 입력하세요.
				setMessage("2202");
				return false;
			}
			
			// 현dataset에서 중복Check시
			var rtn = ds_BZOE210.SearchRow("PROCS = " + quote(vPROCS) + " && PSPNO = " + quote(vPSPNO) + " && REQGU = " + quote(vREQGU),0,ds_BZOE210.rowcount()-2);
			if (rtn != -1){
				alert(setPopMessage("2039")); // [확인] 중복된 작업공정,품번,seq가 존재합니다.
				setMessage("2039");
				return false;
			}
			// 데이타베이스 중복Check시
			//  psimmlib.selectBZOE210_Check사용하면 
			
		}
	}
	
	if(ds_BZOE210.FindRow("CHLQY",null) != -1){
		alert(setPopMessage("2523")); // [알림] 출고수량(0 이상)을 입력하세요?
		setMessage("2523");
		return false;
	}
	return true;
}

