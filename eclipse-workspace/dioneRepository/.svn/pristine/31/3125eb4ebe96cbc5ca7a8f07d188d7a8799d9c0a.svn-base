function ds_BZOE210_CanColumnChange(obj,nRow,strColumnID,varOldValue,varNewValue,nPivotIndex)
{
	if( strColumnID == "PROCS" ){
		if (length(trim(varNewValue)) < 1){
			return false;
		}
		if (!getBM001PR_0(Lib1,varNewValue)){
			alert(setPopMessage("4746"));//[알림] PROCESS MA에 미등록된 공정코드입니다.
			setMessage("4746");			
			displayDiv.ed_PROCS.SetFocus();
			return false;
		}
		// 중복check
		if(chk_duplicate() == "Y"){
			alert(setPopMessage("1509"));  // [경고] SEQ를 확인바랍니다.
			setMessage("1509");
			return false;
		}
	}
	if( strColumnID == "PSPNO" ){
		if (length(trim(displayDiv.ed_PROCS.Text)) < 1){
			alert(setPopMessage("1509"));  // [경고]  먼저 품번 입력바랍니다.
			setMessage("1509");
			displayDiv.ed_PROCS.SetFocus();
			return false;
		}
		if (length(trim(varNewValue)) < 1){
			return false;
		}
		// 1. 품번 Check
		if (getBM008PR_0(Lib1,varNewValue)){
			can_ITDSC = ds_BM008PR.GetColumn(0,"ITDSC");
		}else{
			can_ITDSC="";
			alert(setPopMessage("1509"));  // [경고] Item Master에 미등록된 품번입니다.
			setMessage("1509");
			displayDiv.ed_PSPNO.SetFocus();
			return false;
		}
		// 중복check
		if(chk_duplicate() == "Y"){
			alert(setPopMessage("2309"));  // [알림] 중복된 데이터가 존재합니다.
			setMessage("2309");
			return false;
		}
		// 2. cigma , 가입고품번 check 
		if (getBZI100PF_0(Lib2,ComboBoxPlant.Value,varNewValue)){
			displayDiv.ed_VNDNR.Enable = true; 
			can_GAIPGO = "Y";   //가입고품번
		}else{
			displayDiv.ed_VNDNR.Enable = false; 
			can_GAIPGO = "N";   //cigma품번
		}
		
		// 3. 이미 출고처리확정된 수량
		if(getBZO210PF_0(Lib2,ComboBoxPlant.Value,varNewValue)){
			can_finishCHLQY = toNumber(ds_BZO210PF.GetColumn(0,"FCHLQY"));
		}else{
			can_finishCHLQY = 0;
		}
		
		// 4. 배수량Check
		if(getBM009PR_0(Lib1,varNewValue)){
			can_LOTSZ = ds_BM009PR.GetColumn(0,"LOTSZ");
		}else{
			can_LOTSZ = "0";
			alert("배수량 확인바랍니다.");  
			//setMessage("1509");
			displayDiv.ed_PSPNO.SetFocus();
			return false;
		}
		
		// 5. 재고량
		if(can_GAIPGO == "N"){  //CIGMA 품번
			if(getHM000PR_0(Lib1,varNewValue,"1")){
				can_MOHTQ = toNumber(ds_HM000PR.GetColumn(0,"MOHTQ"));
				can_MOHTQ_C_G = 'C';
			}else{
				can_MOHTQ = 0;
				can_MOHTQ_C_G = 'G';
			}
		}
	}
	if( strColumnID == "REQGU" ){
		if (length(trim(displayDiv.ed_PROCS.Text)) < 1){
			alert(setPopMessage("6016"));  // [알림]작업공정을 입력하세요.
			setMessage("6016");
			displayDiv.ed_PROCS.SetFocus();
			return false;
		}
		if (length(trim(displayDiv.ed_PSPNO.Text)) < 1){
			alert(setPopMessage("1501"));  // [확인] 품번 확인바랍니다.
			setMessage("1501");
			displayDiv.ed_PSPNO.SetFocus();
			return false;
		}
		if (!(toNumber(varNewValue) >= 10 && toNumber(varNewValue) <= 91)) {
			alert(setPopMessage("2202"));  // [알림] SEQNO를 입력하세요.
			setMessage("2202");
			return false;
		}
		// 중복check
		if(chk_duplicate() == "Y"){
			alert(setPopMessage("2309"));  // [알림] 중복된 데이터가 존재합니다.
			setMessage("2309");
			return false;
		}
		
	}
	if( strColumnID == "CHLQY" ){
		if (length(trim(displayDiv.ed_PSPNO.Text)) < 1){
			alert(setPopMessage("1501"));  // [확인] 품번 확인바랍니다.
			setMessage("1501");
			displayDiv.ed_PSPNO.SetFocus();
			return false;
		}
		if (toNumber(varNewValue) <= 0 ) {
			alert(setPopMessage("6038"));  // [알림]수량은 0보다 커야합니다.
			setMessage("6038");
			//displayDiv.ed_CHLQY.SetFocus();
			return false;
		}
		
		var REQQYS = toNumber(displayDiv.st_REQQY.Text);
		if (REQQYS != 0){
			// 1. 요구수량 < 출고수량 Check
			var temp_LOTSZ = displayDiv.st_LOTSZ.Text;
			if(toNumber(displayDiv.st_LOTSZ.Text) != 0){
				var MULQUO = toInteger(REQQYS/toNumber(temp_LOTSZ));
				var MULREM = toInteger(REQQYS%toNumber(temp_LOTSZ));
				if(MULREM != 0) MULQUO = MULQUO+1;
				var CHLQYF = MULQUO * toNumber(temp_LOTSZ);
				if((toNumber(varNewValue) > CHLQYF) && (REQQYS != 0)){
					alert(setPopMessage("5017"));  // [경고] 확정수량이 요구수량보다 클 수 없습니다.
					setMessage("5017");
					//displayDiv.ed_CHLQY.SetFocus();
					return false;
				}
			}
		}
		// 2. 재고량 - 출고확정량 <  출고수량 CHECK
		if ((toNumber(displayDiv.st_MOHTQ.Text) - toNumber(displayDiv.st_FCHLQY.Text)) < toNumber(varNewValue)){
			alert(setPopMessage("5018"));  // [경고] 확정수량이 재고수량보다 클 수 없습니다.
			setMessage("5018");
			//displayDiv.ed_CHLQY.SetFocus();
			return false;
		}
		
	}
	if( strColumnID == "VNDNR" ){
		if (length(trim(varNewValue)) < 1){
			return false;
		}
		if (length(trim(displayDiv.ed_PSPNO.Text)) < 1){
			alert(setPopMessage("1501"));  // [확인] 품번 확인바랍니다.
			setMessage("1501");
			displayDiv.ed_PSPNO.SetFocus();
			return false;
		}
		// 2. cigma , 가입고품번 check 
		if (getBZI100PF_0(Lib2,ComboBoxPlant.Value,displayDiv.ed_PSPNO.Text)){
			displayDiv.ed_VNDNR.Enable = true; 
			can_GAIPGO = "Y";   //가입고품번
		}else{
			displayDiv.ed_VNDNR.Enable = false; 
			can_GAIPGO = "N";   //cigma품번
		}
		
		if(can_GAIPGO == "Y"){
			var pno_GAIPGO = "F";
			for(var z=0;ds_BZI100PF.GetRowCount();z++){
				if(trim(varNewValue) == trim(ds_BZI100PF.GetColumn(z,"VNDNR"))){
					pno_GAIPGO = "T";
				}
			}
			if(pno_GAIPGO == "F"){
				alert(setPopMessage("5019"));  // [확인] 가입고 업체를 확인하세요.
				setMessage("5019");
				displayDiv.ed_VNDNR.SetFocus();
				return false;
			}
		
			// 재고량 < 출고량 CHECK
			if(getBZI500PF_0(Lib2,ComboBoxPlant.Value,"1",displayDiv.ed_PSPNO.Text,varNewValue)){
				can_MOHTQ = toNumber(ds_BZI500PF.GetColumn(0,"MOHTQ_G"));
				can_MOHTQ_C_G = 'G';
			}
			if ((can_MOHTQ-can_finishCHLQY) < toNumber(displayDiv.ed_CHLQY.Text)){
				alert(setPopMessage("5021"));  // [경고] 출고수량이 재고수량보다 클 수 없습니다.
				setMessage("5021");
				displayDiv.ed_VNDNR.SetFocus();
				return false;
			}
		}else{
			if(trim(varNewValue) != ""){
				alert(setPopMessage("5020"));  // [확인] 가입고 대상품번이 아닌 경우 업체를 입력할 수 없습니다.
				setMessage("5020");
				displayDiv.ed_VNDNR.SetFocus();
				return false;
			}
		}
	}
}

function ds_BZOE210_OnColumnChanged(obj,nRow,strColumnID,varOldValue,varNewValue,nPivotIndex)
{		
	if (strColumnID == "PSPNO"){
		ds_BZOE210.SetColumn(nRow,"ITDSC",trim(can_ITDSC));
		ds_BZOE210.SetColumn(nRow,"FCHLQY",trim(can_finishCHLQY)); //출고확정량
		ds_BZOE210.SetColumn(nRow,"LOTSZ",trim(can_LOTSZ));       //배수량
		ds_BZOE210.SetColumn(nRow,"MOHTQ",trim(can_MOHTQ));       //재고량
		ds_BZOE210.SetColumn(nRow,"MOHTQ_GB",trim(can_MOHTQ_C_G));       //재고량
		
		ds_BZOE210.SetColumn(nRow,"TCODE","IS"); 
		ds_BZOE210.SetColumn(nRow,"JPNNO",substr(trim(displayDiv.ed_PROCS.Text),0,4) + substr(ca_CHLDT.Value,2,6)); //전표번호
		ds_BZOE210.SetColumn(nRow,"REQQY","0"); 
	}
	
}

function ds_BZOE210_OnRowPosChanged(obj,nOldRow,nRow)
{
	//로우타입 insert만 ED박스 enable
	if(obj.GetRowType(obj.currow)=="insert"){
		displayDiv.ed_PROCS.Enable=true;
		displayDiv.ed_PSPNO.Enable=true;
		displayDiv.ed_REQGU.Enable=true;
		displayDiv.ed_CHLQY.Enable=true;
		displayDiv.ed_VNDNR.Enable=false;
	}else{
		displayDiv.ed_PROCS.Enable=false;
		displayDiv.ed_PSPNO.Enable=false;
		displayDiv.ed_REQGU.Enable=false;
		if(ds_BZOE210.GetColumn(nRow,"CGMOK") == "*"){
			displayDiv.ed_CHLQY.Enable=false;
		}else{
			displayDiv.ed_CHLQY.Enable=true;
		}
		displayDiv.ed_VNDNR.Enable=false;
	}
}

function chk_duplicate(){
	var dup = "N";
	if (length(trim(displayDiv.ed_PROCS.Text)) > 0 && length(trim(displayDiv.ed_PSPNO.Text)) > 0 &&	length(trim(displayDiv.ed_REQGU.Text)) > 0){
		var sKind		= "getBZOE210_duCheck";
		var sMethodName = "service::psimmlib/selectBZOE210_duCheck.do";     		// 서버에서 호출 될 Method 명
		var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
		var sOutDataSet = "ds_BZOE210_duCheck=ds_BZOE210_duCheck";    				// 조회 후 돌려 받을 DataSet 명칭
		var sArgument   = "";	
		sArgument += " LIB2=" + quote(Lib2);
		sArgument += " COMPS=" + quote(ComboBoxPlant.Value);
		sArgument += " PLANN=" + quote(trim(ed_PLANN.Text));
		sArgument += " CHLDT=" + quote(ca_CHLDT.Value);
		sArgument += " DPTCD=" + quote(trim(ed_DPTCD.Text));
		sArgument += " PROCS=" + quote(trim(displayDiv.ed_PROCS.Text));
		sArgument += " PSPNO=" + quote(trim(displayDiv.ed_PSPNO.Text));
		sArgument += " REQGU=" + quote(trim(displayDiv.ed_REQGU.Text));
		http.Sync = true;
		Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
		http.Sync = false;
		if(ds_BZOE210_duCheck.GetColumn(0,"DUCNT") != "0"){
			dup = "Y";
			return dup;
		}
	}
	return dup;
}

function displayDiv_ed_Save_OnKeyDown(obj,nChar,bShift,bCtrl, bAlt,LLParam,HLParam)
{
	if(nChar == 13){
		if(displayDiv.ed_Save.Text != "Y") return;
		if(confirm("저장하시겠습니까? ") == false) return;
		fn_Save();
		if(ed_Action.Text == "A") {
			if(confirm("다음Record를 추가하시겠습니까? ") == false) {
				return;
			}else{
				fn_Input();
			}
		}
	}
}
