	gFcOnTopButtonColor("2.2"); //상단 버튼 활성화 여부 (기본:0,조회:1,변경:2)	
}

/********************************************************************** 
 * 내용설명 : 엑셀파일로 저장
 * 주요변수 : 
 **********************************************************************/ 
function setExcel()
{
	var fName   = "자동출고마스타LIST";
	var arrSheets  =  Array();  

	arrSheets[0]  = "MainGrid|자동출고마스타LIST";
	createExcelSheetS2(FileDialog0, fName ,arrSheets);
}

function ed_Action_OnKeyDown(obj,nChar,bShift,bCtrl, bAlt,LLParam,HLParam)
{
	if(nChar == 13){
		switch(obj.text){
			case "A" : fn_Input(); break;
			case "Q" : fn_Search(); break;
			default : alert(setPopMessage("1016")); setMessage("1016");
		}
	}
}

function ds_BCOE100_CanColumnChange(obj,nRow,strColumnID,varOldValue,varNewValue,nPivotIndex)
{	
	if( strColumnID == "PSPNO" ){
		//기등록 check
		if(ds_BCOE100.FindRow("PSPNO", trim(varNewValue)) >= 0){
			alert(setPopMessage("1520"));  //[경고] 이미 등록된 자료입니다.
			setMessage("1520");
			displayDiv.ed_PSPNO.SetFocus();
			return false;
		}
		//품번Check,조회
		http.Sync = true;
		var sKind		= "getBM008PR";		
		var sMethodName = "service::pscomlib/selectBM008PR.do"; 
		var sInDataSet  = "";
		var sOutDataSet = "ds_BM008PR=ds_BM008PR";
		var sArgument   = "";
			sArgument  += " LIB1=" + quote(Lib1);
			sArgument  += " ITNBR=" + quote(varNewValue);
			
		Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
		
		if (ds_BM008PR.GetRowCount() < 1){
			alert(setPopMessage("1509"));  // [경고] Item Master에 미등록된 품번입니다.
			setMessage("1509");	
			displayDiv.ed_PSPNO.SetFocus();
			return false;
		}
		
		can_ITDSC = ds_BM008PR.GetColumn(0,"ITDSC");
		can_UNMSR = ds_BM008PR.GetColumn(0,"UNMSR");
		can_ITTYP = ds_BM008PR.GetColumn(0,"ITTYP");
		can_ITCLS = ds_BM008PR.GetColumn(0,"ITCLS");
		can_PLANN = ds_BM008PR.GetColumn(0,"PLANN");		
	}
	
	
}

function ds_BCOE100_OnColumnChanged(obj,nRow,strColumnID,varOldValue,varNewValue,nPivotIndex)
{		
	if (strColumnID == "PSPNO"){
		
		ds_BCOE100.SetColumn(nRow,"ITDSC",can_ITDSC);
		ds_BCOE100.SetColumn(nRow,"UNMSR",can_UNMSR);
		ds_BCOE100.SetColumn(nRow,"ITTYP",can_ITTYP);
		ds_BCOE100.SetColumn(nRow,"ITCLS",can_ITCLS);
		ds_BCOE100.SetColumn(nRow,"PLANN",can_PLANN);	
	}
}

function ds_BCOE100_OnRowPosChanged(obj,nOldRow,nRow)
{
	//로우타입 insert만 ED박스 enable
	if(obj.GetRowType(obj.currow)=="insert"){
		displayDiv.ed_PSPNO.Enable=true;
	}else{
		displayDiv.ed_PSPNO.Enable=false;
	}
}

function displayDiv_ed_PSPNO_OnKeyDown(obj,nChar,bShift,bCtrl, bAlt,LLParam,HLParam)
{
	if(nChar == 13){
		displayDiv.ed_Flag.SetFocus();  // ed_PSPNO에 데이타셋 이벤트가 발생된 후 텟스트값이 활당되도록 포커스를 줌.
		if(length(trim(displayDiv.ed_PSPNO.Text)) > 0){
			if(confirm("저장하시겠습니까? ") == false) return;
			fn_Save();
			fn_Input();
		}
	}
}
