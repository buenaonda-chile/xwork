var Lib3 = G_ds_user.GetColumn(0,"LIB3");    //인사급여 LIBRARY(PSEDBLIB           PSCDBLIB.PSHDBLIB.PSSDBLIB)
var company = G_ds_user.GetColumn(0,"COMPANY"); //PE      PS
var comps = G_ds_user.GetColumn(0,"PLANT_CD"); //E1,      C1.H1.S1


