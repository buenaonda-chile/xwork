function fn_Favorite()
{
	alert("fn_Favorite");
}

function fn_Search()
{
	ed_Action.Text = "Q";
	getBCOE100();
}

function fn_Input()
{
	if(checkInsertCount("ds_BCOE100")== false) return;
	if(ds_BCOE100.rowcount < 1){
		fn_Search();
	}
	ds_BCOE100.AddRow();
	
	//ds_BCOE100.SetColumn(ds_BCOE100.Row, "COMPS", ComboBoxPlant.Value); // default값 설정
	
	//MainGrid.SetCellPos(0);
	//MainGrid.SetFocus();
	ed_Action.Text = "A";
	displayDiv.ed_PSPNO.Enable = true;
	displayDiv.ed_PSPNO.SetFocus();
}

function fn_Delete()
{
	ds_BCOE100.DeleteRow(ds_BCOE100.currow);
}

function fn_Save()
{
	setBCOE100();
}


function fn_print()
{
	alert(setPopMessage("9999"));
	setMessage("9999");
}

function fn_Excel()
{
	setExcel();
}

function fn_Help()
{
	alert(setPopMessage("9999"));
