/********************************************************************** 
 * 내용설명 : 화면 조회 
 **********************************************************************/ 
function getHPCZ104Q(){

	if(!validationChk()){
		return;
	}
	
/*
	if(ComboBoxPlant.Value =="C1"){
		Lib3="PSCDBLIB";
	}else if(ComboBoxPlant.Value =="H1"){
		Lib3="PSHDBLIB";
	}else if(ComboBoxPlant.Value =="S1"){
		Lib3="PSSDBLIB";
	}
*/
	var sKind		= "getHPCZ104Q";						
	var sMethodName = "service::pshumlib/selectHPCZ104Q.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                            				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_HPCZ104Q=ds_HPCZ104Q";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
		sArgument += "@LIB1=" + quote(Lib1);
		sArgument += "@LIB2=" + quote(Lib2);
		sArgument += "@LIB3=" + quote(Lib3);
		sArgument += "@COMPS=" + quote(ComboBoxPlant.Value);
		sArgument += "@KNYYM=" + quote(substr(cmb_KNYYM.Value,0,6));	//기준년월
		sArgument += "@PAYCD=" + quote(rdoggub_PAY.Value);	//급여구분
		sArgument += "@TYPECD=" + quote(rdoggub_TYPE.Value);	//대상구분
		sArgument += "@TABLENM=" + quote(tableNm);	//테이블구분(급여는 HPC100PF 상여는 HBM100PF)
	var str = dialog("Common::PGMLOAD_HUM.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");	
	http.Sync = false;
	
	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
		setMessage("1004");	
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));
		setMessage("1003");	
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}
}
/********************************************************************** 
 * 내용설명 : Transaction 리턴후 실행 함수
 * 주요변수 : 
 * 			- sMethodName	Transaction 고유 ID
 * 			- ErrorCode 	에러코드
 * 			- ErrorMSG 		에러 메세지
 **********************************************************************/ 
function callback(svcId, rtnCd, rtnMsg){
	if(svcId=="getHPCZ104Q"){
		if(rtnCd<0){
			alert(setPopMessage("1002"));
			setMessage("1002");
			
		}else{
			if(ds_HPCZ104Q.rowcount<1){
				alert(setPopMessage("1003"));
				setMessage("1003");	
			}else{
				alert(setPopMessage("1004"));
				setMessage("1004");	
			}
		}
	}
}
/********************************************************************** 
