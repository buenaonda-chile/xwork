	if(ds_SPYC904Q.rowcount <= 0 || ds_SPYC904Q.GetRowType(ds_SPYC904Q.row) != 'normal') return ;
	
	var plantcd;
	var selected_comp;
	
	if(cb_comp.Value == "") {
		selected_comp="ZZ";
	}else {
		selected_comp=cb_comp.Value;
	}
	
	if(cb_plant.Value == "") {
		plantcd="ZZ";
	}else {
		plantcd=cb_plant.Value;
	}
	var str = dialog("PSSALLIB::SPYC904QD.xml","P_COMPS=" + selected_comp + " " + "P_PLTCD=" + plantcd + " " + "P_PYVND=" + ds_SPYC904Q.GetColumn(ds_SPYC904Q.row,"V") + " " + "P_YYMMF=" + SubStr(ca_plnymF.Value,0,6) + " " + "P_YYMMT=" + SubStr(ca_plnymT.Value,0,6) + " " + "P_PLNGB=" + cb_plngb.Value,1100,600);
}
