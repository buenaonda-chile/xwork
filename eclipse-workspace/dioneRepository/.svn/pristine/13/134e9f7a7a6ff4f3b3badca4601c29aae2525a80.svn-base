 **********************************************************************/ 
function getSPYC904Q()
{
	var month1 = substr(AddMonth(ca_plnymF.Value,1),0,6);
	var month2 = substr(AddMonth(ca_plnymF.Value,2),0,6);
	var month3 = substr(AddMonth(ca_plnymF.Value,3),0,6);
	var month4 = substr(AddMonth(ca_plnymF.Value,4),0,6);
	var month5 = substr(AddMonth(ca_plnymF.Value,5),0,6);
	var month6 = substr(AddMonth(ca_plnymF.Value,6),0,6);
	var month7 = substr(AddMonth(ca_plnymF.Value,7),0,6);
	var month8 = substr(AddMonth(ca_plnymF.Value,8),0,6);
	var month9 = substr(AddMonth(ca_plnymF.Value,9),0,6);
	var month10 = substr(AddMonth(ca_plnymF.Value,10),0,6);
	
	MainGrid.SetCellProp("Head", 2, "text", substr(ca_plnymF.Value,0,4)+"/"+substr(ca_plnymF.Value,4,2));
	MainGrid.SetCellProp("Head", 3, "text", substr(month1,0,4)+"/"+substr(month1,4,2));
	MainGrid.SetCellProp("Head", 4, "text", substr(month2,0,4)+"/"+substr(month2,4,2));
	MainGrid.SetCellProp("Head", 5, "text", substr(month3,0,4)+"/"+substr(month3,4,2));
	MainGrid.SetCellProp("Head", 6, "text", substr(month4,0,4)+"/"+substr(month4,4,2));
	MainGrid.SetCellProp("Head", 7, "text", substr(month5,0,4)+"/"+substr(month5,4,2));
	MainGrid.SetCellProp("Head", 8, "text", substr(month6,0,4)+"/"+substr(month6,4,2));
	MainGrid.SetCellProp("Head", 9, "text", substr(month7,0,4)+"/"+substr(month7,4,2));
	MainGrid.SetCellProp("Head", 10, "text", substr(month8,0,4)+"/"+substr(month8,4,2));
	MainGrid.SetCellProp("Head", 11, "text", substr(month9,0,4)+"/"+substr(month9,4,2));
	MainGrid.SetCellProp("Head", 12, "text", substr(month10,0,4)+"/"+substr(month10,4,2));
	MainGrid.SetCellProp("Head", 13, "text", substr(ca_plnymT.Value,0,4)+"/"+substr(ca_plnymT.Value,4,2));
	
	var sKind		= "getSPYC904Q";						
	var sMethodName = "service::pssallib/selectSPYC904Q.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_SPYC904Q=ds_SPYC904Q";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
	
	sArgument += "@LIB1=" + quote(Lib1);
	sArgument += "@LIB2=" + quote(Lib2);
	sArgument += "@COMPS=" + quote(cb_comp.Value); //사업부
	sArgument += "@PLTCD=" + quote(cb_plant.Value); //공장구분
    sArgument += "@PLNYM_F=" + quote(substr(ca_plnymF.Value,0,6)); //시작계획년월 
    sArgument += "@PLNYM_T=" + quote(substr(ca_plnymT.Value,0,6)); //종료계획년월 
    sArgument += "@PLNYM_1=" + quote(month1); //계획년월+1 
    sArgument += "@PLNYM_2=" + quote(month2); //계획년월+2
    sArgument += "@PLNYM_3=" + quote(month3); //계획년월+3
    sArgument += "@PLNYM_4=" + quote(month4); //계획년월+4
    sArgument += "@PLNYM_5=" + quote(month5); //계획년월+5 
    sArgument += "@PLNYM_6=" + quote(month6); //계획년월+6 
    sArgument += "@PLNYM_7=" + quote(month7); //계획년월+7
    sArgument += "@PLNYM_8=" + quote(month8); //계획년월+8 
    sArgument += "@PLNYM_9=" + quote(month9); //계획년월+9
    sArgument += "@PLNYM_10=" + quote(month10); //계획년월+10
    sArgument += "@PLNGB=" + quote(cb_plngb.Value); //계획구분
    
	var str = dialog("Common::PGMLOAD.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");	
	//Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");
}


