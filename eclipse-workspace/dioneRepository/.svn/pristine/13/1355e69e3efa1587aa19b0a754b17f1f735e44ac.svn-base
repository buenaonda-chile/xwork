	var sOutDataSet = "ds_HMDZ009Q_A=ds_HMDZ009Q_A ds_HMDZ009Q_B=ds_HMDZ009Q_B";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
		sArgument += " LIB1=" + quote(Lib1);
		sArgument += " LIB2=" + quote(Lib2);
		sArgument += " LIB3=" + quote(Lib3);
		sArgument += " COMPS=" + quote(ComboBoxPlant.Value);
		sArgument += " KNYYM=" + quote(substr(cmb_KNYYM.Value,0,6));	//기준일자+12
		sArgument += " YYEAR=" + quote(p_YYEAR);	//년차지급년월
		sArgument += " p_KNYYM1=" + quote(B1yymm);	//M
		sArgument += " p_KNYYM2=" + quote(B2yymm);	//M+1
		sArgument += " p_KNYYM3=" + quote(B3yymm);	//M+2
		sArgument += " p_KNYYM4=" + quote(B4yymm);	//M+3
		sArgument += " p_KNYYM5=" + quote(B5yymm);	//M+4
		sArgument += " p_KNYYM6=" + quote(B6yymm);	//M+5
		sArgument += " p_KNYYM7=" + quote(B7yymm);	//M+6
		sArgument += " p_KNYYM8=" + quote(B8yymm);	//M+7
		sArgument += " p_KNYYM9=" + quote(B9yymm);	//M+8
		sArgument += " p_KNYYM10=" + quote(B10yymm);	//M+9
		sArgument += " p_KNYYM11=" + quote(B11yymm);	//M+10
		sArgument += " p_KNYYM12=" + quote(B12yymm);	//M+11
		sArgument += " p_BYYMM1=" + quote(BYYMM1);	//상여M
		sArgument += " p_BYYMM2=" + quote(BYYMM2);	//상여M1
		sArgument += " p_BYYMM3=" + quote(BYYMM3);	//상여M2
		sArgument += " p_BYYMM4=" + quote(BYYMM4);	//상여M3
		sArgument += " p_BYYMM5=" + quote(BYYMM5);	//상여M4
		sArgument += " p_BYYMM6=" + quote(BYYMM6);	//상여M5
		sArgument += " p_BYYMM7=" + quote(BYYMM7);	//상여M6
		sArgument += " p_BYYMM8=" + quote(BYYMM8);	//상여M7
		sArgument += " p_BYYMM9=" + quote(BYYMM9);	//상여M8
	
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");
	http.Sync = false;

}
/********************************************************************** 
 * 내용설명 : Transaction 리턴후 실행 함수
 * 주요변수 : 
 * 			- sMethodName	Transaction 고유 ID
 * 			- ErrorCode 	에러코드
 * 			- ErrorMSG 		에러 메세지
 **********************************************************************/ 
function callback(svcId, rtnCd, rtnMsg){
	if(svcId=="getHMDZ009Q"){
		if(rtnCd<0){
			alert(setPopMessage("1002"));
			setMessage("1002");
			
		}else{
			if((ds_HMDZ009Q_A.rowcount < 1 && ds_HMDZ009Q_B.rowcount < 1 )){
				alert(setPopMessage("1003"));
				setMessage("1003");	
			}else{
				alert(setPopMessage("1004"));
				setMessage("1004");	
			}
		}
	}
}
