function getComCode(){
	http.Sync 	= true;
	var sKind		= "getComCode";						
	var sMethodName = "service::login/selectComCode.do"; 
	var sInDataSet  = "";
	var sOutDataSet = "ds_ComCode_PLNT=ds_ComCode_PLNT";
	var sArgument   = "";
		sArgument   += " CLASS='PLNT'";
		sArgument   += " CODE1=" + quote(company);		
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	cb_searchCodeName.Value = comps;
}


/********************************************************************** 
 * 내용설명 : 화면조회
 * 주요변수 : 
 **********************************************************************/ 
function getBJOZ118Q(){
	if(length(ca_ISIDT1.Value) < 8){
		alert(setPopMessage("6029"));
		setMessage("6029");
		ca_ISIDT1.SetFocus();
		return;
	}
	if(length(ca_ISIDT2.Value) < 8){
		alert(setPopMessage("6029"));
		setMessage("6029");
		ca_ISIDT2.SetFocus();
		return;
	}
	if(ca_ISIDT1.Value > ca_ISIDT2.Value){
		alert(setPopMessage("1503"));
		setMessage("1503");
		ca_ISIDT1.SetFocus();
		return;
	}
	var p_jp1,p_jp2;
	p_jp1 = iif (length(trim(ed_ISIJP1.Text)) == 0, ' ', trim(ed_ISIJP1.Text));
	p_jp2 = iif (length(trim(ed_ISIJP2.Text)) == 0 , '99999999', trim(ed_ISIJP2.Text));
	var p_dp1,p_dp2;
	p_dp1 = iif (length(trim(ed_ISDPT1.Text)) == 0, ' ', trim(ed_ISDPT1.Text));
	p_dp2 = iif (length(trim(ed_ISDPT2.Text)) == 0 , '9999', trim(ed_ISDPT2.Text));
	var p_cl1,p_cl2;
	p_cl1 = iif (length(trim(ed_ISICL1.Text)) == 0, ' ', trim(ed_ISICL1.Text));
	p_cl2 = iif (length(trim(ed_ISICL2.Text)) == 0, '9', trim(ed_ISICL2.Text));
	var p_psn1,p_psn2;
	p_psn1 = iif (length(trim(ed_ISIPN1.Text)) == 0, ' ', trim(ed_ISIPN1.Text));
	p_psn2 = iif (length(trim(ed_ISIPN2.Text)) == 0 , '999999999999999', trim(ed_ISIPN2.Text));
	
	var sKind		= "getBJOZ118Q";						
	var sMethodName = "service::psimmlib/selectBJOZ118Q.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_BJOZ118Q=ds_BJOZ118Q";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
	sArgument += "@LIB1=" + quote(Lib1);
	sArgument += "@LIB2=" + quote(Lib2);
	sArgument += "@COMPS=" + quote(cb_searchCodeName.Value);
	sArgument += "@ISIJP1=" + quote(p_jp1); // 전표번호1
	sArgument += "@ISIJP2=" + quote(p_jp2); // 전표번호2
	sArgument += "@ISDPT1=" + quote(p_dp1); // 부서1
	sArgument += "@ISDPT2=" + quote(p_dp2); // 부서2
	sArgument += "@ISICL1=" + quote(p_cl1); // 분류1
	sArgument += "@ISICL2=" + quote(p_cl2); // 분류2
	sArgument += "@ISIPN1=" + quote(p_psn1); // 품번1
	sArgument += "@ISIPN2=" + quote(p_psn2); // 품번2
	sArgument += "@ISIDT1=" + quote(ca_ISIDT1.Value); // 일자1 
    sArgument += "@ISIDT2=" + quote(ca_ISIDT2.Value); // 일자1 
	var str = dialog("Common::PGMLOAD.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");	
	//Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");
}


