function getComCode(){
	http.Sync 	= true;
	var sKind		= "getComCode";						
	var sMethodName = "service::login/selectComCode.do"; 
	var sInDataSet  = "";
	var sOutDataSet = "ds_ComCode_PLNT=ds_ComCode_PLNT";
	var sArgument   = "";
		sArgument   += " CLASS='PLNT'";
		sArgument   += " CODE1=" + quote(company);		
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	ds_VComCode_PLNT.Copy("ds_ComCode_PLNT");

	cb_searchCodeName1.Value = comps;
}

/********************************************************************** 
 * 내용설명 : 화면조회
 * 주요변수 : 
 **********************************************************************/ 
function getSMBZ010(){

	if(length(trim(ed_RVBNO.Text)) < 1){
		alert(setPopMessage("4047"));  //[[확인] 어음번호를 입력바랍니다.
		setMessage("4047");	
		ed_RVBNO.SetFocus();
		return;
	}	

	//조회
	var sKind		= "getSMBZ010";						
	var sMethodName = "service::pssallib/selectSMBZ010.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_SMBZ010=ds_SMBZ010";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
	
	sArgument += "@LIB1=" + quote(Lib1);
	sArgument += "@LIB2=" + quote(Lib2);
	sArgument += "@COMPS=" + quote(cb_searchCodeName1.Value);
	sArgument += "@RVBNO=" + quote(ed_RVBNO.Text);

	var str = dialog("Common::PGMLOAD_SAL.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");	
	//Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");
	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
		setMessage("1004");	
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));  //   [알림] 조회 된 데이터가 없습니다.
		setMessage("1003");	
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}
	ed_RVBNO.SetFocus();
}

/********************************************************************** 
 * 내용설명 : 저장
 * 주요변수 : 
 **********************************************************************/ 
 function setSMBZ010(){
	if (MainGrid.ed_RVGMP.Text == "*"){
		alert(setPopMessage("4037"));  //  [확인] 경리마감작업이 완료되어 수정 불가능합니다.
		setMessage("4037");
		return;
	}
	if (MainGrid.ed_RVCMP.Text == "*"){
		alert(setPopMessage("4038"));  //  [확인] 납품대금 마감작업이 완료되어 수정 불가능합니다.
		setMessage("4038");
		return;
	}
	if(ds_SMBZ010.GetRowType(ds_SMBZ010.currow) == "insert" || ds_SMBZ010.GetRowType(ds_SMBZ010.currow) == "update"){
		if(length(trim(MainGrid.st_pyvnd.Value)) < 1){ //발행처
			alert(setPopMessage("4048"));  // [확인] 발행처를 확인바랍니다.
			setMessage("4048");	
			MainGrid.ed_pyvnd.SetFocus();
			return;
		}
		if(length(trim(MainGrid.ca_RVSDT.Value)) < 1){ 
			alert(setPopMessage("4041"));  // [확인] 수금일자를 확인바랍니다.
			setMessage("4041");	
			MainGrid.ca_RVSDT.SetFocus();
			return;
		}
		if(length(trim(MainGrid.st_RVGBN.Value)) < 1){ 
			alert(setPopMessage("4039"));  // [확인] 어음구분을  확인바랍니다.
			setMessage("4039");	
			MainGrid.ed_RVGBN.SetFocus();
			return;
		}
		if(length(trim(MainGrid.ca_RVMDT.Value)) < 1){ 
			alert(setPopMessage("4042"));  // [확인] 발행일자를 확인바랍니다.
			setMessage("4042");	
			MainGrid.ca_RVMDT.SetFocus();
			return;
		}
		if(length(trim(MainGrid.ca_RVPDT.Value)) < 1){ 
			alert(setPopMessage("4043"));  // [확인] 만기일자를 확인바랍니다.
			setMessage("4043");	
			MainGrid.ca_RVPDT.SetFocus();
			return;
		}
		if(length(trim(MainGrid.med_RVBAM.Value)) < 1){ 
			alert(setPopMessage("4040"));  // [확인] 어음금액을  확인바랍니다.
			setMessage("4040");	
			MainGrid.med_RVBAM.SetFocus();
			return;
		}
		if(length(trim(MainGrid.st_RVBNK.Value)) < 1){ 
			alert(setPopMessage("4039"));  // [확인] 지급은행  확인바랍니다.
			setMessage("4039");	
			MainGrid.ed_RVBNK.SetFocus();
			return;
		}
	}
	http.Sync = true;
	var sKind		= "SAVE_SMBZ010";						
	var sMethodName = "service::pssallib/insertSMBZ010.do";
	var sInDataSet  = "ds_SMBZ010=ds_SMBZ010:U";
	var sOutDataSet = "ds_SMBZ010_CHECK=ds_SMBZ010_CHECK";
	var sArgument   = "";
	sArgument += " LIB2=" + quote(Lib2);
		
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");
	
 }
