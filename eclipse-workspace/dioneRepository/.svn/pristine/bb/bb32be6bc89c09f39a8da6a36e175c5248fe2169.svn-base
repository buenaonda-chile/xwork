/********************************************************************** 
 * 내용설명 : Transaction 리턴후 실행 함수
 * 주요변수 : 
 * 			- sMethodName	Transaction 고유 ID
 * 			- ErrorCode 	에러코드
 * 			- ErrorMSG 		에러 메세지
 **********************************************************************/ 
function callback(svcId, rtnCd, rtnMsg){
	if (svcId = "getSMBZ010"){
		alert(ds_SMBZ010_A.GetRowCount());
	}
	if (svcId = "SAVE_SMBZ010"){
		if (ds_SMBZ010_CHECK.GetRowCount() > 0){			
			for(var i=0;i<ds_SMBZ010_CHECK.GetRowCount();i++){		
				alert(ds_SMBZ010_CHECK.GetColumn(i,"RVBNO") + " ="
					+ ds_SMBZ010_CHECK.GetColumn(i,"RTMSG"));	
			}
			return;		
		}
	}
	// tran 실패
	if(rtnCd<0){
		alert(setPopMessage("1002"));
		setMessage("1002");
		
	}else 
	{
		alert(setPopMessage("1001"));
		setMessage("1001");	
		MainGrid.Enable = false;
	}
}

