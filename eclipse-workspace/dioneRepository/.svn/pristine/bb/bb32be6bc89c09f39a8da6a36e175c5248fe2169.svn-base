function ds_SMBZ010_CanColumnChange(obj,nRow,strColumnID,varOldValue,varNewValue,nPivotIndex)
{
	http.usewaitcursor = false;  //transaction호출후 저장이벤트가 발생되지 않아서...
	if (strColumnID == "RVMAK"){
		//득의선Check
		http.Sync 	= true;
		var sKind		= "getOGMPrtno";		
		var sMethodName = "service::pscomlib/selectCUSMAS1.do"; 
		var sInDataSet  = "";
		var sOutDataSet = "ds_CUSMAS1=ds_CUSMAS1";
		var sArgument   = "";
			sArgument  += " LIB1=" + quote(Lib1);
			sArgument  += " CUSNO=" + quote(varNewValue);
		Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
		if (ds_CUSMAS1.GetRowCount() != 1){
			alert(setPopMessage("1502"));
			setMessage("1502");	
			MainGrid.ed_pyvnd.SetFocus();
			return false;
		}
		MainGrid.st_pyvnd.Text = ds_CUSMAS1.GetColumn(0,"CUSNM");
		//PYVND_NM=ds_CUSMAS1.GetColumn(0,"CUSNM");
	}
	if (strColumnID == "RVGBN"){
		http.Sync = true;
		var sKind		= "getmst050pf";		
		var sMethodName = "service::pscomlib/selectMST050PF.do"; 
		var sInDataSet  = "";
		var sOutDataSet = "ds_MST050PF=ds_MST050PF";
		var sArgument   = "";
			sArgument  += " LIB2=" + quote(Lib2);
			sArgument  += " COMPS=" + quote(cb_searchCodeName1.Value);
			sArgument  += " GRPCD=" + quote("BIL");
			sArgument  += " MSTCD=" + quote(varNewValue);
		Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
		http.Sync = false;
		if (ds_MST050PF.GetRowCount() != 1){
			alert(setPopMessage("4039"));
			setMessage("4039");	
			return false;
		}
		MainGrid.st_RVGBN.Text = ds_MST050PF.GetColumn(0,"MSTRM");
		//MKLOC_NM=ds_MST050PF.GetColumn(0,"MSTRM");
	}
	if (strColumnID == "RVBNK"){
		//지급은행check
		http.Sync = true;
		var sKind		= "getmst600pf";		
		var sMethodName = "service::pscomlib/selectMST600PF.do"; 
		var sInDataSet  = "";
		var sOutDataSet = "ds_MST600PF=ds_MST600PF";
		var sArgument   = "";
			sArgument  += " LIB2=" + quote(Lib2);
			sArgument  += " VNDOR=" + quote("000"+varNewValue);
		Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
		http.Sync = false;
		if (ds_MST600PF.GetRowCount() < 1){
			alert(setPopMessage("4064"));
			setMessage("4064");	
			return false;
		}
		MainGrid.st_RVBNK.Text = ds_MST600PF.GetColumn(0,"CAPNM");
		//DRVPN_NM=ds_MST600PF.GetColumn(0,"CAPNM");
	}
	http.usewaitcursor = true;
}
