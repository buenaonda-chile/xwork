function fn_Input()
{
	if(length(trim(ed_RVBNO.Text)) < 1){
		alert(setPopMessage("4047"));  //[[확인] 어음번호를 입력바랍니다.
		setMessage("4047");	
		ed_RVBNO.SetFocus();
		return;
	}
	if(checkInsertCount("ds_SMBZ010")== false) return;
	getSMBZ010_DCHK();
	if (ds_SMBZ010_DCHK.GetRowCount() > 0){
		alert(setPopMessage("1520"));  //[경고] 이미 등록된 자료입니다.
		setMessage("1520");	
		ed_RVBNO.SetFocus();
		return;
	}
	
	ds_SMBZ010.FireEvent=false;  //데이터셋의 이벤트가 발생하지 않도록 셋팅
	ds_SMBZ010.AddRow();
	ds_SMBZ010.SetColumn(ds_SMBZ010.Row, "COMPS", cb_searchCodeName1.Value); // default값 설정
	ds_SMBZ010.SetColumn(ds_SMBZ010.Row, "RVBNO", trim(ed_RVBNO.Text)); // default값 설정
	ds_SMBZ010.FireEvent=true;
	
	MainGrid.ca_RVSDT.value = nowDate;
	MainGrid.ca_RVMDT.value = nowDate;
	MainGrid.ca_RVPDT.value = nowDate;
	MainGrid.ed_pyvnd.SetFocus();
	
	MainGrid.Enable = true;
}

function fn_Delete()
{
	if (MainGrid.ed_RVGMP.Text == "*"){
		alert(setPopMessage("4037"));  //  [확인] 경리마감작업이 완료되어 수정 불가능합니다.
		setMessage("4037");
		return;
	}
	if (MainGrid.ed_RVCMP.Text == "*"){
		alert(setPopMessage("4038"));  //  [확인] 납품대금 마감작업이 완료되어 수정 불가능합니다.
		setMessage("4038");
		return;
	}
	ds_SMBZ010.DeleteRow(ds_SMBZ010.currow);
	
	alert(setPopMessage("1011"));     //[알림] 화면에서만 제거되었습니다. 저장하시면 삭제됩니다.
	setMessage("1011");	
}

