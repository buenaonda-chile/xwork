 **********************************************************************/ 
function getSMBZ010_DCHK(){
	http.Sync = true;
	var sKind		= "getSMBZ010_DCHK";						
	var sMethodName = "service::pssallib/selectSMBZ010_DCHK.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_SMBZ010_DCHK=ds_SMBZ010_DCHK";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
	
	sArgument += " LIB1=" + quote(Lib1);
	sArgument += " LIB2=" + quote(Lib2);
	sArgument += " COMPS=" + quote(cb_searchCodeName1.Value);
	sArgument += " RVBNO=" + quote(ed_RVBNO.Text);

	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	http.Sync = false;
}
