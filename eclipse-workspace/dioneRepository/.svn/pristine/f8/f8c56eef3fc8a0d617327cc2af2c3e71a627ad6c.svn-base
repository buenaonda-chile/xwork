function getFZZZ002Q(){
	if(rdoggub.Value == "A"){
		if(length(trim(ed_itnbr.Value)) == 0){
			alert(setPopMessage("2017"));	// 20110103l-koowgnojeel, 2017 == 품번을 입력하세요.
			ed_itnbr.SetFocus();
			return;
		}
		
		MainGrid.BindDataset = "ds_main";
		p_seq = "0001"; 
		http.Sync = true;
		var sKind		= "getFZZZ002Q";						
		var sMethodName = "service::psfaclib/selectFZZ002Q.do";     		// 서버에서 호출 될 Method 명
		var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
		var sOutDataSet = "ds_FZZ002Q=ds_FZZ002Q";    				// 조회 후 돌려 받을 DataSet 명칭
		var sArgument   = "";
		sArgument += " LIB2=" + quote(LIB2);
		sArgument += " LIB1=" + quote(LIB1);	
		sArgument += " PINBR=" + quote(ed_itnbr.Value);
		sArgument += " STEP='1'";
		sArgument += " STEP_TXT='1'";
		sArgument += " PNO_SEQ=" + quote(p_seq);
		sArgument += " NOWDATE=" + quote(substr(nowDate,2,6));
		
		Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");
		
	}else if(rdoggub.Value == "B"){
		if (ds_SEND.GetRowCount() < 1){
			alert("[경고] 생성할 data가 없습니다. 확인바랍니다."); 
			return;
		}
		if (ds_SEND.GetRowCount() > 50){
			alert("[확인] 작업시간이 많이 소요됩니다. 건당 1분."); 
		}
		if(confirm("PSDIOLIB.FZZ002PF에 저장됩니다. 작업전 CLEAR바랍니다. 작업하시겠습니까? ") == false) return;

		//MainGrid.BindDataset = "ds_multi";

		st_stime.Text = GetDate();
		for(var i=0;i<ds_SEND.GetRowCount();i++){
			strStep = "1";
			intStep = 1;
			p_seq = right("0000"+(i+1),4); 
			http.Sync = true;
			var sKind		= "getFZZZ002Q";						
			var sMethodName = "service::psfaclib/selectFZZ002Q.do";     		// 서버에서 호출 될 Method 명
			var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
			var sOutDataSet = "ds_FZZ002Q=ds_FZZ002Q";    				// 조회 후 돌려 받을 DataSet 명칭
			var sArgument   = "";
			sArgument += " LIB2=" + quote(LIB2);
			sArgument += " LIB1=" + quote(LIB1);	
			sArgument += " PINBR=" + quote(ds_SEND.GetColumn(i,"PRTNO"));
			sArgument += " STEP='1'";
			sArgument += " STEP_TXT='1'";
			sArgument += " PNO_SEQ=" + quote(p_seq);
			sArgument += " NOWDATE=" + quote(substr(nowDate,2,6));
			Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");
			
			http.Sync = true;
			var sKind		= "setFZZZ002Q";						
			var sMethodName = "service::psfaclib/saveFZZZ002Q.do";     		// 서버에서 호출 될 Method 명
			var sInDataSet  = "ds_multi=ds_multi";            				// 자료 저장 시 보낼 DataSet 명칭
			var sOutDataSet = "";				    				// 조회 후 돌려 받을 DataSet 명칭
			var sArgument   = "";
			sArgument += " LIB2=" + quote(LIB2);
			sArgument += " LIB1=" + quote(LIB1);
			Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
			ds_multi.ClearData();	
		}
		//ds_multi.Sort("ROWNUMBER");
		st_etime.Text = GetDate();
		alert("PSDIOLIB/BZZ002PF에 저장되었습니다.");
	}
		
}

function subFZZZ002Q(dsObj)
{
	var intStep2 = 0;

	strStep = strStep + (intStep+1);

	intStep++;

	ds_tmp2.Copy(object(dsObj));
	ds_tmp.ClearData();

	for(i=0;i<ds_tmp2.rowcount;i++)
	{
		var sArgument   = "";
		
		intStep2++;
		http.Sync = true;
		sArgument += " LIB1=" + quote(LIB1);	
		sArgument += " PINBR=" + quote(ds_tmp2.GetColumn(i,"ITNBR"));
		sArgument += " STEP=" + quote(ds_tmp2.GetColumn(i,"ROWNUMBER"));
		sArgument += " STEP2=" + quote(intStep2);
		sArgument += " STEP_TXT=" + quote(makePoint(intStep) + intStep);
		sArgument += " NOWDATE=" + quote(substr(nowDate,2,6));
		sArgument += " TOPQTYPR=" + quote(ds_tmp2.GetColumn(i,"TQTYPR"));
		Transaction("subFZZZ002Q", "service::psfaclib/selecFZZZ002QSub.do", "", "ds_FZZ002QSub=ds_FZZ002QSub", sArgument, "");
		
		if(rdoggub.Value == "A"){
			ds_main.AppendDataset("ds_FZZ002QSub");
		}else if(rdoggub.Value == "B"){
			ds_multi.AppendDataset("ds_FZZ002QSub");
		}
		ds_tmp.AppendDataset("ds_FZZ002QSub");

	}

	if(object(dsObj).rowcount != 0)
	{
		subFZZZ002Q("ds_tmp");
	} else {
		if(rdoggub.Value == "A"){
			ds_main.Sort("ROWNUMBER");
			alert(setPopMessage("1004"));
			setMessage("1004");			
		} 
		
		
	}
}


function makePoint(cnt)
{
	var returnCnt = cnt -1;
	var returnValue = "";

	for(i=0; i<returnCnt;i++)
	{
		returnValue += ".";
	}
	
	return returnValue;
}

function callback(svcId, rtnCd, rtnMsg){
	// tran 실패
	if(rtnCd<0){
		alert(setPopMessage("1002"));
		setMessage("1002");
		
	}else if(svcId=="getFZZZ002Q"){
		// 조회건수가 없을경우
		if(ds_FZZ002Q.rowcount <= 0){
			if(rdoggub.Value == "A"){
				alert(setPopMessage("1003"));
				setMessage("1003");	
			}else{
				
			}
		// 정상 조회
		} else {
			if(rdoggub.Value == "A"){
				ds_main.Copy("ds_FZZ002Q");
				var nRow = ds_main.AddRow();
				ds_main.SetColumn(nRow,"STEP_TXT","0");
				ds_main.SetColumn(nRow,"PINBR","");
				ds_main.SetColumn(nRow,"ROWNUMBER","0");
				ds_main.SetColumn(nRow,"ITNBR",ds_FZZ002Q.GetColumn(0,"PINBR"));
				ds_main.SetColumn(nRow,"QTYPR","");
				ds_main.SetColumn(nRow,"UNMSR","");
				ds_main.SetColumn(nRow,"ITTYP","");
				ds_main.SetColumn(nRow,"MDATE","");
				ds_main.SetColumn(nRow,"ITDSC","");
				ds_main.SetColumn(nRow,"STEP","");
				ds_main.SetColumn(nRow,"STEP2","");
				ds_main.SetColumn(nRow,"EDATM","");
				ds_main.SetColumn(nRow,"EDATO","");
				ds_main.SetColumn(nRow,"CONNO","");
				ds_main.SetColumn(nRow,"SPCOD","");
				ds_main.SetColumn(nRow,"TQTYPR","");
				subFZZZ002Q("ds_FZZ002Q");
			}else if(rdoggub.Value == "B"){
				
				var nRow = ds_multi.AddRow();
				ds_multi.SetColumn(nRow,"STEP_TXT","0");
				ds_multi.SetColumn(nRow,"PINBR","");
				ds_multi.SetColumn(nRow,"ROWNUMBER",p_seq);
				ds_multi.SetColumn(nRow,"ITNBR",ds_FZZ002Q.GetColumn(0,"PINBR"));
				ds_multi.SetColumn(nRow,"QTYPR","");
				ds_multi.SetColumn(nRow,"UNMSR","");
				ds_multi.SetColumn(nRow,"ITTYP","");
				ds_multi.SetColumn(nRow,"MDATE","");
				ds_multi.SetColumn(nRow,"ITDSC","");
				ds_multi.SetColumn(nRow,"STEP","");
				ds_multi.SetColumn(nRow,"STEP2","");
				ds_multi.SetColumn(nRow,"EDATM","");
				ds_multi.SetColumn(nRow,"EDATO","");
				ds_multi.SetColumn(nRow,"CONNO","");
				ds_multi.SetColumn(nRow,"SPCOD","");
				ds_multi.SetColumn(nRow,"TQTYPR","");
				ds_multi.AppendDataset("ds_FZZ002Q");
				subFZZZ002Q("ds_FZZ002Q");

			}
			
			
			
			
		}
	// tran 성공	
	}else if(svcId=="setFZZZ002Q"){
		alert(setPopMessage("1001"));
		setMessage("1001");
	}	
}


