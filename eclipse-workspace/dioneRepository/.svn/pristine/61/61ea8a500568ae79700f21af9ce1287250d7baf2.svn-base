}

function getComCode(){
	var sKind		= "getComCode";						
	var sMethodName = "service::login/selectComCode.do"; 
	var sInDataSet  = "";
	var sOutDataSet = "ds_ComCode_PLNT=ds_ComCode_PLNT";
	var sArgument   = "";
		sArgument   += " CLASS='PLNT'";
		sArgument   += " CODE1=" + quote(company);		
	http.Sync=true;
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	http.Sync=false;
	ComboBoxPlant.Value=comps;
	ComboBoxPlant.Enable = false; 
}

function getDesign(){
	//MainGrid.ToolTipText = setPopMessage("2053");
	MainGrid.BkColor = G_ds_design.GetColumn(0,"BkColor");
	MainGrid.BkColor2 = G_ds_design.GetColumn(0,"BkColor2");
	MainGrid.BkSelColor = G_ds_design.GetColumn(0,"BkSelColor");
	MainGrid.Face3dColor = G_ds_design.GetColumn(0,"Face3dColor");
	MainGrid.BoldHead = G_ds_design.GetColumn(0,"BoldHead");
	MainGrid.HeadHeight = G_ds_design.GetColumn(0,"HeadHeight");
	MainGrid.Width = G_ds_design.GetColumn(0,"Width");
	MainGrid.UserData = MainGrid.Height;
	
	searchBar.Width = G_ds_design.GetColumn(0,"Width");
	searchBar.Top = G_ds_design.GetColumn(0,"SBTop");
}

function getFZME102Q(){

	if(!validationChk()){
		return;
	}

	var p_pyvnd1, p_pyvnd2, p_exp2;
	p_pyvnd1 = iif (length(trim(ed_PYVND_F.Text)) == 0, '0000', trim(ed_PYVND_F.Text));
	p_pyvnd2 = iif (length(trim(ed_PYVND_T.Text)) == 0 , '9999', trim(ed_PYVND_T.Text));
	p_exp2 = iif (length(trim(ed_EXP_T.Text)) == 0 , '9', trim(ed_EXP_T.Text));

	getCGMPARMHC();
	var bpnslt="";
	if(ds_CGMPARMHC.GetColumn(0,"SEQMON")==1){
		bpnslt=1;//setOn50 //A.ADSDT-(INT(A.ADSDT)/100*100) > MPPDT
	}else{
		bpnslt=2;//setOFF50
	}
		
	//현재년월51, M-1월 52, M+1월53
	var plnym= substr(cmb_PLYM.Value,0,6);
	var adssym = substr(ds_CGMPARMHC.GetColumn(0,"ADSDT"),0,6);
	var chkFlag="";
	
	if(plnym == adssym){
		chkFlag="51";//현재년월
	}else if(plnym == substr(addMonth(ds_CGMPARMHC.GetColumn(0,"ADSDT"),-1),0,6)){
		chkFlag="52";//M-1년월
	}else if(plnym == substr(addMonth(ds_CGMPARMHC.GetColumn(0,"ADSDT"),1),0,6)){
		chkFlag="53";//M+1년월
	}
	/*
	else{
		alert("[알림]계획년월은 CIGMA년월+1, -1까지 입력가능합니다.");
		//alert(setPopMessage("6055"));  //[알림]계획년월은 CIGMA년월+1까지 입력가능합니다.
		//setMessage("6055");	
		cmb_PLYM.SetFocus();
		return;	
	}
	*/
	//p_SQL이 null이 아닌경우만 동작!!!!
	var p_SQL1, p_SQL2;
	if((chkFlag="51" && bpnslt="2") || (chkFlag="53" && bpnslt="1")){
		p_SQL1 ="A.PPL02";
		p_SQL2 ="A.PPL02 !=0 ";
	}else if((chkFlag="51" && bpnslt="1") || (chkFlag="52" && bpnslt="2")){
		p_SQL1 ="A.PPL01";
		p_SQL2 ="A.PPL01 !=0 ";
	}else if(chkFlag="53" && bpnslt="2") {
		p_SQL1 ="A.PPL03";
		p_SQL2 ="A.PPL03 !=0 ";
	}
	
//trace(" p_SQL = "+p_SQL + "plnym = "+plnym + " adssym = "+adssym+ " chkFlag = "+chkFlag+ " bpnslt = "+bpnslt);
//trace(ds_CGMPARMHC.SaveXML());	
//alert(p_pyvnd1 + "==" + p_pyvnd2 + "==" + ed_EXP_F.Text + "==" + p_exp2);

	var sKind		= "getFZME102Q";						
	var sMethodName = "service::psfaclib/selectFZME102Q.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                            				// 자료 저장 시 보낼 DataSet 명칭
	var	sOutDataSet = "ds_FZME102Q=ds_FZME102Q";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
		sArgument += "@LIB1=" + quote(Lib1);
		sArgument += "@LIB2=" + quote(Lib2);
		sArgument += "@COMPS=" + quote(ComboBoxPlant.Value); //사업부
		sArgument += "@PLYM=" + quote(substr(cmb_PLYM.Value,0,6)); //계획년월
		sArgument += "@PYVNDF=" + quote(p_pyvnd1); //득의선FR
		sArgument += "@PYVNDT=" + quote(p_pyvnd2); //득의선TO
		sArgument += "@EXPF=" + quote(ed_EXP_F.Text); //내수구분FR
		sArgument += "@EXPT=" + quote(p_exp2); //내수구분TO
		sArgument += "@PLSQL1=" + quote(p_SQL1); //생산계획SQL
		sArgument += "@PLSQL2=" + quote(p_SQL2); //생산계획SQL
	var str = dialog("Common::PGMLOAD_FAC.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");
	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
		setMessage("1004");
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));
		setMessage("1003");	
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}

}

