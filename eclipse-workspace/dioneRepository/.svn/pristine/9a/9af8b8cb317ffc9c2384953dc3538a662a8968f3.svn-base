<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
    <!-- [EN] Logger Implementation Definition -->
    <!-- [JP] Logger実装定義 -->
    <settings>
        <!-- [JP] MyBatisのデフォルトLoggerはSLF4Jです -->
        <!-- [EN] Default Logger of MyBatis is SLF4J -->
        <setting name="logImpl" value="LOG4J2" />
        <!-- JDBC type in the case that the field domain is null (Can not be changed) -->
        <setting name="jdbcTypeForNull" value="NULL" />
        <!-- Scope of local cache (Can not be changed) -->
        <setting name="localCacheScope" value="STATEMENT" />
    </settings>

