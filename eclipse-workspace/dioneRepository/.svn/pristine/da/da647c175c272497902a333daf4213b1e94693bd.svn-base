function getSVDE140Q(){
	//사업부별 공통ID(as400 USERID)로 실행
	// var iDget = getRemotecallID(comps);
	var iDget = getRemotecallID("E1");
	if (iDget != "ERROR"){
		callUser = iDget;
		callPass = iDget;
	}else{
		alert(setPopMessage("1444"));
		setMessage("1444");	
		return;
	}
	SetWaitCursor(true);
	SetCapture();
	//리모트콜 실행(SALE210RC용)
	// param = ComboBoxPlant.Value;
	param = "E1";
	for(i=length(param)+1;i<=40;i++){
		param += " ";
	}
	param += callUser;	
	for(j=1;j<=10-length(callUser);j++){
		param += " ";
	}
	param += '000';
	
	WTA_RemoteCall0.RC_SetAttrValue(0,"2984",exeTime);
	
	// 연결 (접속아이피 , 아이디, 비밀번호)	
	var rtn = WTA_RemoteCall0.RC_Connect(callIp,callUser,callPass);

	//접속성공시 
	if(rtn != 0){
		//프로그램 Call (라이브러리명, 프로그램명, 인자값, 크기);
		var rtn2 = WTA_RemoteCall0.RC_RmtPgmCall(pgmLib,pgmName,param,pgmSize);
		//정상적으로 수행했을 시
		if(rtn2 != 0){
			ReleaseCapture();
			SetWaitCursor(false);
			if (substr(WTA_RemoteCall0.RC_GetReturnParm(1),50,3) == "999") {
				WTA_RemoteCall0.RC_Disconnect();	//연결종료
				var orgcomps="";
				orgcomps=ComboBoxPlant.Value;
				/*
				if(ComboBoxPlant.Value=="E1"){
					orgcomps ="PE";
				}
				else{
					orgcomps ="PS";
				}
				*/
				var sKind		= "getHOUSE";		
				var sMethodName = "service::pscomlib/selectMST040PF_HOUSE.do"; 
				var sInDataSet  = "";
				var sOutDataSet = "ds_MST040PF_HOUSE=ds_MST040PF_HOUSE";
				var sArgument   = "";
					sArgument += " LIB2=" + quote(Lib2);
					sArgument += " GCOMPS=" + quote(orgcomps); //회사(PE/PS)
					//sArgument += " COMPS=" + quote(ComboBoxPlant.Value); //사업부
					sArgument += " COMPS=" + quote("E1"); //사업부
				http.Sync=true;
				Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
				http.Sync=false;
				//if (ds_MST040PF_HOUSE.rowcount > 0){
				var arrHouse = "('";
				for(i=0; i< ds_MST040PF_HOUSE.rowcount; i++){
					if(i == ds_MST040PF_HOUSE.rowcount-1){
						arrHouse = arrHouse+ds_MST040PF_HOUSE.GetColumn(i,"MAJCD_H") + "')";
					}else{
						arrHouse = arrHouse+ds_MST040PF_HOUSE.GetColumn(i,"MAJCD_H") + "','";
					}
				}
				var sKind		= "getSVDE140Q";						
				var sMethodName = "service::pssallib/selectSVDE140Q.do";     		// 서버에서 호출 될 Method 명
				var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
				var sOutDataSet = "ds_SVDE140Q=ds_SVDE140Q";    				// 조회 후 돌려 받을 DataSet 명칭
				var sArgument   = "";
				sArgument += "@LIB1=" + quote(Lib1);
				sArgument += "@LIB2=" + quote(Lib2);
				//sArgument += "@COMPS=" + quote(ComboBoxPlant.Value); //사업부
				sArgument += "@COMPS=" + quote("E1"); //사업부
				sArgument += "@HOUSE=" + quote(arrHouse); //사업부
				var str = dialog("Common::PGMLOAD_SAL.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");
				if (str == 'T'){  //정상조회
					alert(setPopMessage("1004"));
					setMessage("1004");
					for(var i=0;i<ds_SVDE140Q.rowcount;i++){
						if(ds_SVDE140Q.GetColumn(i,"PSPNO") == ds_SVDE140Q.GetColumn(i+1,"PSPNO")){
							ds_SVDE140Q.SetColumn(i+1,"SUBQY",0);
							ds_SVDE140Q.SetColumn(i+1,"PSQTY",0);
							ds_SVDE140Q.SetColumn(i+1,"PLNQY",0);
							ds_SVDE140Q.SetColumn(i+1,"SILQY",0);
							ds_SVDE140Q.SetColumn(i+1,"DALSU",0);
				
							ds_SVDE140Q.SetColumn(i+1,"SUBQYT",0);
							ds_SVDE140Q.SetColumn(i+1,"PSQTYT",0);
							ds_SVDE140Q.SetColumn(i+1,"PLNQYT",0);
							ds_SVDE140Q.SetColumn(i+1,"SILQYT",0);
							ds_SVDE140Q.SetColumn(i+1,"DALSU",0);
							ds_SVDE140Q.SetColumn(i+1,"DALSU",0);
				
						}
					}
				}else if (str == 'B'){   //data값이 없을때
					alert(setPopMessage("1003"));
					setMessage("1003");	
				}else if (str == 'E'){   //error발생시
					alert(setPopMessage("1002"));
					setMessage("1002");	
				}
			}else if (substr(WTA_RemoteCall0.RC_GetReturnParm(1),50,3) == "888") {
				WTA_RemoteCall0.RC_Disconnect();	//연결종료
				var ruser = trim(substr(WTA_RemoteCall0.RC_GetReturnParm(1),60,10));
				var rcount = toNumber(substr(WTA_RemoteCall0.RC_GetReturnParm(1),70,2))-1;
				alert("사용자("+ruser+")외 "+rcount+"명이 사용중입니다. 작업이 불가능 합니다.");
				return false;
			}else if (substr(WTA_RemoteCall0.RC_GetReturnParm(1),50,3) == "777") {
				WTA_RemoteCall0.RC_Disconnect();	//연결종료
				alert(setPopMessage("1002")); //[경고] 처리 중 오류가 발생하였습니다.
				setMessage("1002");	
				return false;
			}else {
				WTA_RemoteCall0.RC_Disconnect();	//연결종료
				alert(setPopMessage("1015")); //[경고] AS400 CL 처리 중 오류가 발생하였습니다.
				setMessage("1015");	
				return false;
			}
		}else{
			ReleaseCapture();
			SetWaitCursor(false);
			var bstrErrorMessage = WTA_RemoteCall0.bstrErrorMessage;
			WTA_RemoteCall0.RC_Disconnect();	//연결종료
			//오류 코드 경고창
			alert("에러 메세지=======>" + bstrErrorMessage);
		}
	//접속실패시
	}else{
		ReleaseCapture();
		SetWaitCursor(false);
		var ulErrorCode = WTA_RemoteCall0.ulErrorCode;
		WTA_RemoteCall0.RC_Disconnect();	//연결종료
		//오류 코드 경고창
		alert("접속 실패 오류=======>" + ulErrorCode);
	}

}

