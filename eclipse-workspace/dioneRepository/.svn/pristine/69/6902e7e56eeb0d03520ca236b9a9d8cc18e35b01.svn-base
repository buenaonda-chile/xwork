/********************************************************************** 
 * 내용설명 : 조회
 **********************************************************************/ 
function getOgm321(){

	var sdpcd="";
	var sdpcdVal=trim(cmbsdpcd.Value);
	if(length(sdpcdVal)!=0){
		sdpcd=replace(sdpcdVal,"*","%");
	}

	var sKind		= "getOgm321";						
	var sMethodName = "service::psogmlib/selectOgm321.do"; 
	var sInDataSet  = "";
	var sOutDataSet = "ds_ogm321=ds_ogm321";
	var sArgument   = "";
		sArgument += " LIB2=" + quote(Lib2);
		sArgument += " LIB3=" + quote(LIB3);
		sArgument += " COMPS=" + quote(comps1);
		sArgument += " SDPCD=" + quote(sdpcd);
		sArgument += " EFFCD=" + quote(trim(lbleffcd.Text));
		sArgument += " WNDATS=" + quote(cmbWnDatS.Value); //검색기간START
		sArgument += " WNDATE=" + quote(cmbWnDatE.Value); //검색기간END
	http.Sync = true;
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	http.Sync = false;
	if (ds_ogm321.rowcount() <=0){
		alert(setPopMessage("1003"));
	}
}


/********************************************************************** 
 * 내용설명 : Transaction 리턴후 실행 함수
 **********************************************************************/ 
function callback(svcId, rtnCd, rtnMsg){
	// tran 실패
	if(rtnCd<0){
		alert(setPopMessage("1002"));
		setMessage("1002");
	// tran 성공	
	}else if(svcId=="getOgm321"){
	// 조회건수가 없을경우
		if(ds_ogm321.rowcount <= 0){
			alert(setPopMessage("1003"));
			setMessage("1003");	
		// 정상 조회
		} else {
			alert(setPopMessage("1004"));
			setMessage("1004");				
		}
		// tran 성공	
	}else if(svcId=="setUserList"){
		alert(setPopMessage("1001"));
		setMessage("1001");
	}
}

