						substr(trim(a008)+space(3),0,3) +   //차종
						space(2) +
						qrData2;                            //일련번호
			cmmB570.Output = ESC1 + "RB00;"+ barD + LF;  cmmB570.Output = ext_VB_CHR(0); //39 BARCODE값  
			
			//문자값MOVE
			cmmB570.Output = ESC1 + "RC000;"+ a007 + LF;  cmmB570.Output = ext_VB_CHR(0); // 서열코드
			if(trim(a006) == ""){
				cmmB570.Output = ESC1 + "RC011;"+ substr(a013,2,6) + LF;  cmmB570.Output = ext_VB_CHR(0); // 생산일자
			}else{
				cmmB570.Output = ESC1 + "RC011;"+ a005 + LF;  cmmB570.Output = ext_VB_CHR(0); // KE 일자코드
			}
			cmmB570.Output = ESC1 + "RC003;"+ qrData2 + LF;  cmmB570.Output = ext_VB_CHR(0); // seq
			cmmB570.Output = ESC1 + "RC004;"+ a010 + LF;  cmmB570.Output = ext_VB_CHR(0); // 업체품번
			cmmB570.Output = ESC1 + "RC006;"+ a002 + LF;  cmmB570.Output = ext_VB_CHR(0); // 덴소품번
			//라벨 프린터 발행명령
			cmmB570.Output = ESC1 + ("XS;I,0001,0002C7100") + LF; cmmB570.Output = ext_VB_CHR(0);
			break;
		case "J" :    //1300==>기아  hkmc에서 바코드값 변경요청으로 신규생성 (기존case3 copy)
