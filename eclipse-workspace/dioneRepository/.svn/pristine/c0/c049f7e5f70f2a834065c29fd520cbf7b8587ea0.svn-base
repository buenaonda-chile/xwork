			cmmB570.Output = ESC1 + "PC000;0130,0055,1,15,E,00,B" + LF;  cmmB570.Output = ext_VB_CHR(0); // 서열코드
			cmmB570.Output = ESC1 + "PC011;0255,0050,1,1,H,00,B" + LF;  cmmB570.Output = ext_VB_CHR(0); // 일자코드/일자
			cmmB570.Output = ESC1 + "PC003;0420,0117,05,1,A,00,B" + LF;  cmmB570.Output = ext_VB_CHR(0); // seq
			cmmB570.Output = ESC1 + "PC004;0130,0085,05,05,K,00,B" + LF; cmmB570.Output = ext_VB_CHR(0); // 업체품번
			cmmB570.Output = ESC1 + "PC006;0130,0110,05,05,K,00,B" + LF; cmmB570.Output = ext_VB_CHR(0); // 덴소품번
			var barD = "P068"+ 
						substr(trim(a010)+space(15),0,15) + //부품번호
						substr(trim(a007)+space(5),0,5) +   //서열코드
