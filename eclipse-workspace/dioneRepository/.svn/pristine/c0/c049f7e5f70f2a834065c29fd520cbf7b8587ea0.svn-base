function BarDataPrint(i, j, sqn)
{
	setSleepTime(100);  //0.1초 Sleep	
	cmmB570.InBufferCount = 0;
	cmmB570.OutBufferCount =0;

	//PE품번,10자리
	var qrData1 = substr(ds_FPQE140Q_5.GetColumn(i,"PRTNOL")+space(10),0,10);
	//발행SEQ,3자리
	var qrData2 = right("000"+sqn,3);
	//생산지시일,6자리
	var qrData3 = substr(ds_FPQE140Q_5.GetColumn(i,"PRODYL")+space(6),0,6);
    //업체품번,16자리
    var qrData4 = substr(ds_FPQE140Q_5.GetColumn(i,"CPRTNL")+space(16),0,16);
    //발행구분,2/3점CHECK
    var qrData5 = substr(ds_FPQE140Q_5.GetColumn(i,"CKGUBL")+space(1),0,1);
	var qrData = qrData1+qrData2+qrData3+qrData4+qrData5;
	
	
	var CheckCusno = ds_FPQE140Q_5.GetColumn(i,"CUSNO");
	var CheckLabel;
	switch (CheckCusno)
	{
		case "1100" : //현대자동자(울산)
			CheckLabel = "1";
			break;
		case "1200" : //현대자동자(전주)
			CheckLabel = "2";
			break;
		case "1300" :  //1300==>기아 MOTOR,1
			CheckLabel = "3";
			break;
		case "1400" :  //400==>기아 광주
			CheckLabel = "3";
			break;
		case "2000" :  //2000==>기아A/S
			CheckLabel = "4";
			break;
		case "1500" :  //1500==>GM DAT인천
			CheckLabel = "5";
			break;
		case "1600" :  //1600==>쌍용
			CheckLabel = "6";
			break;
		case "1900" :
			CheckLabel = "7";
			break;
		case "2100" :   //2100==>GM DAT인천(A/S)
			CheckLabel = "8";
			break;
		case "2200" :   //2200==>쌍용A/S
			CheckLabel = "9";
			break;
		case "4200" :  //4600=
			CheckLabel = "B";
			break;
		case "4600" :
			CheckLabel = "B";
			break;
		case "6100" :
			CheckLabel = "B";
			break;
		case "5600" :
			CheckLabel = "5";
			break;
		case "5800" :  //5800 SUZUKI 
			CheckLabel = "D";
			break;
		case "5900" :  //5900 MAZUDA
			CheckLabel = "F";
			break;
		case "5700" :
			CheckLabel = "2";
			break;
		case "6000" :
			CheckLabel = "T";
			break;
	}
	if (rdoggub.Value == "2") CheckLabel = "H";  ////SmartKey ECU LABEL 70mm
	if (cb_2Dbar.Value)  CheckLabel = "I";  //2D Barcode(Datamatrix)
	if (cb_1300.Value)  CheckLabel = "J";  // 1300업체 변경요청 Sample 2015-03-16 (임시로적용하고 4월부터 1300업체 전체적용)
	if (cb_jfcar.Value)  CheckLabel = "K";  // 기아 JF차종 적용 Sample 2015-03-17 (임시로적용하고 6월부터 적용)
	var barBuf;
	var a002 = ds_FPQE140Q_5.GetColumn(i,"PRTNO");  // DNKE품번
	var a005 = ds_FPQE140Q_5.GetColumn(i,"BRDCD");  // KE일자코드
	var a006 = ds_FPQE140Q_5.GetColumn(i,"MLTCD");  // 업체일자코드
	var a007 = ds_FPQE140Q_5.GetColumn(i,"MKALC");  // ALC코드
	var a008 = ds_FPQE140Q_5.GetColumn(i,"CARCD");  // 차종코드
	var a010 = ds_FPQE140Q_5.GetColumn(i,"CPRTN");  //업체품번
	var a013 = ds_FPQE140Q_5.GetColumn(i,"PNYMD");  // 계획일자
	switch (CheckLabel)
	{
		case "1" : //현대자동자(울산)
			if(cb_MOBIS.Value = true || trim(ds_FPQE140Q_5.GetColumn(i,"MKLOC")) == "AS"){
				barBuf = substr(a007+space(4),0,4);
			}else{
				barBuf = a007+a005;
			}
			cmmB570.Output = ESC1 + ("C") + LF;  cmmB570.Output = ext_VB_CHR(0); //BUFFER CLEAR
			
			//X좌표 왼쪽 여백 1.5MM부분이 X좌표원점임
			cmmB570.Output = ESC1 + ("XB00;0050,0010,B,1,03,03,06,06,03,0,0050,+0000000000,0,00") + LF; cmmB570.Output = ext_VB_CHR(0);//39 BARCODE FORMAT,X:5.00MM,Y=1.0MM
			cmmB570.Output = ESC1 + ("XB01;0390,0010,T,L,04,A,0=") + qrData + LF;    cmmB570.Output = ext_VB_CHR(0);  //QR BARCODE FORMAT,X:0400,Y:=4.00MM,Y:=1.0MM
			
			//하기 출력할 문자값,좌표및 특성결정
			cmmB570.Output = ESC1 + "PC000;0355,0010,1,1,A,11,B" + LF; cmmB570.Output = ext_VB_CHR(0);// ALC코드(우측 )
			cmmB570.Output = ESC1 + "PC001;0050,0110,1,1,J,00,B" + LF; cmmB570.Output = ext_VB_CHR(0);//ALC코드 좌측,Y좌표는 문자가 끝나는 제일 마지막 좌표,문자크기 2행지정시는 0.5배단위로
			cmmB570.Output = ESC1 + "PC011;0190,0085,15,1,C,00,B" + LF; cmmB570.Output = ext_VB_CHR(0); //PE일자코드(가운데영문세자리),Y좌표는 문자가 끝나는 제일 마지막 좌표
			cmmB570.Output = ESC1 + "PC003;0420,0117,05,1,A,00,B" + LF; cmmB570.Output = ext_VB_CHR(0); //QR일련번호
			cmmB570.Output = ESC1 + "PC004;0180,0107,05,05,K,00,B" + LF; cmmB570.Output = ext_VB_CHR(0); //업체품번
			cmmB570.Output = ESC1 + "PC006;0180,0128,05,05,K,00,B" + LF; cmmB570.Output = ext_VB_CHR(0); //풍성품번

			cmmB570.Output = ESC1 + "RB00;"+barBuf + LF;  cmmB570.Output = ext_VB_CHR(0);//39 BARCODE값      
			
			//문자값MOVE
			cmmB570.Output = ESC1 + "RC000;"+a007 + LF;  cmmB570.Output = ext_VB_CHR(0);//MKALC       ALC코드(우측 )
			cmmB570.Output = ESC1 + "RC001;"+a007 + LF;  cmmB570.Output = ext_VB_CHR(0);//MKALC       ALC코드 좌측
			cmmB570.Output = ESC1 + "RC011;"+a005 + LF;  cmmB570.Output = ext_VB_CHR(0);//BRDCD
			cmmB570.Output = ESC1 + "RC003;"+qrData2 + LF;  cmmB570.Output = ext_VB_CHR(0); //           QR일련번호
			cmmB570.Output = ESC1 + "RC004;"+a010 + LF;  cmmB570.Output = ext_VB_CHR(0);//CPRTN      업체품번
			cmmB570.Output = ESC1 + "RC006;"+a002 + LF;  cmmB570.Output = ext_VB_CHR(0);//PRTNO      DNKE품번
			
			//라벨 프린터 발행명령
			cmmB570.Output = ESC1 + ("XS;I,0001,0002C7100") + LF;  cmmB570.Output = ext_VB_CHR(0);
			break;
		case "2" : //현대자동자(전주)
			barBuf = a007+a005;
			cmmB570.Output = ESC1 + ("C") + LF;  //BUFFER CLEAR
			cmmB570.Output = ext_VB_CHR(0);
		
			//X좌표 왼쪽 여백 1.5MM부분이 X좌표원점임
									
			cmmB570.Output = ESC1 + ("XB00;0050,0010,B,1,03,03,06,06,03,0,0050,+0000000000,0,00") + LF; //39 BARCODE FORMAT,X:5.00MM,Y=1.0MM
			cmmB570.Output = ext_VB_CHR(0);
