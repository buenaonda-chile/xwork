			cmmB570.Output = ESC1 + "RB00;"+ barBuf + LF; cmmB570.Output = ext_VB_CHR(0); //39 BARCODE값  
			
			//문자값MOVE
			cmmB570.Output = ESC1 + "RC000;"+a007 + LF; cmmB570.Output = ext_VB_CHR(0); //MK ALC
			cmmB570.Output = ESC1 + "RC001;"+a010 + LF; cmmB570.Output = ext_VB_CHR(0); //업체품번  
			cmmB570.Output = ESC1 + "RC002;"+a013 + LF; cmmB570.Output = ext_VB_CHR(0); //계획일자
			cmmB570.Output = ESC1 + "RC003;"+a002 + LF; cmmB570.Output = ext_VB_CHR(0);  //풍성품번  
			cmmB570.Output = ESC1 + "RC004;"+qrData2 + LF; cmmB570.Output = ext_VB_CHR(0); // QR일련번호
			
			//라벨 프린터 발행명령
			cmmB570.Output = ESC1 + ("XS;I,0001,0002C7100") + LF; cmmB570.Output = ext_VB_CHR(0);
