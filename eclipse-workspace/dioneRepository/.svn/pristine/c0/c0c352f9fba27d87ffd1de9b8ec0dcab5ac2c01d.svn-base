function getComCode(){
	http.Sync 	= true;
	var sKind		= "getComCode";						
	var sMethodName = "service::login/selectComCode.do"; 
	var sInDataSet  = "";
	var sOutDataSet = "ds_ComCode_PLNT=ds_ComCode_PLNT";
	var sArgument   = "";
		sArgument   += " CLASS='PLNT'";
		sArgument   += " CODE1=" + quote(company);		
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	http.Sync 	= false;
	ds_VComCode_PLNT.Copy("ds_ComCode_PLNT");

	cb_searchCodeName1.Value = comps;
}

/********************************************************************** 
 * 내용설명 : 발행
 * 주요변수 : 
 **********************************************************************/ 
 function prtinsabontoR()
 {
	var Lib9;
	if (cb_searchCodeName1.Value == "C1"){
		Lib9="SOGLIBPSC";
	}else if (cb_searchCodeName1.Value == "S1"){
		Lib9="SOGLIBPSS";
	}else if (cb_searchCodeName1.Value == "H1"){
		Lib9="SOGLIBPSH";
	}else if (cb_searchCodeName1.Value == "E1"){
		Lib9="SOGLIBPEC";
	}
	var dbpf;
	if(cb_plngb.Value = "A"){
		dbpf = "OMD020GF";
	}else if(cb_plngb.Value = "B"){
		dbpf = "OMD020NF";
	}else if(cb_plngb.Value = "C"){
		dbpf = "OMD020GYF";
	}
	http.Sync 	= true;
	var sKind		= "getinsabontoR";						
	var sMethodName = "service::sample/selectinsabontoR.do"; 
	var sInDataSet  = "";
	var sOutDataSet = "ds_insabontoR=ds_insabontoR";
	var sArgument   = "";
	sArgument += " LIB2=" + quote(Lib2);
	sArgument += " LIB1=" + quote(Lib1);
	sArgument += " LIB3=" + quote(Lib3);
	sArgument += " LIB9=" + quote(Lib9);
	sArgument += " COMPS=" + quote(cb_searchCodeName1.Value);
	sArgument += " DBPF=" + quote(dbpf);
   
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	http.Sync	= false;
	
	//ubi0.setJrfFileDir(G_baseURL+"/ui/PSSALLIB/report/");
	//C:/eclipse/workspace/DENSO/WebContent
	ubi0.setJrfFileDir(G_baseURL+"/ui/Sample/REPORT/");
	
	if (cb_searchCodeName1.Value == "C1"){
		ubi0.setJrfFileName("insabontoR1.jrf");
	}else{
		ubi0.setJrfFileName("insabontoR.jrf");
	}
	ubi0.SetDataset("ds_insabontoR", "ds_insabontoR");	
	ubi0.SetArgument("TITLE", trim(ed_title.Text));
//	ubi0.SetArgument("WRKDT", parent.search_wrkdt.Text);	
//	ubi0.SetArgument("CURRENT_TIME", substr(Getdate(),8,2)+":"+substr(Getdate(),10,2)+":"+substr(Getdate(),12,2));
	ubi0.Retrieve();
}
