 **********************************************************************/ 
function on_LoadCompleted(obj){
	getTitle();	
	getComCode();
	ed_syyyymmdd.Value = nowDate;	
	gFcOnTopButtonColor("1"); //상단 버튼 활성화 여부 (기본:0,조회:1,변경:2)		
	fn_Search();	
}


/********************************************************************** 
 * 개 발 자 : 김정현 
 * 개발일자 : 2012-10-26
 * 내용설명 : 공통코드 조회
 * 주요변수 : 
 **********************************************************************/ 
function getComCode(){

	http.Sync 	= true;

	var sKind		= "getComCode";						
	var sMethodName = "service::login/selectComCode.do"; 
	var sInDataSet  = "";
	var sOutDataSet = "ds_ComCode_PLNT=ds_ComCode_PLNT";
	var sArgument   = "";
		sArgument   += " CLASS='PLNT'";
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");

	ds_ComCode_PLNT.Filter("code2=='" + G_ds_user.GetColumn(0,"PLANT_CD") +"'");

	ds_ppmi001mCode.InsertRow(0);
	ds_ppmi001mCode.SetColumn(0,"ROWGB","");
	ds_ppmi001mCode.SetColumn(0,"ROWNM","[전체]");
	//cmb_rowgb.index = 0;	

}


/********************************************************************** 
 * 개 발 자 : 김정현 
 * 개발일자 : 2012-10-26
 * 내용설명 : 화면조회
 * 주요변수 : 
 **********************************************************************/ 
function getOBSE03(){
			
	var sKind		= "getOBSE03_D";						
	var sMethodName = "service::psogmlib/selectOBSE03_D.do";     	// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "DS_OBSE03_D=DS_OBSE03_D";    		// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
	
	//sArgument += " LIB=" + quote(Lib);
	sArgument += " ORDAT=" + quote(ed_syyyymmdd.Value);
	sArgument += " DAYGU=D";

	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");	
				
	var sKind		= "getOBSE03_N";						
	var sMethodName = "service::psogmlib/selectOBSE03_N.do";     	// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "DS_OBSE03_N=DS_OBSE03_N";    		// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
	
	//sArgument += " LIB=" + quote(Lib);
	sArgument += " ORDAT=" + quote(ed_syyyymmdd.Value);
	sArgument += " DAYGU=N";

	//alert(sArgument);

	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");					
}


/********************************************************************** 
 * 개 발 자 : 김정현 
 * 개발일자 : 2012-10-06
 * 내용설명 : 엑셀파일로 저장
 * 주요변수 : 
 **********************************************************************/ 
function setExcel()
{
	var fName   = '\\' + ed_syyyymmdd.Value + "_전광판Schedule";
	var arrSheets  =  Array();  

	arrSheets[0]  = "Grid1|야간작업Schedule";
	arrSheets[1]  = "Grid0|주간작업Schedule";
	createExcelSheetS2(FileDialog0, fName ,arrSheets);
}

/********************************************************************** 
 * 개 발 자 : 김정현 
 * 개발일자 : 2012-10-26
 * 내용설명 : Transaction 리턴후 실행 함수
 * 주요변수 : 
 * 			- sMethodName	Transaction 고유 ID
 * 			- ErrorCode 	에러코드
 * 			- ErrorMSG 		에러 메세지
 **********************************************************************/ 
function callback(svcId, rtnCd, rtnMsg){
	// tran 실패
	if(rtnCd<0){
		alert(setPopMessage("1002"));
		setMessage("1002");
		
	}else {
		DS_OBSE03_D.rowposition = -1;
		DS_OBSE03_N.rowposition = -1;
	}
}

