/********************************************************************** 
 * 내용설명 : 화면 조회 
 **********************************************************************/ 
function getHAYZ051(){
	
	if(!validationChk()){
		return;
	}
	
	if(parseInt(substr(cmb_PTJYM.Value,4,2)) < 4){
		//alert(parseInt(substr(today(),0,4))-1);
		cmb_PTJYM.Value=parseInt(substr(cmb_PTJYM.Value,0,4))-1+"1231";
	}else{
		cmb_PTJYM.Value=substr(cmb_PTJYM.Value,0,4)+"1231";
	}

	var p_PTJYM="";
	p_PTJYM=substr(cmb_PTJYM.Value,0,4)+"13";
	
	var sKind		= "getHAYZ051";						
	var sMethodName = "service::pshumlib/selectHAYZ051.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                            				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_HAYZ051=ds_HAYZ051";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
		sArgument += "@LIB1=" + quote(Lib1);
		sArgument += "@LIB2=" + quote(Lib2);
		sArgument += "@LIB3=" + quote(Lib3);
		sArgument += "@COMPS=" + quote(ComboBoxPlant.Value);			
	var str = dialog("Common::PGMLOAD_HUM.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");	
	http.Sync = false;
	
	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
		setMessage("1004");	
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));
		setMessage("1003");	
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}
}

/********************************************************************** 
 * 내용설명 : 저장
 * 주요변수 : 
 * 			
 **********************************************************************/ 
 function setHAYZ051(){
 
 	if(!validationChk()){
		return;
	}
	var sKind		= "setHAYZ051";						
	var sMethodName = "service::pshumlib/saveHAYZ051.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "ds_HAYZ051=ds_HAYZ051:U";                      // 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_HAYZ051_CHECK=ds_HAYZ051_CHECK";							// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
		sArgument += " LIB1=" + quote(Lib1);
		sArgument += " LIB2=" + quote(Lib2);
		sArgument += " LIB3=" + quote(Lib3);
		sArgument += " COMPS=" + quote(ComboBoxPlant.Value);
	http.Sync=true;
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");
	http.Sync=false;
 }

/********************************************************************** 
 * 내용설명 : Transaction 리턴후 실행 함수
 * 주요변수 : 
 * 			- sMethodName	Transaction 고유 ID
 * 			- ErrorCode 	에러코드
 * 			- ErrorMSG 		에러 메세지
 **********************************************************************/ 
function callback(svcId, rtnCd, rtnMsg){
	if (ds_HAYZ051_CHECK.GetRowCount() > 0){			
		for(var i=0;i<ds_HAYZ051_CHECK.GetRowCount();i++){		
			alert("사번 : " +ds_HAYZ051_CHECK.GetColumn(i,"SABUN") +" 횟수 : " +ds_HAYZ051_CHECK.GetColumn(i,"LSECNT") + " = " + ds_HAYZ051_CHECK.GetColumn(i,"RTMSG"));
		}
		getHAYZ051();        //error항목이 존재시에는 재조회		
		return;		
	}// tran 실패
	if(rtnCd<0){
		alert(setPopMessage("1002"));
		setMessage("1002");
	}else{
		alert(setPopMessage("1001"));
		setMessage("1001");	
	}
}

