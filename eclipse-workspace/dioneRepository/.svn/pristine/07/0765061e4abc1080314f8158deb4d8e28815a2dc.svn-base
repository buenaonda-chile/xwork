function ds_HAYZ051_CanColumnChange(obj,nRow,strColumnID,varOldValue,varNewValue,nPivotIndex)
{
	http.usewaitcursor = false;
	if (strColumnID == "SABUN"){//작업자 확인

		if(varNewValue!=""){
			//인사마스터
			http.Sync = true;
			var sKind		= "getHMD020PF";		
			var sMethodName = "service::pscomlib/selectINSAMasterPopup.do"; 
			var sInDataSet  = "";
			var sOutDataSet = "ds_HMD020PF=ds_Select";
			var sArgument   = "";
				sArgument  += " LIB2=" + quote(Lib2);
				sArgument  += " LIB3=" + quote(Lib3);
				sArgument  += " COMPS=" + quote(ComboBoxPlant.Value); //사업부
				sArgument  += " SABUN=" + quote(varNewValue);
			Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
			http.Sync = false;
			if (ds_HMD020PF.GetRowCount() < 1){
				alert(setPopMessage("4062")); //[확인] 사용자가 존재하지 않습니다.
				setMessage("4062");
				return;
			}
			KORNM=ds_HMD020PF.GetColumn(0,"A2");
			DPTNM=ds_HMD020PF.GetColumn(0,"A3");
		}
	}
	http.usewaitcursor = true;		
}

function ds_HAYZ051_OnColumnChanged(obj,nRow,strColumnID,varOldValue,varNewValue,nPivotIndex)
{
	http.usewaitcursor = false;
	if (strColumnID == "SABUN"){
		if(varNewValue!=""){
			ds_HAYZ051.SetColumn(nRow, "KORNM", KORNM); // 성명
			ds_HAYZ051.SetColumn(nRow, "DPDPNM", DPTNM); // 부서명 		
			
		}else {
			ds_HAYZ051.SetColumn(nRow, "KORNM", ""); // 성명
			ds_HAYZ051.SetColumn(nRow, "DPDPNM",""); // 부서명
		}
	}
	if (strColumnID == "LSECNT"){
		if(length(trim(varNewValue)) == 0){
			ds_HAYZ051.SetColumn(nRow, "LSECNT", 1); // 횟수
			
		}
	}if (strColumnID == "LSESIZ"){
		if(length(trim(varNewValue)) == 0){
			ds_HAYZ051.SetColumn(nRow, "LSESIZ", 0); // 면적
			
		}
	}if (strColumnID == "LSEDAS"){
		if(length(trim(varNewValue)) == 0){
			ds_HAYZ051.SetColumn(nRow, "LSEDAS", 0); // 계약시작일
			
		}
	}if (strColumnID == "LSEDAE"){
		if(length(trim(varNewValue)) == 0){
			ds_HAYZ051.SetColumn(nRow, "LSEDAE", 0); // 계약종료일
			
		}
	}if (strColumnID == "LSEMTT"){
		if(length(trim(varNewValue)) == 0){
			ds_HAYZ051.SetColumn(nRow, "LSEMTT", 0); // 월세액
			
		}
	}if (strColumnID == "LSEMTD"){
		if(length(trim(varNewValue)) == 0){
			ds_HAYZ051.SetColumn(nRow, "LSEMTD", 0); // 공제금액
			
		}
	}
	http.usewaitcursor = true;		
}
