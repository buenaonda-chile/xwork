 **********************************************************************/ 
function callback(svcId, rtnCd, rtnMsg){
	if (svcId = "setSVHE304UC"){
		if (ds_SVHE304UC_CHECK.GetRowCount() > 0){			
			for(var i=0;i<ds_SVHE304UC_CHECK.GetRowCount();i++){		
				alert(ds_SVHE304UC_CHECK.GetColumn(i,"PYVND") + " , "
					+ ds_SVHE304UC_CHECK.GetColumn(i,"PSPNO") + " = "
					+ ds_SVHE304UC_CHECK.GetColumn(i,"RTMSG"));	
			ds_SVHE304UC_2.AddRow();
			ds_SVHE304UC_2.SetColumn(ds_SVHE304UC_2.row,"SPACE",'4');
			ds_SVHE304UC_2.SetColumn(ds_SVHE304UC_2.row,"PYVND",ds_SVHE304UC_CHECK.GetColumn(i,"PYVND"));
			ds_SVHE304UC_2.SetColumn(ds_SVHE304UC_2.row,"PSPNO",ds_SVHE304UC_CHECK.GetColumn(i,"PSPNO"));
			ds_SVHE304UC_2.SetColumn(ds_SVHE304UC_2.row,"MKPNO",ds_SVHE304UC_CHECK.GetColumn(i,"MKPNO"));
			ds_SVHE304UC_2.SetColumn(ds_SVHE304UC_2.row,"DOCCD",ds_SVHE304UC_CHECK.GetColumn(i,"DOCCD"));
			ds_SVHE304UC_2.SetColumn(ds_SVHE304UC_2.row,"STDDT",ds_SVHE304UC_CHECK.GetColumn(i,"STDDT"));
			ds_SVHE304UC_2.SetColumn(ds_SVHE304UC_2.row,"PLNCD",ds_SVHE304UC_CHECK.GetColumn(i,"PLNCD"));
			ds_SVHE304UC_2.SetColumn(ds_SVHE304UC_2.row,"PATNO",ds_SVHE304UC_CHECK.GetColumn(i,"PATNO"));
			}
			return;	
		}
	}
	// tran 실패
	if(rtnCd<0){
		alert(setPopMessage("1002"));
		setMessage("1002");
	// tran 성공	
	}else if(svcId=="setSVHE304UC"){

		alert(setPopMessage("1001"));
		setMessage("1001");
	}	
}

