.doc-wrap {
	width: 100%;
	/*min-width: 1100px;*/
	min-width: 590px;
}

.displayNone {
	display: none;
}

.visibleHidden {
	visibility: hidden;
}

.inlineBlock {
	display: inline-block;
}

.search-bnt {
	border-radius: 3px;
    background: url(../img/board/search-bnt.png) no-repeat 10px center #363636;
    color: #fff;
    padding: 5px 10px 5px 25px;
    vertical-align: bottom;
}

.AXInput, input[type='text'], .form-control[disabled]{
	height: 35px;
	box-sizing: border-box;
}

.AXButton {
	min-width:45px;
	height: 33px;
	box-sizing: border-box;
	vertical-align: baseline;
}

.AXButton.Black {
	background: #1e1e1e;
    border: 1px solid #000;
    color: #cecece;
    text-shadow: none;
}

.AXButton.Black:focus, .AXButton.Black:hover {
    background: #000;
    text-decoration: none;
    color: #fff;
}

.pjt_board .list-top, .pjt-wrap .list-top {
		overflow: visible;
}

input.calendar_input {
	padding-left: 30px;
	width: 120px;
}

.pjt_board .list-top, .log_board .list-top {
	overflow: visible;
}

.searchWrap {
		margin-left : 20px;
}

	@media screen and (max-width:1000px) {
		.searchWrap {
			margin-left : 0px;
		}
	}

.pjr_top .calendar_icon.black, .calendar_icon {
	position:absolute; 
	top:4px; 
	left:2px; 
	display:inline-block; 
	background:url(/img/content/calendar.png)no-repeat center center; 
	width:28px; 
	height:24px; 
}

