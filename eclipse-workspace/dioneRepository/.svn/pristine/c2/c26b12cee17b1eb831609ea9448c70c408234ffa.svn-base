/********************************************************************** 
 * 내용설명 : Transaction 리턴후 실행 함수
 * 주요변수 : 
 * 			- sMethodName	Transaction 고유 ID
 * 			- ErrorCode 	에러코드
 * 			- ErrorMSG 		에러 메세지
 **********************************************************************/ 
function callback(svcId, rtnCd, rtnMsg){
	if (ds_OBNZ180_CHECK.GetRowCount() > 0){
		for(var i=0;i<ds_OBNZ180_CHECK.GetRowCount();i++){
			alert("설비코드 : " +ds_OBNZ180_CHECK.GetColumn(i,"MSHNO") + " = " + ds_OBNZ180_CHECK.GetColumn(i,"RTMSG"));
		}
		getOBNZ180();  //error항목이 존재시에는 재조회
		return;
	}
	// tran 실패
	if(rtnCd<0){
		alert(setPopMessage("1002"));
		setMessage("1002");
		
	}else {
		alert(setPopMessage("1001"));
		setMessage("1001");	
	}
}
