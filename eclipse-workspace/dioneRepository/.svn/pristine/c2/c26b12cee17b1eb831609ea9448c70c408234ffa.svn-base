function fn_print()
{
	if (ds_OBNZ180.rowcount < 1){
		alert(setPopMessage("1003"));
		setMessage("1003");	
		return;
	}
	//Ubi0.printset();

	getSYSDATE(); //SYSTEM 날짜
	var sysDt =ds_SYSDATE.GetColumn(0,"DT");
	var systm =ds_SYSDATE.GetColumn(0,"TM");
	
	http.Sync = true;
	var sKind		= "getOBNZ180";						
	var sMethodName = "service::psogmlib/selectOBNZ180.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                            				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_OBNZ180=ds_OBNZ180";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
		sArgument += " LIB1=" + quote(Lib1);
		sArgument += " LIB2=" + quote(Lib2);
		sArgument += " LIB3=" + quote(Lib3);
		sArgument += " COMPS=" + quote(ComboBoxPlant.Value);
		sArgument += " KNYYM=" + quote(substr(cmb_KNYYM.Value,0,6));	//계획보전년월
		sArgument += " LINCD=" + quote(trim(sParam2));	//종합알림창에서 이동시 사용-라인코드		
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	http.Sync = false;

	if(ds_OBNZ180.rowcount > 0){
		Ubi0.setJrfFileDir(G_baseURL+"ui/PSOGMLIB/report/");
		Ubi0.setJrfFileName("OBNZ180R.jrf");
		Ubi0.SetDataset("ds_OBNZ180", "ds_OBNZ180");//DATA LIST
		Ubi0.SetDataset("ds_LINCD", "ds_LINCD");//LINE 코드
		Ubi0.SetDataset("ds_MSHNO", "ds_MSHNO");//설비명
		Ubi0.SetDataset("ds_STEP", "ds_STEP");//진도관리
		Ubi0.SetArgument("SYSDATE",sysDt);
		Ubi0.SetArgument("SYSTIME",systm);	
		Ubi0.SetArgument("P_YEAR",substr(cmb_KNYYM.Value,0,4));	
		Ubi0.SetArgument("P_MON",substr(cmb_KNYYM.Value,4,2));	
		//REPORT달력 표시
		var weekmm= substr(cmb_KNYYM.Value,0,6);
		for( i=1; i<=31; i++){
			var chkweek=getDay(weekmm+lpad(i,"0",2));
			addVariable("BKCOL"+i);//동적변수생성
			if(chkweek==-1){
				Ubi0.SetArgument("BKCOL"+i,"N");//동적변수생성

			}
			if(chkweek==0 || chkweek==6){
				Ubi0.SetArgument("BKCOL"+i,"H");//동적변수생성
			}
		}
	Ubi0.Visible=true;
	btnClose.Visible=true;
	Ubi0.Retrieve();
	Ubi0.printset();
	}
}

