function fn_Input()
{
	if(!validationChk()){
		return;
	}
	if(ds_OBNZ180.rowcount < 1) {
		fn_Search();
	}
	
	ds_OBNZ180.AddRow();
	ds_OBNZ180.SetColumn(ds_OBNZ180.currow,"COMPS",ComboBoxPlant.Value);
	
	//회계년도 재계산
	var p_PYEAR=0;
	if(toNumber(substr(cmb_KNYYM.Value,4,2)) < 4){
		p_PYEAR = toNumber(substr(cmb_KNYYM.Value,0,4)) - 1;
	}else{
		p_PYEAR = toNumber(substr(cmb_KNYYM.Value,0,4));
	}
	
	//ds_OBNZ180.SetColumn(ds_OBNZ180.currow,"PYEAR",substr(cmb_KNYYM.Value,0,4));//점검년도
	ds_OBNZ180.SetColumn(ds_OBNZ180.currow,"PYEAR",p_PYEAR);//점검년도
	ds_OBNZ180.SetColumn(ds_OBNZ180.currow,"KNYYM",substr(cmb_KNYYM.Value,0,6));//기준년월
	ds_OBNZ180.SetColumn(ds_OBNZ180.currow,"STPCD",0);//진도
	ds_OBNZ180.SetColumn(ds_OBNZ180.currow,"MHOUR",0);//공수
	ds_OBNZ180.SetColumn(ds_OBNZ180.currow,"OPRNO1",0);//작업자1사번
	ds_OBNZ180.SetColumn(ds_OBNZ180.currow,"OPRNO2",0);//작업자2사번
	ds_OBNZ180.SetColumn(ds_OBNZ180.currow,"OPRNO3",0);//작업자3사번
	ds_OBNZ180.SetColumn(ds_OBNZ180.currow,"OPRNO4",0);//작업자4사번
	//trace(ds_OBNZ180.SaveXML());
}

