function getChkWeek(){
	var weekmm= substr(cmb_KNYYM.Value,0,6);
	for( i=1; i<=31; i++){
		var chkweek=getDay(weekmm+lpad(i,"0",2));
		if(chkweek==-1){
			//alert(weekmm+lpad(i,"0",2));
			MainGrid.SetCellProp("Head", 9+i, "bkcolor", "black");
		}
		if(chkweek==0 || chkweek==6){
			//alert(weekmm+lpad(i,"0",2));
			MainGrid.SetCellProp("Head", 9+i, "bkcolor", "red");
		}
	}
}

