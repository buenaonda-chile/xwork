	ds_ppmi007m.SetColumn(ds_ppmi007m.row,"PROCS",ed_procs.Value);
	ds_ppmi007m.SetColumn(ds_ppmi007m.row,"DAYGU",cd_daygu.Value);

}

function fn_Delete()
{
	if(confirm("정말로 삭제하시겠습니까?              ") == true){
		ds_ppmi007m.DeleteRow(ds_ppmi007m.row);
	}

}

function fn_Save()
{
