	searchBar.Width = G_ds_design.GetColumn(0,"Width");
	searchBar.Top = G_ds_design.GetColumn(0,"SBTop");	
}

function getSMDE123Q(){
	//업체
	if(length(trim(ed_JPNNO1.Text))==0 || length(trim(ed_JPNNO1.Text))==0){
		alert(setPopMessage("4012"));
		setMessage("4012");	
		return;
	}
	if(length(trim(cmb_JPSDT1.Value))==0 || length(trim(cmb_JPSDT2.Value))==0){
		alert(setPopMessage("4027"));
		setMessage("4027");	
		return;
	}
	if((cmb_JPSDT1.Value) > (cmb_JPSDT2.Value)){
		alert(setPopMessage("4027"));
		setMessage("4027");	
		return;
	}
	var sKind		= "getSMDE123Q";						
	var sMethodName = "service::pssallib/selectSMDZ123P.do"; //출고전표발행과 같이사용함    		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_SMDZ123P=ds_SMDZ123P";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
	sArgument += "@LIB1=" + quote(Lib1);
	sArgument += "@LIB2=" + quote(Lib2);
	sArgument += "@COMPS=" + quote(ComboBoxPlant.Value); //사업부
	sArgument += "@JPNNO1=" + quote(trim(ed_JPNNO1.Text));
	sArgument += "@JPNNO2=" + quote(trim(ed_JPNNO2.Text));
	sArgument += "@JPSDT1=" + quote(cmb_JPSDT1.Value);
	sArgument += "@JPSDT2=" + quote(cmb_JPSDT2.Value);
	sArgument += "@PYVND1=" + quote("00000000");   //출고전표발행과 같이사용하기위해 임의로 생성
	sArgument += "@PYVND2=" + quote("99999999");   //출고전표발행과 같이사용하기위해 임의로 생성
	var str = dialog("Common::PGMLOAD_SAL.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");
	ds_SMDZ123P.Sort("SLPNO");
	if (str == 'T'){  //정상조회
		
		alert(setPopMessage("1004"));
		setMessage("1004");
		//ds_SMDZ123P.Sort(JPNNO,SLPNO,JPSDT);
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));  //[알림] 신규입력입니다.
		setMessage("1003");	
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}	
}

