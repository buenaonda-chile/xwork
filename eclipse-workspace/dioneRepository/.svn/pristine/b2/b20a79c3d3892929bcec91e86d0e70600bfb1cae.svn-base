function validationChk(){
	var result = true;
	
	ed_PYVND.BKColor="white";
	cmb_SJTYM.BKColor="white";
	
	if(length(ed_PYVND.Text)==0){
		ed_PYVND.BKColor="paleturquoise";
		alert(setPopMessage("2516"));
		result =false;
	}
	if(cmb_SJTYM.Value==""){
		cmb_SJTYM.BKColor="paleturquoise";
		alert(setPopMessage("4053"));
		result =false;
