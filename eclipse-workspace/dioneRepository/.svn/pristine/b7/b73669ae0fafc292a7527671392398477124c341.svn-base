function detail_View(obj,nRow,nCell,nX,nY,nPivotIndex)
{
	if(ds_SPMC128Q.rowcount <= 0 || ds_SPMC128Q.GetRowType(ds_SPMC128Q.row) != 'normal') return ;
	var p_pyvnd1,p_pyvnd2,p_prdcd1,p_prdcd2;
	p_pyvnd1 = iif (length(trim(ed_PYVND_F.Text)) == 0, '0000', trim(ed_PYVND_F.Text));
	p_pyvnd2 = iif (length(trim(ed_PYVND_T.Text)) == 0 , '9999', trim(ed_PYVND_T.Text));
	p_prdcd1 = iif (length(trim(ed_prdcd_from.Text)) == 0 , '  ', trim(ed_prdcd_from.Text));
	p_prdcd2 = iif (length(trim(ed_prdcd_to.Text)) == 0 , 'ZZ', trim(ed_prdcd_to.Text));
    
	var str = dialog("PSSALLIB::SPMC128QD.xml", 
					" COMPS=" + quote(cb_searchCodeName1.Value) +  //사업부
					" PLTCD=" + quote(cb_searchCodeName2.Value) +  //공장구분
					" PLNYM=" + quote(substr(ca_plnym.Value,0,6)) + //계획년월 
					" PYVND1=" + quote(p_pyvnd1) +
					" PYVND2=" + quote(p_pyvnd2) +
					" PRDCD1=" + quote(p_prdcd1) +
					" PRDCD2=" + quote(p_prdcd2) +
					" MGMCD=" + quote(cb_ad.Value) + //denso,asmo구분
					" CODE=" + quote(ds_SPMC128Q.GetColumn(ds_SPMC128Q.row,"CODE")) + // 
					" CODE1=" + quote(ds_SPMC128Q.GetColumn(ds_SPMC128Q.row,"CODE1")) + // 
					" GUB="+ quote(rdoggub.Value),1100,600);
}
