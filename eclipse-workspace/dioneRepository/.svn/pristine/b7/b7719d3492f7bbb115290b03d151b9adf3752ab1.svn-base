function getogm205_Pop(){
	
	var sKind		= "getogm205_Pop";						
	var sMethodName = "service::psogmlib/selectOgm205_pop.do"; 
	var sInDataSet  = "";
	var sOutDataSet = "ds_ogm205_pop=ds_ogm205_pop";
	var sArgument   = "";
		sArgument += " LIB2=" + quote(Lib2);
		sArgument += " COMPS=" + quote(ComboBoxPlant.Value); //사업부
		sArgument += " PRTNM=" + quote(trim(prtnm_pop.Text)); //예비품No
		sArgument += " MAKER=" + quote(trim(maker_pop.Text)); //MAKER
	http.Sync = true;
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	http.Sync = false;

	if(ds_ogm205_pop.rowcount <= 0){
		alert(setPopMessage("1003"));
		setMessage("1003");
	}else{
		alert(setPopMessage("1004"));
		setMessage("1004");	
	}	
}


