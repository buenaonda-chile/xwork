function getSMDZ120P(){
	
	if(!validationChk()){
		return;
	}
	var sPYVND1="";
	var sPYVND2="";
	var sDateVal="";
	var eDateVal="";
	if(length(ed_PYVND1.Text)==0){
		sPYVND1="00000000";
	}
	else{
		sPYVND1=ed_PYVND1.Text;
	}	
	if(length(ed_PYVND2.Text)==0){
		sPYVND2="99999999";
	}
	else{
		sPYVND2=ed_PYVND2.Text;
	}			
	if(cmb_SDATE.Value == ""){
		sDateVal="00000000";
	}
	else if(cmb_SDATE.Value != ""){
		sDateVal=cmb_SDATE.Value;
	}
	if(cmb_EDATE.Value == ""){
		eDateVal="99999999";
	}
	else if(cmb_EDATE.Value != ""){
		eDateVal=cmb_EDATE.Value;
	}
	
	getSYSDATE(); //SYSTEM 날짜
	var sysDt =ds_SYSDATE.GetColumn(0,"DT");
	var systm =ds_SYSDATE.GetColumn(0,"TM");
			
	var sKind		= "getSMDZ120P";						
	var sMethodName = "service::pssallib/selectSMDZ120P.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_SMDZ120P=ds_SMDZ120P";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
	sArgument += "@LIB1=" + quote(Lib1);
	sArgument += "@LIB2=" + quote(Lib2);
	sArgument += "@COMPS=" + quote(ComboBoxPlant.Value); //사업부
	sArgument += "@PYVND1=" + quote(sPYVND1); //업체
	sArgument += "@PYVND2=" + quote(sPYVND2); //업체
	sArgument += "@SDATE=" + quote(sDateVal); //적용일
	sArgument += "@EDATE=" + quote(eDateVal); //적용일
	var str = dialog("Common::PGMLOAD_SAL.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");
	//http.Sync=true;
	//Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	//http.Sync=false;
	if (str == 'T'){  //정상조회	
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));
		setMessage("1003");
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}
	
	//Ubi0.setJrfFileDir("http://10.74.139.50/ui/PSSALLIB/REPORT/");
	//Ubi0.setJrfFileDir("http://10.72.209.23/ui/PSSALLIB/REPORT/");
	Ubi0.setJrfFileDir(G_baseURL+"ui/PSSALLIB/REPORT/");
	Ubi0.setJrfFileName("SMDZ120P.jrf");
	Ubi0.SetDataset("ds_SMDZ120P", "ds_SMDZ120P");
	Ubi0.SetArgument("SDATE",sDateVal);
	Ubi0.SetArgument("EDATE",eDateVal);
	Ubi0.SetArgument("SYSDATE",sysDt);
	Ubi0.SetArgument("SYSTIME",systm);	
	Ubi0.Retrieve();
	
}

/********************************************************************** 
 * 내용설명 : validation 체크
 **********************************************************************/ 
 
function validationChk(){

	var result = true;
	var sdVal = cmb_SDATE.Value;
	var edVal = cmb_EDATE.Value;
	cmb_SDATE.BKColor="white";
	cmb_EDATE.BKColor="white";
			
	if(sdVal > edVal)
	{
		cmb_SDATE.BKColor="paleturquoise";
		cmb_EDATE.BKColor="paleturquoise";
		alert(setPopMessage("3049"));
		result =false;
	}
	return result;
}

