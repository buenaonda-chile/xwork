function fn_Input()
{
	if(!validationChk()){
		return;
	}
	if(checkInsertCount("ds_HMDZ200")== false) {
		return;
	}
	if(ds_HMDZ200.rowcount < 1) {
		fn_Search();
	}
	ds_HMDZ200.AddRow();
	ds_HMDZ200.SetColumn(ds_HMDZ200.currow,"SABUN",ed_SABUN.Text);
	ds_HMDZ200.SetColumn(ds_HMDZ200.currow,"KORNM",st_KORNM.Text);
	ds_HMDZ200.SetColumn(ds_HMDZ200.currow,"EDTCD",10);//row추가시 Default
	ds_HMDZ200.SetColumn(ds_HMDZ200.currow,"EDPOT",0);//row추가시 Default
	ds_HMDZ200.SetColumn(ds_HMDZ200.currow,"EDCLS",0);//row추가시 Default
	ds_HMDZ200.SetColumn(ds_HMDZ200.currow,"EDBCD","N"); //근태적용여부 기본값

	MainGrid.SetCellPos(1);
	MainGrid.SetFocus();
}

