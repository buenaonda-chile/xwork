function ds_HMDZ200_OnColumnChanged(obj,nRow,strColumnID,varOldValue,varNewValue,nPivotIndex)
{
	if (strColumnID == "EDPOT"){//교육득점
		if(length(varNewValue)==0){
			ds_HMDZ200.SetColumn(nRow,"EDPOT",0);
		}
	}
	if (strColumnID == "EDCLS"){//교육석차
		if(length(varNewValue)==0){
			ds_HMDZ200.SetColumn(nRow,"EDCLS",0);
		}
	}
}

function ds_HMDZ200_CanRowPosChange(obj,nOldRow,nRow)
{
	http.usewaitcursor = false;

	var p_ESDAT=ds_HMDZ200.GetColumn(ds_HMDZ200.currow,"ESDAT"); //교육시작일
	var p_EEDAT=ds_HMDZ200.GetColumn(ds_HMDZ200.currow,"EEDAT"); //교육종료일

	if(length(p_ESDAT)!=0){
		if(length(p_EEDAT)==0){	
			//alert("[경고] 교육완료일을 입력하세요.");
			alert(setPopMessage("6122")); //[경고] 교육완료일을 입력하세요.
			setMessage("6122");	
			return false;
		}
		if(p_ESDAT > p_EEDAT){	
			//alert("[경고] 교육시작일이 교육완료일을 보다 클수 없습니다.");
			alert(setPopMessage("6123")); //[경고] 교육시작일이 교육완료일을 보다 클수 없습니다.
			setMessage("6123");	
			return false;
		}
	}
	return true;
	http.usewaitcursor = true;
