	MainGrid.SetCellProp("Head", 17, "text", substr(ds_MST090PF.GetColumn(3,"PRCDT"),4,2)+"/"+substr(ds_MST090PF.GetColumn(3,"PRCDT"),6,2));
	MainGrid.SetCellProp("Head", 18, "text", substr(ds_MST090PF.GetColumn(4,"PRCDT"),4,2)+"/"+substr(ds_MST090PF.GetColumn(4,"PRCDT"),6,2));
	MainGrid.SetCellProp("Head", 19, "text", substr(ds_MST090PF.GetColumn(5,"PRCDT"),4,2)+"/"+substr(ds_MST090PF.GetColumn(5,"PRCDT"),6,2));
	MainGrid.SetCellProp("Head", 20, "text", substr(ds_MST090PF.GetColumn(6,"PRCDT"),4,2)+"/"+substr(ds_MST090PF.GetColumn(6,"PRCDT"),6,2));
