	if (ds_MST090PF.GetRowCount() < 1){
		alert(setPopMessage("1504"));
		setMessage("1504");	
		cmb_STDDT.SetFocus();
		return;
	}
http.usewaitcursor = true;		

//trace(ds_MST090PF.SaveXML());
	MainGrid.SetCellProp("Head", 14, "text", substr(ds_MST090PF.GetColumn(0,"PRCDT"),4,2)+"/"+substr(ds_MST090PF.GetColumn(0,"PRCDT"),6,2));
