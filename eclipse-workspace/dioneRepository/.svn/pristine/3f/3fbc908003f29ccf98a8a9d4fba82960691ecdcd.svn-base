/********************************************************************** 
 * 내용설명 : 화면조회
 * 주요변수 : 
 **********************************************************************/ 
function getSJSZ900Q()
{
	http.Sync 	= true;
	var sKind		= "getSC008PR";						
	var sMethodName = "service::pscomlib/selectSC008PR.do"; 
	var sInDataSet  = "";
	var sOutDataSet = "ds_SC008PR=ds_SC008PR";
	var sArgument   = "";
		sArgument   += " DTANO='HAR'";
		sArgument   += " LIB1=" + quote(Lib1);	
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	http.Sync 	= false;
	var arrHouseT = trim(ds_SC008PR.GetColumn(0,"CTLDT"));
	var arrHouse = "('";
	for (var i=0 ; toNumber(length(arrHouseT)) > i ; i++){
		if (i == length(arrHouseT)-1){
			arrHouse = arrHouse+substr(arrHouseT,i,1) + "')";
		}else{
			arrHouse = arrHouse+substr(arrHouseT,i,1) + "','";
		}
	}
	
	
	
	
	var jsoyy;
	var jsomm;
	jsomm = toNumber(substr(ca_JSOYM.Value,4,2));
	if(toNumber(substr(ca_JSOYM.Value,4,2)) < 4){
		jsoyy = toNumber(substr(ca_JSOYM.Value,0,4)) - 1;
	}else{
		jsoyy = toNumber(substr(ca_JSOYM.Value,0,4));
	}

	var sKind		= "getSJSZ900Q";					
	var sMethodName = "service::pssallib/selectSJSZ900Q.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_SJSZ900Q=ds_SJSZ900Q";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
	
	sArgument += "@LIB1=" + quote(Lib1);
	sArgument += "@LIB2=" + quote(Lib2);
	sArgument += "@JSOYM=" + quote(substr(ca_JSOYM.Value,0,6)); //년월 
	sArgument += "@JSOYY=" + quote(jsoyy); //년
	sArgument += "@JSOMM=" + quote(jsomm); //월
	sArgument += "@COMPANY=" + quote(company);
	sArgument += "@HOUSE=" + quote(arrHouse);
	var str = dialog("Common::PGMLOAD_SAL.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");
	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
		setMessage("1004");	
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));  //[알림] 조회 된 데이터가 없습니다.
		setMessage("1003");	
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}
}

