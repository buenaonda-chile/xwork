 **********************************************************************/ 
 function setOBMZ105(){
/*
	if(ds_OBMZ105.GetRowType(ds_OBMZ105.currow)=="update" ||ds_OBMZ105.GetRowType(ds_OBMZ105.currow)=="insert" ){
		if(!ds_OBMZ105_CanRowPosChange()){
			return;
		}
