		sArgument += " LINCD=" + quote(ds_LINCD.GetColumn(ds_LINCD.currow,"CD")); //설비코드
	http.Sync 	= true;
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	http.Sync 	= false;
}

