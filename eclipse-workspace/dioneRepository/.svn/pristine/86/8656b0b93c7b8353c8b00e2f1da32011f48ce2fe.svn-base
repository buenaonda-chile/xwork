function setDeviceAccount(){
	//if(!validationChk()) return;
	var sKind		= "setDeviceAccount";						
	var sMethodName = "service::user/saveDevice.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "ds_device=ds_device:U";     				//자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "";    									// 조회 후 돌려 받을 DataSet 명칭
	var sArgument = "";
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");
}


/********************************************************************** 
 * 개 발 자 : 황규철 
 * 개발일자 : 2010-11-26
 * 내용설명 : Transaction 리턴후 실행 함수
 * 주요변수 : 
 * 			- sMethodName	Transaction 고유 ID
 * 			- ErrorCode 	에러코드
 * 			- ErrorMSG 		에러 메세지
 **********************************************************************/ 
function callback(svcId, rtnCd, rtnMsg){
	// tran 실패
	if(rtnCd<0){
	
		alert(setPopMessage("1002"));
		setMessage("1002");
		
	}else if(svcId=="getUserList"){
		// 조회건수가 없을경우
		if(ds_userList.rowcount <= 0){
			alert(setPopMessage("1003"));
			setMessage("1003");	
		// 정상 조회
		} else {
			alert(setPopMessage("1004"));
			setMessage("1004");				
		}
	// tran 성공	
	}else if(svcId=="setDeviceAccount"){
