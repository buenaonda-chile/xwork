function getComCode(){

	http.Sync 	= true;

	var sKind		= "getComCode";						
	var sMethodName = "service::login/selectComCode.do"; 
	var sInDataSet  = "";
	var sOutDataSet = "ds_ComCode_PLNT=ds_ComCode_PLNT";
	var sArgument   = "";
		sArgument   += " CLASS='PLNT'";
		sArgument   += " CODE1=" + quote(company);		
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	ds_VComCode_PLNT.Copy("ds_ComCode_PLNT");

	cb_searchCodeName1.index = 0;
}


/********************************************************************** 
 * 내용설명 : 화면조회
 * 주요변수 : 
 **********************************************************************/ 
function getSOMC300(){
	if(length(trim(st_pyvnd.Text)) < 1){
		alert(setPopMessage("1502"));  //[확인] 거래처코드 확인바랍니다.
		setMessage("1502");	
		ed_pyvnd.SetFocus();
		return;
	}	

	//조회
	//clearEditObject();  //초기화
	var sKind		= "getSOMC300";						
	var sMethodName = "service::pssallib/selectSOMC300.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_SOMC300=ds_SOMC300";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
	
	sArgument += "@LIB1=" + quote(Lib1);
	sArgument += "@LIB2=" + quote(Lib2);
	sArgument += "@COMPS=" + quote(cb_searchCodeName1.Value);
	sArgument += "@PYVND=" + quote(ed_pyvnd.Text);
	sArgument += "@COMYM=" + quote(substr(cal_COMYM.Value,0,6));
	var str = dialog("Common::PGMLOAD_SAL.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");	
	//Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
		setMessage("1004");	
		Maingrid.SetFocus();
		MainGrid.SetCellPos(5);
		ds_SOMC300.rowposition = 0;
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1013"));  //[알림] 신규입력입니다.
		setMessage("1013");	
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}
}

/********************************************************************** 
 * 내용설명 : 저장
 * 주요변수 : 
 **********************************************************************/ 
 function setSOMC300(){

	if(length(trim(st_pyvnd.Text)) < 1){
		alert(setPopMessage("1502"));  //[확인] 거래처코드 확인바랍니다.
		setMessage("1502");	
		ed_pyvnd.SetFocus();
		return;
	}	
	
 	http.Sync = true;
	var sKind		= "SAVE_SOMC300";						
	var sMethodName = "service::pssallib/insertSOMC300.do";
	var sInDataSet  = "ds_SOMC300=ds_SOMC300:U";
	var sOutDataSet = "";
	var sArgument   = "";
	sArgument += " LIB2=" + quote(Lib2);
	sArgument += " COMPS=" + quote(cb_searchCodeName1.Value);
	sArgument += " PYVND=" + quote(ed_pyvnd.Text);
	sArgument += " COMYM=" + quote(substr(cal_COMYM.Value,0,6));
		
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");
 }

