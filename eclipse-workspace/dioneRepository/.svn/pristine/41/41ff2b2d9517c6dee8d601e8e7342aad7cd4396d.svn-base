function getComCode(){
	http.Sync 	= true;
	var sKind		= "getComCode";						
	var sMethodName = "service::login/selectComCode.do"; 
	var sInDataSet  = "";
	var sOutDataSet = "ds_ComCode_PLNT=ds_ComCode_PLNT";
	var sArgument   = "";
		sArgument   += " CLASS='PLNT'";
		sArgument   += " CODE1=" + quote(company);		
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	http.Sync 	= false;
	ds_VComCode_PLNT.Copy("ds_ComCode_PLNT");
	//ds_VComCode_PLNT.InsertRow(0);
	//ds_VComCode_PLNT.SetColumn(0,"CODE2","");
	//ds_VComCode_PLNT.SetColumn(0,"CODE_NAME","[전체]");
	cb_searchCodeName1.index = 0;
}


/********************************************************************** 
 * 내용설명 : 화면조회
 * 주요변수 : 
 **********************************************************************/ 
function getSPME131Q()
{
	var p_pyvnd1,p_pyvnd2;
	p_pyvnd1 = iif (length(trim(ed_PYVND_F.Text)) == 0, '0000', trim(ed_PYVND_F.Text));
	p_pyvnd2 = iif (length(trim(ed_PYVND_T.Text)) == 0 , '9999', trim(ed_PYVND_T.Text));
	var rolcdgb;
	if (cb_ad.Value == "D"){
		rolcdgb = "('2','4')";
	}else if (cb_ad.Value == "O"){
		rolcdgb = "('6','8')";
	}else{
		rolcdgb = "";
	}
	
	http.Sync 	= true;
	var sKind		= "getSC000_003PR";						
	var sMethodName = "service::pscomlib/selectSC000_003PR.do"; 
	var sInDataSet  = "";
	var sOutDataSet = "ds_SC000_003PR=ds_SC000_003PR";
	var sArgument   = "";
		sArgument += " LIB1=" + quote(Lib1);	
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	http.Sync 	= false;
	var b_month;
	var prdPlan;
	var adsdt = ds_SC000_003PR.GetColumn(0,"PCNUM1");
	if (ds_SC000_003PR.GetColumn(0,"PCNUM3") == "50ON"){
		b_month = substr(AddMonth(adsdt,-1),0,6);
	}else{
		b_month = substr(AddMonth(adsdt,-2),0,6);
	}
	if(toNumber(substr(ca_plnym.Value,0,6)) <= toNumber(b_month)){
		prdPlan = "1";  //생산계획이 CIGMA 에　없고 INTERFACE FILE 에　있음
	}else{
		prdPlan = "2";  //생산계획이 CIGMA 에　있음
	}
	
	
	var sKind		= "getSPME131Q";					
	var sMethodName = "service::pssallib/selectSPME131Q.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_SPME131Q=ds_SPME131Q";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
	
	sArgument += "@LIB1=" + quote(Lib1);
	sArgument += "@LIB2=" + quote(Lib2);
	sArgument += "@COMPS=" + quote(cb_searchCodeName1.Value);//사업부
    sArgument += "@PLNYM=" + quote(substr(ca_plnym.Value,0,6)); //계획년월 
	sArgument += "@PYVND1=" + quote(p_pyvnd1);
	sArgument += "@PYVND2=" + quote(p_pyvnd2);	
	sArgument += "@ROLCD=" + quote(rolcdgb);     //생산구분
	sArgument += "@JPSDT=" + quote(ca_JPSDT.Value); //실적일자
	sArgument += "@PRDPLAN=" + quote(prdPlan);   //생산계획구분
    //sArgument += "@G_GUB="  + quote(rdoggub.Value);	//구분
	var str = dialog("Common::PGMLOAD_SAL.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");	
	//Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");
	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
		setMessage("1004");	
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));
		setMessage("1003");	
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}
}

