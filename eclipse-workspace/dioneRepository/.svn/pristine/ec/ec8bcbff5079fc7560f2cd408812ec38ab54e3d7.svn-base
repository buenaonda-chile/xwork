function OnRowPosChanged(obj,nOldRow,nRow)
{



	if(tab0.TabIndex == 0){
		tab0.tab1.ds_ppmi006mTab1Sub.DeleteAll();
		tab0.tab1.procs = ds_ppmi003mGroup.GetColumn(ds_ppmi003mGroup.row,"PROCS");
		tab0.tab1.ordat = ed_syyyymmdd.Value;
		tab0.tab1.getPpmi006mTab1();	
	} else {
		tab0.tab2.procs = ds_ppmi003mGroup.GetColumn(ds_ppmi003mGroup.row,"PROCS");
		tab0.tab2.ordat = ed_syyyymmdd.Value;
		tab0.tab2.getPpmi006mTab2();	
	}


}


