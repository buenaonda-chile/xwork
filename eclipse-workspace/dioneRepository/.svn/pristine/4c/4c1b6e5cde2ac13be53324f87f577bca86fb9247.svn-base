    @Override
    public String toString() {
        JSONObject jsonObj = new JSONObject();
        jsonObj.put("empNumber", this.empNumber);
        jsonObj.put("clsCode", this.clsCode);
        jsonObj.put("locCode", this.locCode);
        jsonObj.put("clsName", this.clsName);
        jsonObj.put("locName", this.locName);
        jsonObj.put("empName", this.empName);
        jsonObj.put("empEmail", this.empEmail);
        jsonObj.put("empId", this.empId);        
        
        jsonObj.put("company", this.company);
        jsonObj.put("workPlace", this.workPlace);
        jsonObj.put("changeCompany", this.changeCompany);

        jsonObj.put("sysSeq", this.sysSeq);
        jsonObj.put("sysName", this.sysName);
        jsonObj.put("systemId", this.systemId);
        jsonObj.put("systemPW", this.systemPW);
        jsonObj.put("systemGroup", this.systemGroup);
        jsonObj.put("remarks", this.remarks);
        jsonObj.put("ssoYn", this.ssoYn);
        
        jsonObj.put("deviceId", this.deviceId);
        jsonObj.put("deviceName", this.deviceName);
        jsonObj.put("useYn", this.useYn);
        jsonObj.put("deviceType", this.deviceType);
        jsonObj.put("deviceId2", this.deviceId2);

        jsonObj.put("className", super.toString());
        String rtnStr = jsonObj.toJSONString();
        
        return rtnStr;
    }
    
    
