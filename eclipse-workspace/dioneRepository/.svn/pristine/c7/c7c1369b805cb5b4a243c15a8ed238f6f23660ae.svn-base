}

/********************************************************************** 
 * 개 발 자 : 제환복 
 * 개발일자 : 2015-06-17
 * 내용설명 : 공통코드 조회
 * 주요변수 : 
 **********************************************************************/ 
function getComCode(){

	http.Sync 	= true;
	var sKind		= "getComCode";						
	var sMethodName = "service::system/getComCode/code"; 
	var sInDataSet  = "";
	var sOutDataSet = "ds_ComCode_COMP=ds_ComCode";
	var sArgument   = "";
		sArgument   += " CLASS='COMP'";
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");	
		
	
	ds_VComCode_COMP.Copy("ds_ComCode_COMP");	
	ds_VComCode_COMP.InsertRow(0);
	ds_VComCode_COMP.SetColumn(0,"CODE1","00");
	ds_VComCode_COMP.SetColumn(0,"CODE_NAME","공통");	
	Combo0.index = 0;

	ds_ComCode_COMP.InsertRow(0);
	ds_ComCode_COMP.SetColumn(0,"CODE1","");
	ds_ComCode_COMP.SetColumn(0,"CODE_NAME","[전체]");
	cb_comp.index = 0;		

	var sKind		= "getComCode";						
	var sMethodName = "service::system/getComCode/code"; 
	var sInDataSet  = "";
	var sOutDataSet = "ds_ComCode_PLNT=ds_ComCode";
	var sArgument   = "";
		sArgument   += " CLASS='PLNT'";
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	
	ds_VComCode_PLNT.Copy("ds_ComCode_PLNT");	
	ds_VComCode_PLNT.InsertRow(0);
	ds_VComCode_PLNT.SetColumn(0,"CODE1","PS");
	ds_VComCode_PLNT.SetColumn(0,"CODE2","00");	
	ds_VComCode_PLNT.SetColumn(0,"CODE_NAME","PS 전체");	
	
	ds_VComCode_PLNT.InsertRow(0);
	ds_VComCode_PLNT.SetColumn(0,"CODE1","PE");
	ds_VComCode_PLNT.SetColumn(0,"CODE2","00");	
	ds_VComCode_PLNT.SetColumn(0,"CODE_NAME","PE 전체");		
	
	ds_VComCode_PLNT.InsertRow(0);
	ds_VComCode_PLNT.SetColumn(0,"CODE1","00");
	ds_VComCode_PLNT.SetColumn(0,"CODE2","00");	
	ds_VComCode_PLNT.SetColumn(0,"CODE_NAME","공통");			
	
	Combo1.index = 0;	
	
	ds_ComCode_PLNT.InsertRow(0);
	ds_ComCode_PLNT.SetColumn(0,"CODE1","");
	ds_ComCode_PLNT.SetColumn(0,"CODE_NAME","[전체]");
	cb_PLNT.index = 0;

	var sKind		= "getComCode";						
	var sMethodName = "service::system/getComCode/code"; 
	var sInDataSet  = "";
	var sOutDataSet = "ds_ComCode_DEVC=ds_ComCode";
	var sArgument   = "";
		sArgument   += " CLASS='DEVC'";
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
}



function compChange(obj,strCode,strText,nOldIndex,nNewIndex)
{
	var sKind		= "getComCode";						
	var sMethodName = "service::login/getComCodeList"; 
	var sInDataSet  = "";
	var sOutDataSet = "ds_ComCode_PLNT=ds_ComCode_PLNT";
	var sArgument   = "";
		sArgument   += " CLASS='PLNT'";
		sArgument   += " CODE1=" + quote(cb_comp.Value);
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");

	ds_ComCode_PLNT.InsertRow(0);
	ds_ComCode_PLNT.SetColumn(0,"CODE1","");
	ds_ComCode_PLNT.SetColumn(0,"CODE_NAME","[전체]");
	cb_PLNT.index = 0;	
}

function compChange2(obj,strCode,strText,nOldIndex,nNewIndex)
{
	ds_VComCode_PLNT.Filter("CODE1=='" + Combo0.Value + "'");
}




/********************************************************************** 
 * 개 발 자 : 제환복 
 * 개발일자 : 2015-06-17
 * 내용설명 : 메인 그리드 조회
 * 주요변수 : 
 **********************************************************************/ 
function getNotice(){
	var sKind		= "getNotice";						
	var sMethodName = "service::system/getNoticeList";     // 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             // 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_notice=ds_notice";    // 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
	
		sArgument += " COMPANY=" + quote(cb_comp.Value);
		sArgument += " PLANT_CD=" + quote(cb_PLNT.Value);
		sArgument += " NTC_SDATE=" + quote(ed_syyyymmdd.Value);
		sArgument += " NTC_EDATE=" + quote(ed_eyyyymmdd.Value);
		sArgument += " NTC_TITLE=" + quote(ed_title.Value);
		
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");
}


/********************************************************************** 
 * 개 발 자 : 제환복 
 * 개발일자 : 2015-06-17
 * 내용설명 : 화면 저장 
 * 주요변수 : 
 **********************************************************************/ 
function setNotice(){
	//if(!validationChk()) return;
	var sKind		= "setNotice";						
	var sMethodName = "service::system/saveNotice";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "ds_notice=ds_notice:U";     				//자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "";    									// 조회 후 돌려 받을 DataSet 명칭
	var sArgument = "";
	    sArgument += " CREATE_USER=" + quote(G_ds_user.GetColumn(0,"USER_ID"));
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");
}


/********************************************************************** 
 * 개 발 자 : 제환복 
 * 개발일자 : 2015-06-17
 * 내용설명 : Transaction 리턴후 실행 함수
 * 주요변수 : 
 * 			- sMethodName	Transaction 고유 ID
 * 			- ErrorCode 	에러코드
 * 			- ErrorMSG 		에러 메세지
 **********************************************************************/ 
function callback(svcId, rtnCd, rtnMsg){
	// tran 실패
	if(rtnCd<0){
	
		alert(setPopMessage("1002"));
		setMessage("1002");
		
	}else if(svcId=="getNotice"){
		// 조회건수가 없을경우
		if(ds_notice.rowcount <= 0){
			alert(setPopMessage("1003"));
			setMessage("1003");	
		// 정상 조회
		} else {
			alert(setPopMessage("1004"));
			setMessage("1004");				
		}
	// tran 성공	
	}else if(svcId=="setNotice"){
		alert(setPopMessage("1001"));
		setMessage("1001");
	}	
}

