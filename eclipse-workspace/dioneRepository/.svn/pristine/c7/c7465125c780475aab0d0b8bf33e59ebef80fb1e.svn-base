	MainGrid.SetCellProp("Head", 12, "text",ym1);
	MainGrid.SetCellProp("Head", 13, "text",ym2);
	MainGrid.SetCellProp("Head", 14, "text",ym3);
	MainGrid.SetCellProp("Head", 18, "text",ym0);
	MainGrid.SetCellProp("Head", 19, "text",ym1);
