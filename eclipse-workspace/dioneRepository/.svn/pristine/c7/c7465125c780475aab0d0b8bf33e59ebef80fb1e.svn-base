/********************************************************************** 
 * 내용설명 : 사업부 선택
 * 주요변수 : 
 **********************************************************************/ 
function getComCode(){
	http.Sync 	= true;

	var sKind		= "getComCode";						
	var sMethodName = "service::login/selectComCode.do"; 
	var sInDataSet  = "";
	var sOutDataSet = "ds_ComCode_PLNT=ds_ComCode_PLNT";
	var sArgument   = "";
		sArgument   += " CLASS='PLNT'";
		sArgument   += " CODE1=" + quote(company);		
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	cb_searchCodeName1.Value = comps;
}

/********************************************************************** 
 * 내용설명 : 화면조회
 * 주요변수 : 
 **********************************************************************/ 
function getBOMZ561Q(){
	var p_vnd1,p_vnd2;
	p_vnd1 = iif (length(trim(ed_CUVND1.Text)) == 0, '', trim(ed_CUVND1.Text));
	p_vnd2 = iif (length(trim(ed_CUVND2.Text)) == 0 , '999', trim(ed_CUVND2.Text));
	var p_prt1,p_prt2;
	p_prt1 = iif (length(trim(ed_PRTNO_from.Text)) == 0, '', trim(ed_CUVND1.Text));
	p_prt2 = iif (length(trim(ed_PRTNO_to.Text)) == 0 , '999999999999999', trim(ed_CUVND2.Text));
	
	var sKind		= "getBOMZ561Q";						
	var sMethodName = "service::psimmlib/selectBOMZ561Q.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_BOMZ561Q=ds_BOMZ561Q";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
	sArgument += "@LIB1=" + quote(Lib1);
	sArgument += "@LIB2=" + quote(Lib2);
	sArgument += "@COMPS=" + quote(cb_searchCodeName1.Value);
	sArgument += "@PRVND1=" + quote(p_vnd1);
	sArgument += "@PRVND2=" + quote(p_vnd2);
	sArgument += "@EXPNO1=" + quote(p_prt1);
	sArgument += "@EXPNO2=" + quote(p_prt2);
	sArgument += "@CURYM=" + quote(substr(nowDate,0,6));
	sArgument += "@YM01=" + quote(substr(AddMonth(nowDate,1),0,4)+substr(AddMonth(nowDate,1),4,2));
	sArgument += "@YM02=" + quote(substr(AddMonth(nowDate,2),0,4)+substr(AddMonth(nowDate,2),4,2));
	sArgument += "@YM03=" + quote(substr(AddMonth(nowDate,3),0,4)+substr(AddMonth(nowDate,3),4,2));
	sArgument += "@SEL=" + quote(cmbSelect.Value);
	sArgument += "@COMPANY=" + quote(company);
	//sArgument += "@NDATE=" + quote(nowDate);
	var str = dialog("Common::PGMLOAD_IMM.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");	
	//Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");
	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
		setMessage("1004");	
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));
		setMessage("1003");	
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}
}


