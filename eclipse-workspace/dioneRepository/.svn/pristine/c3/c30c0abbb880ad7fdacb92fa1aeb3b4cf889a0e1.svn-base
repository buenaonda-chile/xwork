/********************************************************************** 
 * 내용설명 : 사업부 선택
 * 주요변수 : 
 **********************************************************************/ 
function getComCode(){

	http.Sync 	= true;

	var sKind		= "getComCode";						
	var sMethodName = "service::login/selectComCode.do"; 
	var sInDataSet  = "";
	var sOutDataSet = "ds_ComCode_PLNT=ds_ComCode_PLNT";
	var sArgument   = "";
		sArgument   += " CLASS='PLNT'";
		sArgument   += " CODE1=" + quote(company);		
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	CmbCOM.Value=comps;
}


/********************************************************************** 
 * 개 발 자 : 민명규 
 * 개발일자 : 2013.12.09
 * 내용설명 : 화면조회
 * 주요변수 : 
 **********************************************************************/ 
function getHPM100_rep(){

	http.Sync = true;
	
	var sKind		= "getHPM100_rep";						
	var sMethodName = "service::pshumlib/selectHPM100_rep.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_HPM100_rep=ds_HPM100_rep";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
		
	if(length(trim(Calendar0.Value)) == 0){
		alert("조회년월을 선택하세요!");	
		Calendar0.SetFocus();
		return;
	}
			
	sArgument += "@LIB1=" + quote(Lib1);
	sArgument += "@LIB2=" + quote(Lib2);
	sArgument += "@LIB3=" + quote(Lib3);
	sArgument += "@COMPS=" + quote(CmbCOM.Value);
	sArgument += "@KNYYM=" + quote(LEFT(Calendar0.Value,4));
	sArgument += "@USER_ID=" + quote(G_ds_user.GetColumn(0,"USER_ID"));
	
	
	var str = dialog("Common::PGMLOAD_IMM.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");	
	//Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");
	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
		setMessage("1004");	
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));
		setMessage("1003");	
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}
		

	Ubi0.setJrfFileDir(G_baseURL+"ui/PSHUMLIB/REPORT/");

	if(comps == 'E1'){
		Ubi0.setJrfFileName("HPM100.jrf");
	}
	else{
		Ubi0.setJrfFileName("HPM100_1.jrf");
	}
			
	Ubi0.SetDataset("ds_HPM100_rep", "ds_HPM100_rep");
	Ubi0.SetArgument("COMPS", CmbCOM.Value);
	Ubi0.Retrieve();
	
	if(ds_HPM100_rep.GetRowCount() < 1){
		alert("년차 내역이 없습니다.");
		return;
	}	
	
	http.Sync=false;
}


