		groupcomp = "";
	}
	http.Sync = true;
	var sKind		= "getMIZZ229Q2";						
	var sMethodName = "service::budget/getMIZZ229Q2List";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                            				// 자료 저장 시 보낼 DataSet 명칭
