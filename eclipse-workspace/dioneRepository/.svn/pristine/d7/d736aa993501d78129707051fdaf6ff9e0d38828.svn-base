		sArgument += " LIB2=" + quote(Lib2);
		sArgument += " COMPS=" + quote(ComboBoxPlant.Value); //사업부
		sArgument += " DHCODE=" + quote(ed_CODE.Text); //부서코드
		sArgument += " WRKDT=" + quote(cmb_WRKDT.Value); //작업일자
	http.Sync=true;
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");
	http.Sync=false;

	if(ds_FHDZ201R.rowcount>0){
		for(i=0; i<ds_FHDZ201R.rowcount; i++){		
			if(i%2==0){
				ds_FHDZ201R_NEW.AddRow();
				ds_FHDZ201R_NEW.SetColumn(ds_FHDZ201R_NEW.row,"No1",i+1);
				ds_FHDZ201R_NEW.SetColumn(ds_FHDZ201R_NEW.row,"COMPS",ComboBoxPlant.Value);
				ds_FHDZ201R_NEW.SetColumn(ds_FHDZ201R_NEW.row,"DHCODE",ed_CODE.Text);
				ds_FHDZ201R_NEW.SetColumn(ds_FHDZ201R_NEW.row,"CARNM1",ds_FHDZ201R.GetColumn(i,"CARNM"));
				ds_FHDZ201R_NEW.SetColumn(ds_FHDZ201R_NEW.row,"PSPNO1",ds_FHDZ201R.GetColumn(i,"PSPNO"));
				ds_FHDZ201R_NEW.SetColumn(ds_FHDZ201R_NEW.row,"MANHR1",ds_FHDZ201R.GetColumn(i,"MANHR"));
				ds_FHDZ201R_NEW.SetColumn(ds_FHDZ201R_NEW.row,"FHDQY1",ds_FHDZ201R.GetColumn(i,"FHDQY"));
				ds_FHDZ201R_NEW.SetColumn(ds_FHDZ201R_NEW.row,"GIJTM1",ds_FHDZ201R.GetColumn(i,"GIJTM"));
			}
			else{
				ds_FHDZ201R_NEW.SetColumn(ds_FHDZ201R_NEW.row,"No2",i+1);
				ds_FHDZ201R_NEW.SetColumn(ds_FHDZ201R_NEW.row,"CARNM2",ds_FHDZ201R.GetColumn(i,"CARNM"));
				ds_FHDZ201R_NEW.SetColumn(ds_FHDZ201R_NEW.row,"PSPNO2",ds_FHDZ201R.GetColumn(i,"PSPNO"));
				ds_FHDZ201R_NEW.SetColumn(ds_FHDZ201R_NEW.row,"MANHR2",ds_FHDZ201R.GetColumn(i,"MANHR"));
				ds_FHDZ201R_NEW.SetColumn(ds_FHDZ201R_NEW.row,"FHDQY2",ds_FHDZ201R.GetColumn(i,"FHDQY"));
				ds_FHDZ201R_NEW.SetColumn(ds_FHDZ201R_NEW.row,"GIJTM2",ds_FHDZ201R.GetColumn(i,"GIJTM"));		
			}
		}
	
	}
	//var WRKQYT=0;//생산실적
	//var GIJTMT=0; //기준시간
	//var TOTTMT=0;//실적총시간
	//var DOWNPT=0;//저감지수
	//var DALSUT=0;//달성율
	
	WRKQYT=0;//생산실적
	GIJTMT=0; //기준시간
	TOTTMT=0;//실적총시간
	DOWNPT=0;//저감지수
	DALSUT=0;//달성율
		
	var EXPTMG=0; //제외/관리시간
	var SUBTMA=0; //가동시간합계1
	var SUBTMB=0; //가동시간합계2
	var DTCTM=0; //총시간
	var ETBTM=0; //실적총시간
	var AAATM=0; //기준시간
	var GTFTM=0; //작업능률
	var GTETM=0; //가동률
	var HTITM=0; //종합능률
		
	if(ds_FHDZ201R.rowcount>0){//a
		for(i=0; i<ds_FHDZ201R.rowcount; i++){
			GIJTMT+=parseInt(ds_FHDZ201R.GetColumn(i,"GIJTM"));
			WRKQYT+=parseInt(ds_FHDZ201R.GetColumn(i,"FHDQY"));
		}
	}else{
		GIJTMT=0;
		WRKQYT=0;
	}
	if(ds_FHDZ201R_MGMTM.rowcount>0){//b
		for(i=0; i<ds_FHDZ201R_MGMTM.rowcount; i++){
			EXPTMG+=parseInt(ds_FHDZ201R_MGMTM.GetColumn(i,"EJTM"));
			EXPTMG+=parseInt(ds_FHDZ201R_MGMTM.GetColumn(i,"EYTM"));
			EXPTMG+=parseInt(ds_FHDZ201R_MGMTM.GetColumn(i,"MJTM"));
			EXPTMG+=parseInt(ds_FHDZ201R_MGMTM.GetColumn(i,"MYTM"));
		}
	}else{
		EXPTMG=0;
	}	

	SUBTMA = parseInt(ds_FHDZ201R_TIME.GetColumn(0,"SUBTMA"));//c
	SUBTMB = parseInt(ds_FHDZ201R_TIME.GetColumn(0,"SUBTMB"));//d
	
	DTCTM=SUBTMB-SUBTMA; //총시간
	ETBTM=DTCTM-EXPTMG; //실적총시간
	//START 실적총시간/저감지수/달성율
	TOTTMT=ETBTM;//실적총시간
	DOWNPT = truncate(TOTTMT/GIJTMT*100,1); //저감지수
	if(DOWNPT>=999.9){
		DOWNPT=999.9;
	}
	DALSUT = truncate(gisuyl/DOWNPT*100,1); //저감지수
	if(DALSUT>=999.9){
		DALSUT=999.9;
	}	
//alert(TOTTMT+"="+DOWNPT+"="+DALSUT);
	//END
	AAATM=GIJTMT; //기준시간
	GTFTM=truncate(AAATM/ETBTM*100,1); //작업능률
	GTETM=truncate(AAATM/DTCTM*100,1); //가동률
	if(GTETM>=9999.9){
		GTETM=9999.9;
	}
	HTITM=truncate(GTFTM*GTETM/100,1); //종합능률
	ds_FHDZ201R_TOT.AddRow();
	ds_FHDZ201R_TOT.SetColumn(ds_FHDZ201R_TOT.row,"DTCTM",DTCTM);
	ds_FHDZ201R_TOT.SetColumn(ds_FHDZ201R_TOT.row,"ETBTM",ETBTM);
	ds_FHDZ201R_TOT.SetColumn(ds_FHDZ201R_TOT.row,"AAATM",AAATM);
	ds_FHDZ201R_TOT.SetColumn(ds_FHDZ201R_TOT.row,"GTFTM",GTFTM);
	ds_FHDZ201R_TOT.SetColumn(ds_FHDZ201R_TOT.row,"GTETM",GTETM);
	ds_FHDZ201R_TOT.SetColumn(ds_FHDZ201R_TOT.row,"HTITM",HTITM);
	
	
	getSYSDATE(); //SYSTEM 날짜
	var sysDt =ds_SYSDATE.GetColumn(0,"DT");
	var systm =ds_SYSDATE.GetColumn(0,"TM");
	var chkdpnm =st_CODE.Text;
	var chkdpcd =ed_CODE.Text;
	var chkwrkdt =cmb_WRKDT.Value;
	
	if(ds_FHDZ201R.rowcount < 1){
		Ubi0.setJrfFileDir(G_baseURL+"ui/PSFACLIB/REPORT/");
		Ubi0.setJrfFileName("blank.jrf");
	}else{
		//Ubi0.setJrfFileDir("http://dione.dnke.kr/ui/PSFACLIB/REPORT/");
		//Ubi0.setJrfFileDir("http://10.74.139.50/ui/PSFACLIB/REPORT/");
		Ubi0.setJrfFileDir(G_baseURL+"ui/PSFACLIB/REPORT/");
		Ubi0.setJrfFileName("FHDZ201R.jrf");
		Ubi0.SetDataset("ds_FHDZ201R_NEW", "ds_FHDZ201R_NEW");
		Ubi0.SetDataset("ds_FHDZ201R_MGMTM", "ds_FHDZ201R_MGMTM");
		Ubi0.SetDataset("ds_FHDZ201R_TIME", "ds_FHDZ201R_TIME");
		Ubi0.SetDataset("ds_FHDZ201R_TOT", "ds_FHDZ201R_TOT");
		Ubi0.SetArgument("SYSDATE",sysDt);
		Ubi0.SetArgument("SYSTIME",systm);
		Ubi0.SetArgument("CHKWRKDT",chkwrkdt);
		Ubi0.SetArgument("CHKDPNM",chkdpnm);
		Ubi0.SetArgument("CHKDPCD",chkdpcd);
		Ubi0.SetArgument("WRKQYT",WRKQYT);
		Ubi0.SetArgument("GIJTMT",GIJTMT);
		Ubi0.SetArgument("TOTTMT",TOTTMT);
		Ubi0.SetArgument("DOWNPT",DOWNPT);
		Ubi0.SetArgument("DALSUT",DALSUT);
		Ubi0.SetArgument("Goal1",displayDiv.st_Goal1.Text);
		Ubi0.SetArgument("Goal2",displayDiv.st_Goal2.Text);	
		Ubi0.SetArgument("EXPTMG",EXPTMG);
	}
	Ubi0.Retrieve();	
//alert(DTCTM+"="+ETBTM+"="+AAATM+"="+GTFTM+"="+GTETM+"="+HTITM);
}
/********************************************************************** 
 * 내용설명 : Transaction 리턴후 실행 함수
 * 주요변수 : 
 * 			- sMethodName	Transaction 고유 ID
 * 			- ErrorCode 	에러코드
 * 			- ErrorMSG 		에러 메세지
 **********************************************************************/ 
function callback(svcId, rtnCd, rtnMsg){
	// tran 실패
	if(rtnCd<0){
		alert(setPopMessage("1002"));
		setMessage("1002");
	}else if(svcId=="getFHDZ201R"){
		// 조회건수가 없을경우
		if((ds_FHDZ201R.rowcount <= 0) && (ds_FHDZ201R_MGMTM.rowcount <= 0) && (ds_FHDZ201R_TIME.rowcount <= 0)){
			//alert("조회된거 없다!");
			alert(setPopMessage("1003"));
			setMessage("1003");	
		// 정상 조회
		} else {
			//alert("조회됐다.!");
			alert(setPopMessage("1004"));
			setMessage("1004");				
		}
	}
}
