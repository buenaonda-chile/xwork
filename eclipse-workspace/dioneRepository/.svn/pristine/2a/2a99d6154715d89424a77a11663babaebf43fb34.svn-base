		sArgument += "@LIB1=" + quote(Lib1);
		sArgument += "@LIB2=" + quote(Lib2);
		sArgument += "@COMPS=" + quote(ComboBoxPlant.Value);
		sArgument += "@GATCD=" + quote(cmb_GATE.Value);
		//sArgument += "@WKYMD=" +  quote(substr(cal_YMD.Value,2,6));
		sArgument += "@WKYMD=" +  quote(cal_YMD.Value);
		sArgument += "@DAYCD=" +  quote(getDay(cal_YMD.Value)+1);
	var str = dialog("Common::PGMLOAD_SHIP.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");	
	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
