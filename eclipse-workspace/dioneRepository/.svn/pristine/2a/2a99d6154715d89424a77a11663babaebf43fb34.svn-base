//table에 저장할 data생성(table포멧)
function newFSAZ140(){

	ds_FSAZ140_Re.ClearData();
	var p_ChaS="";
	var p_ChaE="";
	var p_TRUCD="";
	
	if(ds_FSAZ140.rowcount>0){
		//alert("OK");
		for(i=0; i<ds_FSAZ140.rowcount; i++){
			for(j=1; j<=10; j++){
				p_ChaS=ds_FSAZ140.GetColumn(i,"CHA"+j+"2S");
				p_ChaE=ds_FSAZ140.GetColumn(i,"CHA"+j+"2E");
				p_TRUCD=ds_FSAZ140.GetColumn(i,"TRUCD"+j);
				if(length(trim(p_ChaS))!=0){
					ds_FSAZ140_Re.AddRow();
					ds_FSAZ140_Re.SetColumn(ds_FSAZ140_Re.currow,"COMPS",ComboBoxPlant.Value);
					ds_FSAZ140_Re.SetColumn(ds_FSAZ140_Re.currow,"WKYMD",cal_YMD.Value);
					//ds_FSAZ140_Re.SetColumn(ds_FSAZ140_Re.currow,"WKYMD",substr(cal_YMD.Value,2,6));
					ds_FSAZ140_Re.SetColumn(ds_FSAZ140_Re.currow,"GATCD",cmb_GATE.Value);
					ds_FSAZ140_Re.SetColumn(ds_FSAZ140_Re.currow,"TRACD",ds_FSAZ140.GetColumn(i,"TRACD"));
					ds_FSAZ140_Re.SetColumn(ds_FSAZ140_Re.currow,"CHASU",j);
					ds_FSAZ140_Re.SetColumn(ds_FSAZ140_Re.currow,"FTIMS",p_ChaS);
					ds_FSAZ140_Re.SetColumn(ds_FSAZ140_Re.currow,"FTIME",p_ChaE);
					ds_FSAZ140_Re.SetColumn(ds_FSAZ140_Re.currow,"TRUCD",p_TRUCD);
					ds_FSAZ140_Re.SetColumn(ds_FSAZ140_Re.currow,"PSEQNO",trim(ds_FSAZ140.GetColumn(i,"TRACD"))+j);
				}
			}
		}
		ds_FSAZ140_Cal.Copy(ds_FSAZ140_Re);
	}
}

