 **********************************************************************/ 
 /*
 * 트럭번호가 없는 경우 저장이 안됨.
 */
function setFSAZ140(){

	if(ds_FSAZ140.GetRowType(ds_FSAZ140.currow)=="update" ||ds_FSAZ140.GetRowType(ds_FSAZ140.currow)=="insert" ){
		if(!ds_FSAZ140_CanRowPosChange()){
			return;
		}
	} 
	if(ds_FSAZ140_Cal.rowcount<1){
		alert("[확인] 처리할 내역이 없습니다.");//[확인] 처리할 내역이 없습니다.
		//alert(setPopMessage("2564"));//[확인] 처리할 내역이 없습니다.
		//setMessage("2564");
		return;
	}
	
	var sKind		= "setFSAZ140";						
	var sMethodName = "service::shipment/setFSAZ140List";     // 서버에서 호출 될 Method 명
	var sInDataSet  = "ds_FSAZ140_Cal=ds_FSAZ140_Cal:U";     //자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet  = "ds_FSAZ140_CHECK=ds_FSAZ140_CHECK";     //자료 저장 시 보낼 DataSet 명칭
	var sArgument   = "";
		//sArgument += " LIB1=" + quote(Lib1);
		sArgument += " LIB2=" + quote(Lib2);
		sArgument += " SUSER=" + quote(userId); //사용자
	http.Sync=true;
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");
	http.Sync=false;
	
}
/********************************************************************** 
 * 내용설명 : Transaction 리턴후 실행 함수
 * 주요변수 : 
 * 			- sMethodName	Transaction 고유 ID
 * 			- ErrorCode 	에러코드
 * 			- ErrorMSG 		에러 메세지
 **********************************************************************/ 
function callback(svcId, rtnCd, rtnMsg){
	if (ds_FSAZ140_CHECK.rowcount > 0){
		for(var i=0;i<ds_FSAZ140_CHECK.GetRowCount();i++){
			alert(" 행선지코드 : " +ds_FSAZ140_CHECK.GetColumn(i,"TRACD") + " 편대차수 : " +ds_FSAZ140_CHECK.GetColumn(i,"CHASU")
				  + " 차량번호 : " +ds_FSAZ140_CHECK.GetColumn(i,"TRUCD") + " = " + ds_FSAZ140_CHECK.GetColumn(i,"RTMSG"));
		}
		getFSAZ140();  //error항목이 존재시에는 재조회
		return;
	}// tran 실패
	if(rtnCd<0){
		alert(setPopMessage("1002"));
		setMessage("1002");
	}else{
		for(i=ds_FSAZ140_Cal.rowcount; i>=0; i--){
			if(length(trim(ds_FSAZ140_Cal.GetColumn(i,"TRUCD")))==0){
				ds_FSAZ140_Cal.DeleteRow(i);
			}
		}
		alert(setPopMessage("1001"));
		setMessage("1001");	
	}
}
