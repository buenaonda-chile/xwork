function ed_Action_OnKeyDown(obj,nChar,bShift,bCtrl, bAlt,LLParam,HLParam)
{
	if(nChar == 13){
		switch(obj.text){
			case "A" : fn_Input(); break;
			case "Q" : fn_Search(); break;
			default : alert(setPopMessage("1016")); setMessage("1016");
		}
	}
}

function ds_BRRE100_CanColumnChange(obj,nRow,strColumnID,varOldValue,varNewValue,nPivotIndex)
{
	if( strColumnID == "MODNO" ){
		if (length(varNewValue) < 1) return false;
		//품번 Check
		var sKind		= "getBM008PR";		
		var sMethodName = "service::pscomlib/selectBM008PR.do"; 
		var sInDataSet  = "";
		var sOutDataSet = "ds_BM008PR=ds_BM008PR";
		var sArgument   = "";
			sArgument  += " LIB1=" + quote(Lib1);
			sArgument  += " ITNBR=" + quote(trim(varNewValue));
		http.Sync = true;	
		Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
		http.Sync = false;
		if (ds_BM008PR.GetRowCount() < 1){
			alert(setPopMessage("1509"));  // [경고] Item Master에 미등록된 품번입니다.
			setMessage("1509");
			displayDiv.ed_MODNO.SetFocus();
			return false;
		}
		can_ITDSC = ds_BM008PR.GetColumn(0,"ITDSC");
		can_UNMSR = ds_BM008PR.GetColumn(0,"UNMSR");
	}
	
	if( strColumnID == "MODPY" ){
		if (length(varNewValue) < 1) return false;
		//업체코드 Check
		var vndnr = varNewValue;
		var sKind		= "getVENNAM";		
		var sMethodName = "service::pscomlib/selectVENNAM.do"; 
		var sInDataSet  = "";
		var sOutDataSet = "ds_VENNAM=ds_VENNAM";
		var sArgument   = "";
			sArgument  += " LIB1=" + quote(Lib1);
			sArgument  += " VNDNR=" + quote(vndnr);
		http.Sync 	= true;
		Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
		http.Sync 	= false;
		if (ds_VENNAM.GetRowCount() != 1){
			alert(setPopMessage("4671"));  //[확인] 업체 확인바랍니다.
			setMessage("4671");	
			return false;
		}
		can_VNAME = ds_VENNAM.GetColumn(0,"VNAME");
	}
	
	
}

function ds_BRRE100_OnColumnChanged(obj,nRow,strColumnID,varOldValue,varNewValue,nPivotIndex)
{		
	if (strColumnID == "MODNO"){
		ds_BRRE100.SetColumn(nRow,"MODNM",trim(can_ITDSC));
		ds_BRRE100.SetColumn(nRow,"MODUM",can_UNMSR);
	}
	if (strColumnID == "MODPY"){
		displayDiv.st_VNAME.Text = can_VNAME; 
	}
}

function ds_BRRE100_OnRowPosChanged(obj,nOldRow,nRow)
{
	//로우타입 insert만 ED박스 enable
	if(obj.GetRowType(obj.currow)=="insert"){
		displayDiv.ed_MODNO.Enable=true;
	}else{
		displayDiv.ed_MODNO.Enable=false;
	}
}

function displayDiv_ed_MODMK_OnKeyDown(obj,nChar,bShift,bCtrl, bAlt,LLParam,HLParam)
{
	if(nChar == 13){
		//displayDiv.ed_Flag.SetFocus();  // ed_PSPNO에 데이타셋 이벤트가 발생된 후 텟스트값이 활당되도록 포커스를 줌.
		if(length(trim(displayDiv.ed_MODNO.Text)) > 0 || length(trim(displayDiv.ed_MODPY.Text)) > 0) {
			if(confirm("저장하시겠습니까? ") == false) return;
			fn_Save();
			ed_Action.SetFocus();//fn_Input();
		}
	}	
}

function ed_MODNO_OnChanged(obj,strText)
{
	ds_BRRE100.ClearData();
}
