/********************************************************************** 
 * 내용설명 : 사업부 선택
 * 주요변수 : 
 **********************************************************************/ 
function getComCode(){

	http.Sync 	= true;

	var sKind		= "getComCode";						
	var sMethodName = "service::login/selectComCode.do"; 
	var sInDataSet  = "";
	var sOutDataSet = "ds_ComCode_PLNT=ds_ComCode_PLNT";
	var sArgument   = "";
		sArgument   += " CLASS='PLNT'";
		sArgument   += " CODE1=" + quote(company);		
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	ComboBoxPlant.Value = comps;
	
	
}


/********************************************************************** 
 * 내용설명 : 화면조회
 * 주요변수 : 
 **********************************************************************/ 
function getBRRE100(){
	var sKind		= "getBRRE100";				
	var sMethodName = "service::psimmlib/selectBRRE100.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_BRRE100=ds_BRRE100";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";	
	sArgument += "@LIB1=" + quote(Lib1);
	sArgument += "@LIB2=" + quote(Lib2);
	sArgument += "@MODNO=" + quote(trim(ed_MODNO.Text));
	http.Sync = true;
	var str = dialog("Common::PGMLOAD_IMM.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");
	http.Sync = false;

}


/********************************************************************** 
 * 내용설명 : 저장
 * 주요변수 : 
 * 			
 **********************************************************************/ 
 
 function setBRRE100(){
 	if(!validationChk()){
		return;
	}
	var sKind		= "setBRRE100";						
	var sMethodName = "service::psimmlib/saveBRRE100.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "ds_BRRE100=ds_BRRE100:U";                      // 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_BRRE100_Check=ds_BRRE100_Check";							// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
	sArgument += " LIB1=" + quote(Lib1);
	sArgument += " LIB2=" + quote(Lib2);
	//sArgument += " JPNNO=" + quote(trim(ed_JPNNO.Text));
	//sArgument += " CONNO=" + quote(ed_CONNO.Text);
	//sArgument += " TRNDT=" + quote(ca_TRNDT.Value);
	//sArgument += " HOUSE=" + quote(ed_HOUSE.Text);
	
	http.Sync=true;
	//var str = dialog("Common::PGMLOAD_IMM.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");	
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");
	http.Sync=false;
 }

/********************************************************************** 
 * 내용설명 : Transaction 리턴후 실행 함수
 * 주요변수 : 
 * 			- sMethodName	Transaction 고유 ID
 * 			- ErrorCode 	에러코드
 * 			- ErrorMSG 		에러 메세지
 **********************************************************************/ 
function callback(svcId, rtnCd, rtnMsg){
	
	if (svcId == "setBRRE100"){
		if (ds_BRRE100_Check.GetRowCount() > 0){			
			for(var i=0;i<ds_BRRE100_Check.GetRowCount();i++){		
				alert(ds_BRRE100_Check.GetColumn(i,"MODNO")+" : "+ds_BRRE100_Check.GetColumn(i,"RTMSG"));	
			}
		}
	}
	// tran 실패
	if(rtnCd<0){
		alert(setPopMessage("1002"));
		setMessage("1002");
	}else {
		alert(setPopMessage("1001"));
		setMessage("1001");	
	}
}
/********************************************************************** 
 * 내용설명 : 컴포넌트 validation 체크
 **********************************************************************/ 
function validationChk(){
	var result = true;
	if(length(trim(displayDiv.ed_MODNO.text)) < 1){
		alert(setPopMessage("1501")); //[[확인] 품번 확인바랍니다.
		setMessage("1501");
		displayDiv.ed_MODNO.SetFocus();
		return false;
	}
	if(length(trim(displayDiv.st_VNAME.text)) < 1){
		alert(setPopMessage("4671")); //[확인] 업체 확인바랍니다.
		setMessage("4671");
		displayDiv.ed_MODPY.SetFocus();
		return false;
	}
	return true;
}


