function getMSTZ100Q(){

	if(length(trim(ed_CARCD.Text)) == 0){
		alert("차종코드를 입력해주세요!");	
		ed_CARCD.SetFocus();
		return;
	}
	

	var sKind		= "getMSTZ100Q";						
	var sMethodName = "service::psmstlib/selectMSTZ100Q.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_MSTZ100Q=ds_MSTZ100Q";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
		sArgument += "@LIB1=" + quote(Lib1);
		sArgument += "@LIB2=" + quote(Lib2);
		sArgument += "@COMPS=" + quote(ComboBoxPlant.Value); //사업부
		sArgument += "@CARCD=" + quote(ed_CARCD.Text); //차종코드
		sArgument += "@RGNCD=" + quote(ed_RGNCD.Text); //지역코드
	
	if(CmbCPGUB.Value == '1'){
		sArgument += "@CPGUB1=" + quote(CmbCPGUB.Value); //지역코드
	} else if (CmbCPGUB.Value == '2'){				
		sArgument += "@CPGUB2=" + quote(CmbCPGUB.Value);
	} else {		
		sArgument += "@CPGUB3=" + quote(CmbCPGUB.Value);
	}
		
	var str = dialog("Common::PGMLOAD_MST.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");

	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
		setMessage("1004");
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));
		setMessage("1003");	
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}
}

/********************************************************************** 
 * 내용설명 : insert, update, delete
 **********************************************************************/ 
 
 //등록시 업체코드 존재유무 체크함. 없으면 동록불가.
function setMSTZ100Q(){
 
 
	if(ds_MSTZ100Q.count() == 0){
		alert("조회 후 저장하여 주십시오!");	
		return;
	}
	 
	if(length(trim(cal_TDATE.Value)) == 0){
		alert("적용일을 선택하세요!");	
		cal_TDATE.SetFocus();
		return;
	}

 
	var sKind		= "setMSTZ100Q";						
	var sMethodName = "service::psmstlib/insertMSTZ100.do";    // 서버에서 호출 될 Method 명
	var sInDataSet  = "ds_MSTZ100Q=ds_MSTZ100Q:U";               //자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_MSTZ100Q_CHECK=ds_MSTZ100Q_CHECK";  	   // 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
		sArgument += " LIB2=" + quote(Lib2);
		sArgument += " CARCD=" + quote(ed_CARCD.Text); //차종코드
		sArgument += " RGNCD=" + quote(ed_RGNCD.Text); //지역코드
		sArgument += " CPDAT=" + quote(cal_TDATE.Value);
		
		
	http.Sync=true;
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");
	http.Sync=false;
}
/********************************************************************** 
 * 내용설명 : Transaction 리턴후 실행 함수
 * 주요변수 : 
 * 			- sMethodName	Transaction 고유 ID
 * 			- ErrorCode 	에러코드
 * 			- ErrorMSG 		에러 메세지
 **********************************************************************/ 
function callback(svcId, rtnCd, rtnMsg){
	
	getMSTZ100Q();  //error항목이 존재시에는 재조회
	
	// tran 실패
	if(rtnCd<0){
		alert(setPopMessage("1002"));
		setMessage("1002");
	}else 
	{
		//getSalz101();  //error항목이 존재시에는 재조회
		alert(setPopMessage("1001"));
		setMessage("1001");	
	}	
}

