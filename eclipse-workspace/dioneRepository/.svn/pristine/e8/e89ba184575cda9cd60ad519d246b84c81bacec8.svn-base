function ds_ogm090A_CanColumnChange(obj,nRow,strColumnID,varOldValue,varNewValue,nPivotIndex)
{
	http.usewaitcursor = false;
	if( strColumnID == "MSHNO" ){
		if( length(trim(varNewValue)) == 0 ){
			alert(setPopMessage("2540"));
			setMessage("2540");
			return false;
		}
		//중복check 삭제 후 데이타셋 새로 만들어서 체크해야 함.
		var sKind		= "getogm090A_Chk";
		var sMethodName = "service::psogmlib/selectOGM090A_Chk.do";     		// 서버에서 호출 될 Method 명
		var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
		var sOutDataSet = "ds_ogm090A_Chk=ds_ogm090A_Chk";    				// 조회 후 돌려 받을 DataSet 명칭
		var sArgument   = "";
		sArgument += " LIB2=" + quote(Lib2);
		sArgument += " COMPS=" + quote(ComboBoxPlant.Value); //사업부
		//sArgument += " TABLENM=" + quote(tableNm); //TABLE명
		//sArgument += " FIELD1=" + quote(field1); //FIELD명1
		sArgument += " FVALUE=" + quote(varNewValue); //FIELD명1
		http.Sync = true;
		Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
		http.Sync = false;
		if (ds_ogm090A_Chk.GetRowCount() != 0){
			alert(setPopMessage("2533"));
			setMessage("2533");
			return false;
		}
	}

	if(strColumnID == "DELGB" ){
		if (varNewValue == "1"){
			var code = ds_ogm090A.GetColumn(nRow,"MSHNO");
					
			//var tableNm = substr(cmb_Code.Value,21,8);
			//var field1 = substr(MSHNO,0,4);
				
			var sKind		= "getogm090A_DelChk";
			var sMethodName = "service::psogmlib/selectOGM090A_DelChk.do";     		// 서버에서 호출 될 Method 명
			var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
			var sOutDataSet = "ds_ogm090A_DelChk=ds_ogm090A_DelChk";    				// 조회 후 돌려 받을 DataSet 명칭
			var sArgument   = "";
			sArgument += " LIB2=" + quote(Lib2);
			sArgument += " COMPS=" + quote(ComboBoxPlant.Value); //사업부
			//sArgument += " TABLENM=" + quote(tableNm); //TABLE명
			//sArgument += " FIELD1=" + quote(field1); //FIELD명1
			sArgument += " CODE=" + quote(code); //FIELD명1
			http.Sync = true;
			Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
			http.Sync = false;
			if (ds_ogm090A_DelChk.GetColumn(0,"MSHNO") != "0"){
				alert(setPopMessage("2537"));
				setMessage("2537");
				return false;
			}
		}
	}
	http.usewaitcursor = true;
}

/********************************************************************** 
 * 내용설명 : 설비번호 소문자를 대문자로 변경
 **********************************************************************/ 
function ds_ogm090A_OnColumnChanged(obj,nRow,strColumnID,varOldValue,varNewValue,nPivotIndex)
{
	if( strColumnID == "MSHNO" ){
		ds_ogm090A.SetColumn(nRow,"MSHNO",toUpper(varNewValue));
	}
	//	http.usewaitcursor = false;
	if( strColumnID == "MSHNM" ){
		//alert("start=="+varNewValue);
		var vval=varNewValue; //GRID에 변경된값 
		var vnrow=nRow; //현재 ROW
		var vcid=strColumnID; //컬럼ID
		var lenLimit = MainGrid.GetCellProp("body","4","limit"); //현재 컬럼의 Maxlength
		//alert("strColumnID=="+vcid=+ "   nRow=="+vnrow+"   varNewValue=="+vval+"   lenLimit=="+lenLimit);
		var newVal = chkbyte_db2(vval, lenLimit); //변경된값과 MAxLength을 chkbyte_db2함수에 보내서 바이트 계싼함.
		if (length(newVal)==0){ //리턴받은 값이 0이면 STOP, 아니면 새로운값은 dataset에 set함.
			return;				//처음변경된값 수행 후 바이트된 계산된 값으로 재수행하기 때문에 2회차일경우 방지함.
		}
		//alert("리턴값 : "+newVal);
		//alert(vnrow+"  "+vcid+"  "+newVal);
		varNewValue=newVal;
		ds_ogm090A.SetColumn(vnrow,vcid,varNewValue);
		//ds_ogm080A.ApplyChange();
	//	alert(ds_ogm080A.GetColumn(vnrow,vcid));

	//	ds_ogm080A.SetColumn(nRow,"F2",toUpper(varNewValue));
	}
	
	if( strColumnID == "TIMNM" ){
		//alert("start=="+varNewValue);
		var vval=varNewValue; //GRID에 변경된값 
		var vnrow=nRow; //현재 ROW
