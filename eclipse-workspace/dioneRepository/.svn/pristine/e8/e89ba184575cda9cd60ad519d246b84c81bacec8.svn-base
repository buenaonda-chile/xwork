function getogm090A(){
	var sCode = trim(ed_Code.Text);
	var sSort = rd_Sort.Value;
	var sKind		= "getogm090A";
	var sMethodName = "service::psogmlib/selectOGM090A.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_ogm090A=ds_ogm090A";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
	sArgument += "@LIB2=" + quote(Lib2);
	sArgument += "@COMPS=" + quote(ComboBoxPlant.Value); //사업부
	sArgument += "@MSHNO=" + quote(sCode); //설비번호
	sArgument += "@FSORT=" + quote(sSort); // 정렬 조건 a: code      b:codenm
	http.Sync = true;
	var str = dialog("Common::PGMLOAD.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");
	http.Sync = false;
	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
		setMessage("1004");	
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));
		setMessage("1003");	
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}
	if(ds_ogm090A.GetRowCount() < 1){
		ds_ogm090A.AddColumn("COMPS");
		ds_ogm090A.AddColumn("MSHNO");
		ds_ogm090A.AddColumn("JSANO");
		ds_ogm090A.AddColumn("JSACD");
		
		ds_ogm090A.AddColumn("MSHNM");
		ds_ogm090A.AddColumn("DPTCD");
		ds_ogm090A.AddColumn("TIMNM");
		ds_ogm090A.AddColumn("PRTNM");
		ds_ogm090A.AddColumn("BANNM");
		ds_ogm090A.AddColumn("SDPCD");
		ds_ogm090A.AddColumn("LPAML");
		ds_ogm090A.AddColumn("LPAMM");
		ds_ogm090A.AddColumn("LPAMS");
		ds_ogm090A.AddColumn("INDAT");
		
		ds_ogm090A.AddColumn("DELGB");
	}
}

