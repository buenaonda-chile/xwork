function getOgm501(){
	var sKind		= "getOgm501";						
	var sMethodName = "service::psogmlib/selectOgm501.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_Ogm501=ds_Ogm501";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
	sArgument += "@LIB2=" + quote(Lib2);
	sArgument += "@COMPS=" + quote(ComboBoxPlant.Value); //사업부
	var str = dialog("Common::PGMLOAD.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");
	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
		setMessage("1004");	
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));
		setMessage("1003");	
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}
	
	if (length(ds_Ogm501.GetColumn(0,"DYSTM")) == 5){
		ca_DYSTM.Value = today()+"0"+ds_Ogm501.GetColumn(0,"DYSTM");
	}else if(length(ds_Ogm501.GetColumn(0,"DYSTM")) == 1){
		ca_DYSTM.Value = today()+"00000"+ds_Ogm501.GetColumn(0,"DYSTM");
	}else{
		ca_DYSTM.Value = today()+ds_Ogm501.GetColumn(0,"DYSTM");
	}
	if (length(ds_Ogm501.GetColumn(0,"DYETM")) == 5){
		ca_DYETM.Value = today()+"0"+ds_Ogm501.GetColumn(0,"DYETM");
	}else if(length(ds_Ogm501.GetColumn(0,"DYETM")) == 1){
		ca_DYETM.Value = today()+"00000"+ds_Ogm501.GetColumn(0,"DYETM");
	}else{
		ca_DYETM.Value = today()+ds_Ogm501.GetColumn(0,"DYETM");
	}
	if (length(ds_Ogm501.GetColumn(0,"NTSTM")) == 5){
		ca_NTSTM.Value = today()+"0"+ds_Ogm501.GetColumn(0,"NTSTM");
	}else if(length(ds_Ogm501.GetColumn(0,"NTSTM")) == 1){
		ca_NTSTM.Value = today()+"00000"+ds_Ogm501.GetColumn(0,"NTSTM");
	}else{
		ca_NTSTM.Value = today()+ds_Ogm501.GetColumn(0,"NTSTM");
	}
	if (length(ds_Ogm501.GetColumn(0,"NTETM")) == 5){
		ca_NTETM.Value = today()+"0"+ds_Ogm501.GetColumn(0,"NTETM");
	}else if(length(ds_Ogm501.GetColumn(0,"NTETM")) == 1){
		ca_NTETM.Value = today()+"00000"+ds_Ogm501.GetColumn(0,"NTETM");
	}else{
		ca_NTETM.Value = today()+ds_Ogm501.GetColumn(0,"NTETM");
	}
}

function setOgm501(){
	var startDay = substr(ca_DYSTM.Value,8,6);
	var endDay = substr(ca_DYETM.Value,8,6);
	var startNight= substr(ca_NTSTM.Value,8,6);
	var endNight= substr(ca_NTETM.Value,8,6);

	var sKind		= "setOgm501";						
	var sMethodName = "service::psogmlib/SaveOgm501.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                                   		// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "";    				                        // 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
	sArgument += " LIB2=" + quote(Lib2);
	sArgument += " COMPS=" + quote(ComboBoxPlant.Value); //사업부
	sArgument += " DYSTM=" + quote(startDay);   //주간시작시간
	sArgument += " DYETM=" + quote(endDay);     //주간종료시간
	sArgument += " NTSTM=" + quote(startNight); //야간시작시간
	sArgument += " NTETM=" + quote(endNight);   //야간종료시간
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");
}

/********************************************************************** 
 * 내용설명 : Transaction 리턴후 실행 함수
 **********************************************************************/ 
function callback(svcId, rtnCd, rtnMsg){
	// tran 실패
	if(rtnCd<0){
		alert(setPopMessage("1002"));
		setMessage("1002");
	// tran 성공	
	}else if(svcId=="setOgm501"){
		alert(setPopMessage("1001"));
		setMessage("1001");
	}	
}

