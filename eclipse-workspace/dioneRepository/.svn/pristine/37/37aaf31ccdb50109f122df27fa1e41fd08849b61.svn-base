#include "script::lib_script_common.js";

var Lib = G_ds_user.GetColumn(0,"LIB2");
var CigmaLib = G_ds_user.GetColumn(0,"LIB1");
var selected;

function on_LoadCompleted(obj){	
	getTitle();	
	getComCode();
	search_ordat.Value = today();	
	gFcOnTopButtonColor("1"); //상단 버튼 활성화 여부 (기본:0,조회:1,변경:2)	
	setTimer(1, 9999999999999999999999);
	getList();
	//getChartList();	
}

function getComCode(){

	http.Sync 	= true;
	var sKind		= "getComCode";						
	var sMethodName = "service::login/selectComCode.do"; 
	var sInDataSet  = "";
	var sOutDataSet = "ds_ComCode_PLNT=ds_ComCode_PLNT";
	var sArgument   = "";
		sArgument   += " CLASS='PLNT'";
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");

	ds_ComCode_PLNT.Filter("code2=='" + G_ds_user.GetColumn(0,"PLANT_CD") +"'");	
	http.Sync 	= false;
}

/********************************************************************** 
 * 개 발 자 : 김정현 
 * 개발일자 : 2012-10-26
 * 내용설명 : 그룹 조회
 * 주요변수 : 
 **********************************************************************/ 
function getList(){
	var sKind		= "getOBSE04Group";						
	var sMethodName = "service::psogmlib/selectOBSE04Group.do";     	// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             					// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_list=DS_OBSE04Group";    						// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
	
	sArgument += " LIB=" + quote(Lib);
	sArgument += " COMPS=" + quote(search_comps.Value);			

	//alert(sArgument);
	
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");		
}

function getChartList(){
	/////////////////////////////
	//alert(left(getDate() + (Timem() - toFloat(Time()+"000")), 10));
	//alert(search_ordat.Value + '17');
	
	var selected = "";

	for(var i =0; i < ds_list.getRowCount(); i++){
		if(ds_list.getColumn(i,"CHK") == 1){
			selected += "'" + ds_list.getColumn(i,"PROCS") + "',";
		}
	}		
	/////////////////////////////

	var sKind		= "getChartList";						
	var sMethodName = "service::chart/selectChart04.do";    // 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             		// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_chart=ds_chart";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";	
	sArgument += " COMPS=" + quote(search_comps.Value);
	sArgument += " ORDAT=" + quote(search_ordat.Value);
	//sArgument += " ROWGB=" + quote(cmb_rowgb.Value);		
	sArgument += " ROWGB=" + quote(left(selected, length(selected)-1));
	
	sArgument += " DAYGU=" + quote(cd_daygu.Value);
	//sArgument += " PROCS_TO=" + quote(search_procsTo.Text);	
	
	//alert(sArgument);
	
	XChart0.ClearData("ClearDataFlag_Data");
	XChart0.ClearData("ClearDataFlag_Labels"); 
	
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");	
}

function loadingChart(){	
	xChart0.Titles[0].Text = "시간대별 생산실적 [공정별]";
	
	var chartCnt = ds_chart.rowCount();
	xChart0.OpenData("COD_Values", chartCnt, 1);
	// OpenData(COD, n1, n2) ==> COD : 키값, n1 : 시리즈수, n2 : 시리즈마다 데이터 수
		
	xChart0.LineWidth = 3;

	for(var i=0;i<chartCnt;i++){		
		//xChart0.Value(i, 0) = 0;
		xChart0.Value(i, 0) = ds_chart.GetColumn(i, "F08T09F2030T2130");
		
		if(cd_daygu.Value = 'D'){		
			if (left(getDate() + (Timem() - toFloat(Time()+"000")), 10) >= search_ordat.Value + '10') {
				xChart0.Value(i, 1) = ds_chart.GetColumn(i, "F09T10F2130T2300");
			}
			if (left(getDate() + (Timem() - toFloat(Time()+"000")), 10) >= search_ordat.Value + '11') {
				xChart0.Value(i, 2) = ds_chart.GetColumn(i, "F10T11F23T00");
			}
			if (left(getDate() + (Timem() - toFloat(Time()+"000")), 10) >= search_ordat.Value + '12') {
				xChart0.Value(i, 3) = ds_chart.GetColumn(i, "F11T12F00T02");
			}
			if (left(getDate() + (Timem() - toFloat(Time()+"000")), 10) >= search_ordat.Value + '13') {
				xChart0.Value(i, 4) = ds_chart.GetColumn(i, "F13T14F02T04");
			}		
			if (left(getDate() + (Timem() - toFloat(Time()+"000")), 10) >= search_ordat.Value + '14') {
				xChart0.Value(i, 5) = ds_chart.GetColumn(i, "F14T15F04T05");
			}
			if (left(getDate() + (Timem() - toFloat(Time()+"000")), 10) >= search_ordat.Value + '15') {
				xChart0.Value(i, 6) = ds_chart.GetColumn(i, "F15T16F05T06");
			}
			if (left(getDate() + (Timem() - toFloat(Time()+"000")), 10) >= search_ordat.Value + '16') {		
				xChart0.Value(i, 7) = ds_chart.GetColumn(i, "F16T17F06T07");
			}
			if (left(getDate() + (Timem() - toFloat(Time()+"000")), 10) >= search_ordat.Value + '17') {
				xChart0.Value(i, 8) = ds_chart.GetColumn(i, "F17T18F07T08");
			}		
			// 주간만
			if (left(getDate() + (Timem() - toFloat(Time()+"000")), 10) >= search_ordat.Value + '18') {
				xChart0.Value(i, 9) = ds_chart.GetColumn(i, "F18T19");
			}
			if (left(getDate() + (Timem() - toFloat(Time()+"000")), 10) >= search_ordat.Value + '19') {
				xChart0.Value(i, 10) = ds_chart.GetColumn(i, "F19T20");			
			}
			//xChart0.Value(i, 11) = ds_chart.GetColumn(i, "F20T21");	
		} else {
			xChart0.Value(i, 1) = ds_chart.GetColumn(i, "F09T10F2130T2300");			
			xChart0.Value(i, 2) = ds_chart.GetColumn(i, "F10T11F23T00");			
			xChart0.Value(i, 3) = ds_chart.GetColumn(i, "F11T12F00T02");
			
			if (left(getDate() + (Timem() - toFloat(Time()+"000")), 10) >= search_ordat.Value + '02') {
				xChart0.Value(i, 4) = ds_chart.GetColumn(i, "F13T14F02T04");
			}		
			if (left(getDate() + (Timem() - toFloat(Time()+"000")), 10) >= search_ordat.Value + '04') {
				xChart0.Value(i, 5) = ds_chart.GetColumn(i, "F14T15F04T05");
			}
			if (left(getDate() + (Timem() - toFloat(Time()+"000")), 10) >= search_ordat.Value + '05') {
				xChart0.Value(i, 6) = ds_chart.GetColumn(i, "F15T16F05T06");
			}
			if (left(getDate() + (Timem() - toFloat(Time()+"000")), 10) >= search_ordat.Value + '06') {		
				xChart0.Value(i, 7) = ds_chart.GetColumn(i, "F16T17F06T07");
			}
			if (left(getDate() + (Timem() - toFloat(Time()+"000")), 10) >= search_ordat.Value + '07') {
				xChart0.Value(i, 8) = ds_chart.GetColumn(i, "F17T18F07T08");
			}
		}
		xChart0.SerLeg(i) = ds_chart.GetColumn(i, "PROCS");
	}
	
	xChart0.CloseData("COD_Values");
	
	if(cd_daygu.Value = 'D') {		
		xChart0.Legend(0) = "08:00~";
		xChart0.Legend(1) = "09:00~";
		xChart0.Legend(2) = "10:00~";
		xChart0.Legend(3) = "11:00~";	
		xChart0.Legend(4) = "12:00~";		
		xChart0.Legend(5) = "14:00~";
