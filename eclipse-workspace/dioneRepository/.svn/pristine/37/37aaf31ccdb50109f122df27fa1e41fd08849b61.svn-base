		xChart0.Legend(7) = "16:00~";
		xChart0.Legend(8) = "17:00~";
		xChart0.Legend(9) = "18:00~";
		xChart0.Legend(10) = "19:00~20:30";		
	} else {		
		xChart0.Legend(0) = "20:30~";
		xChart0.Legend(1) = "21:30~";
		xChart0.Legend(2) = "23:00~";
		xChart0.Legend(3) = "00:00~";	
		xChart0.Legend(4) = "02:00~";
		xChart0.Legend(5) = "04:00~";
		xChart0.Legend(6) = "05:00~";
		xChart0.Legend(7) = "06:00~";		
