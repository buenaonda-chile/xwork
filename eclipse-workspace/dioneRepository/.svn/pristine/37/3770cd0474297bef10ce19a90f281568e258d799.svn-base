function getFSME562Q(){

	if(!validationChk()){
		return;
	}

	var sKind		= "getFSME562Q";						
	var sMethodName = "service::psfaclib/selectFSME562Q.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                            				// 자료 저장 시 보낼 DataSet 명칭
	var	sOutDataSet = "ds_FSME562Q_A=ds_FSME562Q_A ds_FSME562Q_B=ds_FSME562Q_B ds_FSME562Q_C=ds_FSME562Q_C ds_FSME562Q_D=ds_FSME562Q_D";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
		sArgument += " LIB1=" + quote(Lib1);
		sArgument += " LIB2=" + quote(Lib2);
		sArgument += " COMPS=" + quote(ComboBoxPlant.Value); //사업부
		sArgument += " MGMYM=" + quote(substr(cmb_MGMYM.Value,0,6)); //마감년월
	http.Sync=true;
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");
	http.Sync=false;

}

/********************************************************************** 
 * 내용설명 : validation 체크
 **********************************************************************/ 
 
function validationChk(){

	var result = true;
	cmb_MGMYM.BKColor="white";

	if(cmb_MGMYM.Value == ""){
		cmb_MGMYM.BKColor="paleturquoise";
		//alert("[확인] 마감년월을 확인바랍니다.");
		alert(setPopMessage("4020"));//[확인] 마감년월을 확인바랍니다.
		setMessage("4020");
		result = false;
	}
	return result;
}

/********************************************************************** 
 * 내용설명 : Transaction 리턴후 실행 함수
 * 주요변수 : 
 * 			- sMethodName	Transaction 고유 ID
 * 			- ErrorCode 	에러코드
 * 			- ErrorMSG 		에러 메세지
 **********************************************************************/ 
function callback(svcId, rtnCd, rtnMsg){
	// tran 실패
	if(rtnCd<0){
		alert(setPopMessage("1002"));
		setMessage("1002");
	}else if(svcId=="getFSME562Q"){
		// 조회건수가 없을경우
		if((ds_FSME562Q_A.rowcount <= 0) &&(ds_FSME562Q_B.rowcount <= 0)){
			alert(setPopMessage("1003"));
			setMessage("1003");	
		// 정상 조회
		} else {
			getSYSDATE(); //SYSTEM 날짜
			alert(setPopMessage("1004"));
			setMessage("1004");				
		}
	}
}

