			CASE WHEN A.TAXDT>=C.EDATM THEN C.PRICE ELSE '0' END AS PCOST, 						
			CASE WHEN A.TAXDT>=C.EDATM THEN C.EDATM ELSE '0' END AS EDATM						
		FROM $LIB2$.SJT112LF A
		LEFT OUTER JOIN  										
			(
			SELECT Y.*
				FROM $LIB1$.EM001PR Y, 
			(
				SELECT 
