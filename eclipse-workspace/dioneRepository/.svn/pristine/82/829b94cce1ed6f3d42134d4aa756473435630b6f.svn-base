		LEFT OUTER JOIN $LIB2$.MST100PF D  ON A.COMPS=D.COMPS AND A.PSPNO=D.PRTNO 
		LEFT OUTER JOIN (SELECT * FROM $LIB2$.MST050PF 
	                    WHERE COMPS=#COMPS# AND GRPCD='CAR') E ON D.CARCD=E.MSTCD
