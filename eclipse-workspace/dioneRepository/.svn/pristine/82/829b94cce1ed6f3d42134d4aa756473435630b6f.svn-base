		WHERE Y.EDATM = (SELECT MAX(X.EDATM)
		FROM $LIB1$.EM000PR X			
		WHERE Y.CUSNO=X.CUSNO AND Y.PRTNO=X.PRTNO AND X.EDATM <![CDATA[<=]]> $CHLDT$) 		
		) B ON A.PYVND=B.CUSNO AND A.PSPNO=B.PRTNO
	WHERE
		A.CHLDT=$CHLDT$
		<isNotEmpty prepend="AND" property="PYVND">
		A.PYVND=$PYVND$
		</isNotEmpty>
	
