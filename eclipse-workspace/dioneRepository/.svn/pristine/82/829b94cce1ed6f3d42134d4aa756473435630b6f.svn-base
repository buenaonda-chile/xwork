		LEFT OUTER JOIN $LIB2$.MST100PF E ON E.COMPS=#COMPS# AND A.PRTNO=E.PRTNO
		LEFT OUTER JOIN $LIB2$.MST050PF F ON F.COMPS=#COMPS# AND F.GRPCD='CAR' AND E.CARCD=F.MSTCD
