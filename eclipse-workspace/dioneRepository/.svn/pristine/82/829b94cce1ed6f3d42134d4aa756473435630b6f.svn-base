<select id="selectSVHE606Q" parameterClass="java.util.HashMap" resultClass="java.util.HashMap">
<!-- 
SELECT
	A.VNDNR, A.VNAME, A.PYVND, A.GUBNM, A.CARNM, A.ITDSC, A.MKPNO, A.PSPNO, A.CHECK, A.ORDNO, 
