		LEFT OUTER JOIN $LIB1$.BM008PR B ON A.PRTNOC=B.ITNBR
		LEFT OUTER JOIN $LIB1$.BM400PR C ON A.PRTNOC=C.DPPRTN
	WHERE
		A.COMPSC=#COMPS#
