	FROM $LIB2$.$STABLE$ A
	LEFT OUTER JOIN $LIB1$.EM005PR B ON A.PYVND=B.CUSNO
	WHERE
		A.CHLDT=$CHLDT$
		<isNotEmpty prepend="AND" property="PYVND">
		A.PYVND=$PYVND$
		</isNotEmpty>	
	
