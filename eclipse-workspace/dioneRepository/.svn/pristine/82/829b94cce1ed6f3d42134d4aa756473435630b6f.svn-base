		LEFT OUTER JOIN (
			SELECT Z.COMPS, Z.PSPNO,  SUM(Z.TRQTY)- SUM(Z.ASQTY) AS SUBQY FROM $LIB2$.FSM500PF Z
			GROUP BY Z.COMPS, Z.PSPNO) G ON G.COMPS='E1' AND A.PSPNO=G.PSPNO  
