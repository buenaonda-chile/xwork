	COALESCE(F.MOHTQ,0) AS JAGQY01, COALESCE(G.MOHTQ,0) AS JAGQY11, COALESCE(H.MOHTQ,0) AS JAGQY31, COALESCE(I.MOHTQ,0) AS JAGQY41
