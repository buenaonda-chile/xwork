		    CASE WHEN SUM(VMP.P2)=0 OR SUM(VMP.M2)=0 THEN 0 ELSE DECIMAL(SUM(VMP.P2)*100.00/SUM(VMP.M2),10,2) END MRATE			
		FROM(
			SELECT 
				A.COMPS G,A.PYVND V,A.PSPNO, 		
