WHERE Y.PRTNO=X.PRTNO AND Y.CUSNO=X.CUSNO AND X.EDATM  <![CDATA[<=]]>$SJTYM$)
*DATE JPSDT
 -->						
	) G ON A.PYVND=G.CUSNO AND A.PSPNO=G.PRTNO	
ORDER BY A.COMPS, A.PYVND, A.PSPNO, A.JPSDT
</select>
