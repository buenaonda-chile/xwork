			PLNYM=$BEFYM$
		) G ON A.COMPS=G.COMPS AND A.PYVND=G.PYVND AND A.PSPNO=G.PSPNO
	WHERE
		A.COMPS=#COMPS#
		AND A.PLNYM=$PLNYM$
		AND A.PYVND=$PYVND$
