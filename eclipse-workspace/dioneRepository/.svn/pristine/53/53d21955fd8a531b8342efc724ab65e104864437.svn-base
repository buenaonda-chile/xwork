 **********************************************************************/ 
function setColumn(cnt)
{
	var returnTxt = "";

	for(i=1;i<=cnt;i++)
	{
		if(i == cnt) returnTxt += "P_PARAM" + i;
			else returnTxt += "P_PARAM" + i + ",";
	}
	return returnTxt ;
