 **********************************************************************/ 
function getHAYZ091Q(){

	if(!validationChk()){
		return;
	}

	var sKind		= "getHAYZ091Q";						
	var sMethodName = "service::pshumlib/selectHAYZ091Q.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                            				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_HAYZ091Q=ds_HAYZ091Q";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
		sArgument += "@LIB1=" + quote(Lib1);
		sArgument += "@LIB2=" + quote(Lib2);
		sArgument += "@LIB3=" + quote(Lib3);
		sArgument += "@COMPS=" + quote(ComboBoxPlant.Value); //사업부
	var str = dialog("Common::PGMLOAD_HUM.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");	
	http.Sync = false;
	
	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
		setMessage("1004");	
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));
		setMessage("1003");	
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}
	var p_JUNO="";//공단주민번호
	var JUNO="";//연말정산주민번호
	var getGNDAT="";//입사일
	//기준년도(정산년도-1)
	if(ds_HAYZ091Q.rowcount >0){
		for(i=0; i<ds_HAYZ091Q1.rowcount; i++){
			p_JUNO=ds_HAYZ091Q1.GetColumn(i,"P_PARAM8");
			JUNO=ds_HAYZ091Q.FindRow("JUNO",p_JUNO);//연말정산조회
			ds_HAYZ091Q1.SetColumn(i,"ZZ020",ds_HAYZ091Q.GetColumn(JUNO,"ZZ020"));//소득총액
			ds_HAYZ091Q1.SetColumn(i,"ZY002",ds_HAYZ091Q.GetColumn(JUNO,"ZY002"));//국외근로
			ds_HAYZ091Q1.SetColumn(i,"ZZ003",ds_HAYZ091Q.GetColumn(JUNO,"ZZ003"));//심야연장비과세
			ds_HAYZ091Q1.SetColumn(i,"ZZ004",ds_HAYZ091Q.GetColumn(JUNO,"ZZ004"));//기타비과세
			ds_HAYZ091Q1.SetColumn(i,"ZY004",ds_HAYZ091Q.GetColumn(JUNO,"ZY004"));//외국비과세
			ds_HAYZ091Q1.SetColumn(i,"P_PARAM15",ds_HAYZ091Q.GetColumn(JUNO,"TOTAL"));//소득총액+국외근로
			ds_HAYZ091Q1.SetColumn(i,"GNDAT",ds_HAYZ091Q.GetColumn(JUNO,"GNDAT"));//입사일
			
			//근무월수계산
			getGNDAT =ds_HAYZ091Q.GetColumn(JUNO,"GNDAT");
			if(parseInt(substr(getGNDAT,0,4)) <= parseInt(substr(cmb_PTJYM.Value,0,4))){
				ds_HAYZ091Q1.SetColumn(i,"P_PARAM16","12");//근무월수
			}else{
				ds_HAYZ091Q1.SetColumn(i,"P_PARAM16",(12-parseInt(substr(getGNDAT,4,2))+1));//근무월수
			}
			
		}
	}
}
