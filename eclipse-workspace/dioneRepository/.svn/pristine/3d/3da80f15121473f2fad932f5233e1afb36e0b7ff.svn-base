			var sKind		= "getbjiz100";		
			var sMethodName = "service::psimmlib/selectBJOZ110.do"; 
			var sInDataSet  = "";
			var sOutDataSet = "ds_BJO100=ds_BJO100";
			var sArgument   = "";
				sArgument  += " LIB2=" + quote(Lib2);
				sArgument  += " LIB3=" + quote(Lib2);
				sArgument  += " COMPS=" + quote(ComboBoxPlant.Value); //사업부
				sArgument  += " SQLID=" + "selectBJOZ110";  			  				// SQL ID
				sArgument  += " ISICL=" + ds_BJIZ100.GetColumn(nRow, "RPICL");
				sArgument  += " ISIPN=" + ds_BJIZ100.GetColumn(nRow, "RPIPN");
			Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
			http.Sync = false;
			/*
			if (ds_BJO100.GetRowCount() >= 0){
				
				var isiqy = ds_BJO100.GetColumn(0, "ISIQY");				
				if(parseInt(varNewValue)> parseInt(isiqy)){
