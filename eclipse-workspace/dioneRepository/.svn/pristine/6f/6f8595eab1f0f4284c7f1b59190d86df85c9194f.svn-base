 * 내용설명 : 화면 조회 
 **********************************************************************/ 
function getHAYZ094Q(){

	if(!validationChk()){
		return;
	}

	var p_YYEAR;
	if(ComboBoxPlant.Value!="E1"){	//KA사업부는 실지급년도-1년
		p_YYEAR=parseInt(substr(cmd_YYEAR.Value,0,4))-1;
	}else{
		p_YYEAR=substr(cmd_YYEAR.Value,0,4);
	}
	
	var sKind		= "getHAYZ094Q";						
	var sMethodName = "service::pshumlib/selectHAYZ093Q.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                            				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_HAYZ094Q=ds_HAYZ093Q";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
		sArgument += "@LIB1=" + quote(Lib1);
		sArgument += "@LIB2=" + quote(Lib2);
		sArgument += "@LIB3=" + quote(Lib3);
		sArgument += "@COMPS=" + quote(ComboBoxPlant.Value);
		sArgument += "@KNYYMF=" + quote(substr(cmb_KNYYM.Value,0,6));	//기준년월(FR)
		sArgument += "@KNYYMT=" + quote(substr(cmb_KNYYM.Value,0,6));	//기준년월(TO)
		sArgument += "@YYEAR=" + quote(p_YYEAR);	//년차지급년도
	var str = dialog("Common::PGMLOAD_HUM.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");	
	http.Sync = false;
	
	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
		setMessage("1004");	
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));
		setMessage("1003");	
