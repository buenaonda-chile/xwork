function Div1_btn_search_OnClick(obj)
{
	
	if(length(trim(Div1.ed_pyvnd.Text)) != 4){
		alert(setPopMessage("4671"));//[확인] 거래처코드 확인바랍니다.
		setMessage("4671");	
		Div1.ed_pyvnd.SetFocus();
		return;
	}

	FileDialog0.Filter = "ALL File(.xls;.xlsx)|*.xls;*.xlsx";	
	FileDialog0.Type = "OPEN";
	if (!FileDialog0.Open()) {
		alert(setPopMessage("1009"));	//  파일 선택 창을 여는데 실패하였습니다.
		return;
	}
	
	ds_SEND.ClearData();
	ds_SALE202UC.ClearData();
	
	Div2.Visible = true;
	Div1.Enable = false;
	SetCapture();

	var file_name = FileDialog0.FileName;
	var file_path = FileDialog0.FilePath;
	var path = file_path + "\\" + file_name;
	setMessageDirect(path);	
	Div1.ed_path.Text = path;
	ext_ExcelImportByIndex(path,0,"ds_SEND",1,1,0,2);  
	//ext_ExcelImportByIndex(strExcelFilename, intSheetIndex, strTargetDatasetID, [intUseColInfo, intExp, intDecimal,intStartRow]); 


	if (ds_SEND.GetRowCount() < 1){
		//alert("[경고]생성할 data가 없습니다. 확인바랍니다.");
		alert(setPopMessage("6085")); //[경고]생성할 data가 없습니다. 확인바랍니다.
		setMessage("6085");	
		Div2.Visible = false;
		Div1.Enable = true;
		SetCapture(false);
		return;
	}
	ds_SEND.Sort("PSPNO");
	//비교할 정보 가져오기
	check_ALL();

	//excel data 유효성 검사 및 업체재고마스타dataset생성 
	chkExcelData();

	Div2.Visible = false;
	Div1.Enable = true;
	SetCapture(false);
}
/********************************************************************** 
 * 내용설명 : 업체재고마스타를 수정 생성
 * 주요변수 : 
 **********************************************************************/ 
 function setSALE202UC(){
	if (ds_SEND.GetRowCount() < 1) {
		//alert("[경고]생성할 data가 없습니다. 확인바랍니다.");
		alert(setPopMessage("6085")); //[경고]생성할 data가 없습니다. 확인바랍니다.
		setMessage("6085");			 
		return;
	}
	if(length(trim(Div1.st_pyvnd.Text)) = 0){
		alert(setPopMessage("4671"));//[확인] 업체 확인바랍니다.
		setMessage("4671");
		Div1.ed_pyvnd.SetFocus();
		return;
	}

	for(var i=0;i<ds_SEND.GetRowCount();i++){
		if (ds_SEND.getcolumn(i,"UPERR") != "TT"){
			//alert("[경고]error항목 존재하니 생성가능여부를 확인하시고 다시 작업바랍니다."); 
			alert(setPopMessage("6086")); //[경고]error항목 존재하니 생성가능여부를 확인하시고 다시 작업바랍니다.
			setMessage("6086");
			return;
		}
	}
	if(confirm("업체코드 " + Div1.ed_pyvnd.Text+ "의 업체재고마스타를 수정합니다."+"생성하시겠습니까? ") == false) return;
	
 	http.Sync = true;
	var sKind		= "set_SALE202UC";						
	var sMethodName = "service::pssallib/saveSALE202UC.do";
	var sInDataSet  = "ds_SEND=ds_SEND";
	var sOutDataSet = "";
	var sArgument   = "";
	sArgument += " LIB2=" + quote(Lib2);
	//sArgument += " COMPS=" + quote(Div1.ComboBoxPlant.Value);
	//sArgument += " PYVND=" + quote(Div1.ed_pyvnd.Value);
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");
	http.Sync = false;
 }

/********************************************************************** 
 * 내용설명 : Transaction 리턴후 실행 함수
 * 주요변수 : 
 * 			- sMethodName	Transaction 고유 ID
 * 			- ErrorCode 	에러코드
 * 			- ErrorMSG 		에러 메세지
 **********************************************************************/ 
function callback(svcId, rtnCd, rtnMsg){
	// tran 실패
	if(rtnCd<0){
		alert(setPopMessage("1002"));
		setMessage("1002");
		
	}else {
		alert(setPopMessage("1001"));
		setMessage("1001");	
	}
}

function chkExcelData(){
	//ds_SALE202UC_BOX.ClearData();
	for(var z=0;z<ds_SEND.GetRowCount();z++){
		ds_SEND.SetColumn(z,"COMPS",Div1.ComboBoxPlant.Value);
		ds_SEND.SetColumn(z,"PYVND",Div1.ed_pyvnd.Text);
		//null이면 '0'입력
		if(length(trim(ds_SEND.GetColumn(z,"MKHAD"))) < 1) ds_SEND.SetColumn(z,"MKHAD","0");
		if(length(trim(ds_SEND.GetColumn(z,"GIVQY"))) < 1) ds_SEND.SetColumn(z,"GIVQY","0");
		if(length(trim(ds_SEND.GetColumn(z,"REQTY"))) < 1) ds_SEND.SetColumn(z,"REQTY","0");
		ds_SEND.SetColumn(z,"UPERR","TT");
		var pspno = rtrim(ds_SEND.GetColumn(z,"PSPNO"));
		var MKHAD = ds_SEND.GetColumn(z,"MKHAD");
		var GIVQY = ds_SEND.GetColumn(z,"GIVQY");
		var REQTY = ds_SEND.GetColumn(z,"REQTY");
		//length check
		if (length(pspno) < 1) {
			ds_SEND.SetColumn(z,"UPERR","E1"); //LENGTH 확인
		}
		if (length(pspno) > 15 || length(trim(MKHAD)) > 5 || length(trim(GIVQY)) > 5 || length(trim(REQTY)) > 5) {
			ds_SEND.SetColumn(z,"UPERR","E1"); //LENGTH 확인
		}
		//숫자check
		if (!numberChk(MKHAD) || !numberChk(GIVQY) || !numberChk(REQTY)){ 
			ds_SEND.SetColumn(z,"UPERR","E2"); //숫자CHECK
		}
		
		//미등록 자료 check
		var row_sal200 = ds_SALE202UC.FindRow("PSPNO",pspno);
		if (row_sal200 == -1){
			ds_SEND.SetColumn(z,"UPERR","E3"); //마스타에 미등록
		}
	}
}
