/********************************************************************** 
 * 내용설명 : 화면 조회 
 **********************************************************************/ 
function getMIZZ229_0D(){
	var sKind		= "getMIZZ220";						
	var sMethodName = "service::budget/getMIZZ220List";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                            				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_MIZZ220=ds_MIZZ220";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
		sArgument += " CATNO=" + quote(pCATNO);
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	
	var sKind		= "getMIZZ229_0";						
	var sMethodName = "service::budget/getMIZZXXXList";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                            				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_MIZZXXX=ds_MIZZXXX";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
		sArgument += " PFFILE=" + quote("MIZ239PF");
		sArgument += " WHERE=" + quote("CATNO='"+pCATNO+"'");
		sArgument += " ORDERBY=" + quote("COMPS,CATYY,CATGB");
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
}

/********************************************************************** 
 * 내용설명 : Transaction 리턴후 실행 함수
 * 주요변수 : 
 * 			- sMethodName	Transaction 고유 ID
 * 			- ErrorCode 	에러코드
 * 			- ErrorMSG 		에러 메세지
 **********************************************************************/ 
function callback(svcId, rtnCd, rtnMsg){
	if (ds_MIZZ229_0_CHECK.GetRowCount() > 0){
		for(var i=0;i<ds_MIZZ229_0_CHECK.GetRowCount();i++){
			alert("항목코드 : " +ds_MIZZ229_0_CHECK.GetColumn(i,"CATNO") + " = " + ds_MIZZ229_0_CHECK.GetColumn(i,"RTMSG"));
		}
		getMIZZ229_0();  //error항목이 존재시에는 재조회
		return;
	}// tran 실패
	if(rtnCd<0){
		alert(setPopMessage("1002"));
		setMessage("1002");
	}else{
		alert(setPopMessage("1001"));
		setMessage("1001");	
	}
}

