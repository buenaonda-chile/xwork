package com.denso.model;

public class TblSJT {
	
	String pspno = "";
	String cprtn = "";
	String itdsc = "";
	String bdycd = "";
	String carcd = "";
	String pyvnd = "";
	String cusnm = "";
	String taxdtm = "";
	String taxqy = "";
	String taxam = "";
	
	public String getPspno() {
		return pspno;
	}
	public void setPspno(String pspno) {
		this.pspno = setData(pspno);
	}
	public String getCprtn() {
		return cprtn;
	}
	public void setCprtn(String cprtn) {
		this.cprtn = setData(cprtn);
	}
	public String getItdsc() {
		return itdsc;
	}
	public void setItdsc(String itdsc) {
		this.itdsc = setData(itdsc);
	}
	public String getBdycd() {
		return bdycd;
	}
	public void setBdycd(String bdycd) {
		this.bdycd = setData(bdycd);
	}
	public String getCarcd() {
		return carcd;
	}
	public void setCarcd(String carcd) {
		this.carcd = setData(carcd);
	}
	public String getPyvnd() {
		return pyvnd;
	}
	public void setPyvnd(String pyvnd) {
		this.pyvnd = setData(pyvnd);
	}
	public String getCusnm() {
		return cusnm;
	}
	public void setCusnm(String cusnm) {
		this.cusnm = setData(cusnm);
	}
	public String getTaxdtm() {
		return taxdtm;
	}
	public void setTaxdtm(String taxdtm) {
		this.taxdtm = setData(taxdtm);
	}
	public String getTaxqy() {
		return taxqy;
	}
	public void setTaxqy(String taxqy) {
		this.taxqy = setData(taxqy);
	}
	
	private String setData(String temp){
		if(temp != null){
			temp.replaceAll(",", "");
		}
		return temp;
	}
	public String getTaxam() {
		return taxam;
	}
	public void setTaxam(String taxam) {
		this.taxam = taxam;
	}
}
