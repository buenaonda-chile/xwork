function getFCKZ102Q(){

	if(!validationChk()){
		return;
	}
	
	var orgcomps="";
	if(ComboBoxPlant.Value=="E1"){
		orgcomps ="PE";
	}
	else{
		orgcomps ="PS";
	}
	var sKind		= "getHOUSE";		
	var sMethodName = "service::pscomlib/selectMST040PF_HOUSE.do"; 
	var sInDataSet  = "";
	var sOutDataSet = "ds_MST040PF_HOUSE=ds_MST040PF_HOUSE";
	var sArgument   = "";
		sArgument += " LIB2=" + quote(Lib2);
		sArgument += " GCOMPS=" + quote(orgcomps); //회사(PE/PS)
		sArgument += " COMPS=" + quote(ComboBoxPlant.Value); //사업부
		sArgument += " MAJCD=" + quote("true"); //공장별 창고구분을 위해사용
	http.Sync=true;
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	http.Sync=false;
	//if (ds_MST040PF_HOUSE.rowcount > 0){
	var arrHouse = "('";
	for(i=0; i< ds_MST040PF_HOUSE.rowcount; i++){
		if(i == ds_MST040PF_HOUSE.rowcount-1){
			arrHouse = arrHouse+ds_MST040PF_HOUSE.GetColumn(i,"MAJCD_H") + "')";
		}else{
			arrHouse = arrHouse+ds_MST040PF_HOUSE.GetColumn(i,"MAJCD_H") + "','";
		}
	}
	
	/*
	if(ComboBoxPlant.Value == "E1"){
		arrHouse = "('1')";
	}else if(ComboBoxPlant.Value == "C1"){
		arrHouse = "('1')";
	}else if(ComboBoxPlant.Value == "S1"){
		arrHouse = "('2')";
	}else if(ComboBoxPlant.Value == "H1"){
		arrHouse = "('3')";
	}
	*/

	var sKind		= "getFCKZ102Q";						
	var sMethodName = "service::psfaclib/selectFCKZ101Q.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_FCKZ102Q=ds_FCKZ101Q";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
		sArgument += " LIB1=" + quote(Lib1);
		sArgument += " LIB2=" + quote(Lib2);
		sArgument += " LIB3=" + quote(Lib3);
		sArgument += " COMPS=" + quote(ComboBoxPlant.Value); //사업부
		sArgument += " GRPCD=" + quote(cd_GRPCD.Value); //재고가시화 분류 (A:CKD)
		//sArgument += " MAJOR=" + quote("T"); //가시화 대상품목(TOP12)만 조회됨.
		sArgument += " HOUSE=" + quote(arrHouse); //창고코드
	http.Sync=true;
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	http.Sync=false;

	Div_LAYOUT_DEF.st_LastFreshTime.Text="최종갱신시간: "+substr(getDate(),8,2)+":"+substr(getDate(),10,2)+":"+substr(getDate(),12,2);
	
	//2020.03.19추가 : paging Fileter TEST중
	if(Div_LAYOUT_DEF.chk_POPUP = false){
		searchRowCnt = ds_FCKZ102Q.rowcount;
		filterRowCnt = setRowValue;
		ds_FCKZ102Q.UnFilter();
		ds_FCKZ102Q.Filter("row < "+setRowValue);	//디폴트 값
	}	
}

