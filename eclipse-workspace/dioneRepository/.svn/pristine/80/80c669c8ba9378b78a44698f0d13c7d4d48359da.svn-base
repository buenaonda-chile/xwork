//날짜, 시간표시
function timeEvent(){

var sDate = "";
var sTimeHH = "";
var sTimeMM = "";
var sTimeSS = "";
	sDate = getDate();
	nowDate = substr(sDate,0,8);
	sTimeHH = substr(sDate,8,2);
	sTimeMM = substr(sDate,10,2);
	sTimeSS = substr(sDate,12,2);
		
	Div_LAYOUT_DEF.st_DATE.Text=substr(getDate(),0,4)+"."+substr(getDate(),4,2)+"."+substr(getDate(),6,2);
	Div_LAYOUT_DEF.st_TIME.Text=sTimeHH+":"+sTimeMM+":"+sTimeSS;
	if(ds_TMPeriod.FindRow("time", Div_LAYOUT_DEF.st_TIME.Text) != -1){	//매시정각만 리프레쉬함.
		fn_Search();
		//st_LastFreshTime.Text="최종갱신시간: "+st_TIME.Text;
	}
}
