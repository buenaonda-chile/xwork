/********************************************************************** 
 * 내용설명 : 화면 조회 
 **********************************************************************/ 
function getFBRZ504Q(){
	if(length(trim(ca_FRHYM.Value)) < 8){
		alert("년월을 확인 하세요.");
		ca_FRHYM.SetFocus();
		return;
	}

	
	http.Sync = true;
	var sKind		= "getFBRZ504Q";
	var sMethodName = "service::psfaclib/selectFBRZ504Q.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_FBRZ504Q=ds_FBRZ504Q";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
	sArgument += "@LIB1=" + quote(Lib1);
	sArgument += "@LIB2=" + quote(Lib2);
	sArgument += "@HOUSE=" + quote(cb_searchHSCD.text);
	sArgument += "@YYMM=" + quote(substr(ca_FRHYM.Value,0,6));
	var str = dialog("Common::PGMLOAD_FAC.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");	
	//Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");
	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
		setMessage("1004");	
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));
		setMessage("1003");	
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}
}

