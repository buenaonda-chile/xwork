* {
    box-sizing: border-box;
}

html, body {
    padding: 0;
    margin: 0;
    height: 100%
}

p, h2 {
    padding: 0;
    margin: 0;
}

body {
    position: relative;
    min-height: 100%;
}

.a4-size {
    background: white;
    width: 100%;
    height: 900px;
}

.a4-conteiner {
    text-align: center;
}

.conteiner-size h2 {
    font-size: 50px;
}

.conteiner-size {
    padding: 50px;
}

.conteiner-size-p {
    margin-top: 50px;
    text-align: initial;
}

.conteiner-size-p p {
    font-size: 20px;
    line-height: 2em;
}

.conteiner-size-p p:nth-child(1) {
    text-align: left;
}

.conteiner-size-p p:nth-child(2) {
    font-size: 15px;
}

.conteiner-ssain {
    margin-top: 20px;
}

.conteiner-ssain span {
    font-size: 20px;
}

.conteiner-ssain span:nth-child(2) {
    margin-right: 60px;
}

.conteiner-size-p {
    
    text-align: initial;
}

.conteiner-ssain p:nth-child(1) {
    padding-bottom: 50px;
    font-size: 20px;
}

.conteiner-ssain img {
    width: 100px;
    
}