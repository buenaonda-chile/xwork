	searchBar.Width = G_ds_design.GetColumn(0,"Width");
	searchBar.Top = G_ds_design.GetColumn(0,"SBTop");	
}


function getSJTZ112Q(){

	var sPYVND1="";
	var sPYVND2="";
	var sTAXYM1="";
	var sTAXYM2="";
	var sSINFO1="";
	var sSINFO2="";
	
	//업체
	if(length(ed_PYVND1.Text)==0){
		sPYVND1="00000000";
	}
	else{
		sPYVND1=ed_PYVND1.Text;
	}	
	if(length(ed_PYVND2.Text)==0){
		sPYVND2="99999999";
	}
	else{
		sPYVND2=ed_PYVND2.Text;
	}
	//년월
	if(cmb_TAXYM1.Value == ""){
		sTAXYM1="199701";
	}
	else if(cmb_TAXYM1.Value != ""){
		sTAXYM1=substr(cmb_TAXYM1.Value,0,6);
	}
	if(cmb_TAXYM2.Value == ""){
		sTAXYM2="299912";
	}
	else if(cmb_TAXYM2.Value != ""){
		sTAXYM2=substr(cmb_TAXYM2.Value,0,6);
	}
	//계산서서번호
	if(length(ed_SINFO2.Text)==0){
		sSINFO2="99999999";
	}
	else{
		sSINFO2=ed_SINFO2.Text;
	}
	
	var sKind		= "getSJTZ112Q";						
	var sMethodName = "service::pssallib/selectSJTZ112Q.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_SJTZ112Q=ds_SJTZ112Q";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
	sArgument += "@LIB1=" + quote(Lib1);
	sArgument += "@LIB2=" + quote(Lib2);
	sArgument += "@COMPS=" + quote(ComboBoxPlant.Value); //사업부
	sArgument += "@TAXYM1=" + quote(sTAXYM1);
	sArgument += "@TAXYM2=" + quote(sTAXYM2);
	sArgument += "@PYVND1=" + quote(sPYVND1);
	sArgument += "@PYVND2=" + quote(sPYVND2);
	sArgument += "@SINFO1=" + quote(ed_SINFO1.Text);
	sArgument += "@SINFO2=" + quote(sSINFO2);

	var str = dialog("Common::PGMLOAD_SAL.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");
	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
		setMessage("1004");	
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));  //[알림] 신규입력입니다.
		setMessage("1003");	
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}
}

