	searchBar.Top = G_ds_design.GetColumn(0,"SBTop");	

	//Static0.BkColor = G_ds_design.GetColumn(0,"BTBkColor2");
	//Static3.BkColor = G_ds_design.GetColumn(0,"BTBkColor2");
}

function getCGMPARMHC(){
	var sKind		= "getCGMPARMHC";						
	var sMethodName = "service::pscomlib/selectCGMPARMHC.do"; 
	var sInDataSet  = "";
	var sOutDataSet = "ds_CGMPARMHC=ds_CGMPARMHC";
	var sArgument   = "";
		sArgument += " LIB1=" + quote(Lib1);
	http.Sync=true;
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	http.Sync=false;
	next_month = addMonth(ds_CGMPARMHC.GetColumn(0,"ADSDT"),1);
}

function getFPME101Q(){

	if(!validationChk()){
		return;
	}
	var procs2 = "";
	procs2 = ed_PROCS2.Text;
	if(procs2==""){
		procs2="99999";
	}
	getCGMPARMHC();
	//trace(next_month);
	var plnYM = substr(cmb_PYEAR.Value,0,6);
	if(plnYM > substr(next_month,0,6)){
		//alert("[알림]계획년월은 CIGMA년월+1까지 입력가능합니다.");
		alert(setPopMessage("6055"));  //[알림]계획년월은 CIGMA년월+1까지 입력가능합니다.
		setMessage("6055");	
		cmb_PYEAR.SetFocus();
		return;
	}
	var bpnslt="";
	var chgAdsdt="";
	if(ds_CGMPARMHC.rowcount<1){
		alert(setPopMessage("6058"));  //[알림]ORIGINAL DAY(CIGMA일자)가 존재하지않습니다.
		setMessage("6058");	
		return;
	}
	if(ds_CGMPARMHC.GetColumn(0,"SEQMON")==1){//당월이 M 인경우
		bpnslt=1;
		chgAdsdt = addMonth(ds_CGMPARMHC.GetColumn(0,"ADSDT"),-1);
		//chgAdsdt = addMonth(cmb_PYEAR.Value,-1);
	}else{//당월이 M+1인경우
		bpnslt=2;
		chgAdsdt = addMonth(ds_CGMPARMHC.GetColumn(0,"ADSDT"),-2);
		//chgAdsdt = addMonth(cmb_PYEAR.Value,-2);
	}

	var cigmaChk="";
	if(substr(cmb_PYEAR.Value,0,6) <= substr(chgAdsdt,0,6)){//원본!
	//if(substr(chgAdsdt,0,6)<=substr(ds_CGMPARMHC.GetColumn(0,"ADSDT"),0,6)){ //fpmc101도 수정할것!
		cigmaChk="1";
	}else{
		cigmaChk="2";
	}	
//alert(cigmaChk);
	//cigmaChk가 1인경우
	var mbCnt=0;
	var mcCnt=0;
	var mChk=0;
		mbCnt=parseInt(substr(cmb_PYEAR.Value,0,4))*12+parseInt(substr(cmb_PYEAR.Value,4,2));
		mcCnt=parseInt(substr(ds_CGMPARMHC.GetColumn(0,"ADSDT"),0,4))*12+parseInt(substr(ds_CGMPARMHC.GetColumn(0,"ADSDT"),4,2));
	if(ds_CGMPARMHC.GetColumn(0,"SEQMON")==1){//당월이 M 인경우
		mChk=mbCnt-mcCnt;
	}else{//당월이 M+1인경우
		mChk=(mbCnt-mcCnt)+1;
	}
//alert(mbCnt+"=="+mcCnt+"=="+mChk);
	var sKind		= "getFPME101Q";						
	var sMethodName = "service::psfaclib/selectFPME101Q.do";     		// 서버에서 호출 될 Method 명
