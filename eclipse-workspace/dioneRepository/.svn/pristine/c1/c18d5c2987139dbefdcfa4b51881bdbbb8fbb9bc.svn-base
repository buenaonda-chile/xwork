function getNetClientUser(){

	var sKind		= "getNetClientUser";						
	var sMethodName = "service::user/selectNetClientUser.do";     	// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             					// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_N_USER_IF=ds_N_USER_IF"; 
	var sArgument   = "";
	http.Sync=true;
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");
	http.Sync=false;
	
	if(ds_N_USER_IF.rowcount > 0){
		ds_N_USER_IF.AppendDataset(ds_N_USER_SHARE);
		ds_N_USER_IF.Sort("COMPS,DPSQNO,DPTCD,GRADE,SABUN");
		setNetClientUser();//생성
	}	
}

function setNetClientUser(){

/*
	if(ds_N_USER_IF.rowcount<1){
		alert("[알림]조회된 자료가 없습니다.");
		return;
	}
*/
	http.TimeOut=600;
	var sKind		= "setNetClientUser";						
	var sMethodName = "service::user/saveNetClientUser.do";     	// 서버에서 호출 될 Method 명
	var sInDataSet  = "ds_N_USER_IF=ds_N_USER_IF";               //자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_ERROR=ds_ERROR"; 
	var sArgument   = "";
	http.Sync=true;
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");
	http.Sync=false;
	http.TimeOut=300;
}

function callback(svcId, rtnCd, rtnMsg){
	// tran 실패
	if(rtnCd<0){
		alert(setPopMessage("1002"));
		setMessage("1002");
	}else if(svcId=="getNetClientUser"){
		if(ds_N_USER_IF.rowcount <= 0){
			alert(setPopMessage("1003"));
			setMessage("1003");	
		}
	}else if(svcId=="setNetClientUser"){
		if(ds_ERROR.rowcount <= 0){
			alert(setPopMessage("1001"));
			setMessage("1001");
		// 정상 조회
		} else {
			//alert(setPopMessage("1002"));
			//setMessage("1002");
			alert(ds_ERROR.GetColumn(0,"RTMSG") + " 오류명 : " +ds_ERROR.GetColumn(0,"RTERR"));
			setMessage("1002");
		}
	}
}

