 * 주요변수 : 
 **********************************************************************/ 
function getBUYZ107Q(){
	//조회
	var spcodFrom;
	var spcodTo;
	spcodFrom = iif(trim(ed_SPCOD_from.Text) == null,"9000",ed_SPCOD_from.Text);
	spcodTo = iif(trim(ed_SPCOD_to.Text) == null,"9999",ed_SPCOD_to.Text);
	var prtnoFrom;
	var prtnoTo;
	prtnoFrom = iif(trim(ed_PRTNO_from.Text) == null," ",ed_PRTNO_from.Text);
	prtnoTo = iif(trim(ed_PRTNO_to.Text) == null,"999999999999999",ed_PRTNO_to.Text);
	var sKind		= "getBUYZ107Q";
	var sMethodName = "service::psimmlib/selectBUYZ107Q.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_BUYZ107Q=ds_BUYZ107Q";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
	sArgument += "@LIB1=" + quote(Lib1);
	sArgument += "@CONNO1=" + quote(spcodFrom);
	sArgument += "@CONNO2=" + quote(spcodTo);
	sArgument += "@PRTNO1=" + quote(prtnoFrom);
	sArgument += "@PRTNO2=" + quote(prtnoTo);
	var str = dialog("Common::PGMLOAD_IMM.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");	
	//Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	
	/* RPG에서는 제일마지막 구매업체만 선택하였으나 흐름상 전구매업체를 보여줌
	for(var i=0;i<ds_BUYZ107Q.rowcount;i++){
		if(i>0){
			if(prtno == ds_BUYZ107Q.GetColumn(i,"SPRTN")){
				ds_BUYZ107Q.DeleteRow(i-1);
			}
		}
		var prtno=ds_BUYZ107Q.GetColumn(i,"SPRTN");
	}
	*/
	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
		setMessage("1004");	
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));  //[알림] 조회 된 데이터가 없습니다.
		setMessage("1003");	
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}

}

