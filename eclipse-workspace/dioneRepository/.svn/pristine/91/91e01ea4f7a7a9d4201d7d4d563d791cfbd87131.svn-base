/********************************************************************** 
 * 내용설명 : 사업부 선택
 * 주요변수 : 
 **********************************************************************/ 
function getComCode(){
	http.Sync 	= true;

	var sKind		= "getComCode";						
	var sMethodName = "service::login/selectComCode.do"; 
	var sInDataSet  = "";
	var sOutDataSet = "ds_ComCode_PLNT=ds_ComCode_PLNT";
	var sArgument   = "";
		sArgument   += " CLASS='PLNT'";
		sArgument   += " CODE1=" + quote(company);		
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	cb_searchCodeName1.Value = comps;
}

/********************************************************************** 
 * 내용설명 : 화면조회
 * 주요변수 : 
 **********************************************************************/ 
function getBHZZ201Q(){
	if(length(trim(ed_HOUSE.Value)) < 1){
		alert(setPopMessage("1513"));  //[확인] 창고를 획인하세요
		setMessage("1513");	
		ed_HOUSE.SetFocus();
		return;
	}
	var p_plann1,p_plann2;
	p_plann1 = iif (length(trim(ed_PLANN1.Text)) == 0, '00001', trim(ed_PLANN1.Text));
	p_plann2 = iif (length(trim(ed_PLANN2.Text)) == 0 , '99999', trim(ed_PLANN2.Text));
	var p_vnd1,p_vnd2;
	p_vnd1 = iif (length(trim(ed_CUVND1.Text)) == 0, 'AAAAAA', trim(ed_CUVND1.Text));
	p_vnd2 = iif (length(trim(ed_CUVND2.Text)) == 0 , '999999', trim(ed_CUVND2.Text));
	var p_pno1,p_pno2;
	p_pno1 = iif (length(trim(ed_PSPNO1.Text)) == 0, ' ', trim(ed_PSPNO1.Text));
	p_pno2 = iif (length(trim(ed_PSPNO2.Text)) == 0 , '99999999999999999999', trim(ed_PSPNO2.Text));
	var sKind		= "getBHZZ201Q";						
	var sMethodName = "service::psimmlib/selectBHZZ201Q.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_BHZZ201Q=ds_BHZZ201Q";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
	sArgument += "@LIB1=" + quote(Lib1);
	sArgument += "@LIB2=" + quote(Lib2);
	sArgument += "@PLANN1=" + quote(p_plann1);
	sArgument += "@PLANN2=" + quote(p_plann2);
	sArgument += "@PYVND1=" + quote(p_vnd1);
	sArgument += "@PYVND2=" + quote(p_vnd2);
	sArgument += "@ITNBR1=" + quote(p_pno1);
	sArgument += "@ITNBR2=" + quote(p_pno2);
	sArgument += "@HOUSE=" + quote(trim(ed_HOUSE.Value));
	sArgument += "@D30PR=" + quote("F#D30PR");
	var str = dialog("Common::PGMLOAD_IMM.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");	
	//Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");
	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
		setMessage("1004");	
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));
		setMessage("1003");	
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}
}


