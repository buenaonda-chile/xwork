function setExcel()
{
	if(cmb_SEL.Value==1){
		var fName   = "년 생산계획현황(제품별)";
		var arrSheets  =  Array();  
	
		arrSheets[0]  = "MainGrid|년 생산계획현황(제품별)";
		createExcelSheetS2(FileDialog0, fName ,arrSheets);
	}else if(cmb_SEL.Value==2){
		var fName   = "년 생산계획현황(품목별)";
		var arrSheets  =  Array();  
	
		arrSheets[0]  = "MainGrid2|년 생산계획현황(품목별)";
		createExcelSheetS2(FileDialog0, fName ,arrSheets);	
	}else if(cmb_SEL.Value==3){
		var fName   = "년 생산계획현황(제품별집계표)";
		var arrSheets  =  Array();  
	
		arrSheets[0]  = "MainGrid3|년 생산계획현황(제품별집계표)";
		createExcelSheetS2(FileDialog0, fName ,arrSheets);	
	}
}

