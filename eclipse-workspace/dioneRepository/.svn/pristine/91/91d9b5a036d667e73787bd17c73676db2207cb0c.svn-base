function validationChk(){
	var result = true;
	cmb_PLNYY.BKColor="white";
		
	if (cmb_PLNYY.Value == ""){
		cmb_PLNYY.BKColor="paleturquoise";
		alert(setPopMessage("6040"));//[알림]계획년을 입력하세요.
		setMessage("6040");
		result = false;
	}
	return result;
}

/********************************************************************** 
 * 내용설명 : 상위메뉴 변경시 초기화
 **********************************************************************/ 
function common_OnChanged(obj,strCode,strText,nOldIndex,nNewIndex)
{
	ds_SC003PR.ClearData();
	ds_FPYC201Q.ClearData();
	if(cmb_SEL.Value==1){
		MainGrid.Visible=true;
		MainGrid2.Visible=false;
		MainGrid3.Visible=false;
		ds_FPYC201Q.GroupKey="";
	}else if(cmb_SEL.Value==2){
		MainGrid.Visible=false;
		MainGrid2.Visible=true;
		MainGrid3.Visible=false;
		ds_FPYC201Q.GroupKey="COMPS, COMPS:PRDCD";
	}else if(cmb_SEL.Value==3){
		MainGrid.Visible=false;
		MainGrid2.Visible=false;
		MainGrid3.Visible=true;
		ds_FPYC201Q.GroupKey="";
	}
}

