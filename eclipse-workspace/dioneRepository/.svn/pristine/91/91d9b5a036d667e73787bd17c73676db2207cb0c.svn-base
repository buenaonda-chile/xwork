function getFPYC201Q(){

	if(!validationChk()){
		return;
	}
	getSC003PR();
	var sKind		= "getFPYC201Q";						
	var sMethodName = "service::psfaclib/selectFPYC201Q.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                            				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "";
	var sOutDataSet = "ds_FPYC201Q=ds_FPYC201Q"; 					// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
		sArgument += "@LIB1=" + quote(Lib1);
		sArgument += "@LIB2=" + quote(Lib2);
		sArgument += "@COMPS=" + quote(ComboBoxPlant.Value); //사업부
		sArgument += "@PLNYY1=" + quote(substr(cmb_PLNYY.Value,0,4)+"04"); //계획년FR
		sArgument += "@PLNYY2=" + quote(parseInt(substr(cmb_PLNYY.Value,0,4))+1+"03"); //계획년TO
		sArgument += "@SHPTO=" + quote(""); //SHPTO(미사용)
		sArgument += "@PLNGB=" + quote(cmb_PLNGB.Value); //계획구분
		sArgument += "@SEL=" + quote(cmb_SEL.Value); //선택구분
		//sArgument += "@FLAG=" + quote(flags); //sql선택
	var str = dialog("Common::PGMLOAD_FAC.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");
	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
		setMessage("1004");
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));
		setMessage("1003");	
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}
}
function getSC003PR(){
	var sKind		= "getSC003PR";						
	var sMethodName = "service::pscomlib/selectSC003PR_2.do";    // 서버에서 호출 될 Method 명
	var sInDataSet  = "";               //자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_SC003PR=ds_SC003PR";  	   // 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
		sArgument += " LIB1=" + quote(Lib1);
		sArgument += " LIB2=" + quote(Lib2);
		sArgument += " PLNYY1=" + quote(substr(cmb_PLNYY.Value,0,4)+"04"); //계획년FR
		sArgument += " PLNYY2=" + quote(parseInt(substr(cmb_PLNYY.Value,0,4))+1+"03"); //계획년TO
		sArgument += " PRMNO=" + quote("ABS"); //선택구분
	http.Sync=true;
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	http.Sync=false;
	if(ds_SC003PR.rowcount>0){
		if(cmb_SEL.Value==1){
			for(i=0; i<16; i++){
				MainGrid.SetCellProp("Head", 19+i, "text", ds_SC003PR.GetColumn(0,"CGA"+lpad(i+1,0,2)));
			}	
		}else if(cmb_SEL.Value=2){
			for(i=0; i<16; i++){
				MainGrid2.SetCellProp("Head", 20+i, "text",ds_SC003PR.GetColumn(0,"CGA"+lpad(i+1,0,2)));
			}	
		}else if(cmb_SEL.Value==3){
			for(i=0; i<16; i++){
				MainGrid3.SetCellProp("Head", 19+i, "text",ds_SC003PR.GetColumn(0,"CGA"+lpad(i+1,0,2)));
			}	
		}
	}
}

