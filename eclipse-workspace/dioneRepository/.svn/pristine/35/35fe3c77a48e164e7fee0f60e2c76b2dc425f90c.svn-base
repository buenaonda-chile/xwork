 **********************************************************************/ 
function getComCode(){

	http.Sync 	= true;

	var sKind		= "getComCode";						
	var sMethodName = "service::login/selectComCode.do"; 
	var sInDataSet  = "";
	var sOutDataSet = "ds_ComCode_PLNT=ds_ComCode_PLNT";
	var sArgument   = "";
		sArgument   += " CLASS='PLNT'";
		sArgument   += " CODE1=" + quote(company);		
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	CmbCOM.Value=comps;
	
}


/********************************************************************** 
 * 개 발 자 : 민명규 
 * 개발일자 : 2013-08-29
 * 내용설명 : 화면조회
 * 주요변수 : 
 **********************************************************************/ 
function getHAY081(){

	http.Sync = true;
	
	var sKind		= "getHAY081";						
	var sMethodName = "service::pshumlib/selectHAY081.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_HAY081=ds_HAY081";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
		
	var ssid = "";
	var CName = "";
	var	tName = "";
	var TsId = "";
	var Addr = "";
	var TaxNm = "";
		
	var day = substr(today(),0,4) + " 년   " + substr(today(),4,2) + " 월   " + substr(today(),6,2) + " 일";
	
	if (CmbCOM.Value == 'C1'){
      ssid = "206-81-28245";
      CName = "덴소코리아오토모티브(주)";
      tName = "카토 모토미";
      TsId = "110111-2040585";
      Addr = "경남 창원시 성산구 웅남로 630";
      TaxNm = "창원세무서장";
   }
   else if (CmbCOM.Value == 'E1'){
      ssid = "609-81-01877";
      CName = "덴소코리아일렉트로닉스(주)";
      tName = "김경섭";
      TsId = "194211-0000141";
      Addr = "경남 창원시 성산구 남면로 631 (외동)";
      TaxNm = "창원세무서장";
   }
   else if (CmbCOM.Value == 'H1'){
      ssid = "310-85-09579";
      CName = "덴소코리아오토모티브주식회사";
      tName = "카토 모토미";
      TsId = "110111-2040585";
	  Addr = "충남 홍성군 은하면 구성남로 386";
      TaxNm = "홍성세무서장";
   }
   else if (CmbCOM.Value == 'S1'){
      ssid = "124-85-61218";
      CName = "덴소코리아오토모티브주식회사";
      tName = "카토 모토미";
      TsId = "110111-2040585";
      JName = "덴소코리아오토모티브(주)";
      Addr = "경기도 화성시 장안면 장안공단5길 9";
      TaxNm = "화성세무서장";
   }
   
	sArgument += "@LIB1=" + quote(Lib1);
	sArgument += "@LIB2=" + quote(Lib2);
	sArgument += "@LIB3=" + quote(Lib3);
	sArgument += "@COMPS=" + quote(CmbCOM.Value);

	if(CmbGubun.Value == '2'){
		if (ed_tel_no1.Text != ''){
			sArgument += "@SABUN=" + quote(ed_tel_no1.Text);
		}else{
			alert("사번을 입력하여 주십시오.");
			return;
		}
	} else {
		if (cmdFORCD.Value != '0'){
			sArgument += "@FORCD=" + quote(cmdFORCD.Value);
		}
	
		if (edtDPTCD1.Value != "" && edtDPTCD2.Value != "") {
			sArgument += "@DPTCD1=" + quote(edtDPTCD1.Value); 
			sArgument += "@DPTCD2=" + quote(edtDPTCD2.Value); 		
		}
	}
	
	if (cmb_GUNMU.Value != '0'){
		sArgument += "@PAYCD=" + quote(cmb_GUNMU.Value);
	}
		
	var str = dialog("Common::PGMLOAD_IMM.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");	
	//Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");
	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
		setMessage("1004");
	}else if (str == 'B'){  //data값이 없을때
		alert(setPopMessage("1003"));
		setMessage("1003");	
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}

	Ubi0.setJrfFileDir(G_baseURL+"ui/PSHUMLIB/REPORT/");
		
	if (ds_HAY081.GetColumn(0, "PTJYM") > 201312) {
		Ubi0.setJrfFileName("HAY081_2014.jrf");
	}else {
		if (CmbCOM.Value == 'E1'){
			Ubi0.setJrfFileName("HAY081_1.jrf");
		}
		else{
			Ubi0.setJrfFileName("HAY081_1_1.jrf");
		}
	}
	
	Ubi0.SetDataset("ds_HAY081", "ds_HAY081");
	
	Ubi0.SetArgument("COMPS", CmbCOM.Value);	
	Ubi0.SetArgument("ssid", ssid);
	Ubi0.SetArgument("CName", CName);
	Ubi0.SetArgument("tName", tName);
	Ubi0.SetArgument("TsId", TsId);
	Ubi0.SetArgument("Addr", Addr);
	Ubi0.SetArgument("TaxNm", TaxNm);
	Ubi0.SetArgument("day", day);
	
	getHAY021();
	
	http.Sync=false;
}

