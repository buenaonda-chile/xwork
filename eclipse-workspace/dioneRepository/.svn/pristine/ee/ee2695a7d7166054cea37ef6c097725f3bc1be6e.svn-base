	//Static0.BkColor = G_ds_design.GetColumn(0,"BTBkColor2");
	//Static3.BkColor = G_ds_design.GetColumn(0,"BTBkColor2");
}

function getFPME120Q(){

	var p_plnym1,p_plnym2,p_procs2;
	p_plnym1 = iif (length(trim(cmb_PLNYM_F.Value)) == 0, '000000', substr(trim(cmb_PLNYM_F.Value),0,6));
	p_plnym2 = iif (length(trim(cmb_PLNYM_T.Value)) == 0 , '999999', substr(trim(cmb_PLNYM_T.Value),0,6));
	p_procs2 = iif (length(trim(ed_PROCS_T.Text)) == 0 , '99999', trim(ed_PROCS_T.Text));
	cmb_PLNYM_F.BKColor="white";
	cmb_PLNYM_T.BKColor="white";
	
	if(p_plnym1 > p_plnym2){
		cmb_PLNYM_F.BKColor="paleturquoise";
		cmb_PLNYM_T.BKColor="paleturquoise";
		alert(setPopMessage("6056"));//[알림]계획년월을 확인하세요.
		setMessage("6056");	
		return;
	}
	
	var sKind		= "getFPME120Q";						
	var sMethodName = "service::psfaclib/selectFPME120Q.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                            				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_FPME120Q=ds_FPME120Q";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
		sArgument += "@LIB1=" + quote(Lib1);
		sArgument += "@LIB2=" + quote(Lib2);
		sArgument += "@COMPS=" + quote(ComboBoxPlant.Value); //사업부
		sArgument += "@PLMYMF=" + quote(p_plnym1); //계획년월FR
		sArgument += "@PLMYMT=" + quote(p_plnym2); //계획년월TO
		sArgument += "@PROCSF=" + quote(ed_PROCS_F.Text); //작업공정FR
		sArgument += "@PROCST=" + quote(p_procs2); //작업공정TO
		sArgument += "@ROLGB=" + quote(cmb_ROLGB.Value); //내외구분(OEM구분)
	var str = dialog("Common::PGMLOAD_FAC.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");
	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
		setMessage("1004");
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));
		setMessage("1003");	
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}
}

