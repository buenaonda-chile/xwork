function validationChk(){
	var result = true;
	var sdVal = substr(cmb_PLNYM_F.Value,0,6);
	var edVal = substr(cmb_PLNYM_T.Value,0,6);
	cmb_PLNYM_F.BKColor="white";
	cmb_PLNYM_T.BKColor="white";	

	if(sdVal > edVal){
		cmb_PLNYM_F.BKColor="paleturquoise";
		cmb_PLNYM_T.BKColor="paleturquoise";
		alert("[알림]계획년월을 확인하세요.");
		//alert(setPopMessage("6056"));//[알림]계획년월을 확인하세요.
		//setMessage("6056");	
		result =false;
	}
	return result;
}

