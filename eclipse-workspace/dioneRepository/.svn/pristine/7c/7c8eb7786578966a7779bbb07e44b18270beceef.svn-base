function ds_FHDZ100_CanRowPosChange(obj,nOldRow,nRow)
{

	http.usewaitcursor = false;

	var cpesabn=ds_FHDZ100.GetColumn(ds_FHDZ100.currow,"PESABN"); //사번
	var wrkcd=ds_FHDZ100.GetColumn(ds_FHDZ100.currow,"WRKCD"); //작업코드
	var supdp=ds_FHDZ100.GetColumn(ds_FHDZ100.currow,"SUPDP"); //지원부서
	var spjtm=ds_FHDZ100.GetColumn(ds_FHDZ100.currow,"SPJTM"); //지원시간(주)
	var spytm=ds_FHDZ100.GetColumn(ds_FHDZ100.currow,"SPYTM"); //지원시간(야)
	var clewtm=ds_FHDZ100.GetColumn(ds_FHDZ100.currow,"LEWTM"); //조퇴
	var cltwtm=ds_FHDZ100.GetColumn(ds_FHDZ100.currow,"LTWTM"); //지각
	var cotwtm=ds_FHDZ100.GetColumn(ds_FHDZ100.currow,"OTWTM"); //외출
	var cswkcd=ds_FHDZ100.GetColumn(ds_FHDZ100.currow,"SWKCD"); //특근코드
	//var cstrtm=ds_FHDZ100.GetColumn(ds_FHDZ100.currow,"STRTM"); //시작시간
	//var cendtm=ds_FHDZ100.GetColumn(ds_FHDZ100.currow,"ENDTM"); //종료시간
	//var cwrkhm=ds_FHDZ100.GetColumn(ds_FHDZ100.currow,"WRKHM"); //작업시분
	//var cwrktm=ds_FHDZ100.GetColumn(ds_FHDZ100.currow,"WRKTM"); //잔업시간
	//조,지,외 디폴트 value 0설정
	if(clewtm=""){
		ds_FHDZ100.SetColumn(ds_FHDZ100.currow,"LEWTM",0);
	}
	if(cltwtm=""){
		ds_FHDZ100.SetColumn(ds_FHDZ100.currow,"LTWTM",0);
	}
	if(cotwtm=""){
		ds_FHDZ100.SetColumn(ds_FHDZ100.currow,"OTWTM",0);
	}	
	if(wrkcd>="80"){ //작업코드가 80이상인경우 지원부서 입력불가
		if(length(supdp)!=0){	
			alert(setPopMessage("4701")); //[알림]작업코드가 80이상인경우 지원부서 입력불가능합니다.
			setMessage("4701");	
			return false;
		}
	}
	if(wrkcd>="50" && wrkcd<="59"){ //작업코드가 50이상 59이하인 경우(특근) 조퇴,지각,외출 없슴
		if(clewtm!=0 || cltwtm !=0 || cotwtm !=0){	
			alert(setPopMessage("4702")); //[알림]작업코드가 50이상 59이하인 경우(특근) 조퇴,지각,외출 입력불가능합니다.
			setMessage("4702");	
			//1번. row변경 불가로 할 것인가 2번 0값을 줄것인가.
			//1번
			//ds_FHDZ100.SetColumn(ds_FHDZ100.currow,"LEWTM",0);
			//ds_FHDZ100.SetColumn(ds_FHDZ100.currow,"LTWTM",0);
			//ds_FHDZ100.SetColumn(ds_FHDZ100.currow,"OTWTM",0);
			//2번
			return false;
		}
	}	
	if(wrkcd>="90"){ //작업코드가 90이상인 경우 모든값이 0임.
		ds_FHDZ100.SetColumn(ds_FHDZ100.currow,"STRTM",0);
		ds_FHDZ100.SetColumn(ds_FHDZ100.currow,"ENDTM",0);
		ds_FHDZ100.SetColumn(ds_FHDZ100.currow,"WRKHM",0);
		ds_FHDZ100.SetColumn(ds_FHDZ100.currow,"WRKTM",0);
		//ds_FHDZ100.SetColumn(ds_FHDZ100.currow,"SWKCD","");
		//ds_FHDZ100.SetColumn(ds_FHDZ100.currow,"LEWTM",0);
		//ds_FHDZ100.SetColumn(ds_FHDZ100.currow,"LTWTM",0);
		//ds_FHDZ100.SetColumn(ds_FHDZ100.currow,"OTWTM",0);			
		//ds_FHDZ100.SetColumn(ds_FHDZ100.currow,"SUPDP","");
		//ds_FHDZ100.SetColumn(ds_FHDZ100.currow,"SPJTM",0);
		//ds_FHDZ100.SetColumn(ds_FHDZ100.currow,"SPYTM",0);
		
		if(length(cswkcd) >1 ||clewtm!=0 || cltwtm !=0 || cotwtm !=0||length(supdp) >1 ||spjtm!=0||spytm!=0  ){	
			alert(setPopMessage("4703")); //[알림]작업코드가 90이상인 경우 잔업코드, 조퇴, 지각, 외출, 지원부서, 지원시간 값이 0입니다.
			setMessage("4703");	
		return false;
		}
	}		
	if(length(supdp)!=0){ //지원부서가 null이 아닌경우 지원시간입력
		if((length(spjtm)==0||spjtm<1)&&(length(spytm)==0||spytm<1)){
			alert(setPopMessage("4707")); //[알림]지원시간을 입력하세요.
			setMessage("4707");	
			return false;
		}
	}
	if(ds_FHDZ100.GetRowType(ds_FHDZ100.currow)=="update" ||ds_FHDZ100.GetRowType(ds_FHDZ100.currow)=="insert" ){
		var duChk ="";
		var duVal =0;
		var manChk ="";
		var week12 =0;
		var weekmn =0;
		if(length(ds_FHDZ100.GetColumn(ds_FHDZ100.currow,"PESABN")) > 1){ 
		//중복체크 -사번, 작업코드(현재의 data확인)
			duChk = ds_FHDZ100.GetColumn(ds_FHDZ100.currow,"PESABN")+"||"+ds_FHDZ100.GetColumn(ds_FHDZ100.currow,"WRKCD");
/*
			for(i=0; i <ds_FHDZ100.GetRowCount(); i++){
				if(ds_FHDZ100.currow==i){
					i++;
				}
				if(duChk == ds_FHDZ100.GetColumn(i,"PESABN")+"||"+ds_FHDZ100.GetColumn(i,"WRKCD")){
					alert("작업자의 동일한 근무코드가 존재합니다.");
					return false;
				}
			}
*/	
			for(i=0; i <ds_FHDZ100.GetRowCount(); i++){
				if(duChk == ds_FHDZ100.GetColumn(i,"PESABN")+"||"+ds_FHDZ100.GetColumn(i,"WRKCD")){
					duVal++;
				}
				if(duVal > 1){
					alert(setPopMessage("4704")); //[알림]작업자의 동일한 근무코드가 존재합니다.
					setMessage("4704");	
					return false;
				}	
			}

		//중복체크 - 타부서등록여부(FHD110확인)
			http.Sync=true;
			var sKind		= "getDUPBUCHK";						
			var sMethodName = "service::psfaclib/selectFHDZ100_BUCHK.do";     		// 서버에서 호출 될 Method 명
			var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
			var sOutDataSet = "ds_BUCHK=ds_BUCHK";    				// 조회 후 돌려 받을 DataSet 명칭
			var sArgument   = "";
				sArgument += " LIB2=" + quote(Lib2);
				sArgument += " COMPS=" + quote(ComboBoxPlant.Value); //사업부
				sArgument += " CODE=" + quote(ed_CODE.Text); //부서코드
				sArgument += " SABUN=" + quote(cpesabn); //사번
				sArgument += " WRKDT=" + quote(cmb_WRKDT.Value); //작업일
				//sArgument += " WRKCD=" + quote(cwrkcd); //작업코드
			Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
			http.Sync=false;
			if (ds_BUCHK.GetRowCount() > 0){
				alert(setPopMessage("4705")); //[알림]타부서에 등록된 작업자입니다.
				setMessage("4705");	
				return false;
			}
		//법정근무시간　주근무시간　주중 12 시간잔업금지
			if(ds_MST090PF.GetColumn(0,"PRCMK")="-" || ds_MST090PF.GetColumn(0,"PRCMK")=="T"){
				weekmn = parseInt(ds_FHDZ100.GetColumn(ds_FHDZ100.currow,"WEEKMN")); //현작업자의 작업일 이전까지의 누적 잔업시간
				manChk = ds_FHDZ100.GetColumn(ds_FHDZ100.currow,"PESABN"); //현재작업자
				for(i=0; i <ds_FHDZ100.GetRowCount(); i++){
					if(manChk == ds_FHDZ100.GetColumn(i,"PESABN")){ //동일작업자가 있으면 작업일의 작업시간 합계
						week12 = parseInt(ds_FHDZ100.GetColumn(i,"TMPWEEKMN")); 
						weekmn+=week12;
					}
				}
				if(weekmn > 12){
					alert(manChk+"사번의 작업자의 주근무시간이 "+weekmn+ "시간으로 12시간을 초과합니다.");
					return false;
				}
			}else{
				weekmn = 0; //휴일작업시간 누계
				manChk = ds_FHDZ100.GetColumn(ds_FHDZ100.currow,"PESABN"); //현재작업자
				for(i=0; i <ds_FHDZ100.GetRowCount(); i++){
					if(manChk == ds_FHDZ100.GetColumn(i,"PESABN")){ //동일작업자가 있으면 작업일의 작업시간 합계
						week12 = parseInt(ds_FHDZ100.GetColumn(i,"TMPWEEKMN")); 
						weekmn+=week12;
						
					}
				}
				if(weekmn > 8){ //휴일잔업가능시간은 8시간
					alert(manChk+"사번의 작업자의 휴일근무시간이 "+weekmn+ "시간으로 8시간을 초과합니다.");
					return false;
				}			
			
			}
			return true;
		}
	}
	http.usewaitcursor = true;
}

