 **********************************************************************/ 
 function setFHDZ100(){

	if(ds_FHDZ100.GetRowType(ds_FHDZ100.currow)=="update" ||ds_FHDZ100.GetRowType(ds_FHDZ100.currow)=="insert" ){
		if(!ds_FHDZ100_CanRowPosChange()){
			return;
		}
	} 
	if(length(trim(st_CODE.Text)) < 1){
		alert(setPopMessage("2544")); //[확인] 작업일자를 확인바랍니다.
		setMessage("2544");	
		ed_CODE.SetFocus();
		return;
	}	
	if(cmb_WRKDT.Value == ""){
		alert(setPopMessage("4061"));  //[경고] 필수입력항목입니다. (부서코드)
		setMessage("4061");	
		cmb_WRKDT.SetFocus();
		return;
	}

//1.마감처리 -> 등록시 마감여부 확인
	http.Sync=true;
	var sKind		= "getFHD110PF";		
	var sMethodName = "service::psfaclib/selectFHD110_MAGAM.do"; 
	var sInDataSet  = "";
	var sOutDataSet = "ds_FHD110PF_MAGAM=ds_FHD110PF_MAGAM";
	var sArgument   = "";
		sArgument += " LIB2=" + quote(Lib2);
		sArgument += " COMPS=" + quote(ComboBoxPlant.Value); //사업부
		sArgument += " DPCODE=" + quote(ed_CODE.Text);
		sArgument += " WRKDT=" + quote(cmb_WRKDT.Value); //작업일자
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	http.Sync=false;
	if (ds_FHD110PF_MAGAM.GetRowCount() > 0){
		//alert("[알림] 마감 처리가 진행하지 않은 날짜입니다.);
		alert(setPopMessage("4696"));
		setMessage("4696");	
		return;
	}
//trace(ds_FHDZ100.SaveXML());
	var sKind		= "setFHDZ100";						
	var sMethodName = "service::psfaclib/saveFHDZ100.do";
	var sInDataSet  = "ds_FHDZ100=ds_FHDZ100:U";
	var sOutDataSet = "ds_FHDZ100_CHECK=ds_FHDZ100_CHECK";
	var sArgument   = "";
		sArgument += " LIB1=" + quote(Lib1);
