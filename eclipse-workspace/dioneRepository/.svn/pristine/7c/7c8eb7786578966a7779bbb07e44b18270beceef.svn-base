	//trace(ds_SLTZ101_CHECK.SaveXML());
	if (ds_FHDZ100_CHECK.GetRowCount() > 0){			
		for(var i=0;i<ds_FHDZ100_CHECK.GetRowCount();i++){		
			alert("사번 : " +ds_FHDZ100_CHECK.GetColumn(i,"PESABN") + ",작업 : " +ds_FHDZ100_CHECK.GetColumn(i,"WRKCD") + " = " + ds_FHDZ100_CHECK.GetColumn(i,"RTMSG"));
		}
		getFHDZ100();        //error항목이 존재시에는 재조회		
		return;		
	}	
	// tran 실패
	if(rtnCd<0){
		alert(setPopMessage("1002"));
		setMessage("1002");
		
	}else 
	{
		//setSLTZ101_110();
		alert(setPopMessage("1001"));
		setMessage("1001");	
	}
}

