				ds_FHDZ100.SetColumn(nRow, "WRKHM", 0); // 작업시간I					
				//ds_FHDZ100.SetColumn(nRow, "TMPWEEKMN", 0); // 작업일 잔업시간(임시용)
				ds_FHDZ100.SetColumn(nRow, "WEEKMN", WRKTIME); // WRKTM
				ds_FHDZ100.SetColumn(nRow, "TMPWEEKMN",0); // 작업일 잔업시간(임시용)	
				}
			//else추가필요	
			}
			//ds_FHDZ100.SetColumn(nRow, "WEEKMN", WRKTIME); // WRKTM

		}
		else {
			ds_FHDZ100.SetColumn(nRow, "KORNM", ""); // 거래처명 설정
			ds_FHDZ100.SetColumn(nRow, "ORGDP", ""); // 원부서 
			ds_FHDZ100.SetColumn(nRow, "WEEKMN", ""); // WRKTM
			ds_FHDZ100.SetColumn(nRow, "TMPWEEKMN",""); // 작업일 잔업시간(임시용)	
		}
	}
	if (strColumnID == "WRKCD"){
		if(varNewValue!=""){
			if(ds_MST090PF.GetColumn(0,"PRCMK")!="H" && ds_MST090PF.GetColumn(0,"PRCMK")!="S") {
				ds_FHDZ100.SetColumn(nRow, "WRKTM", WRKTM); // 잔업시간I
				ds_FHDZ100.SetColumn(nRow, "WRKNM", WRKNM); // 근무명
				ds_FHDZ100.SetColumn(nRow, "STRTM", STRTM); // 시작시간I
				ds_FHDZ100.SetColumn(nRow, "ENDTM", ENDTM); // 종료시간I
				ds_FHDZ100.SetColumn(nRow, "WRKHM", WRKHM); // 작업시간I
				//ds_FHDZ100.SetColumn(nRow, "TMPWEEKMN", WRKTM); // 작업일 잔업시간(임시용)
				if(ds_MST090PF.GetColumn(0,"PRCMK")=="-" && ds_FHDZ100.GetColumn(nRow,"WRKCD") >= "20" && ds_FHDZ100.GetColumn(nRow,"WRKCD") <= "29"){
					ds_FHDZ100.SetColumn(nRow, "TMPWEEKMN", (parseInt(WRKTM)-480)/60); // 작업일 잔업시간(임시용)
				}
				else if(ds_MST090PF.GetColumn(0,"PRCMK")=="T" && ds_FHDZ100.GetColumn(nRow,"WRKCD") >= "20" && ds_FHDZ100.GetColumn(nRow,"WRKCD") <="29"){
					ds_FHDZ100.SetColumn(nRow, "TMPWEEKMN", (parseInt(WRKTM)-240)/60); // 작업일 잔업시간(임시용)
				}
				else if((ds_FHDZ100.GetColumn(nRow,"WRKCD") >= "10" && ds_FHDZ100.GetColumn(nRow,"WRKCD") <="19") ||(ds_FHDZ100.GetColumn(nRow,"WRKCD") >= "30" && ds_FHDZ100.GetColumn(nRow,"WRKCD") <="89")) {
					ds_FHDZ100.SetColumn(nRow, "TMPWEEKMN", parseInt(WRKTM)/60); // 작업일 잔업시간(임시용)
				}else{
					ds_FHDZ100.SetColumn(nRow, "TMPWEEKMN",0); // 작업일 잔업시간(임시용)		
				}
			}else{//else추가필요
				ds_FHDZ100.SetColumn(nRow, "WRKTM", WRKTM); // 잔업시간I
				ds_FHDZ100.SetColumn(nRow, "WRKNM", ""); // 근 무명
				ds_FHDZ100.SetColumn(nRow, "STRTM", STRTM); // 시작시간I
				ds_FHDZ100.SetColumn(nRow, "ENDTM", ENDTM); // 종료시간I
				ds_FHDZ100.SetColumn(nRow, "WRKHM", WRKHM); // 작업시간I
				//ds_FHDZ100.SetColumn(nRow, "TMPWEEKMN", WRKTM); // 작업일 잔업시간(임시용)
				if(ds_MST090PF.GetColumn(0,"PRCMK")=="-" && ds_FHDZ100.GetColumn(nRow,"WRKCD") >= "20" && ds_FHDZ100.GetColumn(nRow,"WRKCD") <= "29"){
					ds_FHDZ100.SetColumn(nRow, "TMPWEEKMN", (parseInt(WRKTM)-480)/60); // 작업일 잔업시간(임시용)
				}
				else if(ds_MST090PF.GetColumn(0,"PRCMK")=="T" && ds_FHDZ100.GetColumn(nRow,"WRKCD") >= "20" && ds_FHDZ100.GetColumn(nRow,"WRKCD") <="29"){
					ds_FHDZ100.SetColumn(nRow, "TMPWEEKMN", (parseInt(WRKTM)-240)/60); // 작업일 잔업시간(임시용)
				}
				else if((ds_FHDZ100.GetColumn(nRow,"WRKCD") >= "10" && ds_FHDZ100.GetColumn(nRow,"WRKCD") <="19") ||(ds_FHDZ100.GetColumn(nRow,"WRKCD") >= "30" && ds_FHDZ100.GetColumn(nRow,"WRKCD") <="89")) {
					ds_FHDZ100.SetColumn(nRow, "TMPWEEKMN", parseInt(WRKTM)/60); // 작업일 잔업시간(임시용)
				}else{
					ds_FHDZ100.SetColumn(nRow, "TMPWEEKMN",0); // 작업일 잔업시간(임시용)		
				}
			}
		}
		else {
			ds_FHDZ100.SetColumn(nRow, "WRKNM", "정상근무"); // 근무명
			ds_FHDZ100.SetColumn(nRow, "WRKTM", 0); // 잔업시간I
			ds_FHDZ100.SetColumn(nRow, "STRTM", 0); // 시작시간I
			ds_FHDZ100.SetColumn(nRow, "ENDTM", 0); // 종료시간I
			ds_FHDZ100.SetColumn(nRow, "WRKHM", 0); // 작업시간I
			ds_FHDZ100.SetColumn(nRow, "TMPWEEKMN", 0); // 작업일 잔업시간(임시용)
		}
	}
	if (strColumnID == "SWKCD"){
		if(varNewValue!=""){
			ds_FHDZ100.SetColumn(nRow, "SWKCD", SWKCD); // 잔업코드
			ds_FHDZ100.SetColumn(nRow, "SWKCR", SWKCR); // 잔업코드명
		}		
		else {
			ds_FHDZ100.SetColumn(nRow, "SWKCR", ""); // 잔업코드명
		}
	}		
	if (strColumnID == "SUPDP"){
		if(varNewValue!=""){
			ds_FHDZ100.SetColumn(nRow, "SUPDP", SUPDP); // 지원부서
			//ds_FHDZ100.SetColumn(nRow, "SPJTM", 0); // 지원시간(주)
			//ds_FHDZ100.SetColumn(nRow, "SPYTM", 0); // 지원시간(야)
		}
		else{ //지원부서가 null인경우 지원시간은 0
			ds_FHDZ100.SetColumn(nRow, "SUPDP", ""); // 지원부서
			ds_FHDZ100.SetColumn(nRow, "SPJTM", 0); // 지원시간(주)
			ds_FHDZ100.SetColumn(nRow, "SPYTM", 0); // 지원시간(야)
		
		}
	}
	http.usewaitcursor = true;
}

