function validationChk(){
	var result = true;
	cmb_WRKDT.BKColor="white";
		
	if(cmb_WRKDT.Value == ""){
		cmb_WRKDT.BKColor="paleturquoise";
		//alert("[확인] 확인년월을 입력하세요.");
		alert(setPopMessage("6087"));//[확인] 확인년월을 입력하세요.
		setMessage("6087");
		result = false;
	}
	return result;
}

