	//MainGrid.UserData = MainGrid.Height;
	
	searchBar.Width = G_ds_design.GetColumn(0,"Width");
	searchBar.Top = G_ds_design.GetColumn(0,"SBTop");	
}

function getFHDZ103Q(){

	if(!validationChk()){
		return;
	}
	//등록년월CHECK $EDCTL
	if(length(trim(cmb_WRKDT.Value)) < 1) return;
	http.usewaitcursor = false;
	http.Sync 	= true;
	var sKind		= "getMST090PF";		
	var sMethodName = "service::pscomlib/selectMST090PF.do"; 
	var sInDataSet  = "";
	var sOutDataSet = "ds_MST090PF=ds_MST090PF";
	var sArgument   = "";
		sArgument += " LIB2=" + quote(Lib2);
		sArgument += " COMPS=" + quote(ComboBoxPlant.Value); //사업부
		sArgument += " PRCDT=" + quote(substr(cmb_WRKDT.Value,0,6)+"01"); //작업일자
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");	
	http.Sync 	= false;
	if (ds_MST090PF.GetRowCount() < 1){
		//alert("[알림]달력마스타에 미등록된 일자입니다.");//[알림]달력마스타에 미등록된 일자입니다.
		alert(setPopMessage("6084"));//[알림]달력마스타에 미등록된 일자입니다.
		setMessage("6084");
		cmb_WRKDT.SetFocus();
		return;
	}

	var sKind		= "getFHDZ103Q";						
	var sMethodName = "service::psfaclib/selectFHDZ103Q.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                            				// 자료 저장 시 보낼 DataSet 명칭
	var	sOutDataSet = "ds_FHDZ103Q=ds_FHDZ103Q";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
		sArgument += "@LIB1=" + quote(Lib1);
		sArgument += "@LIB2=" + quote(Lib2);
		sArgument += "@COMPS=" + quote(ComboBoxPlant.Value); //사업부
		sArgument += "@WRKDTFR=" + quote(substr(cmb_WRKDT.Value,0,6)+"01"); //작업일자FR
		sArgument += "@WRKDTTO=" + quote(substr(cmb_WRKDT.Value,0,6)+"31"); //작업일자TO
	var str = dialog("Common::PGMLOAD_FAC.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");
	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
		setMessage("1004");
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));
		setMessage("1003");	
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}
}

/********************************************************************** 
