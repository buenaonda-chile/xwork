function getOgm011d(){

	var sKind		= "getOgm011d";						
	var sMethodName = "service::psogmlib/selectOGM011d1.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_ogm011d1=ds_ogm011d1";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
	sArgument += " LIB1=" + quote(OLIB1);
	sArgument += " LIB2=" + quote(OLIB2);
	sArgument += " COMPS=" + quote(COMPS); //사업부
	sArgument += " GROUP=" + quote(GROUP); //그룹
	sArgument += " PRTNO=" + quote(lblprtno.Text); //품번
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");
	
	var sKind		= "getOgm011d";						
	var sMethodName = "service::psogmlib/selectOGM011d2.do";     		// 서버에서 호출 될 Method 명
