	var strArg = "";
	Transaction("updatPoint", "service::sample/updateFlashMap.do", inds, outds, strArg, "fnCallback");
}


function fnCallback( svcId, errorCode, errorMsg){

	if(svcId =="viewPoint"){
		ds_view.row = 0;
