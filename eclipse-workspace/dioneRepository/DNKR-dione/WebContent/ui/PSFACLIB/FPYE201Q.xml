function getFPYE201Q(){

	if(!validationChk()){
		return;
	}
	
	var procs2 = "";
	procs2 = ed_PROCS2.Text;
	if(procs2==""){
		procs2="99999";
	}
	var sKind		= "getFPYE201Q";						
	var sMethodName = "service::psfaclib/selectFPYE201Q.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                            				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "";
	if(rdoggub.Value==1){
		sOutDataSet = "ds_FPYE201Q=ds_FPYE201Q";    				// 조회 후 돌려 받을 DataSet 명칭
	}else{
		sOutDataSet = "ds_FPYE202Q=ds_FPYE201Q";    				// 조회 후 돌려 받을 DataSet 명칭
	}
	var sArgument   = "";
		sArgument += "@LIB1=" + quote(Lib1);
		sArgument += "@LIB2=" + quote(Lib2);
		sArgument += "@COMPS=" + quote(ComboBoxPlant.Value); //사업부
		sArgument += "@PLNYY1=" + quote(substr(cmb_PLNYY.Value,0,4)+"04"); //계획년FR
		sArgument += "@PLNYY2=" + quote(parseInt(substr(cmb_PLNYY.Value,0,4))+1+"03"); //계획년TO
		sArgument += "@SHPTO=" + quote(""); //SHPTO(미사용)
		sArgument += "@PLNGB=" + quote(cmb_PLNGB.Value); //계획구분
		sArgument += "@QYEXT=" + quote(cmb_QYEXT.Value); //C/M선택
		sArgument += "@ROLGB=" + quote(cmb_ROLGB.Value); //내외구분
		sArgument += "@PROCS1=" + quote(ed_PROCS1.Text); //작업공정FR
		sArgument += "@PROCS2=" + quote(procs2); //작업공정TO
		sArgument += "@FLAG=" + quote(flags); //sql선택
	var str = dialog("Common::PGMLOAD_FAC.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");
	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
		setMessage("1004");
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));
		setMessage("1003");	
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}
}

