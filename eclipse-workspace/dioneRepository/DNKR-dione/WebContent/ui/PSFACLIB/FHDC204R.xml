function getFHDC204R(){
	
	if(!validationChk()){
		return;
	}
	getSYSDATE(); //SYSTEM 날짜
	var sysDt =ds_SYSDATE.GetColumn(0,"DT");
	var systm =ds_SYSDATE.GetColumn(0,"TM");
	
	//회계년도 계산(04~다음년도03월)
	var p_YMDF, p_YMDT, p_WKYY, p_YYMM;
		p_YMDF = substr(cmb_WRKDT.Value,0,4)+"0401";
		p_YMDT = substr(cmb_WRKDT.Value,0,6)+"31";
		if(parseInt(substr(cmb_WRKDT.Value,4,2))<4){
			p_YMDF= addMonth(p_YMDF,-12);
		}
		p_WKYY =substr(p_YMDF,0,4);
//trace(p_YMDF+"=="+p_YMDT+"=="+p_WKYY);
	
	//제외 팀코드
	var arrDPPAT1 = "('";
	for(i=0; i< ds_DPPAT1.rowcount; i++){
		if(i == ds_DPPAT1.rowcount-1){
			arrDPPAT1 = arrDPPAT1+ds_DPPAT1.GetColumn(i,"cd") + "')";
		}else{
			arrDPPAT1 = arrDPPAT1+ds_DPPAT1.GetColumn(i,"cd") + "','";
		}
	}
//trace(arrDPPAT1);

	var sKind		= "getFHDC204R";						
	var sMethodName = "service::psfaclib/selectFHDC204R.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_FHDC204R=ds_FHDC204R";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
		sArgument += "@LIB1=" + quote(Lib1);
		sArgument += "@LIB2=" + quote(Lib2);
		sArgument += "@COMPS=" + quote(ComboBoxPlant.Value); //사업부
		sArgument += "@YMDF=" + quote(p_YMDF); //근태년월FR
		sArgument += "@YMDT=" + quote(p_YMDT); //근태년월TO
		sArgument += "@DPPAT1=" + quote(arrDPPAT1); //제외코드
	var str = dialog("Common::PGMLOAD_FAC.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");	
	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
		setMessage("1004");	
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));
		setMessage("1003");	
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}
	Ubi0.setJrfFileDir(G_baseURL+"ui/PSFACLIB/REPORT/");
	Ubi0.setJrfFileName("FHDC204R.jrf");		
	Ubi0.SetDataset("ds_FHDC204R", "ds_FHDC204R");
	Ubi0.SetArgument("WKYY",p_WKYY);
	Ubi0.SetArgument("SYSDATE",sysDt);
	Ubi0.SetArgument("SYSTIME",systm);
	Ubi0.Retrieve();
	/*	
	if(ds_FHDC204R.GetRowCount() < 1){
		alert(setPopMessage("1003"));//[알림] 조회 된 데이터가 없습니다.
		setMessage("1003");	
		return;
	}
	*/
}

