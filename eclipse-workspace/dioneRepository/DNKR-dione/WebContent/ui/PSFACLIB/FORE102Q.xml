function validationChk(){
	var result = true;
	cmb_PRODY.BKColor="white";
		
	if (length(trim(cmb_PRODY.Value)) == 0){
		cmb_PRODY.BKColor="paleturquoise";
		//alert("[알림]PRODUCTION DATE를 입력하세요.");
		alert(setPopMessage("6073"));//[알림]PRODUCTION DATE를 입력하세요.
		setMessage("6073");
		result = false;
	}
	return result;
}

