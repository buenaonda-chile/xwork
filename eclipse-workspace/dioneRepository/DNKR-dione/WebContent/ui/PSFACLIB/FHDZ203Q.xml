	http.Sync 		= true;
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	http.Sync 		= false;
	var jobyl=0;
		jobyl=parseInt(ds_MST040PF.GetColumn(0,"MJINT"));
//alert(p_YMDF+"============"+p_YMDT);
	var sKind		= "getFHDZ203Q";						
	var sMethodName = "service::psfaclib/selectFHDZ203Q.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_FHDZ203Q=ds_FHDZ203Q";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
		sArgument += "@LIB1=" + quote(Lib1);
		sArgument += "@LIB2=" + quote(Lib2);
		//sArgument += "@LIB3=" + quote(Lib3);
		sArgument += "@COMPS=" + quote(ComboBoxPlant.Value); //사업부
		sArgument += "@DPCODE_F=" + quote(ed_DHCODE_F.Text); //작업반FR
		sArgument += "@DPCODE_T=" + quote(p_DHCODET); //작업반TO
		sArgument += "@WRKDT_F=" + quote(p_YMDF); //근태년월FR
		sArgument += "@WRKDT_T=" + quote(p_YMDT); //근태년월TO
		sArgument += "@JOBYL=" + quote(jobyl); //월도목표지수
	var str = dialog("Common::PGMLOAD_FAC.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");
	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
		setMessage("1004");
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));
		setMessage("1003");
		return;
	}else if (str == 'E'){   //erro r발생시
