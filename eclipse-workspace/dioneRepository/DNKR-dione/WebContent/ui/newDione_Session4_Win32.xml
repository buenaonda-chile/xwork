	<Script><![CDATA[var sessionId;

function gFcOnLoadCompleted(obj, url) {


	G_sysId = obj.ID;

	var Glob = Split(GetReg("GlobalVal"),"::");
	
	sessionId = Glob[0];
	var userId = Glob[1];
	var userName = Glob[2];
	var company = Glob[3];
	var plantCd = Glob[4];
	var password = Glob[5];
	var lib1 = Glob[6];
	var lib2 = Glob[7];	
	var lib3 = Glob[8];	
	var lib4 = Glob[9];	
	var menuPw = Glob[10];	

	G_ds_user.AddRow();

	if(length(trim(userId))>0) {

		SetReg("GlobalVal", "");
		JSESSIONID=sessionId;
		G_ds_user.SetColumn(0,"USER_ID", userId);
		G_ds_user.SetColumn(0,"USER_NAME", userName);
		G_ds_user.SetColumn(0,"COMPANY", company);
		G_ds_user.SetColumn(0,"PLANT_CD", plantCd);
		G_ds_user.SetColumn(0,"PASSWORD", password);
		G_ds_user.SetColumn(0,"LIB1", lib1);
		G_ds_user.SetColumn(0,"LIB2", lib2);
		G_ds_user.SetColumn(0,"LIB3", lib3);
		G_ds_user.SetColumn(0,"MENU_PW", menuPw);

	}	
}


function gFcOnCommunication(obj, bStatus) {

	if(G_transactionFlag) {

		if(!bStatus) {

			G_transactionFlag = 0;
		}
	}
}
]]></Script>
