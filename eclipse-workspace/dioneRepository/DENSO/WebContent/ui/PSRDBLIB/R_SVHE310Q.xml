function getSVHE310Q(){
	if(length(trim(cal_YMD.Value)) < 1){
		alert(setPopMessage("2016"));  //[확인] 거래처코드 확인바랍니다.
		setMessage("2016");	
		cal_YMD.SetFocus();
		return;
	}
	var sKind		= "getSVHE310Q";						
	var sMethodName = "service::pssallib/selectSVHE310Q.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_SVHE310Q=ds_SVHE310Q";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
	sArgument += "@LIB1=" + quote(Lib1);
	sArgument += "@LIB2=" + quote(Lib2);
	sArgument += "@COMPS=" + quote(ComboBoxPlant.Value); //사업부
	sArgument += "@PYVND=" + quote(ed_pyvnd.Text);
	sArgument += "@DUMCB=" + quote(cal_YMD.Value);
	sArgument += "@DUMYM=" + quote(substr(cal_YMD.Value,0,6));
	var str = dialog("Common::PGMLOAD.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");

	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
		setMessage("1004");	
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));
		setMessage("1003");	
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}
}

function ed_pyvnd_OnCharChanged(obj,strPreText,strPostText)
{
