function getFZME108Q(){

	if(!validationChk()){
		return;
	}
	
	var sKind		= "getFZME108Q";						
	var sMethodName = "service::psfaclib/selectFZME108Q.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                            				// 자료 저장 시 보낼 DataSet 명칭
	var	sOutDataSet = "ds_FZME108Q=ds_FZME108Q";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
		sArgument += "@LIB1=" + quote(Lib1);
		sArgument += "@LIB2=" + quote(Lib2);
		sArgument += "@COMPS=" + quote(ComboBoxPlant.Value); //사업부
		sArgument += "@PYVND=" + quote(ed_PYVND.Text); //득의선
		sArgument += "@PSPNO=" + quote(ed_PSPNO.Text); //품번
	var str = dialog("Common::PGMLOAD_FAC.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");
	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
		setMessage("1004");
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));
		setMessage("1003");	
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}
}

/********************************************************************** 
 * 내용설명 : validation 체크
 **********************************************************************/ 
 
function validationChk(){

	var result = true;
	ed_PYVND.BKColor="white";
	ed_PSPNO.BKColor="white";

	if(length(trim(st_PYVND.Text)) == 0){
		ed_PYVND.BKColor="paleturquoise";
		//alert("[알림]득의선을 입력하세요.");
		alert(setPopMessage("6043"));//[알림]득의선을 입력하세요.
		setMessage("6043");
		result = false;
	}else if(length(trim(st_PSPNO.Text)) == 0){
		ed_PSPNO.BKColor="paleturquoise";
		//alert("[알림]품번을 입력하세요.");
		alert(setPopMessage("2017"));//[알림] 품번을 입력하세요.
		setMessage("2017");
		result = false;
	}
	return result;
}

