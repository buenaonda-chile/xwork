function getSJSZ104Q(){
	
	if(!validationChk()){
		return;
	}
	var sPYVND1="";
	var sPYVND2="";
	//업체
	if(length(ed_PYVND1.Text)==0){
		sPYVND1="00000000";
	}
	else{
		sPYVND1=ed_PYVND1.Text;
	}	
	if(length(ed_PYVND2.Text)==0){
		sPYVND2="99999999";
	}
	else{
		sPYVND2=ed_PYVND2.Text;
	}
	
	//회계년도
	var sChkym=substr(cmb_SDATE.Value,0,4);
	if(substr(cmb_SDATE.Value,4,2) <= '03'){
		sChkym = parseInt(sChkym)-1;
	}
	var sKind		= "getSJSZ104Q";						
	var sMethodName = "service::pssallib/selectSJSZ104Q.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_SJSZ104Q=ds_SJSZ104Q";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
	sArgument += "@LIB1=" + quote(Lib1);
	sArgument += "@LIB2=" + quote(Lib2);
	sArgument += "@COMPS=" + quote(ComboBoxPlant.Value); //사업부
	//sArgument += "@SDATE=" + quote(substr(cmb_SDATE.Value,0,6)+"01"); //적송년월
	sArgument += "@SDATE=" + quote(substr(cmb_SDATE.Value,0,6)+"31"); //적송년월
	sArgument += "@JSOYY=" + quote(sChkym); //적송년(적송년월FR의 년도만보냄)
	sArgument += "@JSOMM=" + quote(substr(cmb_SDATE.Value,4,2)); //적송자료 조회 조건
	sArgument += "@PYVND1=" + quote(sPYVND1); //업체
	sArgument += "@PYVND2=" + quote(sPYVND2); //업체	
	var str = dialog("Common::PGMLOAD_SAL.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");

