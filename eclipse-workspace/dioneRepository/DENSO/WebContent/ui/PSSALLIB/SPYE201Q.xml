function getComCode(){

	http.Sync 	= true;
	
	var sKind		= "getComCode";						
	var sMethodName = "service::login/selectComCode.do"; 
	var sInDataSet  = "";
	var sOutDataSet = "ds_ComCode_PLNT=ds_ComCode_PLNT";
	var sArgument   = "";
		sArgument   += " CLASS='PLNT'";
		sArgument   += " CODE1=" + quote(company);		
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	ds_VComCode_PLNT.Copy("ds_ComCode_PLNT");

	ds_VComCode_PLNT.InsertRow(0);
	ds_VComCode_PLNT.SetColumn(0,"CODE2","");
	ds_VComCode_PLNT.SetColumn(0,"CODE_NAME","[전체]");
	cb_searchCodeName1.index = 0;

}


/********************************************************************** 
 * 내용설명 : 화면조회
 * 주요변수 : 
 **********************************************************************/ 
function getSPYE201Q(){
	//조회구분 선택시 grid제목변경 및 특정컬럼숨김
	switch (rdoggub.Value){
		case '1' : 
			MainGrid.SetCellProp("Head", 3, "text", "규격");
			MainGrid.SetCellProp("Head", 4, "text", "규격명");
			break;
		case '2' :
			MainGrid.SetCellProp("Head", 3, "text", "규격");
			MainGrid.SetCellProp("Head", 4, "text", "규격명");
			break;
		case '3' :
			MainGrid.SetCellProp("Head", 3, "text", "차종");
			MainGrid.SetCellProp("Head", 4, "text", "차종명");
			break;
		case '4' :
			MainGrid.SetCellProp("Head", 3, "text", "차종");
			MainGrid.SetCellProp("Head", 4, "text", "차종명");
			break;
	}
	var sum_qty_gb;
	if (rdoggub.Value = '2' || rdoggub.Value == '4' || rdoggub.Value == '6'){
		MainGrid.SetColProp(5,"Width",0);  //품번숨김
		MainGrid.SetColProp(6,"Width",0);  //품명숨김
		sum_qty_gb = 'S';  //집계
	}else{
		MainGrid.SetColProp(5,"Width",115);
		MainGrid.SetColProp(6,"Width",220);
		sum_qty_gb = 'D';  //상세
	}
	if (rdoggub.Value = '5' || rdoggub.Value == '6'){
		MainGrid.SetColProp(3,"Width",0);  //코드숨김
		MainGrid.SetColProp(4,"Width",0);  //코드명숨김
	}else{
		MainGrid.SetColProp(3,"Width",60);
		MainGrid.SetColProp(4,"Width",120);
	}

	var yyyy1 = cmb_yyyy.Value;
	var yyyy2 = toNumber(cmb_yyyy.Value) + 1;
	var p_pyvnd1,p_pyvnd2;
	p_pyvnd1 = iif (length(trim(ed_pyvnd_from.Text)) == 0, '0000', trim(ed_pyvnd_from.Text));
	p_pyvnd2 = iif (length(trim(ed_pyvnd_to.Text)) == 0 , '9999', trim(ed_pyvnd_to.Text));
	
	var sKind		= "getSPYE201Q";						
	var sMethodName = "service::pssallib/selectSPYE201Q.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_SPYE201Q=ds_SPYE201Q";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
	
	sArgument += "@LIB1=" + quote(Lib1);
	sArgument += "@LIB2=" + quote(Lib2);
	sArgument += "@COMPS=" + quote(cb_searchCodeName1.Value);
	sArgument += "@EXDSP=" + quote(cmb_EXDSP.Value);
	sArgument += "@PLNGB=" + quote(cb_plngb.Value);
	sArgument += "@PLNYM04=" + quote(yyyy1+'04');
	sArgument += "@PLNYM05=" + quote(yyyy1+'05');	
	sArgument += "@PLNYM06=" + quote(yyyy1+'06');
	sArgument += "@PLNYM07=" + quote(yyyy1+'07');	
	sArgument += "@PLNYM08=" + quote(yyyy1+'08');
	sArgument += "@PLNYM09=" + quote(yyyy1+'09');	
	sArgument += "@PLNYM10=" + quote(yyyy1+'10');
	sArgument += "@PLNYM11=" + quote(yyyy1+'11');	
	sArgument += "@PLNYM12=" + quote(yyyy1+'12');
	sArgument += "@PLNYM01=" + quote(yyyy2+'01');	
	sArgument += "@PLNYM02=" + quote(yyyy2+'02');
	sArgument += "@PLNYM03=" + quote(yyyy2+'03');
	
	sArgument += "@PYVND1=" + quote(p_pyvnd1);
	sArgument += "@PYVND2=" + quote(p_pyvnd2);	
	sArgument += "@G_GUB="  + quote(rdoggub.Value);	// 규격별/차종별/거래처별 구분
	sArgument += "@Q_GUB="  + quote(sum_qty_gb);    // 상세/집계 구분
	var str = dialog("Common::PGMLOAD_SAL.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");	
	//Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");
	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
		setMessage("1004");	
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));
		setMessage("1003");	
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}
	ed_pyvnd_from.SetFocus();
}


