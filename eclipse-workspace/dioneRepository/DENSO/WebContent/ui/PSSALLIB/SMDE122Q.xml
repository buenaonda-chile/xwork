function getSMDE122Q(){

	if(!validationChk()){
		return;
	}
	var sPYVND1="";
	var sPYVND2="";

	//업체
	if(length(ed_PYVND1.Text)==0){
		sPYVND1="00000000";
	}
	else{
		sPYVND1=ed_PYVND1.Text;
	}	
	if(length(ed_PYVND2.Text)==0){
		sPYVND2="99999999";
	}
	else{
		sPYVND2=ed_PYVND2.Text;
	}
			
	var sKind		= "getSMDE122Q";						
	var sMethodName = "service::pssallib/selectSMDE122Q.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_SMDE122Q=ds_SMDE122Q";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
	sArgument += "@LIB1=" + quote(Lib1);
	sArgument += "@LIB2=" + quote(Lib2);
	sArgument += "@COMPS=" + quote(ComboBoxPlant.Value); //사업부
	sArgument += "@PYVND1=" + quote(sPYVND1); //업체1
	sArgument += "@PYVND2=" + quote(sPYVND2); //업체2
	sArgument += "@JPYYM=" + quote(substr(cmb_JPYYM.Value,0,4)); //전표일자1
	sArgument += "@JPYYM2=" + quote(parseInt(substr(cmb_JPYYM.Value,0,4))+1); //전표일자2
	sArgument += "@JPSDT=" + quote(cmb_JPSDT.Value); //전표일자1
	var str = dialog("Common::PGMLOAD_SAL.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");

	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
		setMessage("1004");	
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));
		setMessage("1003");	
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}
}

/********************************************************************** 
 * 내용설명 : validation 체크
 **********************************************************************/ 
 
function validationChk(){

	var result = true;
	var jpyVal = substr(cmb_JPYYM.Value,0,6);
	var	jpsVal = substr(cmb_JPSDT.Value,0,6);
	var tChk = parseInt(substr(cmb_JPYYM.Value,0,4))+1+'0331';
	cmb_JPYYM.BKColor="white";
	cmb_JPSDT.BKColor="white";

	if(jpyVal==""){
		cmb_JPYYM.BKColor="paleturquoise";
		alert(setPopMessage("4029"));
		setMessage("4029");	
		result =false;
	}else if(jpsVal==""){
		cmb_JPSDT.BKColor="paleturquoise";
		alert(setPopMessage("4030"));
		setMessage("4030");	
		result =false;
	}else if(substr(jpyVal,0,4) > substr(jpsVal,0,4)){
		cmb_JPSDT.BKColor="paleturquoise";
		alert(setPopMessage("4029"));
		setMessage("4029");	
		result =false;	
	}else if(cmb_JPSDT.Value > tChk){
		cmb_JPSDT.BKColor="paleturquoise";
		alert(setPopMessage("4030"));
		setMessage("4030");	
		result =false;	
	}
	return result;
}

