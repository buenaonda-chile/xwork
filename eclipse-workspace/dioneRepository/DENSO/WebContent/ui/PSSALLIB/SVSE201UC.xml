/********************************************************************************************************** 
 * 내용설명 : host up
 * 주요변수 : 
 * ERPPC.CSV --> ds_SVSE201UC --> ds_SVSE201UC_1(거래처,품번생성) --> ds_SVSE201UC(집계)--> svh330pf
 *                              --> ds_SVSE201UC_2(error File)
 **********************************************************************************************************/ 
function setSVSE201UC(){
	if(length(trim(cal_PLNYM.Value)) < 8){
		alert(setPopMessage("4036"));  //[[확인] 정산년월을 확인바랍니다.
		setMessage("4036");	
		cal_PLNYM.SetFocus();
		return;
	}
	var ymd = substr(cal_PLNYM.Value,0,4)+"년"+substr(cal_PLNYM.Value,4,2)+"월"+substr(cal_PLNYM.Value,6,2)+"일";
	if(confirm(ymd+" 쌍용 부품소요계획 HostUP 작업입니다. 작업하시겠습니까? ") == false) return;
	
	var nowDate = today();
	div_ing.Visible = true;
	
	ds_SVSE201UC.ClearData();
	ds_SVSE201UC_0.ClearData();
	ds_SVSE201UC_1.ClearData();
	ds_SVSE201UC_2.ClearData();
	
	var file_name = "SSVAN.xls";
	var file_path = "C:\\VAN\\SSYOUNG";
	var path = file_path + "\\" + file_name;

	setMessageDirect(path);	
	ext_ExcelImportByIndex(path,0,"ds_SVSE201UC",1,1,0,4);
	//ext_ExcelImportByIndex(strExcelFilename, intSheetIndex, strTargetDatasetID, [intUseColInfo, intExp, intDecimal,intStartRow]); 
	if(ds_SVSE201UC.rowcount <1){
		div_ing.Visible = false;
		SetWaitCursor(false);
		alert("C:\\VAN\\SSYOUNG\SSVAN.xls 파일이 존재하는지 확인바랍니다.");
		return;
	}
	
	var mkpno_cvt;
	for(var i=0;i<ds_SVSE201UC.rowcount;i++){
		mkpno_cvt = ds_SVSE201UC.GetColumn(i,"MKPNO");
		if (ds_SVSE201UC.GetColumn(i,"MKPNO") == null){
			ds_SVSE201UC.SetColumn(i,"MKPNO","");
		}else{
			ds_SVSE201UC.SetColumn(i,"MKPNO",substr(mkpno_cvt,0,5)+"-"+substr(mkpno_cvt,5,5)); //OEM품번에 '-' 추가
		}
		
		if (ds_SVSE201UC.GetColumn(i,"MKPNM") == null){
			ds_SVSE201UC.SetColumn(i,"MKPNM"," ");
		}
		if (ds_SVSE201UC.GetColumn(i,"JDGUB") == null){
			ds_SVSE201UC.SetColumn(i,"JDGUB"," ");
		}
		if (ds_SVSE201UC.GetColumn(i,"JEQTY") == null){
			ds_SVSE201UC.SetColumn(i,"JEQTY","0");
		}
		if (ds_SVSE201UC.GetColumn(i,"OVQTY") == null){
			ds_SVSE201UC.SetColumn(i,"OVQTY","0");
		}
		if (ds_SVSE201UC.GetColumn(i,"SUBPL") == null){
			ds_SVSE201UC.SetColumn(i,"SUBPL"," ");
		}
	}
	//DENSO 품번 생성후 Insert
	http.Sync = true;
	var sKind		= "setSVSE201UC_1";					
	var sMethodName = "service::pssallib/insertSVSE201UC.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "ds_SVSE201UC=ds_SVSE201UC:A";         		// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_SVSE201UC_0=ds_SVSE201UC_0"; 					// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
	sArgument += " LIB1=" + quote(Lib1);
	sArgument += " LIB2=" + quote(Lib2);
	sArgument += " PYVND=" + quote("1600");  // 쌍용자동차 업체 코드
	sArgument += " PLNDT=" + quote(cal_PLNYM.Value);
	sArgument += " CURDT=" + quote(nowDate);
	sArgument += " USRID=" + quote(userID);
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
	http.Sync = false;
	
	if(ds_SVSE201UC_0.rowcount <1){
		div_ing.Visible = false;
		SetWaitCursor(false);
		alert(setPopMessage("4714"));
		setMessage("4714");
		return;
	}
	
	for(var i=0;i<ds_SVSE201UC_0.rowcount;i++){
		if (trim(ds_SVSE201UC_0.GetColumn(i,"PSPNO")) != ""){
			ds_SVSE201UC_1.addRow();
			ds_SVSE201UC_1.copyrow(ds_SVSE201UC_1.row,"ds_SVSE201UC_0",i);
		}else{
			ds_SVSE201UC_2.addRow();
			ds_SVSE201UC_2.copyrow(ds_SVSE201UC_2.row,"ds_SVSE201UC_0",i);
		}
	}
	div_ing.Visible = false;
	SetWaitCursor(false);
}
