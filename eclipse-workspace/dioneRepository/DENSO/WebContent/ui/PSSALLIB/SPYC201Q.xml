	cb_searchCodeName2.index = 0;	
}


/********************************************************************** 
 * 내용설명 : 화면조회
 * 주요변수 : 
 **********************************************************************/ 
function getSPYC201Q(){
	//조회구분 선택시 grid제목변경 및 특정컬럼숨김
	switch (rdoggub.Value){
		case '1' : 
			MainGrid.SetCellProp("Head", 0, "text", "제품");
			MainGrid.SetCellProp("Head", 1, "text", "제품명");
			break;
		case '2' :
			MainGrid.SetCellProp("Head", 0, "text", "제품");
			MainGrid.SetCellProp("Head", 1, "text", "제품명");
			break;
		case '3' :
			MainGrid.SetCellProp("Head", 0, "text", "거래처");
			MainGrid.SetCellProp("Head", 1, "text", "거래처명");
			break;
		case '4' :
			MainGrid.SetCellProp("Head", 0, "text", "거래처");
			MainGrid.SetCellProp("Head", 1, "text", "거래처명");
			break;
	}
	if (rdoggub.Value = '2' || rdoggub.Value == '4'){
		MainGrid.SetColProp(2,"Width",0);
		MainGrid.SetColProp(3,"Width",0);
	}else{
		MainGrid.SetColProp(2,"Width",115);
		MainGrid.SetColProp(3,"Width",220);
	}

	var yyyy1 = cmb_yyyy.Value;
	var yyyy2 = toNumber(cmb_yyyy.Value) + 1;
	var p_pyvnd1,p_pyvnd2,p_prdcd1,p_prdcd2;
	p_pyvnd1 = iif (length(trim(ed_pyvnd_from.Text)) == 0, '0000', trim(ed_pyvnd_from.Text));
	p_pyvnd2 = iif (length(trim(ed_pyvnd_to.Text)) == 0 , '9999', trim(ed_pyvnd_to.Text));
	p_prdcd1 = iif (length(trim(ed_prdcd_from.Text)) == 0 , '  ', trim(ed_prdcd_from.Text));
	p_prdcd2 = iif (length(trim(ed_prdcd_to.Text)) == 0 , 'ZZ', trim(ed_prdcd_to.Text));
	
	var sKind		= "getSPYC201Q";						
	var sMethodName = "service::pssallib/selectSPYC201Q.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                             				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_SPYC201Q=ds_SPYC201Q";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
	
	sArgument += "@LIB1=" + quote(Lib1);
	sArgument += "@LIB2=" + quote(Lib2);
	sArgument += "@COMPS=" + quote(cb_searchCodeName1.Value);
	sArgument += "@PLTCD=" + quote(cb_searchCodeName2.Value);
	sArgument += "@PLNGB=" + quote(cb_plngb.Value);
	sArgument += "@PLNYM04=" + quote(yyyy1+'04');
	sArgument += "@PLNYM05=" + quote(yyyy1+'05');	
	sArgument += "@PLNYM06=" + quote(yyyy1+'06');
	sArgument += "@PLNYM07=" + quote(yyyy1+'07');	
	sArgument += "@PLNYM08=" + quote(yyyy1+'08');
	sArgument += "@PLNYM09=" + quote(yyyy1+'09');	
	sArgument += "@PLNYM10=" + quote(yyyy1+'10');
	sArgument += "@PLNYM11=" + quote(yyyy1+'11');	
	sArgument += "@PLNYM12=" + quote(yyyy1+'12');
	sArgument += "@PLNYM01=" + quote(yyyy2+'01');	
	sArgument += "@PLNYM02=" + quote(yyyy2+'02');
	sArgument += "@PLNYM03=" + quote(yyyy2+'03');
	
	sArgument += "@PYVND1=" + quote(p_pyvnd1);
	sArgument += "@PYVND2=" + quote(p_pyvnd2);	
	sArgument += "@PRDCD1=" + quote(p_prdcd1);
	sArgument += "@PRDCD2=" + quote(p_prdcd2);
	sArgument += "@G_GUB="  + quote(rdoggub.Value);	//구분
	
	var str = dialog("Common::PGMLOAD_SAL.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");	
	//Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");
	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
		setMessage("1004");	
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));
		setMessage("1003");	
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}
	ed_pyvnd_from.SetFocus();
}


