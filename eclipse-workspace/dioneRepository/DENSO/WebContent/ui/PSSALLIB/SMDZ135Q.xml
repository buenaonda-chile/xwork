	var result = true;
	var sdVal = cmb_JPYYM1.Value;
	cmb_JPYYM1.BKColor="white";

	if(sdVal=="")
	{
		cmb_JPYYM1.BKColor="paleturquoise";
		alert(setPopMessage("4028"));
		setMessage("4028");	
		result =false;
	}
	return result;
}

