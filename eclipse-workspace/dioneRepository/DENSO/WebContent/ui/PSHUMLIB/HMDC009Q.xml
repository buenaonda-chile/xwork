		sArgument += " p_KNYYM11=" + quote(B11yymm);	//M+10
		sArgument += " p_KNYYM12=" + quote(B12yymm);	//M+11	
	Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "callback");
	http.Sync = false;

}
/********************************************************************** 
