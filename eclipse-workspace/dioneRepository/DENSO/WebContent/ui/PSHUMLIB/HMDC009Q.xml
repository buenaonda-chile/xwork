function getHMDC009Q(){

	var arrSABUN ="";
	if(rdoggub.Value=="F"){
		if(ds_HUMAN.rowcount > 0 ){
			arrSABUN = "('";
			for(i=0; i< ds_HUMAN.rowcount; i++){
				if(i == ds_HUMAN.rowcount-1){
					arrSABUN = arrSABUN+ds_HUMAN.GetColumn(i,"SABUN") + "')";
				}else{
					arrSABUN = arrSABUN+ds_HUMAN.GetColumn(i,"SABUN") + "','";
				}
			}
		}
	}
	
	if(!validationChk()){
		return;
	}
	ds_HMDC009Q.ClearData();

	var B1yymm,B2yymm,B3yymm,B4yymm,B5yymm,B6yymm,B7yymm,B8yymm,B9yymm,B10yymm,B11yymm,B12yymm;
	B1yymm = substr(cmb_KNYYM.Value,0,6);
	B2yymm = substr(addMonth(cmb_KNYYM.Value, 1),0,6);
	B3yymm = substr(addMonth(cmb_KNYYM.Value, 2),0,6);
	B4yymm = substr(addMonth(cmb_KNYYM.Value, 3),0,6);
	B5yymm = substr(addMonth(cmb_KNYYM.Value, 4),0,6);
	B6yymm = substr(addMonth(cmb_KNYYM.Value, 5),0,6);
	B7yymm = substr(addMonth(cmb_KNYYM.Value, 6),0,6);
	B8yymm = substr(addMonth(cmb_KNYYM.Value, 7),0,6);
	B9yymm = substr(addMonth(cmb_KNYYM.Value, 8),0,6);
	B10yymm = substr(addMonth(cmb_KNYYM.Value, 9),0,6);
	B11yymm = substr(addMonth(cmb_KNYYM.Value, 10),0,6);
	B12yymm = substr(addMonth(cmb_KNYYM.Value, 11),0,6);
	
	MainGrid.SetCellProp("Head", 18, "text", substr(B1yymm,0,4)+"년"+substr(B1yymm,4,2)+"월");
	MainGrid.SetCellProp("Head", 19, "text", substr(B2yymm,0,4)+"년"+substr(B2yymm,4,2)+"월");
	MainGrid.SetCellProp("Head", 20, "text", substr(B3yymm,0,4)+"년"+substr(B3yymm,4,2)+"월");
	MainGrid.SetCellProp("Head", 21, "text", substr(B4yymm,0,4)+"년"+substr(B4yymm,4,2)+"월");
	MainGrid.SetCellProp("Head", 22, "text", substr(B5yymm,0,4)+"년"+substr(B5yymm,4,2)+"월");
	MainGrid.SetCellProp("Head", 23, "text", substr(B6yymm,0,4)+"년"+substr(B6yymm,4,2)+"월");
	MainGrid.SetCellProp("Head", 24, "text", substr(B7yymm,0,4)+"년"+substr(B7yymm,4,2)+"월");
	MainGrid.SetCellProp("Head", 25, "text", substr(B8yymm,0,4)+"년"+substr(B8yymm,4,2)+"월");
	MainGrid.SetCellProp("Head", 26, "text", substr(B9yymm,0,4)+"년"+substr(B9yymm,4,2)+"월");
	MainGrid.SetCellProp("Head", 27, "text", substr(B10yymm,0,4)+"년"+substr(B10yymm,4,2)+"월");
	MainGrid.SetCellProp("Head", 28, "text", substr(B11yymm,0,4)+"년"+substr(B11yymm,4,2)+"월");
	MainGrid.SetCellProp("Head", 29, "text", substr(B12yymm,0,4)+"년"+substr(B12yymm,4,2)+"월");
		
	var p_YYEAR;
