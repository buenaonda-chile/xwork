		p_YYEAR=parseInt(substr(cmb_YYEAR.Value,0,4))-1;
	}else{
		p_YYEAR=substr(cmb_YYEAR.Value,0,4);
	}

	var sKind		= "getHMDC009Q";						
	var sMethodName = "service::pshumlib/selectHMDC009Q.do";     		// 서버에서 호출 될 Method 명
