function fn_HumanDelete()
{
	if(ds_HUMAN.rowcount < 1) return;			//삭제가 없으면 건너뛰기		
	ds_HUMAN.DeleteRow(ds_HUMAN.currow);	//삭제		
}
function ds_HUMAN_CanColumnChange(obj,nRow,strColumnID,varOldValue,varNewValue,nPivotIndex)
{
	if(strColumnID=="SABUN"){ //중복체크		
		if(varNewValue!=""){
			http.Sync = true;
			var sKind		= "getHMD020PF";		
			var sMethodName = "service::pscomlib/selectINSAMasterPopup.do"; 
			var sInDataSet  = "";
			var sOutDataSet = "ds_HMD020PF=ds_Select";
			var sArgument   = "";
				sArgument  += " LIB2=" + quote(Lib2);
				sArgument  += " LIB3=" + quote(Lib3);
				sArgument  += " COMPS=" + quote(ComboBoxPlant.Value); //사업부
				sArgument  += " SABUN=" + quote(varNewValue);
				sArgument  += " PAYCD=" + quote(0);
			Transaction(sKind, sMethodName, sInDataSet, sOutDataSet, sArgument, "");
			http.Sync = false;
			if (ds_HMD020PF.GetRowCount() < 1){
				alert(setPopMessage("4062")); //[확인] 사용자가 존재하지 않습니다.
				setMessage("4062");	
				return false;
			}	
			KORNM=ds_HMD020PF.GetColumn(0,"A2");
			
			if(ds_HUMAN.FindRow("SABUN",varNewValue)!="-1"){
				alert(setPopMessage("2533"));
				ds_HUMAN.SelectRow(ds_HUMAN.FindRow("SABUN",varNewValue));
				ds_HUMAN.DeleteRow(nRow);
				return false;
			}
		}
	}
}
function ds_HUMAN_CanRowPosChange(obj,nOldRow,nRow)
{
	http.usewaitcursor = false;
	var p_SABUN=ds_HUMAN.GetColumn(ds_HUMAN.currow,"SABUN"); 
	if(length(p_SABUN)==0){
		ds_HUMAN.DeleteRow(ds_HUMAN.currow);	//삭제	
	}
	return true;
	http.usewaitcursor = true;
}
function ds_HUMAN_OnColumnChanged(obj,nRow,strColumnID,varOldValue,varNewValue,nPivotIndex)
{
	http.usewaitcursor = false;
	if (strColumnID == "SABUN"){
		if(varNewValue!=""){
			ds_HUMAN.SetColumn(nRow, "KORNM", KORNM); // 거래처명 설정
		}
	}
	http.usewaitcursor = true;
}]]></Script>
