 **********************************************************************/ 
function getHAYZ082Q(){
		
	if(!validationChk()){
		return;
	}
	var p_PTJYM="";
	if(cmb_GUBUN.Value = 1){//연말정산인 경우
		p_PTJYM=substr(cmb_PTJYM.Value,0,4)+"13";
	}else{//중도정산인경우
		p_PTJYM =substr(cmb_PTJYM.Value,0,6);
	}
	
	var sKind		= "getHAYZ082Q";						
	var sMethodName = "service::pshumlib/selectHAYZ082Q.do";     		// 서버에서 호출 될 Method 명
	var sInDataSet  = "";                            				// 자료 저장 시 보낼 DataSet 명칭
	var sOutDataSet = "ds_HAYZ082Q=ds_HAYZ082Q";    				// 조회 후 돌려 받을 DataSet 명칭
	var sArgument   = "";
		sArgument += "@LIB1=" + quote(Lib1);
		sArgument += "@LIB2=" + quote(Lib2);
		sArgument += "@LIB3=" + quote(Lib3);
		sArgument += "@SABUN=" + quote(ed_SABUN.Text);			
		sArgument += "@TOTYM=" + quote(p_PTJYM);	//정산년월
		sArgument += "@MAGAM=" + quote(chkEND.Value);	//마감여부
	var str = dialog("Common::PGMLOAD_HUM.xml","sKind=" + sKind + " " + "sMethodName=" + sMethodName + " " + "sOutDataSet=" + sOutDataSet + " " + "sArgument=" + sArgument,272,64,"TitleBar=false");	
	http.Sync = false;
	
	if (str == 'T'){  //정상조회
		alert(setPopMessage("1004"));
		setMessage("1004");	
	}else if (str == 'B'){   //data값이 없을때
		alert(setPopMessage("1003"));
		setMessage("1003");	
	}else if (str == 'E'){   //error발생시
		alert(setPopMessage("1002"));
		setMessage("1002");	
	}	
}

