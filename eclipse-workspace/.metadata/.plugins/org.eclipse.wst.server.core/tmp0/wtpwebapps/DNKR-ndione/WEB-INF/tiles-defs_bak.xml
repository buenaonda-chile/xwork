    <!-- [EN] Based layout  -->
    <!-- [JP] ベースレイアウト  -->
    <definition name=".tiles.layout.base" template="/WEB-INF/jsp/base/base.jsp">
        <put-attribute name="header" value="/WEB-INF/jsp/base/header.jsp" cascade="true" />
        <put-attribute name="footer" value="/WEB-INF/jsp/base/footer.jsp" cascade="true" />
        <put-attribute name="body" value="/WEB-INF/jsp/base/body.jsp" />
    </definition>
    
