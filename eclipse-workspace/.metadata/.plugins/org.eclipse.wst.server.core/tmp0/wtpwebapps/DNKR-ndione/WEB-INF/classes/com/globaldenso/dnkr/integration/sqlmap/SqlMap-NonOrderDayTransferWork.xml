        <result property="adsdt" column="ADSDT" />        

        <result property="lib1" column="lib1" />
        <result property="lib2" column="lib2" />
        <result property="lib3" column="lib3" />     
    </resultMap>
    
    <!-- 検索 SQL文（任意条件） -->
    <select id="searchCigmaDate" resultMap="NonOrderDayTransferWork" parameterType="NonOrderDayTransferWorkDomain">
		SELECT 
			ADSDT
		FROM 
			${lib1}.SC000PR 
		WHERE 
		      'SqlMap-NonOrderDayTransferWork.searchCigmaDate' = 'SqlMap-NonOrderDayTransferWork.searchCigmaDate'
    </select>

